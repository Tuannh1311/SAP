  <source>*----------------------------------------------------------------------*
***INCLUDE ZVNFI_CF_MAINTENANCE_I01.
*----------------------------------------------------------------------*
*&amp;---------------------------------------------------------------------*
*&amp;      Module  EXIT  INPUT
*&amp;---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
MODULE exit INPUT.
  IF ok_code = &apos;BACK&apos; OR ok_code = &apos;EXIT&apos; OR ok_code = &apos;CANC&apos;.
    FREE ok_code.
    LEAVE TO SCREEN 0.
  ENDIF.
ENDMODULE.
*&amp;---------------------------------------------------------------------*
*&amp;      Module  USER_COMMAND_0100  INPUT
*&amp;---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
MODULE user_command_0100 INPUT.
  DATA(command) = ok_code.
  FREE ok_code.
  CASE command.
    WHEN &apos;LAYOUTD&apos;.
      CALL TRANSACTION &apos;ZFICF_LAYOUT_SHOW&apos;.
    WHEN &apos;LAYOUTE&apos; .
      CALL TRANSACTION &apos;ZFICF_LAYOUT_EDIT&apos;.
    WHEN &apos;TOTALD&apos;.
      CALL TRANSACTION &apos;ZFICF_TOTALS_SHOW&apos;.
    WHEN &apos;TOTALE&apos; .
      CALL TRANSACTION &apos;ZFICF_TOTALS_EDIT&apos;.
    WHEN &apos;CONSTD&apos;.
      CALL TRANSACTION &apos;ZFICF_CONSTANTS_SHOW&apos;.
    WHEN &apos;CONSTE&apos; .
      CALL TRANSACTION &apos;ZFICF_CONSTANTS_EDIT&apos;.
    WHEN &apos;TEXTD&apos;.
      CALL TRANSACTION &apos;ZFICF_TEXTS_SHOW&apos;.
    WHEN &apos;TEXTE&apos; .
      CALL TRANSACTION &apos;ZFICF_TEXTS_EDIT&apos;.
    WHEN &apos;TYPED&apos;.
      CALL TRANSACTION &apos;ZFICF_DOCTYPES_SHOW&apos;.
    WHEN &apos;TYPEE&apos; .
      CALL TRANSACTION &apos;ZFICF_DOCTYPES_EDIT&apos;.
    WHEN &apos;ACCD&apos;.
      CALL TRANSACTION &apos;ZFICF_ACCOUNTS_SHOW&apos;.
    WHEN &apos;ACCE&apos; .
      CALL TRANSACTION &apos;ZFICF_ACCOUNTS_EDIT&apos;.
      WHEN &apos;ACCD&apos;.
      CALL TRANSACTION &apos;ZFICF_ACCOUNTS_SHOW&apos;.
    WHEN &apos;COLE&apos; .
      CALL TRANSACTION &apos;ZFICF_COLUMNS_EDIT&apos;.
    WHEN &apos;COLD&apos; .
      CALL TRANSACTION &apos;ZFICF_COLUMNS_SHOW&apos;.
  ENDCASE.
ENDMODULE.</source>
 </PROG>
