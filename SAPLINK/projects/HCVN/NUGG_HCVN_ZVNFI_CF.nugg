  <interfaceMethod CLSNAME="ZFI_CF_REPORT" CPDNAME="ZFI_REPORT_SUB_VAL~SUBSTITUTION_GET_TRIAL_BALANCE">
   <source>METHOD zfi_report_sub_val~substitution_get_trial_balance.
    DATA: posting_date_prev_low  TYPE d VALUE &apos;11110101&apos;,
          posting_date_prev_high TYPE d,
          posting_date_act_low   TYPE d,
          posting_date_act_high  TYPE d,
          lt_balance_prev        TYPE TABLE OF zficf_report_trb,
          lt_balance             TYPE TABLE OF zficf_report_trb2.
    posting_date_act_low   =  gjahr &amp;&amp; poper+1(2) &amp;&amp; &apos;01&apos;.
    posting_date_prev_high = prev_gjahr &amp;&amp; prev_poper+1(2) &amp;&amp; &apos;01&apos;.
    CALL FUNCTION &apos;HR_HCP_GET_LAST_DAY_OF_MONTH&apos;
      EXPORTING
        im_date              = posting_date_prev_high
      IMPORTING
        ex_last_day_of_month = posting_date_prev_high.
    CALL FUNCTION &apos;HR_HCP_GET_LAST_DAY_OF_MONTH&apos;
      EXPORTING
        im_date              = posting_date_act_low
      IMPORTING
        ex_last_day_of_month = posting_date_act_high.

    SELECT
       SUM( debitamountincocodecrcy ) AS prev_debitamount,
       SUM( creditamountincocodecrcy ) AS prev_creditamount,
       currencycode AS currency
       FROM zfi_trial_balance( p_frompostingdate = @posting_date_prev_low,
                             p_topostingdate = @posting_date_prev_high  )

       WHERE companycode = @bukrs
         AND ledger = @rldnr
         AND glaccount IN ( SELECT hkont FROM zficf_accounts  WHERE link_id = @link_id )
         GROUP BY currencycode
         INTO CORRESPONDING FIELDS OF TABLE @lt_balance_prev.

    SELECT
      SUM( debitamountincocodecrcy ) AS debitamount,
      SUM( creditamountincocodecrcy ) AS creditamount,
      currencycode AS currency
      FROM zfi_trial_balance( p_frompostingdate = @posting_date_act_low,
                            p_topostingdate = @posting_date_act_high  )

      WHERE companycode = @bukrs
        AND ledger = @rldnr
        AND glaccount IN ( SELECT hkont FROM zficf_accounts  WHERE link_id = @link_id )
        GROUP BY currencycode
        INTO CORRESPONDING FIELDS OF TABLE @lt_balance.

*    LOOP AT lt_balance REFERENCE INTO DATA(balance).
*      IF abs( balance-&gt;debitamount ) GT abs( balance-&gt;creditamount ).
*        DATA(closing_debit) = abs( balance-&gt;debitamount ) - abs( balance-&gt;creditamount ).
*      ELSE.
*        DATA(closing_credit) = abs( balance-&gt;creditamount ) - abs( balance-&gt;debitamount ).
*      ENDIF.
*      LOOP AT lt_balance_prev REFERENCE INTO DATA(balance_prev).
*        IF abs( balance_prev-&gt;prev_debitamount ) GT abs( balance_prev-&gt;prev_creditamount ).
*          DATA(closing_debit_prev) = abs( balance_prev-&gt;prev_debitamount ) - abs( balance_prev-&gt;prev_creditamount ).
*        ELSE.
*          DATA(closing_credit_prev) = abs( balance_prev-&gt;prev_creditamount ) - abs( balance_prev-&gt;prev_debitamount ).
*        ENDIF.
*      ENDLOOP.
*    ENDLOOP.
    LOOP AT lt_balance_prev REFERENCE INTO DATA(balance_prev).
      IF abs( balance_prev-&gt;prev_debitamount ) GT abs( balance_prev-&gt;prev_creditamount ).
        DATA(closing_debit_prev) = abs( balance_prev-&gt;prev_debitamount ) - abs( balance_prev-&gt;prev_creditamount ).
      ELSE.
        DATA(closing_credit_prev) = abs( balance_prev-&gt;prev_creditamount ) - abs( balance_prev-&gt;prev_debitamount ).
      ENDIF.
      LOOP AT lt_balance REFERENCE INTO DATA(balance).
        DATA(balance_debit) = abs( balance-&gt;debitamount ) + abs( closing_debit_prev ).
        DATA(balance_credit) = abs( balance-&gt;creditamount ) + abs( closing_credit_prev ).
        IF abs( balance_debit ) GT abs( balance_credit ).
          DATA(closing_debit) = abs( balance_debit ) - abs( balance_credit ).
        ELSE.
          DATA(closing_credit) = abs( balance_credit ) - abs( balance_debit ).
        ENDIF.
      ENDLOOP.
      IF sy-subrc NE 0.
        balance_debit = abs( closing_debit_prev ).
        balance_credit = abs( closing_credit_prev ).
        IF abs( balance_debit ) GT abs( balance_credit ).
          closing_debit = abs( balance_debit ) - abs( balance_credit ).
        ELSE.
          closing_credit = abs( balance_credit ) - abs( balance_debit ).
        ENDIF.
      ENDIF.
    ENDLOOP.
    IF sy-subrc NE 0.
      LOOP AT lt_balance REFERENCE INTO balance.
        balance_debit = abs( balance-&gt;debitamount ).
        balance_credit = abs( balance-&gt;creditamount ).
        IF abs( balance_debit ) GT abs( balance_credit ).
          closing_debit = abs( balance_debit ) - abs( balance_credit ).
        ELSE.
          closing_credit = abs( balance_credit ) - abs( balance_debit ).
        ENDIF.
      ENDLOOP.
    ENDIF.
*  IF NOT closing_debit IS INITIAL.
*    MOVE closing_debit TO amount.
*  ELSE.
*    MOVE closing_credit TO amount.
*  ENDIF.
    amount = ( closing_debit_prev - closing_credit_prev ) - ( closing_debit - closing_credit ).
    amount = amount / 100.
  ENDMETHOD.</source>
  </interfaceMethod>
