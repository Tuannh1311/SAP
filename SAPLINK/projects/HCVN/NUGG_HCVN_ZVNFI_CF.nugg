   <parameter CLSNAME="ZFI_CF_REPORT" CMPNAME="GET_AMOUNT_BALANCE" SCONAME="LINK_ID" VERSION="1" LANGU="E" DESCRIPT="CF Maitenance Link ID" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="ZFICF_LINK_ID"/>
   <parameter CLSNAME="ZFI_CF_REPORT" CMPNAME="GET_AMOUNT_BALANCE" SCONAME="PREVIOUS" VERSION="1" LANGU="E" DESCRIPT="Data element for domain BOOLE: TRUE (=&apos;X&apos;) and FALSE (=&apos; &apos;)" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="BOOLE_D" PAROPTIONL="X"/>
   <parameter CLSNAME="ZFI_CF_REPORT" CMPNAME="GET_AMOUNT_BALANCE" SCONAME="AMOUNT" VERSION="1" LANGU="E" DESCRIPT="Amount in Company Code Currency" CMPTYPE="1" MTDTYPE="0" EDITORDER="3 " DISPID="0 " PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="FINS_VHCUR12"/>
   <source>METHOD get_amount_balance.
    IF previous = abap_false.
      DATA(l_year) = gjahr.
      DATA(l_poper) = poper.
    ELSE.
      l_year = prev_gjahr.
      l_poper = prev_poper.
    ENDIF.
    DATA(fiscalper) = l_year &amp;&amp; l_poper.
    SELECT SUM( hsl )
      INTO @DATA(credit)
      FROM zficf_credit_amount
      WHERE link_id = @link_id
        AND rldnr = @rldnr
        AND rbukrs = @bukrs
        AND gjahr = @l_year &quot;hq_dvy SAP-3133
        AND fiscyearper LE @fiscalper.
    SELECT SUM( hsl )
    INTO @DATA(debit)
    FROM zficf_debit_amount
    WHERE link_id = @link_id
      AND rldnr = @rldnr
      AND rbukrs = @bukrs
      AND gjahr = @l_year &quot;hq_dvy SAP-3133
      AND fiscyearper LE @fiscalper.
    amount = credit + debit.
  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZFI_CF_REPORT" CMPNAME="GET_CONSTANTS" VERSION="1" LANGU="E" EXPOSURE="0" STATE="1" EDITORDER="4 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZFI_CF_REPORT" CMPNAME="GET_CONSTANTS" SCONAME="LINK_ID" VERSION="1" LANGU="E" DESCRIPT="CF Maitenance Link ID" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="ZFICF_LINK_ID"/>
   <parameter CLSNAME="ZFI_CF_REPORT" CMPNAME="GET_CONSTANTS" SCONAME="DATA" VERSION="1" LANGU="E" DESCRIPT="FI: CF report constants" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="ZFICF_CONSTANTS"/>
   <source>METHOD get_constants.
    SELECT SINGLE * FROM zficf_constants
                    INTO data
                    WHERE link_id = link_id.
  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZFI_CF_REPORT" CMPNAME="GET_DATA" VERSION="1" LANGU="E" DESCRIPT="Get report data in output format" EXPOSURE="0" STATE="1" EDITORDER="1 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" MTDNEWEXC="X" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZFI_CF_REPORT" CMPNAME="GET_DATA" SCONAME="I_REPORT_TYPE" VERSION="1" LANGU="E" DESCRIPT="CF Maintenance Report Type" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="ZFICF_REPORT_TYPE" PAROPTIONL="X"/>
   <parameter CLSNAME="ZFI_CF_REPORT" CMPNAME="GET_DATA" SCONAME="DATA" VERSION="1" LANGU="E" DESCRIPT="CF reporting output" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="ZFICF_REPORT_T"/>
   <exception CLSNAME="ZFI_CF_REPORT" CMPNAME="GET_DATA" SCONAME="ZCX_FI_CF_REPORT" VERSION="1" LANGU="E" DESCRIPT="FI: Cash Flow Reporting exceptions" MTDTYPE="0" EDITORDER="1 "/>
   <source>METHOD get_data.
    DATA(lv_report_type) = report_type.
    IF NOT i_report_type IS INITIAL.
      lv_report_type = i_report_type.
    ENDIF.
    TRY.
        DATA(layout_t) =  get_report_structure( lv_report_type ).
      CATCH zcx_fi_cf_report INTO DATA(exceptions).
        RAISE EXCEPTION exceptions.
    ENDTRY.
*reports with data based on other ones
    IF lv_report_type = &apos;SBV&apos;.
      DATA(data_vas) = get_data( &apos;VAS&apos; ).
      DATA(data_bs) = get_data( &apos;GBS&apos; ).
    ELSEIF lv_report_type = &apos;SCF&apos;.
      DATA(data_plv) = get_data( &apos;PLV&apos; ).
      DATA(data_bsv) = get_data( &apos;BSV&apos; ).
    ELSEIF lv_report_type = &apos;BSI&apos;.
      DATA(data_pli) = get_data( &apos;PLI&apos; ).
    ENDIF.
    LOOP AT layout_t REFERENCE INTO DATA(layout).
      IF layout-&gt;hidden = abap_true.
        CONTINUE.
      ENDIF.
      APPEND INITIAL LINE TO data REFERENCE INTO DATA(entry).
      entry-&gt;link_id = layout-&gt;link_id.
      entry-&gt;waers = c_local_currency.
      DATA(stext) = get_text( link_id = entry-&gt;link_id
                              i_langu = &apos;E&apos; ).
      entry-&gt;text_en = stext-text.
      stext = get_text( link_id = entry-&gt;link_id
                              i_langu = &apos;ÏÅ©&apos; ).
      entry-&gt;text_vn = stext-text.
      IF layout-&gt;line_type = c_line_free.
        CONTINUE.
      ENDIF.
      DATA(constants) = get_constants( entry-&gt;link_id ).
      IF NOT constants IS INITIAL.
        MOVE-CORRESPONDING constants TO entry-&gt;*.
      ENDIF.
      IF layout-&gt;line_type = c_line_amount.
* balance per period
        IF lv_report_type = &apos;VAS&apos; OR lv_report_type = &apos;SBV&apos; OR lv_report_type = &apos;SCF&apos;.
          entry-&gt;amount_actual = get_amount( link_id = entry-&gt;link_id ).
          entry-&gt;amount_previous = get_amount( link_id = entry-&gt;link_id
                                               previous = abap_true ).
          entry-&gt;amount_delta = entry-&gt;amount_previous - entry-&gt;amount_actual.
        ELSEIF lv_report_type = &apos;PLV&apos; OR lv_report_type = &apos;GPL&apos; OR lv_report_type = &apos;PLI&apos;.
* balance per period + cumulative
          entry-&gt;amount_actual = get_amount( link_id = entry-&gt;link_id ).
          entry-&gt;amount_previous = get_amount( link_id = entry-&gt;link_id
                                               previous = abap_true ).
          entry-&gt;amount_delta = entry-&gt;amount_previous - entry-&gt;amount_actual.
          entry-&gt;amount_actual_year = get_amount_balance( link_id = entry-&gt;link_id ).
          entry-&gt;amount_previous_year = get_amount_balance( link_id = entry-&gt;link_id
                                                        previous = abap_true ).
          entry-&gt;amount_delta_year = entry-&gt;amount_previous_year - entry-&gt;amount_actual_year.
        ELSE.
* cumulative balance
          entry-&gt;amount_actual = get_amount_balance( link_id = entry-&gt;link_id ).
          entry-&gt;amount_previous = get_amount_balance( link_id = entry-&gt;link_id
                                                        previous = abap_true ).
          entry-&gt;amount_delta = entry-&gt;amount_previous - entry-&gt;amount_actual.
        ENDIF.
      ENDIF.
    ENDLOOP.
*substitutions
    LOOP AT layout_t REFERENCE INTO layout WHERE substitution &lt;&gt; space.
      LOOP AT data REFERENCE INTO DATA(amount_entry) WHERE link_id = layout-&gt;link_id.
        DATA(method) = &apos;ZFI_REPORT_SUB_VAL~&apos; &amp;&amp; layout-&gt;substitution.
        CALL METHOD (method)
          EXPORTING
            link_id = layout-&gt;link_id
          CHANGING
            amount  = amount_entry-&gt;amount_actual.
        CALL METHOD (method)
          EXPORTING
            link_id  = layout-&gt;link_id
            previous = abap_true
          CHANGING
            amount   = amount_entry-&gt;amount_previous.
        IF layout-&gt;substitution = &apos;SUBSTITUTION_GET_OPOSITE&apos;.
          CALL METHOD (method)
            EXPORTING
              link_id = layout-&gt;link_id
            CHANGING
              amount  = amount_entry-&gt;amount_actual_year.
          CALL METHOD (method)
            EXPORTING
              link_id  = layout-&gt;link_id
              previous = abap_true
            CHANGING
              amount   = amount_entry-&gt;amount_previous_year.
        ENDIF.
      ENDLOOP.
    ENDLOOP.
*validations
    LOOP AT layout_t REFERENCE INTO layout WHERE validation &lt;&gt; space.
      LOOP AT data REFERENCE INTO amount_entry WHERE link_id = layout-&gt;link_id.
        method = &apos;ZFI_REPORT_SUB_VAL~&apos; &amp;&amp; layout-&gt;validation.
        CALL METHOD (method)
          EXPORTING
            link_id = layout-&gt;link_id
          CHANGING
            amount  = amount_entry-&gt;amount_actual.
        CALL METHOD (method)
          EXPORTING
            link_id  = layout-&gt;link_id
            previous = abap_true
          CHANGING
            amount   = amount_entry-&gt;amount_previous.
      ENDLOOP.
    ENDLOOP.
*substitutions after validation result
    LOOP AT layout_t REFERENCE INTO layout WHERE final_substitutio &lt;&gt; space.
      LOOP AT data REFERENCE INTO amount_entry WHERE link_id = layout-&gt;link_id.
        method = &apos;ZFI_REPORT_SUB_VAL~&apos; &amp;&amp; layout-&gt;final_substitutio.
        CALL METHOD (method)
          EXPORTING
            link_id = layout-&gt;link_id
          CHANGING
            amount  = amount_entry-&gt;amount_actual.
        CALL METHOD (method)
          EXPORTING
            link_id  = layout-&gt;link_id
            previous = abap_true
          CHANGING
            amount   = amount_entry-&gt;amount_previous.
      ENDLOOP.
    ENDLOOP.
*delta for BSV after validation / substitution
    IF lv_report_type = &apos;BSV&apos;.
      LOOP AT data REFERENCE INTO entry.
        entry-&gt;amount_delta = entry-&gt;amount_previous - entry-&gt;amount_actual.
      ENDLOOP.
    ENDIF.
* totals
    DATA(try_again) = abap_true.
    DO 15 TIMES. &quot;sorry for this but we&apos;re catching totals from not processed totals :-)
      IF try_again = abap_false.
        EXIT.
      ENDIF.
      FREE try_again.
      LOOP AT layout_t REFERENCE INTO layout WHERE line_type = c_line_total.
        READ TABLE data REFERENCE INTO entry WITH KEY link_id = layout-&gt;link_id.
        IF sy-subrc NE 0.
          CONTINUE.
        ENDIF.
        FREE: entry-&gt;amount_actual , entry-&gt;amount_previous, entry-&gt;amount_actual_year, entry-&gt;amount_previous_year .
        DATA(range) = get_total_range( link_id = entry-&gt;link_id
                                       operation = &apos;+&apos; ).
        IF NOT range IS INITIAL.
* report own data
          LOOP AT data REFERENCE INTO amount_entry WHERE link_id IN range.
            ADD: amount_entry-&gt;amount_actual TO entry-&gt;amount_actual,
                 amount_entry-&gt;amount_previous TO entry-&gt;amount_previous,
                 amount_entry-&gt;amount_actual_year TO entry-&gt;amount_actual_year,
                 amount_entry-&gt;amount_previous_year TO entry-&gt;amount_previous_year.
          ENDLOOP.
* other dependent reports source data
          IF sy-subrc NE 0 AND  lv_report_type = &apos;SBV&apos;.
            LOOP AT data_vas REFERENCE INTO amount_entry WHERE link_id IN range.
              ADD: amount_entry-&gt;amount_actual TO entry-&gt;amount_actual,
                   amount_entry-&gt;amount_previous TO entry-&gt;amount_previous,
                 amount_entry-&gt;amount_actual_year TO entry-&gt;amount_actual_year,
                 amount_entry-&gt;amount_previous_year TO entry-&gt;amount_previous_year.
            ENDLOOP.
            IF sy-subrc NE 0.
              LOOP AT data_bs REFERENCE INTO amount_entry WHERE link_id IN range.
                ADD: amount_entry-&gt;amount_actual TO entry-&gt;amount_actual,
                     amount_entry-&gt;amount_previous TO entry-&gt;amount_previous,
                 amount_entry-&gt;amount_actual_year TO entry-&gt;amount_actual_year,
                 amount_entry-&gt;amount_previous_year TO entry-&gt;amount_previous_year.
              ENDLOOP.
            ENDIF.
          ELSEIF sy-subrc NE 0 AND  lv_report_type = &apos;SCF&apos;.
            LOOP AT data_plv REFERENCE INTO amount_entry WHERE link_id IN range.
              ADD: amount_entry-&gt;amount_actual TO entry-&gt;amount_actual,
                   amount_entry-&gt;amount_previous TO entry-&gt;amount_previous,
               amount_entry-&gt;amount_actual_year TO entry-&gt;amount_actual_year,
               amount_entry-&gt;amount_previous_year TO entry-&gt;amount_previous_year.
            ENDLOOP.
            LOOP AT data_bsv REFERENCE INTO amount_entry WHERE link_id IN range.
              ADD: amount_entry-&gt;amount_delta TO entry-&gt;amount_actual.
            ENDLOOP.
          ELSEIF sy-subrc NE 0 AND  lv_report_type = &apos;BSI&apos;.
            LOOP AT data_pli REFERENCE INTO amount_entry WHERE link_id IN range.
              ADD: amount_entry-&gt;amount_actual TO entry-&gt;amount_actual,
                   amount_entry-&gt;amount_previous TO entry-&gt;amount_previous,
               amount_entry-&gt;amount_actual_year TO entry-&gt;amount_actual_year,
               amount_entry-&gt;amount_previous_year TO entry-&gt;amount_previous_year.
            ENDLOOP.
          ELSE.
            try_again = abap_true.
          ENDIF.
        ENDIF.
        range = get_total_range( link_id = entry-&gt;link_id
                                       operation = &apos;-&apos; ).
        IF NOT range IS INITIAL.
          LOOP AT data REFERENCE INTO amount_entry WHERE link_id IN range.
            SUBTRACT : amount_entry-&gt;amount_actual FROM entry-&gt;amount_actual,
                       amount_entry-&gt;amount_previous FROM entry-&gt;amount_previous,
                 amount_entry-&gt;amount_actual_year FROM entry-&gt;amount_actual_year,
                 amount_entry-&gt;amount_previous_year FROM entry-&gt;amount_previous_year.
          ENDLOOP.
          IF sy-subrc NE 0 AND  lv_report_type = &apos;SBV&apos;.
            LOOP AT data_vas REFERENCE INTO amount_entry WHERE link_id IN range.
              SUBTRACT : amount_entry-&gt;amount_actual FROM entry-&gt;amount_actual,
                         amount_entry-&gt;amount_previous FROM entry-&gt;amount_previous,
                 amount_entry-&gt;amount_actual_year FROM entry-&gt;amount_actual_year,
                 amount_entry-&gt;amount_previous_year FROM entry-&gt;amount_previous_year.
            ENDLOOP.
            IF sy-subrc NE 0.
              LOOP AT data_bs REFERENCE INTO amount_entry WHERE link_id IN range.
                SUBTRACT : amount_entry-&gt;amount_actual FROM entry-&gt;amount_actual,
                           amount_entry-&gt;amount_previous FROM entry-&gt;amount_previous,
                 amount_entry-&gt;amount_actual_year FROM entry-&gt;amount_actual_year,
                 amount_entry-&gt;amount_previous_year FROM entry-&gt;amount_previous_year.
              ENDLOOP.
            ENDIF.
          ELSEIF sy-subrc NE 0 AND  lv_report_type = &apos;SCF&apos;.
            LOOP AT data_plv REFERENCE INTO amount_entry WHERE link_id IN range.
              SUBTRACT : amount_entry-&gt;amount_actual FROM entry-&gt;amount_actual,
                           amount_entry-&gt;amount_previous FROM entry-&gt;amount_previous,
                 amount_entry-&gt;amount_actual_year FROM entry-&gt;amount_actual_year,
                 amount_entry-&gt;amount_previous_year FROM entry-&gt;amount_previous_year..
            ENDLOOP.
            LOOP AT data_bsv REFERENCE INTO amount_entry WHERE link_id IN range.
              SUBTRACT amount_entry-&gt;amount_delta FROM entry-&gt;amount_actual.
            ENDLOOP.
          ELSEIF sy-subrc NE 0 AND  lv_report_type = &apos;BSI&apos;.
            LOOP AT data_pli REFERENCE INTO amount_entry WHERE link_id IN range.
              SUBTRACT : amount_entry-&gt;amount_actual FROM entry-&gt;amount_actual,
                           amount_entry-&gt;amount_previous FROM entry-&gt;amount_previous,
                 amount_entry-&gt;amount_actual_year FROM entry-&gt;amount_actual_year,
                 amount_entry-&gt;amount_previous_year FROM entry-&gt;amount_previous_year..
            ENDLOOP.
          ELSE.
            try_again = abap_true.
          ENDIF.
        ENDIF.
        entry-&gt;amount_delta = entry-&gt;amount_previous - entry-&gt;amount_actual.
        entry-&gt;amount_delta_year = entry-&gt;amount_previous_year - entry-&gt;amount_actual_year.
      ENDLOOP.
    ENDDO.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZFI_CF_REPORT" CMPNAME="GET_OUTPUT" VERSION="1" LANGU="E" EXPOSURE="2" STATE="1" EDITORDER="7 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZFI_CF_REPORT" CMPNAME="GET_OUTPUT" SCONAME="SALV" VERSION="1" LANGU="E" DESCRIPT="Basis Class for Simple Tables" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="3" TYPE="CL_SALV_TABLE"/>
   <source>METHOD get_output.
    TRY.
        DATA(data) = get_data( ).
      CATCH zcx_fi_cf_report INTO DATA(exceptions).
        RAISE EXCEPTION exceptions.
    ENDTRY.
    TRY.
        cl_salv_table=&gt;factory( IMPORTING r_salv_table = salv
                                CHANGING  t_table      = data ).
      CATCH cx_salv_msg.
    ENDTRY.
*hide columns by report type
    SELECT SINGLE * FROM zficf_columns
                    INTO @DATA(columns_display)
                    WHERE report_type = @report_type.
    IF sy-subrc EQ 0.
      DATA(out_components) = CAST cl_abap_structdescr( cl_abap_typedescr=&gt;describe_by_name( &apos;ZFICF_COLUMNS&apos; ) )-&gt;components .
      LOOP AT out_components REFERENCE INTO DATA(component) FROM 3.
        ASSIGN COMPONENT component-&gt;name OF STRUCTURE columns_display TO FIELD-SYMBOL(&lt;fs_value&gt;).
        TRY.
            CAST cl_salv_column_table( salv-&gt;get_columns( )-&gt;get_column( component-&gt;name ) )-&gt;set_visible( &lt;fs_value&gt; ).

          CATCH cx_salv_data_error.
          CATCH cx_salv_not_found.
        ENDTRY.
      ENDLOOP.
    ENDIF.
* set currency reference to amount fields
    out_components = CAST cl_abap_structdescr( cl_abap_typedescr=&gt;describe_by_name( &apos;ZFICF_REPORT&apos; ) )-&gt;components .
    LOOP AT out_components REFERENCE INTO component.
      IF component-&gt;type_kind = &apos;P&apos;.
        ASSIGN COMPONENT component-&gt;name OF STRUCTURE columns_display TO &lt;fs_value&gt;.
        TRY.
            CAST cl_salv_column_table( salv-&gt;get_columns( )-&gt;get_column( component-&gt;name ) )-&gt;set_currency_column( &apos;WAERS&apos; ).
            CAST cl_salv_column_table( salv-&gt;get_columns( )-&gt;get_column( component-&gt;name ) )-&gt;set_fixed_header_text( &apos;L&apos; ).
          CATCH cx_salv_data_error.
          CATCH cx_salv_not_found.
        ENDTRY.
      ENDIF.
    ENDLOOP.
    CAST cl_salv_column_table( salv-&gt;get_columns( )-&gt;get_column( &apos;TEXT_EN&apos; ) )-&gt;set_leading_spaces( abap_true ).
    CAST cl_salv_column_table( salv-&gt;get_columns( )-&gt;get_column( &apos;TEXT_VN&apos; ) )-&gt;set_leading_spaces( abap_true ).
    DATA(columns) = salv-&gt;get_columns( ).
    columns-&gt;set_optimize( abap_true ) .
    salv-&gt;set_screen_status( pfstatus      = c_pfkey
                             report        = c_repid
                             set_functions = salv-&gt;c_functions_all ).
    set_report_heading( changing salv = salv ).

    salv-&gt;display( ).
  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZFI_CF_REPORT" CMPNAME="GET_REPORT_STRUCTURE" VERSION="1" LANGU="E" EXPOSURE="0" STATE="1" EDITORDER="3 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" MTDNEWEXC="X" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZFI_CF_REPORT" CMPNAME="GET_REPORT_STRUCTURE" SCONAME="I_REPORT_TYPE" VERSION="1" LANGU="E" DESCRIPT="CF Maintenance Report Type" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="ZFICF_REPORT_TYPE" PAROPTIONL="X"/>
   <parameter CLSNAME="ZFI_CF_REPORT" CMPNAME="GET_REPORT_STRUCTURE" SCONAME="DATA" VERSION="1" LANGU="E" DESCRIPT="FI: CF report layout" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="ZFICF_LAYOUT_T"/>
   <exception CLSNAME="ZFI_CF_REPORT" CMPNAME="GET_REPORT_STRUCTURE" SCONAME="ZCX_FI_CF_REPORT" VERSION="1" LANGU="E" DESCRIPT="FI: Cash Flow Reporting exceptions" MTDTYPE="0" EDITORDER="1 "/>
   <source>METHOD get_report_structure.
    DATA(lv_report_type) = report_type.
    IF NOT i_report_type IS INITIAL.
      lv_report_type = i_report_type.
    ENDIF.
    SELECT * FROM zficf_layout
             INTO TABLE data
             WHERE report_type = lv_report_type
      ORDER BY sequence.
  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZFI_CF_REPORT" CMPNAME="GET_TEXT" VERSION="1" LANGU="E" EXPOSURE="0" STATE="1" EDITORDER="5 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZFI_CF_REPORT" CMPNAME="GET_TEXT" SCONAME="LINK_ID" VERSION="1" LANGU="E" DESCRIPT="CF Maitenance Link ID" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="ZFICF_LINK_ID"/>
   <parameter CLSNAME="ZFI_CF_REPORT" CMPNAME="GET_TEXT" SCONAME="I_LANGU" VERSION="1" LANGU="E" DESCRIPT="Language Key" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="LANGU" PAROPTIONL="X"/>
   <parameter CLSNAME="ZFI_CF_REPORT" CMPNAME="GET_TEXT" SCONAME="DATA" VERSION="1" LANGU="E" DESCRIPT="FI: CF report texts" CMPTYPE="1" MTDTYPE="0" EDITORDER="3 " DISPID="0 " PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="ZFICF_TEXTS"/>
   <source>METHOD get_text.
    FREE data.
    DATA(lv_langu) = i_langu.
    IF i_langu IS INITIAL.
      MOVE langu TO lv_langu.
    ENDIF.
    SELECT SINGLE * FROM zficf_texts
             INTO data
             WHERE link_id = link_id
               AND langu = lv_langu.
  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZFI_CF_REPORT" CMPNAME="GET_TOTAL_RANGE" VERSION="1" LANGU="E" EXPOSURE="0" STATE="1" EDITORDER="8 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZFI_CF_REPORT" CMPNAME="GET_TOTAL_RANGE" SCONAME="LINK_ID" VERSION="1" LANGU="E" DESCRIPT="CF reporting output" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="ZFICF_LINK_ID"/>
   <parameter CLSNAME="ZFI_CF_REPORT" CMPNAME="GET_TOTAL_RANGE" SCONAME="OPERATION" VERSION="1" LANGU="E" DESCRIPT="Single-Character Flag" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="CHAR1"/>
   <parameter CLSNAME="ZFI_CF_REPORT" CMPNAME="GET_TOTAL_RANGE" SCONAME="RANGE" VERSION="1" LANGU="E" DESCRIPT="FI: Cask flow Link ID range" CMPTYPE="1" MTDTYPE="0" EDITORDER="3 " DISPID="0 " PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="ZFICF_RANGE_LINK_ID_T"/>
   <source>METHOD get_total_range.
    FREE range.
    CASE operation.
      WHEN &apos;+&apos;.
        SELECT zsign AS sign
               zoption AS option
               zlow AS low
               zhigh AS high
               FROM zficf_totals
               INTO CORRESPONDING FIELDS OF TABLE range
               WHERE link_id = link_id
                 AND subtract = abap_false.
      WHEN &apos;-&apos;.
        SELECT zsign AS sign
           zoption AS option
           zlow AS low
           zhigh AS high
           FROM zficf_totals
           INTO CORRESPONDING FIELDS OF TABLE range
           WHERE link_id = link_id
             AND subtract = abap_true.
    ENDCASE.
  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZFI_CF_REPORT" CMPNAME="SET_REPORT_HEADING" VERSION="1" LANGU="E" EXPOSURE="0" STATE="1" EDITORDER="10 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZFI_CF_REPORT" CMPNAME="SET_REPORT_HEADING" SCONAME="SALV" VERSION="1" LANGU="E" DESCRIPT="Basis Class for Simple Tables" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="2" PARPASSTYP="1" TYPTYPE="3" TYPE="CL_SALV_TABLE"/>
   <source>METHOD set_report_heading.
    DATA: lr_grid   TYPE REF TO cl_salv_form_layout_grid,
          lr_grid_1 TYPE REF TO cl_salv_form_layout_grid,
          lr_flow   TYPE REF TO cl_salv_form_layout_flow,
          lr_label  TYPE REF TO cl_salv_form_label,
          lr_text   TYPE REF TO cl_salv_form_text,
          l_text    TYPE string.
    CREATE OBJECT lr_grid.
    lr_grid_1 = lr_grid-&gt;create_grid( row = 1
                                      column = 1 ).
    lr_flow = lr_grid_1-&gt;create_flow( row = 1
                                      column = 1 ).
    lr_label = lr_flow-&gt;create_label( text = &apos;Report type:&apos;
                                      tooltip = &apos;Report type:&apos; ).
    lr_text = lr_flow-&gt;create_text( text = report_type
                                    tooltip = report_type ).
    lr_flow = lr_grid_1-&gt;create_flow( row = 1
                                      column = 2 ).
    lr_label = lr_flow-&gt;create_label( text = &apos;Company Code:&apos;
                                      tooltip = &apos;Company Code:&apos; ).
    lr_text = lr_flow-&gt;create_text( text = bukrs
                                    tooltip = bukrs ).
    lr_flow = lr_grid_1-&gt;create_flow( row = 1
                                      column = 3 ).
    lr_label = lr_flow-&gt;create_label( text = &apos;Ledger:&apos;
                                      tooltip = &apos;Ledger:&apos; ).
    lr_text = lr_flow-&gt;create_text( text = rldnr
                                    tooltip = rldnr ).
    lr_flow = lr_grid_1-&gt;create_flow( row = 2
                                      column = 1 ).
    lr_label = lr_flow-&gt;create_label( text = &apos;Actual Period / Fiscal year:&apos;
                                      tooltip = &apos;Actual Period / Fiscal year:&apos; ).
    DATA(act_period) = poper &amp;&amp; &apos;/&apos; &amp;&amp; gjahr.
    lr_text = lr_flow-&gt;create_text( text = act_period
                                    tooltip = act_period ).
    lr_flow = lr_grid_1-&gt;create_flow( row = 2
                                     column = 2 ).
    lr_label = lr_flow-&gt;create_label( text = &apos;Previous Period / Fiscal year:&apos;
                                      tooltip = &apos;Previous Period / Fiscal year:&apos; ).
    DATA(prev_period) = prev_poper &amp;&amp; &apos;/&apos; &amp;&amp; prev_gjahr.
    lr_text = lr_flow-&gt;create_text( text = prev_period
                                    tooltip = prev_period ).
    salv-&gt;set_top_of_list( lr_grid ).
  ENDMETHOD.</source>
  </method>
