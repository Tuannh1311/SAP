  <interfaceMethod CLSNAME="ZFI_CF_REPORT" CPDNAME="ZFI_REPORT_SUB_VAL~SUBSTITUTION_GET_CREDIT">
   <source>METHOD ZFI_REPORT_SUB_VAL~SUBSTITUTION_GET_CREDIT.
    IF previous = abap_false.
      DATA(l_year) = gjahr.
      DATA(l_poper) = poper.
    ELSE.
      l_year = prev_gjahr.
      l_poper = prev_poper.
    ENDIF.
    SELECT SUM( hsl )
      INTO @DATA(credit)
      FROM zficf_credit_amount
      WHERE link_id = @link_id
        AND rldnr = @rldnr
        AND rbukrs = @bukrs
        AND gjahr = @l_year
        AND poper = @l_poper.
    amount = credit.
  ENDMETHOD.</source>
  </interfaceMethod>
