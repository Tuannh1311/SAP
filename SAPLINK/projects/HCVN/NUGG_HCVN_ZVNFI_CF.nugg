  <method CLSNAME="ZFI_CF_REPORT" CMPNAME="GET_AMOUNT" VERSION="1" LANGU="E" EXPOSURE="0" STATE="1" EDITORDER="6 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZFI_CF_REPORT" CMPNAME="GET_AMOUNT" SCONAME="LINK_ID" VERSION="1" LANGU="E" DESCRIPT="CF Maitenance Link ID" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="ZFICF_LINK_ID"/>
   <parameter CLSNAME="ZFI_CF_REPORT" CMPNAME="GET_AMOUNT" SCONAME="PREVIOUS" VERSION="1" LANGU="E" DESCRIPT="Data element for domain BOOLE: TRUE (=&apos;X&apos;) and FALSE (=&apos; &apos;)" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="BOOLE_D" PAROPTIONL="X"/>
   <parameter CLSNAME="ZFI_CF_REPORT" CMPNAME="GET_AMOUNT" SCONAME="AMOUNT" VERSION="1" LANGU="E" DESCRIPT="Amount in Company Code Currency" CMPTYPE="1" MTDTYPE="0" EDITORDER="3 " DISPID="0 " PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="FINS_VHCUR12"/>
   <source>METHOD get_amount.
    IF previous = abap_false.
      DATA(l_year) = gjahr.
      DATA(l_poper) = poper.
    ELSE.
      l_year = prev_gjahr.
      l_poper = prev_poper.
    ENDIF.
    SELECT SUM( hsl )
      INTO @DATA(credit)
      FROM zficf_credit_amount
      WHERE link_id = @link_id
        AND rldnr = @rldnr
        AND rbukrs = @bukrs
        AND gjahr = @l_year
        AND poper = @l_poper.
    SELECT SUM( hsl )
    INTO @DATA(debit)
    FROM zficf_debit_amount
    WHERE link_id = @link_id
      AND rldnr = @rldnr
      AND rbukrs = @bukrs
      AND gjahr = @l_year
      AND poper = @l_poper.
    amount = credit + debit.
  ENDMETHOD.</source>
  </method>
