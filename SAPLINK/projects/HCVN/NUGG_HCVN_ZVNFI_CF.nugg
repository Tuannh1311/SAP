 <PROG NAME="ZFI_CF_MAP_ACCOUNTS" VARCL="X" SUBC="1" RMAND="700" RLOAD="E" FIXPT="X" UCCHECK="X">
  <textPool>
   <language SPRAS="E">
    <textElement ID="R" ENTRY="Program ZFI_CF_MAP_ACCOUNTS" LENGTH="27 "/>
   </language>
  </textPool>
  <source>*&amp;---------------------------------------------------------------------*
*&amp; Report ZFI_CF_MAP_ACCOUNTS
*&amp;---------------------------------------------------------------------*
*&amp;
*&amp;---------------------------------------------------------------------*
REPORT zfi_cf_map_accounts.
SELECT * FROM zficf_accounts INTO @DATA(accounts).
  IF accounts-altkt IS INITIAL.
    MOVE accounts-hkont TO accounts-altkt.
    SELECT SINGLE saknr FROM skb1 INTO @accounts-hkont
           WHERE altkt = @accounts-altkt
             AND bukrs = &apos;VN01&apos;.
    IF sy-subrc EQ 0.
      UPDATE zficf_accounts FROM accounts.

    ENDIF.
  ENDIF.
ENDSELECT.
COMMIT WORK.</source>
 </PROG>
 <PROG NAME="ZFI_CF_TRIAL" VARCL="X" SUBC="1" RMAND="700" RLOAD="E" FIXPT="X" UCCHECK="X">
  <textPool>
   <language SPRAS="E">
    <textElement ID="R" ENTRY="Program ZFI_CF_TRIAL" LENGTH="20 "/>
    <textElement ID="S" KEY="LEDGER" ENTRY="D       ." LENGTH="9 "/>
    <textElement ID="S" KEY="S_BUDAT" ENTRY="D       ." LENGTH="9 "/>
    <textElement ID="S" KEY="S_HKONT" ENTRY="D       ." LENGTH="9 "/>
   </language>
  </textPool>
  <source>*&amp;---------------------------------------------------------------------*
*&amp; Report ZFI_CF_TRIAL
*&amp;---------------------------------------------------------------------*
*&amp;
*&amp;---------------------------------------------------------------------*
REPORT zfi_cf_trial.
TABLES acdoca.
SELECT-OPTIONS: s_budat FOR acdoca-budat NO-EXTENSION OBLIGATORY,
                s_hkont FOR acdoca-racct OBLIGATORY NO-EXTENSION NO INTERVALS.
PARAMETERS ledger TYPE rldnr OBLIGATORY.
READ TABLE s_budat INDEX 1 REFERENCE INTO DATA(lr_budat).
SELECT
       SUM( debitamountincocodecrcy ) AS debitamount,
       SUM( creditamountincocodecrcy ) AS creditamount,
       SUM( endingbalanceamtincocodecrcy ) AS endingbalanceamt
       FROM i_glacctbalance( p_frompostingdate = @lr_budat-&gt;low,
                             p_topostingdate = @lr_budat-&gt;high  )
  WHERE companycode = &apos;VN01&apos;
    AND ledger = @ledger
    AND fiscalyear = @lr_budat-&gt;high(4)
    AND glaccount IN @s_hkont
    AND  clearingaccountingdocument IS INITIAL
  INTO TABLE @DATA(lt_balance).
LOOP AT lt_balance  REFERENCE INTO DATA(ls_balance).
  WRITE:
       / &apos;Debit Amount&apos;, ls_balance-&gt;debitamount CURRENCY &apos;VND&apos;,
       / &apos;Credit Amount&apos;, ls_balance-&gt;creditamount CURRENCY &apos;VND&apos;,
       / &apos;Ending Balance&apos;, ls_balance-&gt;endingbalanceamt CURRENCY &apos;VND&apos;.
ENDLOOP.</source>
 </PROG>
