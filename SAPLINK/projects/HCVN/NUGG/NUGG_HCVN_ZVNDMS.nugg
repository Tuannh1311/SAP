  <attribute CLSNAME="ZCL_VNDMS_IFDD" CMPNAME="AS_TOADV" VERSION="1" LANGU="E" DESCRIPT="config: SAP ArchiveLink: Global document types" EXPOSURE="0" STATE="1" EDITORDER="4 " ATTDECLTYP="0" ATTEXPVIRT="0" TYPTYPE="1" TYPE="TOADV" SRCROW1="0 " SRCCOLUMN1="0 " SRCROW2="0 " SRCCOLUMN2="0 " TYPESRC_LENG="0 "/>
  <attribute CLSNAME="ZCL_VNDMS_IFDD" CMPNAME="AS_TOAOM" VERSION="1" LANGU="E" DESCRIPT="config: SAP ArchiveLink: Meta table for links" EXPOSURE="0" STATE="1" EDITORDER="3 " ATTDECLTYP="0" ATTEXPVIRT="0" TYPTYPE="1" TYPE="TOAOM" SRCROW1="0 " SRCCOLUMN1="0 " SRCROW2="0 " SRCCOLUMN2="0 " TYPESRC_LENG="0 "/>
