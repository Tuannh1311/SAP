  </method>
  <method CLSNAME="ZCL_VNDMS_IFDD" CMPNAME="CONV_FPATH" VERSION="1" LANGU="E" DESCRIPT="convert: log-file + file -&gt; full physical path" EXPOSURE="0" STATE="1" EDITORDER="8 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="1" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNDMS_IFDD" CMPNAME="CONV_FPATH" SCONAME="IV_FNAME" VERSION="1" LANGU="E" DESCRIPT="DMS: file name" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="ZVNDMS_IFLD-FNAME"/>
   <parameter CLSNAME="ZCL_VNDMS_IFDD" CMPNAME="CONV_FPATH" SCONAME="IV_LFILE" VERSION="1" LANGU="E" DESCRIPT="Logical file name" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="ZVNDMS_IFLD-LFILE"/>
   <parameter CLSNAME="ZCL_VNDMS_IFDD" CMPNAME="CONV_FPATH" SCONAME="RV_FPATH" VERSION="1" LANGU="E" DESCRIPT="DMS: scan station full path (dir+file)" CMPTYPE="1" MTDTYPE="0" EDITORDER="3 " DISPID="0 " PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="ZVNDMS_IFLD-FPATH"/>
   <source>METHOD conv_fpath.

    &quot;convert: log-file + file -&gt; full physical path
    CALL FUNCTION &apos;FILE_GET_NAME&apos;
      EXPORTING
        logical_filename = iv_lfile
        parameter_1      = iv_fname
      IMPORTING
        file_name        = rv_fpath
      EXCEPTIONS
        file_not_found   = 1
        OTHERS           = 2.
    ASSERT sy-subrc = 0.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFDD" CMPNAME="IFDD_DIRLIST" VERSION="1" LANGU="E" DESCRIPT="downloader: directory listing" EXPOSURE="0" STATE="1" EDITORDER="4 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD ifdd_dirlist.

    &quot;&amp; list directory

    CALL FUNCTION &apos;EPS_GET_DIRECTORY_LISTING&apos;
      EXPORTING
        dir_name               = CONV epsf-epsdirnam( me-&gt;av_ppath )
      TABLES
        dir_list               = me-&gt;at_dlist
      EXCEPTIONS
        invalid_eps_subdir     = 1
        sapgparam_failed       = 2
        build_directory_failed = 3
        no_authorization       = 4
        read_directory_failed  = 5
        too_many_read_errors   = 6
        empty_directory_list   = 7
        OTHERS                 = 8.
    ASSERT sy-subrc =  0 or
           sy-subrc =  7    .


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFDD" CMPNAME="IFDD_IFLD" VERSION="1" LANGU="E" DESCRIPT="downloader: loop at files, process to IFLD" EXPOSURE="0" STATE="1" EDITORDER="5 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD ifdd_ifld.

    &quot;&amp; process files to IFLD
    LOOP AT me-&gt;at_dlist ASSIGNING FIELD-SYMBOL(&lt;dlist&gt;).

      &quot;init IFLD record
      APPEND INITIAL LINE TO me-&gt;at_ifld REFERENCE INTO DATA(lr_ifld).

      MOVE: sy-datum                TO lr_ifld-&gt;rdate,
            sy-uzeit                TO lr_ifld-&gt;rtime,
            me-&gt;as_c000-arobj       TO lr_ifld-&gt;arobj,
            me-&gt;as_toaom-doc_type   TO lr_ifld-&gt;ardoc,
            me-&gt;as_toaom-archiv_id  TO lr_ifld-&gt;acrep,
            &apos;&apos;                      TO lr_ifld-&gt;ardid, &quot;(not yet saved, empty)
            &apos;&apos;                      TO lr_ifld-&gt;sobid, &quot;(linked later in linked ILFD)
            me-&gt;as_c000-spobj       TO lr_ifld-&gt;sobty,
            me-&gt;as_c000-lfile       TO lr_ifld-&gt;lfile,
            &apos;&apos;                      TO lr_ifld-&gt;fpath, &quot;(see conv below)
            &lt;dlist&gt;-name            TO lr_ifld-&gt;fname.

      lr_ifld-&gt;fpath = me-&gt;conv_fpath( iv_fname = lr_ifld-&gt;fname
                                       iv_lfile = lr_ifld-&gt;lfile ).

      &quot;process IFLD record
      me-&gt;ifdd_ifld_save( ir_ifld = lr_ifld ).  &quot;save to ArchiveLink
      me-&gt;ifdd_ifld_dele( ir_ifld = lr_ifld ).  &quot;delete source file


    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFDD" CMPNAME="IFDD_IFLD_DELE" VERSION="1" LANGU="E" DESCRIPT="IFLD single file: delete from source folder" EXPOSURE="0" STATE="1" EDITORDER="7 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNDMS_IFDD" CMPNAME="IFDD_IFLD_DELE" SCONAME="IR_IFLD" VERSION="1" LANGU="E" DESCRIPT="DMS: DMS document log" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="ZVNDMS_IFLD"/>
   <source>METHOD ifdd_ifld_dele.

    &quot;&amp; delete file from shared folder

    CHECK: ir_ifld-&gt;*-_sta-stupl NE &apos;E&apos;. &quot;no deletion if no upload

    CALL FUNCTION &apos;EPS_DELETE_FILE&apos;
      EXPORTING
        file_name              = CONV epsf-epsfilnam( ir_ifld-&gt;*-fname )
        dir_name               = CONV epsf-epsdirnam( me-&gt;av_ppath )
      EXCEPTIONS
        invalid_eps_subdir     = 1
        sapgparam_failed       = 2
        build_directory_failed = 3
        no_authorization       = 4
        build_path_failed      = 5
        delete_failed          = 6
        OTHERS                 = 7.
    ASSERT sy-subrc = 0.

    &quot;update deletion lfag
    MOVE zif_vndms_const=&gt;gc_stdel_d TO ir_ifld-&gt;*-stdel.

    &quot;save to db IFLD
    MODIFY zvndms_ifld FROM ir_ifld-&gt;*.
    COMMIT WORK AND WAIT.



  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFDD" CMPNAME="IFDD_IFLD_SAVE" VERSION="1" LANGU="E" DESCRIPT="IFLD single file: save to ArchiveLink" EXPOSURE="0" STATE="1" EDITORDER="6 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNDMS_IFDD" CMPNAME="IFDD_IFLD_SAVE" SCONAME="IR_IFLD" VERSION="1" LANGU="E" DESCRIPT="DMS: DMS document log" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="ZVNDMS_IFLD"/>
   <source>METHOD ifdd_ifld_save.

    &quot;&amp; save file to ArchiveLink
    WRITE: / &apos;       time save start: &apos;, sy-uzeit, ir_ifld-&gt;fpath.
    CALL FUNCTION &apos;ARCHIVOBJECT_CREATE_FILE&apos;
      EXPORTING
        archiv_id                = ir_ifld-&gt;acrep
        document_type            = ir_ifld-&gt;ardoc
        path                     = ir_ifld-&gt;fpath
      IMPORTING
        archiv_doc_id            = ir_ifld-&gt;ardid
      EXCEPTIONS
        error_archiv             = 1
        error_communicationtable = 2
        error_upload             = 3
        error_kernel             = 4
        blocked_by_policy        = 5
        OTHERS                   = 6.
    &quot;ASSERT sy-subrc = 0.
    WRITE: / &apos;       time save end  : &apos;, sy-uzeit, ir_ifld-&gt;fpath.

    IF sy-subrc EQ 0.
      MOVE &apos;U&apos; TO: ir_ifld-&gt;*-_sta-stupl.
    ELSE.
      &quot;msg{Error in ARCHIVOBJECT_CREATE_FILE: &amp; &amp; &amp; &amp;.}
      MESSAGE e007(zvndms) WITH ir_ifld-&gt;acrep
                                ir_ifld-&gt;ardoc
                                ir_ifld-&gt;fpath
                                sy-subrc        INTO ir_ifld-&gt;*-_msg-msgtx.
      MOVE: &apos;ZVNDMS&apos;  TO ir_ifld-&gt;*-_msg-msgid,
            &apos;007&apos;     TO ir_ifld-&gt;*-_msg-msgno,
            &apos;E&apos;       TO ir_ifld-&gt;*-_msg-msgty.
      MOVE &apos;E&apos; TO: ir_ifld-&gt;*-_sta-stupl.

    ENDIF.


    &quot;save to db IFLD
    MODIFY zvndms_ifld FROM ir_ifld-&gt;*.
    COMMIT WORK AND WAIT.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFDD" CMPNAME="IFDD_INIT" VERSION="1" LANGU="E" DESCRIPT="downloader: initialization" EXPOSURE="0" STATE="1" EDITORDER="3 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD ifdd_init.

    &quot;get customizing
    SELECT SINGLE * FROM toadv INTO @me-&gt;as_toadv WHERE ar_object  = @me-&gt;as_c000-arobj.
    SELECT SINGLE * FROM toaom INTO @me-&gt;as_toaom WHERE sap_object = @me-&gt;as_c000-spobj
                                                    AND ar_object  = @me-&gt;as_c000-arobj
                                                    AND ar_status  = &apos;X&apos;.

    &quot;get shared folder (log-file -&gt; log-path -&gt; phys-path)
    CALL FUNCTION &apos;FILE_GET_NAME&apos;
      EXPORTING
        logical_filename = me-&gt;as_c000-lfile
      IMPORTING
        file_name        = me-&gt;av_ppath
      EXCEPTIONS
        file_not_found   = 1
        OTHERS           = 2.
    ASSERT sy-subrc = 0.



  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFDD" CMPNAME="PROCESS" VERSION="1" LANGU="E" DESCRIPT="downloader: process" EXPOSURE="2" STATE="1" EDITORDER="2 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD process.

    &quot;process shared folder

    me-&gt;ifdd_init(  ).
    me-&gt;ifdd_dirlist( ).
    me-&gt;ifdd_ifld( ).


  ENDMETHOD.</source>
  </method>
 </CLAS>
