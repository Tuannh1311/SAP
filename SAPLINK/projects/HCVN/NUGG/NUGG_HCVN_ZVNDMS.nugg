   <parameter CLSNAME="ZCL_VNDMS_IFDD" CMPNAME="CONSTRUCTOR" SCONAME="IV_C000" VERSION="1" LANGU="E" DESCRIPT="DMS: DMS configuration (logical file, shared folder)" CMPTYPE="1" MTDTYPE="2" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="ZVNDMS_C000"/>
   <source>METHOD CONSTRUCTOR.

    &quot;set C000
    MOVE: iv_c000 TO me-&gt;as_c000.

