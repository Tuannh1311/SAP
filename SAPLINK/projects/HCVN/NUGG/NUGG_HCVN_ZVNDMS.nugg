  <attribute CLSNAME="ZCL_VNDMS_IFMD" CMPNAME="LC_REPID" VERSION="1" LANGU="E" DESCRIPT="calling program" EXPOSURE="0" STATE="1" EDITORDER="1 " ATTDECLTYP="2" ATTVALUE="&apos;ZVNDMS_IFMD&apos;" ATTEXPVIRT="0" TYPTYPE="1" TYPE="SY-REPID" SRCROW1="0 " SRCCOLUMN1="0 " SRCROW2="0 " SRCCOLUMN2="0 " TYPESRC_LENG="0 "/>
  <method CLSNAME="ZCL_VNDMS_IFMD" CMPNAME="CALL_ARLINK_ADD" VERSION="1" LANGU="E" DESCRIPT="call: ARlink add" EXPOSURE="0" STATE="1" EDITORDER="24 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD call_arlink_add.

    &quot;get selected line
    DATA(lr_sdat) = me-&gt;salv_line_get( ).
    CHECK lr_sdat IS BOUND.

    &quot;check anything to link
    IF lr_sdat-&gt;sobid IS INITIAL.
      MESSAGE w006(zvndms). &quot;msg{No SAP-Object ID in selected line, nothing to be linked.}
      RETURN.
    ENDIF.

    &quot;link
    CALL FUNCTION &apos;ARCHIV_CONNECTION_INSERT&apos;
      EXPORTING
        archiv_id             = lr_sdat-&gt;acrep
        arc_doc_id            = lr_sdat-&gt;ardid
        ar_object             = lr_sdat-&gt;arobj
        object_id             = lr_sdat-&gt;sobid
        sap_object            = lr_sdat-&gt;sobty
        filename              = CONV toaat-filename( lr_sdat-&gt;fname )
        &quot;Descr      = &apos; &apos;
        creator               = sy-uname
      EXCEPTIONS
        error_connectiontable = 1
        OTHERS                = 2.

    IF sy-subrc &lt;&gt; 0.
      MESSAGE ID sy-msgid TYPE &apos;W&apos; NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4 DISPLAY LIKE &apos;E&apos;.
      RETURN.
    ENDIF.

    &quot;update IFLD db
    MOVE: zif_vndms_const=&gt;gc_stlnk_l TO lr_sdat-&gt;stlnk.
    MODIFY zvndms_ifld FROM lr_sdat-&gt;*-_ifld.
    COMMIT WORK AND WAIT.


    &quot;refresh data
    me-&gt;salv_refr( ).



  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFMD" CMPNAME="CALL_ARLINK_DEL" VERSION="1" LANGU="E" DESCRIPT="call: ARlink delete" EXPOSURE="0" STATE="1" EDITORDER="23 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD call_arlink_del.

    &quot;get selected line
    DATA(lr_sdat) = me-&gt;salv_line_get( ).
    CHECK lr_sdat IS BOUND.

    &quot;check anything to un-link
    IF lr_sdat-&gt;sobid IS INITIAL.
      MESSAGE w005(zvndms). &quot;msg{No SAP-Object ID in selected line, nothing to be un-linked.}
      RETURN.
    ENDIF.

    &quot;call deletion
    CALL FUNCTION &apos;ARCHIV_DELETE_META&apos;
      EXPORTING
        archiv_id                = lr_sdat-&gt;acrep
        arc_doc_id               = lr_sdat-&gt;ardid
        ar_object                = lr_sdat-&gt;arobj
        delete_flag              = 0               &quot;0= with dialog
        object_id                = lr_sdat-&gt;sobid
        sap_object               = lr_sdat-&gt;sobty
        client                   = sy-mandt
        single_entry             = &apos;X&apos;
        documentclass            = lr_sdat-&gt;ardoc
*       NO_AUTH_CHECK            = NO_AUTH_CHECK
*     IMPORTING
*       ALL_CONNECTIONS_DELETED  = ALL_CONNECTIONS_DELETED
      EXCEPTIONS
        error_connectiontable    = 1
        error_parameter          = 2
        error_archiv             = 3
        error_kernel             = 4
        error_communicationtable = 5
        error_authority          = 6
        OTHERS                   = 7.
    IF sy-subrc &lt;&gt; 0.
      MESSAGE ID sy-msgid TYPE &apos;W&apos; NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4 DISPLAY LIKE &apos;E&apos;.
      RETURN.
    ENDIF.


    &quot;check is deleted (because the dialog confirmation in fm above does not return any status whan cancelled)
    DATA: lt_cons TYPE STANDARD TABLE OF toav0.
    CALL FUNCTION &apos;ARCHIV_GET_CONNECTIONS_INT&apos;
      EXPORTING
        objecttype       = lr_sdat-&gt;sobty
        object_id        = lr_sdat-&gt;sobid
        client           = sy-mandt
        archiv_id        = lr_sdat-&gt;acrep
        arc_doc_id       = lr_sdat-&gt;ardid
      TABLES
        connections      = lt_cons
      EXCEPTIONS
        nothing_found    = 1
        error_authorithy = 2
        error_parameter  = 3
        OTHERS           = 4.
    IF sy-subrc &lt;&gt; 0.  ENDIF.

    LOOP AT lt_cons ASSIGNING FIELD-SYMBOL(&lt;con&gt;) WHERE sap_object = lr_sdat-&gt;sobty
                                                    AND object_id  = lr_sdat-&gt;sobid.
      &quot;found? then not deleted
      RETURN.
    ENDLOOP.


    &quot;update IFLD db
    MOVE: zif_vndms_const=&gt;gc_stlnk_x TO lr_sdat-&gt;stlnk,
          &apos;&apos;                          TO lr_sdat-&gt;sobid.
    MODIFY zvndms_ifld FROM lr_sdat-&gt;*-_ifld.
    COMMIT WORK AND WAIT.


    &quot;refresh data
    me-&gt;salv_refr( ).




  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFMD" CMPNAME="CALL_DISP_ARDOC" VERSION="1" LANGU="E" DESCRIPT="call: display ArchiveLink document" EXPOSURE="0" STATE="1" EDITORDER="21 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD call_disp_ardoc.

    &quot;get selected line
    DATA(lr_sdat) = me-&gt;salv_line_get( ).
    CHECK lr_sdat IS BOUND.

    &quot;call display

    CALL FUNCTION &apos;ARCHIVOBJECT_DISPLAY&apos;
      EXPORTING
        archiv_doc_id            = lr_sdat-&gt;ardid
        archiv_id                = lr_sdat-&gt;acrep
      EXCEPTIONS
        error_archiv             = 1
        error_communicationtable = 2
        error_kernel             = 3
        OTHERS                   = 4.
    IF sy-subrc &lt;&gt; 0. MESSAGE ID sy-msgid TYPE &apos;W&apos; NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4 DISPLAY LIKE &apos;E&apos;. ENDIF.



  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFMD" CMPNAME="CALL_DISP_SOBID" VERSION="1" LANGU="E" DESCRIPT="call: display SAP-Object" EXPOSURE="0" STATE="1" EDITORDER="22 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD call_disp_sobid.

    &quot;get selected line
    DATA(lr_sdat) = me-&gt;salv_line_get( ).
    CHECK lr_sdat IS BOUND.

    &quot;call display
    CASE lr_sdat-&gt;sobty.

      WHEN &apos;BUS2012&apos;  &quot;Purchase Order
        OR &apos;BUS2014&apos;. &quot;Outline Agreement

        CALL FUNCTION &apos;ME_DISPLAY_PURCHASE_DOCUMENT&apos;
          EXPORTING
            i_ebeln              = CONV ekko-ebeln( lr_sdat-&gt;*-sobid )
          EXCEPTIONS
            not_found            = 1
            no_authority         = 2
            invalid_call         = 3
            preview_not_possible = 4
            OTHERS               = 5.
        IF sy-subrc &lt;&gt; 0. MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4. ENDIF.

      WHEN &apos;BUS2081&apos;. &quot;Incoming Invoice

        SET PARAMETER ID &apos;RBN&apos; FIELD lr_sdat-&gt;*-sobid(10).
        SET PARAMETER ID &apos;GJR&apos; FIELD lr_sdat-&gt;*-sobid+10(4).

        CALL TRANSACTION &apos;MIR4&apos; WITH AUTHORITY-CHECK AND SKIP FIRST SCREEN.


    ENDCASE.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFMD" CMPNAME="CALL_IFLD" VERSION="1" LANGU="E" DESCRIPT="call: maintenance view ifld" EXPOSURE="0" STATE="1" EDITORDER="20 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNDMS_IFMD" CMPNAME="CALL_IFLD" SCONAME="IR_SDAT" VERSION="1" LANGU="E" DESCRIPT="enhance table IFLD for SALV" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="TS_SDAT"/>
   <source>METHOD call_ifld.

    &quot;call maintenance view for IFLD record

    &quot;prep sel-opts
    DATA lt_sel TYPE TABLE OF vimsellist.
    lt_sel = VALUE #( BASE lt_sel ( viewfield = &apos;RDATE&apos; operator = &apos;EQ&apos; value = ir_sdat-&gt;*-rdate and_or = &apos;AND&apos; ) ).
    lt_sel = VALUE #( BASE lt_sel ( viewfield = &apos;RTIME&apos; operator = &apos;EQ&apos; value = ir_sdat-&gt;*-rtime and_or = &apos;AND&apos; ) ).
    lt_sel = VALUE #( BASE lt_sel ( viewfield = &apos;AROBJ&apos; operator = &apos;EQ&apos; value = ir_sdat-&gt;*-arobj and_or = &apos;AND&apos; ) ).
    lt_sel = VALUE #( BASE lt_sel ( viewfield = &apos;ARDOC&apos; operator = &apos;EQ&apos; value = ir_sdat-&gt;*-ardoc and_or = &apos;AND&apos; ) ).
    lt_sel = VALUE #( BASE lt_sel ( viewfield = &apos;ACREP&apos; operator = &apos;EQ&apos; value = ir_sdat-&gt;*-acrep and_or = &apos;AND&apos; ) ).
    lt_sel = VALUE #( BASE lt_sel ( viewfield = &apos;ARDID&apos; operator = &apos;EQ&apos; value = ir_sdat-&gt;*-ardid and_or = &apos;   &apos; ) ).

    &quot;call view: ZVNDMS_IFLD &quot;DMS: DMS document log&quot;
    CALL FUNCTION &apos;VIEW_MAINTENANCE_CALL&apos;
      EXPORTING
        action               = &apos;S&apos;
        show_selection_popup = space
        view_name            = &apos;ZVNDMS_IFLD&apos;
      TABLES
        dba_sellist          = lt_sel.


    &quot;refresh salv
    me-&gt;salv_refr( ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFMD" CMPNAME="CONSTRUCTOR" VERSION="1" LANGU="E" EXPOSURE="2" STATE="1" EDITORDER="1 " DISPID="0 " MTDTYPE="2" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNDMS_IFMD" CMPNAME="CONSTRUCTOR" SCONAME="IT_KEYS" VERSION="1" LANGU="E" DESCRIPT="DMS: DMS document log: key (tty)" CMPTYPE="1" MTDTYPE="2" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="ZVNDMS_IFLD_KEY_T"/>
   <source>METHOD constructor.

    me-&gt;at_keys = it_keys.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFMD" CMPNAME="DATA_SELC" VERSION="1" LANGU="E" DESCRIPT="select data per keys" EXPOSURE="0" STATE="1" EDITORDER="1 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD data_selc.

    &quot;get all IFLD as per keys

    CHECK  me-&gt;at_keys[] IS NOT INITIAL.

    SELECT * FROM zvndms_ifld INTO TABLE @me-&gt;at_ifld
      FOR ALL ENTRIES IN @me-&gt;at_keys
     WHERE rdate = @me-&gt;at_keys-rdate
       AND rtime = @me-&gt;at_keys-rtime
       AND arobj = @me-&gt;at_keys-arobj
       AND ardoc = @me-&gt;at_keys-ardoc
       AND acrep = @me-&gt;at_keys-acrep
       AND ardid = @me-&gt;at_keys-ardid.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFMD" CMPNAME="ON_DBCL" VERSION="1" LANGU="E" DESCRIPT="handler: on double-click" EXPOSURE="0" STATE="1" EDITORDER="18 " DISPID="0 " MTDTYPE="1" MTDDECLTYP="0" REFCLSNAME="CL_SALV_EVENTS_TABLE" REFCMPNAME="DOUBLE_CLICK" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNDMS_IFMD" CMPNAME="ON_DBCL" SCONAME="ROW" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="1" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="0"/>
   <parameter CLSNAME="ZCL_VNDMS_IFMD" CMPNAME="ON_DBCL" SCONAME="COLUMN" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="1" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="0"/>
   <source>METHOD on_dbcl.

    &quot;get double-clicked row
    READ TABLE me-&gt;at_sdat REFERENCE INTO DATA(lr_sdat) INDEX row.

    me-&gt;call_ifld( lr_sdat ). &quot;call m-views for ilfd

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFMD" CMPNAME="ON_UCOM" VERSION="1" LANGU="E" DESCRIPT="handler: on user-command" EXPOSURE="0" STATE="1" EDITORDER="19 " DISPID="0 " MTDTYPE="1" MTDDECLTYP="0" REFCLSNAME="CL_SALV_EVENTS" REFCMPNAME="ADDED_FUNCTION" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNDMS_IFMD" CMPNAME="ON_UCOM" SCONAME="E_SALV_FUNCTION" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="1" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="0"/>
   <source>METHOD on_ucom.

    CASE e_salv_function.

      WHEN &apos;REFRESH&apos;.     me-&gt;salv_refr( ).

      WHEN &apos;DISP_ARDOC&apos;.  me-&gt;call_disp_ardoc( ).

      WHEN &apos;DISP_SOBID&apos;.  me-&gt;call_disp_sobid( ).

      WHEN &apos;ARLINK_DEL&apos;.  me-&gt;call_arlink_del( ).

      WHEN &apos;ARLINK_ADD&apos;.  me-&gt;call_arlink_add( ).

    ENDCASE.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFMD" CMPNAME="REPORT" VERSION="1" LANGU="E" DESCRIPT="main report" EXPOSURE="2" STATE="1" EDITORDER="2 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD report.

    me-&gt;data_selc( ).
    me-&gt;sdat_data( ).

    me-&gt;sdat_enha( ).


    me-&gt;salv_init( ).
    me-&gt;salv_disp( ).


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFMD" CMPNAME="SALV_DISP" VERSION="1" LANGU="E" DESCRIPT="salv-object: display" EXPOSURE="0" STATE="1" EDITORDER="16 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_disp.

***    &quot; optimise columns
***    me-&gt;ar_salv-&gt;get_columns( )-&gt;set_optimize( abap_true ).


    &quot; and finally display SALV table as full screen
    me-&gt;ar_salv-&gt;display( ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFMD" CMPNAME="SALV_INIT" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise" EXPOSURE="0" STATE="1" EDITORDER="6 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init.

    &quot; create SAVL table object
    TRY.
        cl_salv_table=&gt;factory( IMPORTING r_salv_table = me-&gt;ar_salv
                                CHANGING  t_table      = me-&gt;at_sdat ).
      CATCH cx_salv_msg.
    ENDTRY.


    me-&gt;salv_init_mlts( ).  &quot;set multi-sel
    me-&gt;salv_init_excp( ).  &quot;set exception columns
    me-&gt;salv_init_func( ).  &quot;set all grid-functions
    me-&gt;salv_init_pfst( ).  &quot;set own gui-status
    me-&gt;salv_init_ccol( ).  &quot;set colours per column
    me-&gt;salv_init_ccel( ).  &quot;set colours per cell-row
    me-&gt;salv_init_invs( ).  &quot;set column invisible
    me-&gt;salv_init_hndl( ).  &quot;set event handlers


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFMD" CMPNAME="SALV_INIT_CCEL" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: colours cell(row)" EXPOSURE="0" STATE="1" EDITORDER="12 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_ccel.

    &quot;set colours per cell/row

    &quot;get field list (from columns)
    DATA(lo_cols) =  me-&gt;ar_salv-&gt;get_columns( ).
    DATA(lt_cols) = lo_cols-&gt;get( ).

    &quot;set column SCOL
    TRY. lo_cols-&gt;set_color_column( value = &apos;TSCOL&apos; ). CATCH cx_salv_data_error. ENDTRY.

    &quot;for each line..
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      &quot;..prepare dynamic colour per stlnks
      DATA: ls_scol TYPE lvc_s_colo.
      CASE &lt;sdat&gt;-stlnk.
        WHEN zif_vndms_const=&gt;gc_stlnk_x. ls_scol-col = 3. &quot;yellow
        WHEN zif_vndms_const=&gt;gc_stlnk_e. ls_scol-col = 7. &quot;orange
        WHEN zif_vndms_const=&gt;gc_stlnk_l. ls_scol-col = 1. &quot;blue
      ENDCASE.

      &quot;..for each column
      LOOP AT lt_cols ASSIGNING FIELD-SYMBOL(&lt;cols&gt;).
        APPEND INITIAL LINE TO &lt;sdat&gt;-tscol ASSIGNING FIELD-SYMBOL(&lt;tscol&gt;).

        CASE &lt;cols&gt;-columnname.
          WHEN &apos;RDATE&apos;
            OR &apos;RTIME&apos;
            OR &apos;AROBJ&apos;
            OR &apos;ARDOC&apos;
            OR &apos;ACREP&apos;
            OR &apos;ARDID&apos;.
            &quot;key fields: highlighted
            MOVE: &lt;cols&gt;-columnname TO &lt;tscol&gt;-fname,
                  ls_scol           TO &lt;tscol&gt;-color,
                  1                 TO &lt;tscol&gt;-color-int.

          WHEN OTHERS.
            &quot;data fields: normal
            MOVE: &lt;cols&gt;-columnname TO &lt;tscol&gt;-fname,
                  ls_scol           TO &lt;tscol&gt;-color,
                  0                 TO &lt;tscol&gt;-color-int.

        ENDCASE.

      ENDLOOP.


    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFMD" CMPNAME="SALV_INIT_CCOL" VERSION="1" LANGU="E" DESCRIPT="***salv-object: initialise: colours row" EXPOSURE="0" STATE="1" EDITORDER="11 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_ccol.

***    &quot;init: column colours
***
***    DATA: lr_col   TYPE REF TO cl_salv_column_list, &quot; object of column, allowing set_color
***          ls_color TYPE        lvc_s_colo.          &quot; colours structure
***
***    DATA(lr_cols) = me-&gt;ar_salv-&gt;get_columns( ).
***    DATA(lt_cols) = lr_cols-&gt;get( ).
***
***
***    LOOP AT lt_cols ASSIGNING FIELD-SYMBOL(&lt;col&gt;).
***      TRY.
***          lr_col ?= lr_cols-&gt;get_column( &lt;col&gt;-columnname ).
***        CATCH cx_salv_not_found.
***      ENDTRY.
***      &quot;setting colours
***      CASE &lt;col&gt;-columnname.
***        WHEN &apos;RDATE&apos;
***          OR &apos;RTIME&apos;
***          OR &apos;DEALR&apos;                         . ls_color-col = col_heading.   &quot;key  = grey/blue
***        WHEN &apos;STATU&apos;
***          OR &apos;STATU_TX&apos;
***          OR &apos;STATU_IC&apos;
***          OR &apos;EBELN&apos;
***          OR &apos;EBELP&apos;                          . ls_color-col = col_group   .  &quot;sta  = orange
***        WHEN &apos;MSGID&apos;
***          OR &apos;MSGNO&apos;
***          OR &apos;MSGTY&apos;
***          OR &apos;MSGTXT&apos;                         . ls_color-col = col_total.     &quot;msg  = yellow
***        WHEN &apos;CACTV&apos;
***          OR &apos;CSTAT&apos;                          . ls_color-col = col_positive.  &quot;dat  = green
***      ENDCASE.
***      lr_col-&gt;set_color( ls_color ).
***    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFMD" CMPNAME="SALV_INIT_EXCP" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: exception columns" EXPOSURE="0" STATE="1" EDITORDER="8 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_excp.

    &quot; set column for lights
    DATA(lr_cols) = me-&gt;ar_salv-&gt;get_columns( ).
    TRY.
        lr_cols-&gt;set_exception_column( value = &apos;STLNK_IC&apos; ).
      CATCH cx_salv_data_error.
    ENDTRY.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFMD" CMPNAME="SALV_INIT_FUNC" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: functions" EXPOSURE="0" STATE="1" EDITORDER="9 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_func.

    &quot;salv all functions
    me-&gt;ar_salv-&gt;get_functions( )-&gt;set_all( abap_true ).

    &quot; layout save &amp; default (&apos;all functions&apos; above are not really all)
    DATA: lr_layout TYPE REF TO cl_salv_layout,
          ls_key    TYPE salv_s_layout_key.

    ls_key-report = me-&gt;lc_repid.

    lr_layout     = me-&gt;ar_salv-&gt;get_layout( ).
    lr_layout-&gt;set_key( ls_key ).         &quot;
    lr_layout-&gt;set_save_restriction( cl_salv_layout=&gt;restrict_none ). &quot;User can save
    lr_layout-&gt;set_default( abap_true ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFMD" CMPNAME="SALV_INIT_HNDL" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: event handlers" EXPOSURE="0" STATE="1" EDITORDER="14 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_hndl.

    &quot;set handler
    SET HANDLER me-&gt;on_dbcl FOR me-&gt;ar_salv-&gt;get_event( ).
    SET HANDLER me-&gt;on_ucom FOR me-&gt;ar_salv-&gt;get_event( ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFMD" CMPNAME="SALV_INIT_INVS" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: invisible fields" EXPOSURE="0" STATE="1" EDITORDER="13 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_invs.

    &quot;set invisible fields

    DATA: lr_col   TYPE REF TO cl_salv_column_list. &quot; object of column, allowing set_color

    DATA(lr_cols) = me-&gt;ar_salv-&gt;get_columns( ).
    DATA(lt_cols) = lr_cols-&gt;get( ).

    &quot; remove MANDT
    LOOP AT lt_cols ASSIGNING FIELD-SYMBOL(&lt;col&gt;) WHERE columnname = &apos;MANDT&apos;.
      TRY.
          lr_col ?= lr_cols-&gt;get_column( &lt;col&gt;-columnname ).
        CATCH cx_salv_not_found.
      ENDTRY.
      lr_col-&gt;set_technical( if_salv_c_bool_sap=&gt;true ).
    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFMD" CMPNAME="SALV_INIT_MLTS" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: mutli-selection" EXPOSURE="0" STATE="1" EDITORDER="7 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_mlts.

    &quot;set multiple-selection
    DATA(lr_sel) = me-&gt;ar_salv-&gt;get_selections( ).
    lr_sel-&gt;set_selection_mode( if_salv_c_selection_mode=&gt;row_column ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFMD" CMPNAME="SALV_INIT_PFST" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: gui status" EXPOSURE="0" STATE="1" EDITORDER="10 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_pfst.

    &quot;init: gui status from calling program

    &quot;set own function by own screen status (seriously, I hate SALV)
    me-&gt;ar_salv-&gt;set_screen_status(
                                    pfstatus      = me-&gt;lc_pfkey
                                    report        = me-&gt;lc_repid
                                    set_functions = me-&gt;ar_salv-&gt;c_functions_all ).


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFMD" CMPNAME="SALV_LINE_GET" VERSION="1" LANGU="E" DESCRIPT="salv-object: get single selected line" EXPOSURE="0" STATE="1" EDITORDER="15 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNDMS_IFMD" CMPNAME="SALV_LINE_GET" SCONAME="ER_SDAT" VERSION="1" LANGU="E" DESCRIPT="enhance table IFLD for SALV" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="3" TYPE="TS_SDAT"/>
   <source>METHOD salv_line_get.

    &quot;get selected lines
    DATA(lt_rows) = me-&gt;ar_salv-&gt;get_selections( )-&gt;get_selected_rows( ).
    &quot;get selected cell
    DATA(lt_cell) = me-&gt;ar_salv-&gt;get_selections( )-&gt;get_selected_cells( ).

    &quot;check single
    IF NOT ( lines( lt_rows ) = 1 OR
             lines( lt_cell ) = 1   ).
      MESSAGE w004(zvndms). &quot;msg{Select exactly one line for processing.}
      RETURN.
    ENDIF.

    &quot;read line..
    &quot;..per line
    IF lines( lt_rows ) = 1.
      READ TABLE lt_rows ASSIGNING FIELD-SYMBOL(&lt;row&gt;) INDEX 1.
      READ TABLE me-&gt;at_sdat REFERENCE INTO er_sdat INDEX &lt;row&gt;.
    ENDIF.
    &quot;..per cell
    IF lines( lt_cell ) = 1.
      READ TABLE lt_cell ASSIGNING FIELD-SYMBOL(&lt;cell&gt;) INDEX 1.
      READ TABLE me-&gt;at_sdat REFERENCE INTO er_sdat INDEX &lt;cell&gt;-row.
    ENDIF.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFMD" CMPNAME="SALV_REFR" VERSION="1" LANGU="E" DESCRIPT="salv-object: refresh" EXPOSURE="0" STATE="1" EDITORDER="17 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_refr.

    &quot;refresh SALV

    &quot;..re-read data
    CLEAR: me-&gt;at_sdat.

    me-&gt;data_selc( ).
    me-&gt;sdat_data( ).
    me-&gt;sdat_enha( ).

    me-&gt;salv_init_ccel( ).    &quot;re-colour cells (per row-line)


    &quot;..refresh salv
    me-&gt;ar_salv-&gt;refresh( ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFMD" CMPNAME="SDAT_DATA" VERSION="1" LANGU="E" DESCRIPT="salv-data: from data" EXPOSURE="0" STATE="1" EDITORDER="2 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_data.

    &quot;pass IFLD to SDAT for SALV processing
    LOOP AT me-&gt;at_ifld ASSIGNING FIELD-SYMBOL(&lt;ifld&gt;).
      APPEND INITIAL LINE TO me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).
      MOVE: &lt;ifld&gt; TO &lt;sdat&gt;-_ifld.
    ENDLOOP.

    &quot; default sort
    SORT me-&gt;at_sdat BY rdate DESCENDING
                        rtime DESCENDING.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFMD" CMPNAME="SDAT_ENHA" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance" EXPOSURE="0" STATE="1" EDITORDER="3 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha.

    &quot;enhance salv-data

    me-&gt;sdat_enha_stlnk_tx( ).
    me-&gt;sdat_enha_stlnk_ic( ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFMD" CMPNAME="SDAT_ENHA_STLNK_IC" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: link-status icon" EXPOSURE="0" STATE="1" EDITORDER="5 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_stlnk_ic.

    &quot;set status icon/exception
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      CASE &lt;sdat&gt;-stlnk.
        WHEN zif_vndms_const=&gt;gc_stlnk_x. &lt;sdat&gt;-stlnk_ic = 2. &quot;_ = not processed =&gt; yellow
        WHEN zif_vndms_const=&gt;gc_stlnk_l. &lt;sdat&gt;-stlnk_ic = 3. &quot;L = linked        =&gt; green
        WHEN zif_vndms_const=&gt;gc_stlnk_e. &lt;sdat&gt;-stlnk_ic = 1. &quot;E = error         =&gt; red
      ENDCASE.

    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFMD" CMPNAME="SDAT_ENHA_STLNK_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: link-status text" EXPOSURE="0" STATE="1" EDITORDER="4 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_stlnk_tx.

    &quot;enhance: status text
    DATA(lo_dref) = CAST cl_abap_elemdescr( cl_abap_elemdescr=&gt;describe_by_name( p_name = &apos;ZVNDMS_STLNK&apos; ) ).
    DATA(lt_vals) = lo_dref-&gt;get_ddic_fixed_values( ).

    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      READ TABLE lt_vals ASSIGNING FIELD-SYMBOL(&lt;vals&gt;) WITH KEY low = &lt;sdat&gt;-stlnk. CHECK sy-subrc = 0.

      MOVE: &lt;vals&gt;-ddtext TO &lt;sdat&gt;-stlnk_tx.

    ENDLOOP.

  ENDMETHOD.</source>
  </method>
 </CLAS>
