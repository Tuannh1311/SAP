 <PROG NAME="ZVNDMS_IFPD_SELSC" VARCL="X" SUBC="I" APPL="*" RSTAT="K" RMAND="700" RLOAD="E" UCCHECK="X">
  <textPool>
   <language SPRAS="E">
    <textElement ID="R" ENTRY="Include ZVNDMS_IFPD_SELSC" LENGTH="25 "/>
   </language>
  </textPool>
  <source>*&amp;---------------------------------------------------------------------*
*&amp; Include          ZVNDMS_IFPD_SELSC
*&amp;---------------------------------------------------------------------*


SELECTION-SCREEN: BEGIN OF BLOCK s01 WITH FRAME TITLE TEXT-s01. &quot;block: selection (1) downloader
  PARAMETERS:     pa_run1d TYPE xfeld DEFAULT &apos; &apos;.
  SELECT-OPTIONS: so_lfile FOR zvndms_c000-lfile MATCHCODE OBJECT zvndms_c000_lfile_h.

SELECTION-SCREEN: END   OF BLOCK s01.


SELECTION-SCREEN: BEGIN OF BLOCK s02 WITH FRAME TITLE TEXT-s02. &quot;block: selection (2) linker
  PARAMETERS:     pa_run2l TYPE xfeld DEFAULT &apos; &apos;.
  SELECT-OPTIONS: so_rdate FOR zvndms_ifld-rdate,
                  so_rtime FOR zvndms_ifld-rtime,
                  so_arobj FOR zvndms_ifld-arobj,
                  so_ardid FOR zvndms_ifld-ardid,
                  so_stupl FOR zvndms_ifld-stupl,
                  so_stdel FOR zvndms_ifld-stdel,
                  so_stlnk FOR zvndms_ifld-stlnk.
