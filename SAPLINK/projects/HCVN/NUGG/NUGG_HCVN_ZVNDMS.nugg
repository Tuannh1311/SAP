  <method CLSNAME="ZCL_VNDMS_IFLD" CMPNAME="CONV_STRIP" VERSION="1" LANGU="E" DESCRIPT="convert: strip filename from filename.ext" EXPOSURE="0" STATE="1" EDITORDER="10 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="1" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNDMS_IFLD" CMPNAME="CONV_STRIP" SCONAME="IV_FNAM" VERSION="1" LANGU="E" DESCRIPT="full name: filename.ext" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="ZVNDMS_FNAME"/>
   <parameter CLSNAME="ZCL_VNDMS_IFLD" CMPNAME="CONV_STRIP" SCONAME="RV_SNAM" VERSION="1" LANGU="E" DESCRIPT="stripped name: filename (no ext)" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="ZVNDMS_FNAME"/>
   <source>METHOD conv_strip.

    &quot;strip file name from extention
    SPLIT iv_fnam AT &apos;.&apos; INTO rv_snam DATA(lv_ext).
    CONDENSE rv_snam NO-GAPS.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFLD" CMPNAME="IFLD_LINK" VERSION="1" LANGU="E" DESCRIPT="linker: link single record" EXPOSURE="0" STATE="1" EDITORDER="3 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNDMS_IFLD" CMPNAME="IFLD_LINK" SCONAME="IR_IFLD" VERSION="1" LANGU="E" DESCRIPT="DMS: DMS document log" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="ZVNDMS_IFLD"/>
   <source>METHOD ifld_link.

    &quot;link single ARfile to SAP object

    &quot;1 determine key
    me-&gt;ifld_link_okey( ir_ifld ).

    &quot;2 link to object
    me-&gt;ifld_link_save( ir_ifld ).

    &quot;3 save log
    me-&gt;ifld_link_dblg( ir_ifld ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFLD" CMPNAME="IFLD_LINK_DBLG" VERSION="1" LANGU="E" DESCRIPT="linker: log IFLD db" EXPOSURE="0" STATE="1" EDITORDER="9 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNDMS_IFLD" CMPNAME="IFLD_LINK_DBLG" SCONAME="IR_IFLD" VERSION="1" LANGU="E" DESCRIPT="DMS: DMS document log" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="ZVNDMS_IFLD"/>
   <source>METHOD ifld_link_dblg.

    &quot;save IFLD to db
    MODIFY zvndms_ifld FROM ir_ifld-&gt;*.
    COMMIT WORK.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFLD" CMPNAME="IFLD_LINK_OKEY" VERSION="1" LANGU="E" DESCRIPT="linker: determine key of SAP object" EXPOSURE="0" STATE="1" EDITORDER="4 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNDMS_IFLD" CMPNAME="IFLD_LINK_OKEY" SCONAME="IR_IFLD" VERSION="1" LANGU="E" DESCRIPT="DMS: DMS document log" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="ZVNDMS_IFLD"/>
   <source>METHOD ifld_link_okey.

    &quot;determine key
    CASE ir_ifld-&gt;sobty.

      WHEN &apos;BUS2012&apos;  &quot;Purchase Order
        OR &apos;BUS2014&apos;. &quot;Outline Agreement

        me-&gt;ifld_link_okey_ekko( ir_ifld ).

      WHEN &apos;BUS2081&apos;. &quot;Incoming Invoice

        me-&gt;ifld_link_okey_rbkp( ir_ifld ).

      WHEN &apos;BKPF&apos;   . &quot;FI document

        me-&gt;ifld_link_okey_bkpf( ir_ifld ).

    ENDCASE.

    &quot;error: no object determined
    IF ir_ifld-&gt;sobid IS INITIAL.
      ir_ifld-&gt;stlnk = zif_vndms_const=&gt;gc_stlnk_e.
      MESSAGE e002(zvndms) INTO ir_ifld-&gt;msgtx. &quot;msg{No object link determined!}
      MOVE: sy-msgid TO ir_ifld-&gt;msgid,
            sy-msgno TO ir_ifld-&gt;msgno,
            sy-msgty TO ir_ifld-&gt;msgty.
    ENDIF.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFLD" CMPNAME="IFLD_LINK_OKEY_BKPF" VERSION="1" LANGU="E" DESCRIPT="linker: determine key of FI Document" EXPOSURE="0" STATE="1" EDITORDER="7 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNDMS_IFLD" CMPNAME="IFLD_LINK_OKEY_BKPF" SCONAME="IR_IFLD" VERSION="1" LANGU="E" DESCRIPT="DMS: DMS document log" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="ZVNDMS_IFLD"/>
   <source>METHOD ifld_link_okey_bkpf.

    &quot;strip filename to Document Number
    DATA(lv_file)  = me-&gt;conv_strip( ir_ifld-&gt;fname ).
    DATA(lv_belnr) = CONV bkpf-belnr( lv_file(10) ).

    &quot;document can be provided with/without year (so NNNNNNNNNN or NNNNNNNNNNYYYY)
    &quot;..and when without year it can be from current or previous
    DATA(lv_provy) = CONV bkpf-gjahr( lv_file+10(4) ).

    &quot;§1 check document in provided year
    IF lv_provy IS NOT INITIAL.

      SELECT SINGLE bukrs, belnr, gjahr INTO @DATA(lk_bkpf) FROM bkpf WHERE bukrs = @zif_vndms_const=&gt;gc_bukrs
                                                                        AND belnr = @lv_belnr
                                                                        AND gjahr = @lv_provy .
      IF sy-subrc = 0.
        ir_ifld-&gt;sobid = lk_bkpf-bukrs &amp;&amp; lk_bkpf-belnr &amp;&amp; lk_bkpf-gjahr.
      ENDIF.
      RETURN.

    ELSE.
      &quot;§2 no year provided, can be from current or previous year

      &quot;§2.0 get current &amp; previous year
      DATA: lv_curry TYPE bkpf-gjahr,
            lv_prevy TYPE bkpf-gjahr.
      CALL FUNCTION &apos;GET_CURRENT_YEAR&apos;
        EXPORTING
          bukrs = zif_vndms_const=&gt;gc_bukrs
          date  = sy-datum
        IMPORTING
          curry = lv_curry
          prevy = lv_prevy.

      &quot;§2.1 check document in current year
      SELECT SINGLE bukrs, belnr, gjahr INTO @lk_bkpf FROM bkpf WHERE bukrs = @zif_vndms_const=&gt;gc_bukrs
                                                                  AND belnr = @lv_belnr
                                                                  AND gjahr = @lv_curry.
      IF sy-subrc = 0.
        ir_ifld-&gt;sobid = lk_bkpf-bukrs &amp;&amp; lk_bkpf-belnr &amp;&amp; lk_bkpf-gjahr.
        RETURN.
      ELSE.

        &quot;..check document in previous year
        SELECT SINGLE bukrs, belnr, gjahr INTO @lk_bkpf FROM bkpf WHERE bukrs = @zif_vndms_const=&gt;gc_bukrs
                                                                    AND belnr = @lv_belnr
                                                                    AND gjahr = @lv_prevy.
        IF sy-subrc = 0.
          ir_ifld-&gt;sobid = lk_bkpf-bukrs &amp;&amp; lk_bkpf-belnr &amp;&amp; lk_bkpf-gjahr.
        ENDIF.
      ENDIF.
    ENDIF.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFLD" CMPNAME="IFLD_LINK_OKEY_EKKO" VERSION="1" LANGU="E" DESCRIPT="linker: determine key of Purchasing Document (POrd, OAgr)" EXPOSURE="0" STATE="1" EDITORDER="5 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNDMS_IFLD" CMPNAME="IFLD_LINK_OKEY_EKKO" SCONAME="IR_IFLD" VERSION="1" LANGU="E" DESCRIPT="DMS: DMS document log" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="ZVNDMS_IFLD"/>
   <source>METHOD ifld_link_okey_ekko.

    &quot;strip filename to Purchase Document number
    DATA(lv_file)  = me-&gt;conv_strip( ir_ifld-&gt;fname ).
    DATA(lv_ebeln) = CONV ekko-ebeln( lv_file ).

    &quot;check existence
    SELECT SINGLE ebeln FROM ekko INTO @DATA(lv_xebeln) WHERE ebeln = @lv_ebeln.

    &quot;pass as key
    MOVE: lv_xebeln TO ir_ifld-&gt;sobid.



  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFLD" CMPNAME="IFLD_LINK_OKEY_RBKP" VERSION="1" LANGU="E" DESCRIPT="linker: determine key of Incoming Invoice" EXPOSURE="0" STATE="1" EDITORDER="6 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNDMS_IFLD" CMPNAME="IFLD_LINK_OKEY_RBKP" SCONAME="IR_IFLD" VERSION="1" LANGU="E" DESCRIPT="DMS: DMS document log" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="ZVNDMS_IFLD"/>
   <source>METHOD ifld_link_okey_rbkp.

    &quot;strip filename to Invoice Number
    DATA(lv_file)  = me-&gt;conv_strip( ir_ifld-&gt;fname ).
    DATA(lv_belnr) = CONV rbkp-belnr( lv_file(10) ).

    &quot;invoice can be provided with/without year (so NNNNNNNNNN or NNNNNNNNNNYYYY)
    &quot;..and when without year it can be from current or previous
    DATA(lv_provy) = CONV rbkp-gjahr( lv_file+10(4) ).

    &quot;§1 check invoice in provided year
    IF lv_provy IS NOT INITIAL.

      SELECT SINGLE belnr, gjahr INTO @DATA(lk_rbkp) FROM rbkp WHERE belnr = @lv_belnr
                                                                 AND gjahr = @lv_provy .
      IF sy-subrc = 0.
        ir_ifld-&gt;sobid = lk_rbkp-belnr &amp;&amp; lk_rbkp-gjahr.
      ENDIF.
      RETURN.

    ELSE.
      &quot;§2 no year provided, can be from current or previous year

      &quot;§2.0 get current &amp; previous year
      DATA: lv_curry TYPE bkpf-gjahr,
            lv_prevy TYPE bkpf-gjahr.
      CALL FUNCTION &apos;GET_CURRENT_YEAR&apos;
        EXPORTING
          bukrs = zif_vndms_const=&gt;gc_bukrs
          date  = sy-datum
        IMPORTING
          curry = lv_curry
          prevy = lv_prevy.

      &quot;§2.1 check Invoice in current year
      SELECT SINGLE belnr, gjahr INTO @lk_rbkp FROM rbkp WHERE belnr = @lv_belnr
                                                           AND gjahr = @lv_curry.
      IF sy-subrc = 0.
        ir_ifld-&gt;sobid = lk_rbkp-belnr &amp;&amp; lk_rbkp-gjahr.
        RETURN.
      ELSE.

        &quot;..check Invoice in previous year
        SELECT SINGLE belnr, gjahr INTO @lk_rbkp FROM rbkp WHERE belnr = @lv_belnr
                                                             AND gjahr = @lv_prevy.
        IF sy-subrc = 0.
          ir_ifld-&gt;sobid = lk_rbkp-belnr &amp;&amp; lk_rbkp-gjahr.
        ENDIF.
      ENDIF.
    ENDIF.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFLD" CMPNAME="IFLD_LINK_SAVE" VERSION="1" LANGU="E" DESCRIPT="linker: save link" EXPOSURE="0" STATE="1" EDITORDER="8 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNDMS_IFLD" CMPNAME="IFLD_LINK_SAVE" SCONAME="IR_IFLD" VERSION="1" LANGU="E" DESCRIPT="DMS: DMS document log" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="ZVNDMS_IFLD"/>
   <source>METHOD ifld_link_save.

    &quot;save determined link
    CHECK: ir_ifld-&gt;sobid IS NOT INITIAL.


    CALL FUNCTION &apos;ARCHIV_CONNECTION_INSERT&apos;
      EXPORTING
        archiv_id             = ir_ifld-&gt;acrep
        arc_doc_id            = ir_ifld-&gt;ardid
        &quot;Ar_date    = &apos; &apos;
        ar_object             = ir_ifld-&gt;arobj
        object_id             = ir_ifld-&gt;sobid
        sap_object            = ir_ifld-&gt;sobty
        filename              = CONV toaat-filename( ir_ifld-&gt;fname )
        &quot;Descr      = &apos; &apos;
        creator               = sy-uname
      EXCEPTIONS
        error_connectiontable = 1
        OTHERS                = 2.
    IF sy-subrc &lt;&gt; 0.
      ir_ifld-&gt;stlnk = zif_vndms_const=&gt;gc_stlnk_e.
      &quot;error: no connection
      MESSAGE e003(zvndms) INTO ir_ifld-&gt;msgtx. &quot;msg{Error in object linking!}
      MOVE: sy-msgid TO ir_ifld-&gt;msgid,
            sy-msgno TO ir_ifld-&gt;msgno,
            sy-msgty TO ir_ifld-&gt;msgty.
    ELSE.
      &quot;link status
      ir_ifld-&gt;stlnk = zif_vndms_const=&gt;gc_stlnk_l.

    ENDIF.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNDMS_IFLD" CMPNAME="PROCESS" VERSION="1" LANGU="E" DESCRIPT="linker: process" EXPOSURE="2" STATE="1" EDITORDER="2 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD process.

    &quot;process archived files
    LOOP AT me-&gt;at_ifld REFERENCE INTO DATA(lr_ifld).

      &quot;link single ARfile to SAP object
      me-&gt;ifld_link( ir_ifld = lr_ifld ).

    ENDLOOP.

  ENDMETHOD.</source>
  </method>
