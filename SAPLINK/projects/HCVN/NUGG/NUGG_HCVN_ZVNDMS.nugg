  <interfaceMethod CLSNAME="ZCL_VNDMS_CV_OD_ATTCH_PERSI" CPDNAME="IF_EX_CV_ODATA_ATTACH_PERSIST~DETERMINE_PERSISTENCY_TYPE">
   <source>METHOD if_ex_cv_odata_attach_persist~determine_persistency_type.

    &quot;**SAP-2887 Fiori PReq attachment into GOS (instead of DMS)
    &quot;context:
    &quot;         fiori app for preq is by default using DMS for attachments
    &quot;         see method ~FINALIZE_OBJECT_KEYS for object key handling
    &quot;feature:
    &quot;         for EBAN-DMS switch to BUS2105-GOS
    &quot;note   :
    &quot;         see (badi impl ZME_PROCESS_REQ_CUST) class~method ZCL_IM_ME_PROCESS_REQ_CUST~ADD_ATTA_TO_GOS..
    &quot;         ..where furher processing/connection of gos attachment is done
    &quot;         (this is mere first step in proper gos reconnection)
    IF cv_object_type                 = &apos;EBAN&apos; AND
       cv_attach_storage_persistency  = &apos;DMS&apos;.
      cv_object_type                  = &apos;BUS2105&apos;. &quot;BUS2105: works for sapgui-files
      cv_attach_storage_persistency   = &apos;GOS&apos;.
    ENDIF.

  ENDMETHOD.</source>
  </interfaceMethod>
  <interfaceMethod CLSNAME="ZCL_VNDMS_CV_OD_ATTCH_PERSI" CPDNAME="IF_EX_CV_ODATA_ATTACH_PERSIST~FINALIZE_OBJECT_KEYS">
   <source>METHOD if_ex_cv_odata_attach_persist~finalize_object_keys.

    &quot;**SAP-2887 Fiori PReq attachment into GOS (instead of DMS)
    &quot;context:
    &quot;         fiori app for preq is by default using DMS for attachments, switched by this badi to GOS
    &quot;         see method ~DETERMINE_PERSISTENCY_TYPE for object/type switching
    &quot;feature:
    &quot;         check PReq exist, if so, use key of only doc-num (like sapgui GOS), not doc-num+itm-num
    &quot;         (if preq doesn&apos;t exist, it means we are working with preq draft, key cannot be modified)
    &quot;note   :
    &quot;         see (badi impl ZME_PROCESS_REQ_CUST) class~method ZCL_IM_ME_PROCESS_REQ_CUST~ADD_ATTA_TO_GOS..
    &quot;         ..where furher processing/connection of gos attachment is done
    IF iv_objecttype = &apos;EBAN&apos;.
      SELECT SINGLE banfn FROM eban INTO @DATA(lv_banfn) WHERE banfn = @cv_finalized_objectkey(10) .
      IF sy-subrc = 0.
        cv_finalized_objectkey = cv_finalized_objectkey(10).
      ENDIF.
    ENDIF.

  ENDMETHOD.</source>
  </interfaceMethod>
  <interfaceMethod CLSNAME="ZCL_VNDMS_CV_OD_ATTCH_PERSI" CPDNAME="IF_EX_CV_ODATA_ATTACH_PERSIST~PERFORM_AFTER_UPLOAD_TASKS">
   <source>METHOD if_ex_cv_odata_attach_persist~perform_after_upload_tasks.
  ENDMETHOD.</source>
  </interfaceMethod>
 </CLAS>
