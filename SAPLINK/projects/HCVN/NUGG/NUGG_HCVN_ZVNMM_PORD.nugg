  <method CLSNAME="ZCL_IM_ME_PROCESS_PO_CUST" CMPNAME="VAL_HEAD_XBLCK" VERSION="1" LANGU="E" DESCRIPT="validate: Vendor blocked (via BP3100)" EXPOSURE="0" STATE="1" EDITORDER="5 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_IM_ME_PROCESS_PO_CUST" CMPNAME="VAL_HEAD_XBLCK" SCONAME="IM_HEAD" VERSION="1" LANGU="E" DESCRIPT="PO Header External View" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="IF_PURCHASE_ORDER_MM"/>
   <source>METHOD val_head_xblck.
    INCLUDE: mm_messages_mac.

    &quot;DEV_MM_96(HQ_TTa) validate: POrg Vendor block
    &quot;context:
    &quot;         with note 2893710 (104-SP02) referred (as opposed to reference) POrg data cannot be maintained in BP-master
    &quot;         since we want &amp; need POrg VN01 to server as reference POrg for VN02, we are not able to block BP on POrg level
    &quot;feature:
    &quot;         custom block check is stored in BP3100 &apos;Additional information&apos;
    &quot;         validate Vendor agains POrg and this block


    DATA(ls_dhdr) = im_head-&gt;get_data( ).
    CHECK: ls_dhdr-lifnr IS NOT INITIAL.

    &quot;get customizing
    &quot;(***TBD: add addType/dataType to customizing? will it bring anything??)


    &quot;Vendor -&gt; BuPa
    SELECT SINGLE * FROM zvnmm_bupa_vend INTO @DATA(ls_bupa) WHERE vendor = @ls_dhdr-lifnr. CHECK sy-subrc = 0.
    &quot;BuPa: additional information
    SELECT SINGLE * FROM bp3100 INTO @DATA(ls_3100) WHERE partner   = @ls_bupa-partner
                                                      AND criter    = &apos;&apos;
                                                      AND addtype   = &apos;93&apos;  &quot;93= PurchasingOrg block
                                                      AND data_type = &apos;93&apos;  &quot;93= PurchasingOrg block
                                                      AND ct_field  = @ls_dhdr-ekorg.       CHECK sy-subrc = 0.
    IF ls_3100-xfeld = &apos;X&apos;. &quot;blocked

      &quot;enh with addData descriptions
      SELECT SINGLE * FROM tpz20t INTO @DATA(ls_z20t) WHERE spras   = @sy-langu
                                                        AND addtype = &apos;93&apos;.

      mmpur_message &apos;E&apos; &apos;ZVNMM_PORD&apos; 005 ls_dhdr-lifnr ls_dhdr-ekorg ls_z20t-addtype ls_z20t-text.
      &quot;msg{Vendor &amp; is blocked for PurchasingOrg &amp;!}
      im_head-&gt;invalidate( ).

    ENDIF.



  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_IM_ME_PROCESS_PO_CUST" CMPNAME="VAL_HEAD_XFAST" VERSION="1" LANGU="E" DESCRIPT="validate: Vendor allowed for FAST Process" EXPOSURE="0" STATE="1" EDITORDER="4 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_IM_ME_PROCESS_PO_CUST" CMPNAME="VAL_HEAD_XFAST" SCONAME="IM_HEAD" VERSION="1" LANGU="E" DESCRIPT="PO Header External View" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="IF_PURCHASE_ORDER_MM"/>
   <source>METHOD val_head_xfast.
    INCLUDE: mm_messages_mac.

    &quot;DEV_MM_12(HQ_TTa) validate: Vendor allowed for FAST

    DATA(ls_dhdr) = im_head-&gt;get_data( ).
    CHECK: ls_dhdr-lifnr IS NOT INITIAL.

    &quot;get customizing
    SELECT SINGLE * FROM zvnmm_xpord_c001 INTO @DATA(ls_c001) WHERE bstyp = @ls_dhdr-bstyp
                                                                AND bsart = @ls_dhdr-bsart.
    IF sy-subrc = 0.


      &quot;Vendor -&gt; BuPa
      SELECT SINGLE * FROM zvnmm_bupa_vend INTO @DATA(ls_bupa) WHERE vendor = @ls_dhdr-lifnr. CHECK sy-subrc = 0.
      &quot;BuPa: additional information
      SELECT SINGLE * FROM bp3100 INTO @DATA(ls_3100) WHERE partner   = @ls_bupa-partner
                                                        AND criter    = &apos;&apos;
                                                        AND addtype   = @ls_c001-bpadd
                                                        AND data_type = @ls_c001-bpdat
                                                        AND xfeld     = &apos;X&apos;.
      IF sy-subrc NE 0.
        &quot;enh with addData descriptions
        SELECT SINGLE * FROM tpz20t INTO @DATA(ls_z20t) WHERE spras   = @sy-langu
                                                          AND addtype = @ls_c001-bpadd.

        mmpur_message &apos;E&apos; &apos;ZVNMM_PORD&apos; 001 ls_dhdr-lifnr ls_dhdr-bsart ls_z20t-addtype ls_z20t-text.
        &quot;msg{Vendor &amp; is not allowed for Document Type &amp;!}
        im_head-&gt;invalidate( ).

      ENDIF.

    ENDIF.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_IM_ME_PROCESS_PO_CUST" CMPNAME="VAL_ITEM_MATDT" VERSION="1" LANGU="E" DESCRIPT="validate: MatMas allowed for DocType" EXPOSURE="0" STATE="1" EDITORDER="3 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
