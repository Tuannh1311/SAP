  <method CLSNAME="ZCL_IM_ME_PROCESS_PO_CUST" CMPNAME="DER_HEAD_DEFTX" VERSION="1" LANGU="E" DESCRIPT="derive: default text (e.g. F90 Arbitration)" EXPOSURE="0" STATE="1" EDITORDER="9 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_IM_ME_PROCESS_PO_CUST" CMPNAME="DER_HEAD_DEFTX" SCONAME="IM_HEAD" VERSION="1" LANGU="E" DESCRIPT="PO Header External View" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="IF_PURCHASE_ORDER_MM"/>
   <source>METHOD der_head_deftx.

    &quot;DEV_MM_98(HQ_TTa) derive: default text F90 Arbitration
    DATA(ls_dhdr) = im_head-&gt;get_data( ).

    CHECK: ls_dhdr-ekorg IS NOT INITIAL,
           ls_dhdr-bsart IS NOT INITIAL.

    &quot;§0 get customizing
    SELECT * FROM zvnmm_xpord_c007 INTO TABLE @DATA(lt_c007) WHERE ekorg = @ls_dhdr-ekorg
                                                               AND bsart = @ls_dhdr-bsart.

    &quot;§1 text line defined in customizing
    LOOP AT lt_c007 ASSIGNING FIELD-SYMBOL(&lt;c007&gt;) WHERE tdline IS NOT INITIAL.

      im_head-&gt;if_longtexts_mm~get_text( EXPORTING im_tdid           = &lt;c007&gt;-tdid
                                         IMPORTING ex_textlines      = DATA(lt_txlin) ).

      &quot;any text: do nothing
      IF lt_txlin IS NOT INITIAL.
        CONTINUE.
      ENDIF.

      &quot;no text: set default
      IF lt_txlin IS INITIAL.
        CLEAR: lt_txlin[].
        APPEND INITIAL LINE TO lt_txlin ASSIGNING FIELD-SYMBOL(&lt;txlin&gt;).
        MOVE: &apos;EKKO&apos;        TO &lt;txlin&gt;-tdobject,
              &lt;c007&gt;-tdid   TO &lt;txlin&gt;-tdid,
              &apos;*&apos;           TO &lt;txlin&gt;-tdformat,
              &lt;c007&gt;-tdline TO &lt;txlin&gt;-tdline.
        im_head-&gt;if_longtexts_mm~set_text( EXPORTING im_tdid      = &lt;c007&gt;-tdid
                                                     im_textlines = lt_txlin     ).
      ENDIF.

    ENDLOOP.

    &quot;§2 text of Payment Term
    &quot;+2020-02-04(HQ_TTa) CBL-12177 SAP(VN) - MM - Purchase Order Form requirements
    LOOP AT lt_c007 ASSIGNING &lt;c007&gt; WHERE xt052 IS NOT INITIAL.

      im_head-&gt;if_longtexts_mm~get_text( EXPORTING im_tdid           = &lt;c007&gt;-tdid
                                         IMPORTING ex_textlines      = lt_txlin    ).

      &quot;
      im_head-&gt;get_previous_data( IMPORTING   ex_data = DATA(ls_prev)
                                  EXCEPTIONS  no_data = 1       &quot; No Data Available
                                              OTHERS  = 2         ).
      IF sy-subrc &lt;&gt; 0. CLEAR ls_prev. ENDIF.

      &quot;any text &amp; no change of PmntTerm: do nothing
      IF lt_txlin IS NOT INITIAL       AND
         ls_dhdr-zterm = ls_prev-zterm    .
        CONTINUE.
      ELSE.

        &quot;set Payment Term

        &quot;get header-PaymentTerm
        SELECT SINGLE * FROM t052u INTO @DATA(ls_t052u) WHERE spras = @ls_dhdr-spras
                                                          AND zterm = @ls_dhdr-zterm. CHECK sy-subrc = 0.
        &quot;set PmntTerm text
        CLEAR: lt_txlin[].
        APPEND INITIAL LINE TO lt_txlin ASSIGNING &lt;txlin&gt;.
        MOVE: &apos;EKKO&apos;          TO &lt;txlin&gt;-tdobject,
              &lt;c007&gt;-tdid     TO &lt;txlin&gt;-tdid,
              &apos;*&apos;             TO &lt;txlin&gt;-tdformat,
              ls_t052u-text1  TO &lt;txlin&gt;-tdline.
        im_head-&gt;if_longtexts_mm~set_text( EXPORTING im_tdid      = &lt;c007&gt;-tdid
                                                     im_textlines = lt_txlin     ).
      ENDIF.
    ENDLOOP.



  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_IM_ME_PROCESS_PO_CUST" CMPNAME="DER_HEAD_KOSTL" VERSION="1" LANGU="E" DESCRIPT="derive: CCtr of CC &amp; HD + AfNam into header (for wf)" EXPOSURE="0" STATE="1" EDITORDER="8 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_IM_ME_PROCESS_PO_CUST" CMPNAME="DER_HEAD_KOSTL" SCONAME="IM_HEAD" VERSION="1" LANGU="E" DESCRIPT="PO Header External View" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="IF_PURCHASE_ORDER_MM"/>
   <source>METHOD der_head_kostl.

    &quot;WF_MM_02(HQ_TTa) appr: Purchase Order
    &quot; feature: summarise cctrs into header for wf

    &quot;get items
    DATA(lt_ritm) = im_head-&gt;get_items( ).
    DATA: lt_ditm TYPE TABLE OF mepoitem.
    LOOP AT lt_ritm ASSIGNING FIELD-SYMBOL(&lt;ritm&gt;).
      DATA(ls_ditm) = &lt;ritm&gt;-item-&gt;get_data( ).
      CHECK ls_ditm-loekz NE &apos;L&apos;.
      APPEND ls_ditm TO lt_ditm.
    ENDLOOP.

    &quot;from item -&gt; head
    LOOP AT lt_ditm ASSIGNING FIELD-SYMBOL(&lt;ditm&gt;).
      DATA(lv_zzekko_kokrs)    = &lt;ditm&gt;-zzekpo_kokrs.
      DATA(lv_zzekko_kostl_cc) = &lt;ditm&gt;-zzekpo_kostl.
      DATA(lv_zzekko_kostl_hd) = &lt;ditm&gt;-zzekpo_kostl. &quot;(HD like CC, only summarized by HDORG)
      DATA(lv_zzekko_hdorg)    = &lt;ditm&gt;-zzekpo_hdorg.
      DATA(lv_zzekko_afnam)    = &lt;ditm&gt;-afnam.
      EXIT.
    ENDLOOP.
    &quot; clear by differences
    LOOP AT lt_ditm ASSIGNING &lt;ditm&gt;.
      IF &lt;ditm&gt;-zzekpo_kostl NE lv_zzekko_kostl_cc. CLEAR lv_zzekko_kostl_cc. ENDIF.
      IF &lt;ditm&gt;-zzekpo_hdorg NE lv_zzekko_hdorg   . CLEAR lv_zzekko_kostl_hd. ENDIF.
      IF &lt;ditm&gt;-afnam        NE lv_zzekko_afnam   . CLEAR lv_zzekko_afnam   . ENDIF.
    ENDLOOP.

    &quot;set to header
    DATA(ls_dhdr) = im_head-&gt;get_data( ).
    IF ls_dhdr-zzekko_kokrs    NE lv_zzekko_kokrs    OR
       ls_dhdr-zzekko_kostl_cc NE lv_zzekko_kostl_cc OR
       ls_dhdr-zzekko_kostl_hd NE lv_zzekko_kostl_hd OR
       ls_dhdr-zzekko_afnam    NE lv_zzekko_afnam      .
      MOVE: lv_zzekko_kokrs    TO ls_dhdr-zzekko_kokrs,
            lv_zzekko_kostl_cc TO ls_dhdr-zzekko_kostl_cc,
            lv_zzekko_kostl_hd TO ls_dhdr-zzekko_kostl_hd,
            lv_zzekko_afnam    TO ls_dhdr-zzekko_afnam.

      im_head-&gt;set_data( im_data = ls_dhdr ).

    ENDIF.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_IM_ME_PROCESS_PO_CUST" CMPNAME="DER_HEAD_XDIFF" VERSION="1" LANGU="E" DESCRIPT="derive: XDiff difference in PReq-POrd value" EXPOSURE="0" STATE="1" EDITORDER="7 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_IM_ME_PROCESS_PO_CUST" CMPNAME="DER_HEAD_XDIFF" SCONAME="IM_HEAD" VERSION="1" LANGU="E" DESCRIPT="PO Header External View" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="IF_PURCHASE_ORDER_MM"/>
   <source>METHOD der_head_xdiff.
    INCLUDE: mm_messages_mac.

    &quot;WF_MM_02(HQ_TTa) appr: Purchase Order
    &quot; context:
    &quot;          pord wf has more approval steps in case price differs from preq
    &quot; feature:
    &quot;          compare preq-to-pord price, set flag xdiff (later used in wf)


    &quot;§0 get customizing
    DATA(ls_dhdr) = im_head-&gt;get_data( ).
    SELECT SINGLE * FROM zvnmm_xpord_c005 INTO @DATA(ls_c005) WHERE bsart = @ls_dhdr-bsart
                                                                AND ekorg = @ls_dhdr-ekorg.
    CHECK: sy-subrc = 0,
           ( ls_c005-dfprc &lt;&gt; 0 OR
             ls_c005-dfamt &lt;&gt; 0   ).


    &quot;§1.1 get pord items / acc-asgn
    DATA: lt_ditm TYPE TABLE OF mepoitem,
          lt_dacc TYPE TABLE OF mepoaccounting.
    DATA(lt_ritm) = im_head-&gt;get_items( ).

    LOOP AT lt_ritm ASSIGNING FIELD-SYMBOL(&lt;ritm&gt;).
      DATA(ls_ditm) = &lt;ritm&gt;-item-&gt;get_data( ).
      APPEND ls_ditm TO lt_ditm.
      &quot;acc-asgn
      DATA(lt_racc) = &lt;ritm&gt;-item-&gt;get_accountings( ).
      LOOP AT lt_racc ASSIGNING FIELD-SYMBOL(&lt;racc&gt;).
        DATA(ls_dacc) = &lt;racc&gt;-accounting-&gt;get_data( ).
        APPEND ls_dacc TO lt_dacc.
      ENDLOOP.
    ENDLOOP.
    &quot;remove deleted
    LOOP AT lt_ditm ASSIGNING FIELD-SYMBOL(&lt;ditm&gt;) WHERE loekz = &apos;L&apos;.
      DELETE lt_dacc WHERE ebelp = &lt;ditm&gt;-ebelp.
    ENDLOOP.
    DELETE: lt_ditm WHERE loekz = &apos;L&apos;,
            lt_dacc WHERE loekz = &apos;X&apos;.


    &quot;§1.2 get preq items
    CHECK lt_ditm IS NOT INITIAL.
    SELECT banfn,
           bnfpo,
           waers,
           rlwrt  &quot;= total value upon release
      FROM eban INTO TABLE @DATA(lt_eban)
      FOR ALL ENTRIES IN @lt_ditm
     WHERE banfn = @lt_ditm-banfn
       AND bnfpo = @lt_ditm-bnfpo.


    &quot;conv currencies (all pord to be in cust-curr, on pord-date)
    IF ls_dhdr-waers &lt;&gt; ls_c005-waers.
      LOOP AT lt_ditm ASSIGNING &lt;ditm&gt; WHERE effwr &lt;&gt; 0.
        CALL FUNCTION &apos;CONVERT_TO_LOCAL_CURRENCY&apos;
          EXPORTING
            date             = ls_dhdr-bedat
            foreign_amount   = &lt;ditm&gt;-effwr
            foreign_currency = ls_dhdr-waers
            local_currency   = ls_c005-waers
          IMPORTING
            local_amount     = &lt;ditm&gt;-effwr
          EXCEPTIONS
            no_rate_found    = 1
            overflow         = 2
            no_factors_found = 3
            no_spread_found  = 4
            derived_2_times  = 5
            OTHERS           = 6.
        IF sy-subrc &lt;&gt; 0.
          MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4.
        ELSE.
        ENDIF.
      ENDLOOP.
    ENDIF.


    &quot;conv currencies (all preqs to be in cust-curr, on pord-date)
    LOOP AT lt_eban ASSIGNING FIELD-SYMBOL(&lt;eban&gt;) WHERE waers &lt;&gt; ls_c005-waers
                                                     AND rlwrt &lt;&gt; 0.
      CALL FUNCTION &apos;CONVERT_TO_LOCAL_CURRENCY&apos;
        EXPORTING
          date             = ls_dhdr-bedat
          foreign_amount   = &lt;eban&gt;-rlwrt
          foreign_currency = &lt;eban&gt;-waers
          local_currency   = ls_c005-waers
        IMPORTING
          local_amount     = &lt;eban&gt;-rlwrt
        EXCEPTIONS
          no_rate_found    = 1
          overflow         = 2
          no_factors_found = 3
          no_spread_found  = 4
          derived_2_times  = 5
          OTHERS           = 6.
      IF sy-subrc &lt;&gt; 0.
        MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4.
      ELSE.
        MOVE: ls_c005-waers TO &lt;eban&gt;-waers.
      ENDIF.

    ENDLOOP.


    &quot;§ compare diffs
    DATA: lv_zzxdiff TYPE ekko-zzekko_xdiff.

    &quot;§ no preq -&gt; always diff
    IF lt_eban[] IS INITIAL.
      lv_zzxdiff = &apos;X&apos;.
    ENDIF.

    &quot;§ compare on item level: check diff **%
    LOOP AT lt_ditm ASSIGNING &lt;ditm&gt;.
      READ TABLE lt_eban ASSIGNING &lt;eban&gt; WITH KEY banfn = &lt;ditm&gt;-banfn
                                                   bnfpo = &lt;ditm&gt;-bnfpo. CHECK sy-subrc = 0.
      CHECK: &lt;ditm&gt;-effwr IS NOT INITIAL,
             &lt;eban&gt;-rlwrt IS NOT INITIAL.

      DATA(lv_dfprc) = ( &lt;ditm&gt;-effwr - &lt;eban&gt;-rlwrt ) / &lt;eban&gt;-rlwrt * 100.

      CHECK lv_dfprc &gt; 0.

      IF ls_c005-dfprc IS NOT INITIAL AND
         lv_dfprc &gt;= ls_c005-dfprc       .
        MOVE: &apos;X&apos; TO lv_zzxdiff,
              &apos;X&apos; TO &lt;ditm&gt;-zzekpo_xdiff.
      ENDIF.
    ENDLOOP.


    &quot;§ compare on cctr level: check diff **Mđ
    DATA: lt_sacc TYPE TABLE OF zvnmm_pord_sacc1.

    &quot;§ summ acc asgn to cctr-level
    LOOP AT lt_ditm ASSIGNING &lt;ditm&gt;.

      &quot;add POrd
      READ TABLE lt_dacc ASSIGNING FIELD-SYMBOL(&lt;dacc&gt;) WITH KEY ebelp = &lt;ditm&gt;-ebelp. CHECK sy-subrc = 0.
      READ TABLE lt_sacc ASSIGNING FIELD-SYMBOL(&lt;sacc&gt;) WITH KEY kostl = &lt;dacc&gt;-kostl.
      IF sy-subrc = 0. &quot;exist: add
        ADD &lt;ditm&gt;-effwr TO &lt;sacc&gt;-effwr.
      ELSE.            &quot;non-ex: create
        APPEND INITIAL LINE TO lt_sacc ASSIGNING &lt;sacc&gt;.
        MOVE: &lt;ditm&gt;-ebeln  TO &lt;sacc&gt;-ebeln,
              &lt;dacc&gt;-kostl  TO &lt;sacc&gt;-kostl,
              &lt;ditm&gt;-effwr  TO &lt;sacc&gt;-effwr,
              ls_c005-waers TO &lt;sacc&gt;-waers.
      ENDIF.

      &quot;add PReq
      READ TABLE lt_eban ASSIGNING &lt;eban&gt; WITH KEY banfn = &lt;ditm&gt;-banfn
                                                   bnfpo = &lt;ditm&gt;-bnfpo.
      IF sy-subrc = 0.
        ADD &lt;eban&gt;-rlwrt TO &lt;sacc&gt;-rlwrt.
      ENDIF.
    ENDLOOP.

    &quot;check cctr-diffs
    LOOP AT lt_sacc ASSIGNING &lt;sacc&gt;.
      DATA(lv_dfamt) = &lt;sacc&gt;-effwr - &lt;sacc&gt;-rlwrt.
      IF ls_c005-dfamt IS NOT INITIAL AND
         lv_dfamt &gt;= ls_c005-dfamt     .
        MOVE: &apos;X&apos; TO lv_zzxdiff,
              &apos;X&apos; TO &lt;ditm&gt;-zzekpo_xdiff.
      ENDIF.
    ENDLOOP.


    &quot;§ set XDIFF to pord-header
    ls_dhdr = im_head-&gt;get_data( ).
    IF lv_zzxdiff &lt;&gt; ls_dhdr-zzekko_xdiff.
      MOVE: lv_zzxdiff TO ls_dhdr-zzekko_xdiff.
      im_head-&gt;set_data( im_data = ls_dhdr ).
      &quot;notify user
      IF lv_zzxdiff = &apos;X&apos;.
        mmpur_message &apos;W&apos; &apos;ZVNMM_PORD&apos; 004 0 0 0 0.
      ENDIF.
    ENDIF.




  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_IM_ME_PROCESS_PO_CUST" CMPNAME="DER_ITEM_AFNTX" VERSION="1" LANGU="E" DESCRIPT="derive: AfNam Requisitioner&apos;s name" EXPOSURE="0" STATE="1" EDITORDER="11 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_IM_ME_PROCESS_PO_CUST" CMPNAME="DER_ITEM_AFNTX" SCONAME="IM_ITEM" VERSION="1" LANGU="E" DESCRIPT="PO Item External View" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="IF_PURCHASE_ORDER_ITEM_MM"/>
   <source>METHOD der_item_afntx.

    &quot;derive AFNAM_TX Requisitioners name
    DATA(ls_ditm) = im_item-&gt;get_data( ).

    im_item-&gt;get_previous_data( IMPORTING ex_data = DATA(ls_prev)
                               EXCEPTIONS no_data = 1
                                          OTHERS  = 2                 ).
    &quot;(no data not to be checked, diffs anyway)

    &quot;AFNAM change
    IF ls_ditm-afnam NE ls_prev-afnam OR
       ls_ditm-zzekpo_afnam_tx IS INITIAL.
      &quot;..det AfNam_tx
      ls_ditm-afnam           = zvnmm_util=&gt;afnam_to_pernr( iv_afnam = ls_ditm-afnam ).
      ls_ditm-zzekpo_afnam_tx = zvnmm_util=&gt;afnam_to_afnam_tx( iv_afnam = ls_ditm-afnam ).
      &quot;..set AfNam_tx
      im_item-&gt;set_data( im_data = ls_ditm ).
    ENDIF.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_IM_ME_PROCESS_PO_CUST" CMPNAME="DER_ITEM_FCOPY" VERSION="1" LANGU="E" DESCRIPT="derive: item std-copy fields (to POrd grid item overview)" EXPOSURE="0" STATE="1" EDITORDER="15 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_IM_ME_PROCESS_PO_CUST" CMPNAME="DER_ITEM_FCOPY" SCONAME="IM_ITEM" VERSION="1" LANGU="E" DESCRIPT="PO Item External View" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="IF_PURCHASE_ORDER_ITEM_MM"/>
   <source>METHOD der_item_fcopy.

    &quot;DEV_MM_94(HQ_TTa) derrive: item standard fields to custom
    &quot;                           (allow in pord-grid item overview)

    DATA(ls_ditm) = im_item-&gt;get_data( ).

    IF ls_ditm-netwr NE ls_ditm-zzekpo_netwr2 OR
       ls_ditm-brtwr NE ls_ditm-zzekpo_brtwr2 OR
       ls_ditm-effwr NE ls_ditm-zzekpo_effwr2 OR
       ls_ditm-navnw NE ls_ditm-zzekpo_navnw2  .

      MOVE: ls_ditm-netwr TO ls_ditm-zzekpo_netwr2,
            ls_ditm-brtwr TO ls_ditm-zzekpo_brtwr2,
            ls_ditm-effwr TO ls_ditm-zzekpo_effwr2,
            ls_ditm-navnw TO ls_ditm-zzekpo_navnw2  .

      im_item-&gt;set_data( im_data = ls_ditm ).

    ENDIF.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_IM_ME_PROCESS_PO_CUST" CMPNAME="DER_ITEM_GRIND" VERSION="1" LANGU="E" DESCRIPT="derive: item indicators GR/GR non-valuated" EXPOSURE="0" STATE="1" EDITORDER="14 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_IM_ME_PROCESS_PO_CUST" CMPNAME="DER_ITEM_GRIND" SCONAME="IM_ITEM" VERSION="1" LANGU="E" DESCRIPT="PO Item External View" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="IF_PURCHASE_ORDER_ITEM_MM"/>
   <source>METHOD der_item_grind.

    &quot;CBL-13397(HQ_TTa) derrive: item GR/GR-non-val indicators
    &quot;(bypass standard limitation of having the GR/GRnv inds predefined on AccAsgn level..
    &quot; ..missing definition per DocType etc                                               )

    DATA(ls_ditm) = im_item-&gt;get_data( ).
    DATA(lr_head) = im_item-&gt;get_header( ).
    DATA(ls_dhdr) = lr_head-&gt;get_data( ).

    SELECT SINGLE * FROM zvnmm_xpord_c008 INTO @DATA(ls_c008) WHERE ekorg = @ls_dhdr-ekorg
                                                                AND bsart = @ls_dhdr-bsart
                                                                AND knttp = @ls_ditm-knttp. CHECK sy-subrc = 0.

    CHECK: ls_ditm-wepos NE ls_c008-wepos OR
           ls_ditm-weunb NE ls_c008-weunb   .

    MOVE: ls_c008-wepos TO ls_ditm-wepos,
          ls_c008-weunb TO ls_ditm-weunb.

    im_item-&gt;set_data( im_data = ls_ditm ).



  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_IM_ME_PROCESS_PO_CUST" CMPNAME="DER_ITEM_HDORG" VERSION="1" LANGU="E" DESCRIPT="derive: Head of Division (Division) from CCtr (for wf)" EXPOSURE="0" STATE="1" EDITORDER="13 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_IM_ME_PROCESS_PO_CUST" CMPNAME="DER_ITEM_HDORG" SCONAME="IM_ITEM" VERSION="1" LANGU="E" DESCRIPT="PO Item External View" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="IF_PURCHASE_ORDER_ITEM_MM"/>
   <source>METHOD der_item_hdorg.

    &quot;WF_MM_02(HQ_TTa) appr: Purchase Order
    &quot; feature:
    &quot;          derive HD Division from CCtr

    DATA(ls_ditm) = im_item-&gt;get_data( ).

    CHECK: ls_ditm-zzekpo_kostl IS NOT INITIAL.

    &quot;cctr us-owner from cctr
    SELECT SINGLE * FROM csks INTO @DATA(ls_csks) WHERE kokrs =  @ls_ditm-zzekpo_kokrs
                                                    AND kostl =  @ls_ditm-zzekpo_kostl
                                                    AND datbi &gt;= @sy-datum.               CHECK sy-subrc = 0.


    &quot; path: user to all upper org-units
    DATA: lt_usoo TYPE TABLE OF swhactor.
    CALL FUNCTION &apos;RH_STRUC_GET&apos;
      EXPORTING
        act_otype       = &apos;US&apos;
        act_objid       = ls_csks-verak_user
        act_wegid       = &apos;ZWFUS_OO&apos;
        authority_check = &apos;&apos;
      TABLES
        result_tab      = lt_usoo
      EXCEPTIONS
        no_plvar_found  = 1
        no_entry_found  = 2
        OTHERS          = 3.
    IF sy-subrc &lt;&gt; 0. RETURN.  ENDIF.

    &quot; by-attr: from org-unit, find attr 2= Division (Division Head)
    LOOP AT lt_usoo ASSIGNING FIELD-SYMBOL(&lt;usso&gt;) WHERE otype = &apos;O&apos;.
      &quot;(via single selects, FAE needs exact type matching)
      SELECT SINGLE * FROM /sappce/hrp1961 INTO @DATA(ls_h1916)
        WHERE plvar = &apos;01&apos;
          AND otype = &apos;O&apos;
          AND objid = @&lt;usso&gt;-objid(8)
          AND subty = &apos;&apos;
          AND istat = 1
          AND begda &lt;= @sy-datum
          AND endda &gt;= @sy-datum
          AND varyf = &apos;&apos;
          AND seqnr = &apos;000&apos;
          AND categ = &apos;2&apos;.      &quot;2= Division (Division Head)
      IF sy-subrc = 0. EXIT. ENDIF. &quot;(one is enough)
    ENDLOOP.

    CHECK ls_h1916 IS NOT INITIAL.

    &quot;if changed, pass to item
    IF ls_h1916-objid NE ls_ditm-zzekpo_hdorg.
      MOVE: ls_h1916-objid TO ls_ditm-zzekpo_hdorg.
      im_item-&gt;set_data( im_data = ls_ditm ).

    ENDIF.





  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_IM_ME_PROCESS_PO_CUST" CMPNAME="DER_ITEM_KONNR" VERSION="1" LANGU="E" DESCRIPT="derive: OAgr number from PReq to POrd (for T161-BSAKZ= R)" EXPOSURE="0" STATE="1" EDITORDER="10 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_IM_ME_PROCESS_PO_CUST" CMPNAME="DER_ITEM_KONNR" SCONAME="IM_ITEM" VERSION="1" LANGU="E" DESCRIPT="PO Item External View" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="IF_PURCHASE_ORDER_ITEM_MM"/>
   <source>METHOD der_item_konnr.

    &quot;set KONNR Contract
    &quot;(since we want to have PReq doc-type NB with T161-BSAKZ= R &apos;Purchase requisition for outline agreement&apos;..
    &quot; ..in order to allow consumption of preq via oagr creation, we also need to cater for case when NB is processed..
    &quot; ..directly into POrd; in such case, PReq assigned OAgr should be take over into POrd item                       )

    &quot;check if previous data
    DATA(ls_ditm) = im_item-&gt;get_data( ).
    im_item-&gt;get_previous_data( IMPORTING  ex_data = DATA(lo_prev)
                                EXCEPTIONS no_data = 1
                                           OTHERS  = 2            ).
    IF sy-subrc &lt;&gt; 0 OR lo_prev IS INITIAL OR &quot;no previous data: initial processing..
      ( lo_prev-banfn NE ls_ditm-banfn AND    &quot;..or change of preq
        lo_prev-bnfpo NE ls_ditm-bnfpo    ).
      &quot;reference to preq..
      IF ls_ditm-banfn IS NOT INITIAL.
        &quot;..get preq contract assignment
        SELECT SINGLE * FROM eban INTO @DATA(ls_eban) WHERE banfn = @ls_ditm-banfn
                                                        AND bnfpo = @ls_ditm-bnfpo.
        &quot;set contract from preq to pord
        IF sy-subrc = 0.
          MOVE: ls_eban-konnr TO ls_ditm-konnr,
                ls_eban-ktpnr TO ls_ditm-ktpnr.
        ELSE.
          CLEAR: ls_ditm-konnr,
                 ls_ditm-ktpnr.
        ENDIF.
      ELSE. &quot;(no preq ref, initial)
        CLEAR: ls_ditm-konnr,
               ls_ditm-ktpnr.
      ENDIF.
      im_item-&gt;set_data( im_data = ls_ditm ).
    ELSE.
      &quot;previous data exist: do nothing
      RETURN.
    ENDIF.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_IM_ME_PROCESS_PO_CUST" CMPNAME="DER_ITEM_KOSTL" VERSION="1" LANGU="E" DESCRIPT="derive: Cost Centre from ekkn-&gt;ekpo (for wf)" EXPOSURE="0" STATE="1" EDITORDER="12 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_IM_ME_PROCESS_PO_CUST" CMPNAME="DER_ITEM_KOSTL" SCONAME="IM_ITEM" VERSION="1" LANGU="E" DESCRIPT="PO Item External View" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="IF_PURCHASE_ORDER_ITEM_MM"/>
   <source>METHOD der_item_kostl.

    &quot;WF_MM_02(HQ_TTa) appr: Purchase Order
    &quot; feature:
    &quot;          propagate CCtr from acc-asgn to item

    DATA(ls_ditm) = im_item-&gt;get_data( ).

    DATA(lt_racc) = im_item-&gt;get_accountings( ).

    LOOP AT lt_racc ASSIGNING FIELD-SYMBOL(&lt;racc&gt;).
      DATA(ls_dacc) = &lt;racc&gt;-accounting-&gt;get_data( ).
      CHECK ls_dacc-loekz IS INITIAL.

      IF ls_dacc-kostl NE ls_ditm-zzekpo_kostl OR
         ls_dacc-kokrs NE ls_ditm-zzekpo_kokrs   .
        MOVE: ls_dacc-kostl TO ls_ditm-zzekpo_kostl,
              ls_dacc-kokrs TO ls_ditm-zzekpo_kokrs.
        im_item-&gt;set_data( im_data = ls_ditm ).

      ENDIF.
      EXIT. &quot;one is enough

    ENDLOOP.
    &quot;(+CBL-12417 Inventory Management): derive from Requestor
    IF sy-subrc NE 0.
      SELECT SINGLE * FROM pa0001 INTO @DATA(ls_pa01) WHERE pernr =  @ls_ditm-afnam
                                                        AND subty =   &apos;&apos;
                                                        AND objps =   &apos;&apos;
                                                        AND sprps =   &apos;&apos;
                                                        AND begda &lt;= @sy-datum
                                                        AND endda &gt;= @sy-datum
                                                        AND seqnr =   000.
      MOVE: ls_pa01-kostl TO ls_ditm-zzekpo_kostl,
            ls_pa01-kokrs TO ls_ditm-zzekpo_kokrs.
      im_item-&gt;set_data( im_data = ls_ditm ).
    ENDIF.




  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_IM_ME_PROCESS_PO_CUST" CMPNAME="EVT_POST_DELCOMPLETED" VERSION="1" LANGU="E" DESCRIPT="event: Delivery Completed (for qual-eval)" EXPOSURE="0" STATE="1" EDITORDER="17 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_IM_ME_PROCESS_PO_CUST" CMPNAME="EVT_POST_DELCOMPLETED" SCONAME="IM_HEAD" VERSION="1" LANGU="E" DESCRIPT="PO Header External View" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="IF_PURCHASE_ORDER_MM"/>
   <source>METHOD evt_post_delcompleted.

    &quot;WF_MM_09(HQ_TTa) enh: Quality Evaluation
    &quot;context:
    &quot;         qual-eval is entered by Requestor after DeliveryCompleted..
    &quot;         ..requested by workflow
    &quot;feature:
    &quot;         if ekpo-ELIKZ DelComp is set, raise event
    &quot;(note   : event also raised from goods-receipt)

    &quot;get items
    DATA(lt_ritm) = im_head-&gt;get_items( ).

    &quot;per each item
    LOOP AT lt_ritm ASSIGNING FIELD-SYMBOL(&lt;ritm&gt;).
      DATA(ls_ditm) = &lt;ritm&gt;-item-&gt;get_data( ).

      &quot;if elikz currently set..
      CHECK ls_ditm-elikz = &apos;X&apos;.

      &quot;..get previous data
      &lt;ritm&gt;-item-&gt;get_persistent_data( IMPORTING  ex_data = DATA(ls_pitm)
                                        EXCEPTIONS no_data = 1
                                                   OTHERS  = 2       ).
      IF sy-subrc &lt;&gt; 0.
        &quot;..no pers, yet del-compl; this makes no sense
        CONTINUE.
      ENDIF.

      &quot;..if changed/set
      IF ls_ditm-elikz = &apos;X&apos; AND ls_pitm-elikz = &apos; &apos;.
        &quot;..then raise event
        DATA(lr_eval) = NEW zvnmm_wfrl_eval( iv_ebeln = ls_ditm-ebeln
                                             iv_ebelp = ls_ditm-ebelp ).
        lr_eval-&gt;raise_delcompl( ).

      ENDIF.


    ENDLOOP.



  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_IM_ME_PROCESS_PO_CUST" CMPNAME="REL_POST_RESET_BY_PROCSTAT" VERSION="1" LANGU="E" DESCRIPT="release: reset release when Rejected" EXPOSURE="0" STATE="1" EDITORDER="16 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_IM_ME_PROCESS_PO_CUST" CMPNAME="REL_POST_RESET_BY_PROCSTAT" SCONAME="IM_HEAD" VERSION="1" LANGU="E" DESCRIPT="PO Header External View" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="IF_PURCHASE_ORDER_MM"/>
   <source>METHOD rel_post_reset_by_procstat.

    &quot;WF_MM_02(HQ_TTa) appr: Purchase Order
    &quot;                 full reset by 1st relco when rejected

    &quot;§0 check customizing
    DATA(ls_dhdr) = im_head-&gt;get_data( ).

    SELECT SINGLE * FROM zvnmm_xpord_c006 INTO @DATA(ls_c006) WHERE bsart = @ls_dhdr-bsart.
    CHECK: sy-subrc      = 0  ,
           ls_c006-xrstb = &apos;X&apos;.

    &quot;check change
    im_head-&gt;get_persistent_data( IMPORTING  ex_data = DATA(ls_phdr)
                                  EXCEPTIONS no_data = 1
                                             OTHERS  = 2         ).  CHECK sy-subrc = 0.
    CHECK: ls_dhdr-procstat = &apos;08&apos;,               &quot;08= Rejected
           ls_dhdr-procstat &lt;&gt; ls_phdr-procstat.  &quot;changed

    &quot;ugh, no if_release here??
    &quot;**DEMO DEMO NA TO

    IF ls_dhdr-frgzu(1) = &apos;X&apos;.

      DATA: lt_ret   TYPE TABLE OF bapireturn,
            ls_rpost TYPE bapirlcopo.

      CALL FUNCTION &apos;BAPI_PO_GETRELINFO&apos;
        EXPORTING
          purchaseorder          = ls_dhdr-ebeln
        IMPORTING
          release_already_posted = ls_rpost.
      CHECK ls_rpost-rel_code1 IS NOT INITIAL.

      &quot;do under SAP_WFRT, with full authorisation
      DATA(lv_uname) = sy-uname.
      sy-uname = zvnmm_util=&gt;gc_sap_wfrt.

      CALL FUNCTION &apos;BAPI_PO_RESET_RELEASE&apos;
        EXPORTING
          purchaseorder            = ls_dhdr-ebeln
          po_rel_code              = ls_rpost-rel_code1
        TABLES
          return                   = lt_ret
        EXCEPTIONS
          authority_check_fail     = 1
          document_not_found       = 2
          enqueue_fail             = 3
          prerequisite_fail        = 4
          release_already_posted   = 5
          responsibility_fail      = 6
          no_release_already       = 7
          no_new_release_indicator = 8
          OTHERS                   = 9.
      IF sy-subrc &lt;&gt; 0.
        MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4.
      ENDIF.

      sy-uname = lv_uname.

      ls_dhdr = im_head-&gt;get_data( ).


    ENDIF.



  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_IM_ME_PROCESS_PO_CUST" CMPNAME="VAL_CHCK_EXPVL" VERSION="1" LANGU="E" DESCRIPT="validate: Vendor&apos;s expected Purchasing Volume" EXPOSURE="0" STATE="1" EDITORDER="6 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_IM_ME_PROCESS_PO_CUST" CMPNAME="VAL_CHCK_EXPVL" SCONAME="IM_HEADER" VERSION="1" LANGU="E" DESCRIPT="PO Header External View" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="IF_PURCHASE_ORDER_MM"/>
   <parameter CLSNAME="ZCL_IM_ME_PROCESS_PO_CUST" CMPNAME="VAL_CHCK_EXPVL" SCONAME="CH_FAILED" VERSION="1" LANGU="E" DESCRIPT="check failed" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="2" PARPASSTYP="1" TYPTYPE="1" TYPE="MMPUR_BOOL"/>
   <source>METHOD val_chck_expvl.
    INCLUDE: mm_messages_mac.

    &quot;DEV_MM_13(HQ_TTa) validate: Vendor&apos;s expected Purchasing Volume

    DATA(ls_dhdr) = im_header-&gt;get_data( ).
    CHECK ls_dhdr-lifnr IS NOT INITIAL.

    &quot;check customizing active
    SELECT SINGLE * FROM zvnmm_xpord_c002 INTO @DATA(ls_c002) WHERE bstyp = @ls_dhdr-bstyp
                                                                AND bsart = @ls_dhdr-bsart.
    CHECK sy-subrc = 0. &quot;no customizing

    &quot;Vendor-&gt;BuPa
    SELECT SINGLE * FROM zvnmm_bupa_vend INTO @DATA(ls_vend) WHERE vendor = @ls_dhdr-lifnr. ASSERT sy-subrc = 0.


    &quot;BuPa: add data
    SELECT * FROM bp3100 INTO TABLE @DATA(lt_3100) WHERE partner   =  @ls_vend-partner
                                                     AND criter    =  &apos;&apos;
                                                     AND addtype   =  @ls_c002-bpadd
                                                     AND data_type =  @ls_c002-bpdat
                                                     AND xfeld     =  &apos;X&apos;
                                                     AND datefr    &lt;= @ls_dhdr-bedat
                                                     AND dateto    &gt;= @ls_dhdr-bedat.
    CHECK sy-subrc = 0. &quot;no bupa data

    &quot;meantime, calc pord total
    DATA: lv_tdoc TYPE ekko-ktwrt.
    DATA(lt_ritm) = im_header-&gt;get_items( ).
    LOOP AT lt_ritm ASSIGNING FIELD-SYMBOL(&lt;ritm&gt;).
      DATA(ls_ditm) = &lt;ritm&gt;-item-&gt;get_data( ).
      ADD ls_ditm-netwr TO lv_tdoc.
    ENDLOOP.

    &quot;1st, check 2E-error (so we can skip warning, if failed)
    READ TABLE lt_3100 ASSIGNING FIELD-SYMBOL(&lt;312e&gt;) WITH KEY ct_field = &apos;2E&apos;.
    IF sy-subrc = 0.
      DATA(lv_tall) = zvnmm_util=&gt;calc_vend_tvol( iv_lifnr = ls_dhdr-lifnr
                                                 iv_waers = &lt;312e&gt;-curr
                                                 iv_excld = ls_dhdr-ebeln
                                                 iv_datfr = &lt;312e&gt;-datefr
                                                 iv_datto = &lt;312e&gt;-dateto   ).

      IF ( lv_tall + lv_tdoc ) &gt; &lt;312e&gt;-amnt. &quot;treshold reached: error
        DATA: lv_camt TYPE char20.
        WRITE: &lt;312e&gt;-amnt TO lv_camt CURRENCY &lt;312e&gt;-curr.
        mmpur_message &apos;E&apos; &apos;ZVNMM_PORD&apos; 002 lv_camt &lt;312e&gt;-curr &lt;312e&gt;-datefr &lt;312e&gt;-dateto .
        MOVE: &apos;X&apos; TO ch_failed.
        RETURN.

      ENDIF.
    ENDIF.

    &quot;2nd, check 1W-warning
    READ TABLE lt_3100 ASSIGNING FIELD-SYMBOL(&lt;311w&gt;) WITH KEY ct_field = &apos;1W&apos;.
    IF sy-subrc = 0.
      IF NOT ( &lt;312e&gt; IS ASSIGNED             AND
               &lt;312e&gt;-curr   EQ &lt;311w&gt;-curr   AND       &quot;volume recalc only if diffent pars..
               &lt;312e&gt;-datefr EQ &lt;311w&gt;-datefr AND       &quot;..bit of perf-opti
               &lt;312e&gt;-dateto EQ &lt;311w&gt;-dateto     ).
        lv_tall = zvnmm_util=&gt;calc_vend_tvol( iv_lifnr = ls_dhdr-lifnr
                                              iv_waers = &lt;311w&gt;-curr
                                              iv_excld = ls_dhdr-ebeln
                                              iv_datfr = &lt;311w&gt;-datefr
                                              iv_datto = &lt;311w&gt;-dateto   ).
      ENDIF.
      IF ( lv_tall + lv_tdoc ) &gt; &lt;311w&gt;-amnt. &quot;treshold reached: error
        WRITE: &lt;311w&gt;-amnt TO lv_camt CURRENCY &lt;311w&gt;-curr.
        mmpur_message &apos;W&apos; &apos;ZVNMM_PORD&apos; 003 lv_camt &lt;311w&gt;-curr &lt;311w&gt;-datefr &lt;311w&gt;-dateto .
        RETURN.

      ENDIF.
    ENDIF.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_IM_ME_PROCESS_PO_CUST" CMPNAME="VAL_CHCK_PGORG" VERSION="1" LANGU="E" DESCRIPT="validate: PGrp allowed for POrg" EXPOSURE="0" STATE="1" EDITORDER="1 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_IM_ME_PROCESS_PO_CUST" CMPNAME="VAL_CHCK_PGORG" SCONAME="IM_HEADER" VERSION="1" LANGU="E" DESCRIPT="PO Header External View" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="IF_PURCHASE_ORDER_MM"/>
