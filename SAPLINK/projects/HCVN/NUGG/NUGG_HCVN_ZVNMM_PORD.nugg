  <interfaceMethod CLSNAME="ZCL_VNMM_ME_BAPI_PO_CUST" CPDNAME="IF_EX_ME_BAPI_PO_CREATE_02~MAP2E_EXTENSIONOUT">
   <source>METHOD if_ex_me_bapi_po_create_02~map2e_extensionout.

    CASE im_name.
      WHEN &apos;CI_EKKODB&apos;.
        &quot;nothing, standard ok

      WHEN &apos;CI_EKPODB&apos;.
        &quot;map packed-curr-fields to text
        DATA: lr_struct TYPE REF TO cl_abap_structdescr.
        lr_struct ?= cl_abap_typedescr=&gt;describe_by_data( p_data = im_struc ).
        CHECK lr_struct-&gt;absolute_name = &apos;\TYPE=BAPI_TE_MEPOITEM&apos;.

        FIELD-SYMBOLS: &lt;mepoitem&gt; TYPE bapi_te_mepoitem.
        ASSIGN im_struc TO &lt;mepoitem&gt;. CHECK sy-subrc = 0.
        DATA: ls_clike TYPE zvnmm_bapi_te_mepoitem_clike.
        MOVE: &lt;mepoitem&gt;-po_item      TO ls_clike-po_item,
              &lt;mepoitem&gt;-_csi         TO ls_clike-_csi,
              &lt;mepoitem&gt;-zvnmm_ekpodb TO ls_clike-zvnmm_ekpodb.
        &quot;packed-curr-fields: one by one
        MOVE: &lt;mepoitem&gt;-zvnmm_ekpodb_copy-zzekpo_netwr2 TO ls_clike-zzekpo_netwr2,
              &lt;mepoitem&gt;-zvnmm_ekpodb_copy-zzekpo_brtwr2 TO ls_clike-zzekpo_brtwr2,
              &lt;mepoitem&gt;-zvnmm_ekpodb_copy-zzekpo_effwr2 TO ls_clike-zzekpo_effwr2,
              &lt;mepoitem&gt;-zvnmm_ekpodb_copy-zzekpo_navnw2 TO ls_clike-zzekpo_navnw2.

        MOVE: ls_clike TO ch_container.

    ENDCASE.



  ENDMETHOD.</source>
  </interfaceMethod>
  <interfaceMethod CLSNAME="ZCL_VNMM_ME_BAPI_PO_CUST" CPDNAME="IF_EX_ME_BAPI_PO_CREATE_02~MAP2I_EXTENSIONIN">
   <source>METHOD if_ex_me_bapi_po_create_02~map2i_extensionin.

    CASE im_name.
      WHEN &apos;CI_EKKODB&apos;.
        &quot;nothing, standard ok

      WHEN &apos;CI_EKPODB&apos;.
        &quot;map packed-curr-fields to text
        DATA: lr_struct TYPE REF TO cl_abap_structdescr.
        lr_struct ?= cl_abap_typedescr=&gt;describe_by_data( p_data = ch_struc ).
        CHECK lr_struct-&gt;absolute_name = &apos;\TYPE=BAPI_TE_MEPOITEM&apos;.

        DATA: ls_clike TYPE zvnmm_bapi_te_mepoitem_clike.
        MOVE: im_container TO ls_clike.

        FIELD-SYMBOLS: &lt;mepoitem&gt; TYPE bapi_te_mepoitem.
        ASSIGN ch_struc TO &lt;mepoitem&gt;. CHECK sy-subrc = 0.
        MOVE: ls_clike-po_item      TO &lt;mepoitem&gt;-po_item,
              ls_clike-_csi         TO &lt;mepoitem&gt;-_csi,
              ls_clike-zvnmm_ekpodb TO &lt;mepoitem&gt;-zvnmm_ekpodb.
        &quot;packed-curr-fields: one by one
        MOVE: ls_clike-zvnmm_ekpodb_copy-zzekpo_netwr2 TO &lt;mepoitem&gt;-zzekpo_netwr2,
              ls_clike-zvnmm_ekpodb_copy-zzekpo_brtwr2 TO &lt;mepoitem&gt;-zzekpo_brtwr2,
              ls_clike-zvnmm_ekpodb_copy-zzekpo_effwr2 TO &lt;mepoitem&gt;-zzekpo_effwr2,
              ls_clike-zvnmm_ekpodb_copy-zzekpo_navnw2 TO &lt;mepoitem&gt;-zzekpo_navnw2.

    ENDCASE.


  ENDMETHOD.</source>
