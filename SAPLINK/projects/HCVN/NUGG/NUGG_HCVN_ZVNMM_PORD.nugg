  <interfaceMethod CLSNAME="ZCL_BADI_MM_PO_OC_EMAIL" CPDNAME="IF_BADI_MM_PO_OC_EMAIL~CHANGE_SENDER">
   <source>METHOD if_badi_mm_po_oc_email~change_sender.

    &quot;DEV_MM_08(HQ_TTa) communication: Purchase Order by eMail

    &quot;§0. set attributes (data of POrd, customizing)
    me-&gt;as_nast = im_nast_entry.
    me-&gt;set_atts( ).

    &quot;§1. replace sended by Purchasing Group User
    IF me-&gt;as_c003-xspgr = &apos;X&apos;.
      MOVE: me-&gt;as_c004-uname TO ch_sender_name.
    ENDIF.





  ENDMETHOD.</source>
  </interfaceMethod>
  <interfaceMethod CLSNAME="ZCL_BADI_MM_PO_OC_EMAIL" CPDNAME="IF_BADI_MM_PO_OC_EMAIL~CHANGE_SUBJECT">
   <source>METHOD if_badi_mm_po_oc_email~change_subject.

    &quot;DEV_MM_08(HQ_TTa) communication: Purchase Order by eMail

    &quot;set attributes (data of POrd, customizing)
    me-&gt;as_nast = im_nast_entry.
    me-&gt;set_atts( ).

    &quot;replace: text title
    me-&gt;set_txts( ).
    READ TABLE me-&gt;at_tttl ASSIGNING FIELD-SYMBOL(&lt;tttl&gt;) INDEX 1.
    IF sy-subrc = 0.
      MOVE: &lt;tttl&gt;-tdline TO chv_subject.
    ENDIF.




  ENDMETHOD.</source>
  </interfaceMethod>
  <method CLSNAME="ZCL_BADI_MM_PO_OC_EMAIL" CMPNAME="SET_ATTS" VERSION="1" LANGU="E" DESCRIPT="set attributes: Purchase Order, customizing" EXPOSURE="2" STATE="1" EDITORDER="1 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD set_atts.

    &quot;DEV_MM_08(HQ_TTa) communication: Purchase Order by eMail

    &quot;set attributes
    CHECK: me-&gt;as_nast IS NOT INITIAL,
           me-&gt;ax_atts IS     INITIAL.


    &quot;set EKKO Purchase Order header
    SELECT SINGLE * FROM ekko INTO @me-&gt;as_ekko WHERE ebeln = @me-&gt;as_nast-objky.

    &quot;set T024 Purchasing Group
    SELECT SINGLE * FROM t024 INTO @me-&gt;as_t024 WHERE ekgrp = @me-&gt;as_ekko-ekgrp.

    &quot;set C003 MM: Purchase Order emailing
    SELECT SINGLE * FROM zvnmm_xpord_c003 INTO @me-&gt;as_c003 WHERE bsart = @me-&gt;as_ekko-bsart
                                                              AND ekorg = @me-&gt;as_ekko-ekorg
                                                              AND kschl = @me-&gt;as_nast-kschl.

    &quot;set C004 MM: Purchasing Groups to User
    SELECT SINGLE * FROM zvnmm_xpord_c004 INTO @me-&gt;as_c004 WHERE ekgrp =  @me-&gt;as_ekko-ekgrp.


    &quot;flag atts as set (prevent multi runs)
    MOVE: &apos;X&apos; TO me-&gt;ax_atts.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_BADI_MM_PO_OC_EMAIL" CMPNAME="SET_TXTS" VERSION="1" LANGU="E" DESCRIPT="set texts: title, body (via fm, with symbol replacement)" EXPOSURE="2" STATE="1" EDITORDER="2 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD set_txts.

    &quot;DEV_MM_08(HQ_TTa) communication: Purchase Order by eMail
    CHECK: me-&gt;as_nast IS NOT INITIAL,
           me-&gt;ax_txts IS     INITIAL.

    &quot;get texts with symbol replacement
    CALL FUNCTION &apos;ZVNMM_UTIL_PORD_EMAIL_TEXTS&apos;
      EXPORTING
        iv_nast  = me-&gt;as_nast
      IMPORTING
        ev_lnttl = me-&gt;at_tttl
        ev_lnbdy = me-&gt;at_tbdy.

    &quot;flag texts as set (prevent multi runs)
    MOVE: &apos;X&apos; TO me-&gt;ax_txts.


  ENDMETHOD.</source>
  </method>
 </CLAS>
