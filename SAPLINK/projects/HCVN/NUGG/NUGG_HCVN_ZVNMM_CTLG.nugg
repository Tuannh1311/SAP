  <method CLSNAME="ZCL_VNMM_CTLG_RUPL" CMPNAME="ON_UCOM" VERSION="1" LANGU="E" DESCRIPT="handler: on user-command" EXPOSURE="0" STATE="1" EDITORDER="20 " DISPID="0 " MTDTYPE="1" MTDDECLTYP="0" REFCLSNAME="CL_SALV_EVENTS" REFCMPNAME="ADDED_FUNCTION" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNMM_CTLG_RUPL" CMPNAME="ON_UCOM" SCONAME="E_SALV_FUNCTION" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="1" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="0"/>
   <source>METHOD on_ucom.

    &quot;default: get selected rows
    me-&gt;at_rsel = me-&gt;ar_salv-&gt;get_selections( )-&gt;get_selected_rows( ).


    CASE e_salv_function.

      WHEN &apos;REFRESH&apos;. me-&gt;salv_refr( ).

      WHEN  &apos;DOWNLOAD&apos;. me-&gt;on_ucom_download( ).
      WHEN  &apos;UPLOAD&apos;  . me-&gt;on_ucom_upload( )  .   me-&gt;salv_refr( ).
      WHEN  &apos;PUBLISH&apos; . me-&gt;on_ucom_publish( ) .   me-&gt;salv_refr( ).


    ENDCASE.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_CTLG_RUPL" CMPNAME="ON_UCOM_DOWNLOAD" VERSION="1" LANGU="E" DESCRIPT="call: download" EXPOSURE="0" STATE="1" EDITORDER="21 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD on_ucom_download.
    CLEAR: me-&gt;at_cdat[].

    &quot;excel (template) download..
    &quot;..done via para c-like table, to handle type diffs etc

    &quot;§ prepare header..
    DATA: lt_dfies TYPE STANDARD TABLE OF dfies.
    CALL FUNCTION &apos;DDIF_FIELDINFO_GET&apos;
      EXPORTING
        tabname   = &apos;ZVNMM_CTLG_RUPL_CDAT&apos;
      TABLES
        dfies_tab = lt_dfies.
    &quot;..-line1: technical names
    APPEND INITIAL LINE TO me-&gt;at_cdat ASSIGNING FIELD-SYMBOL(&lt;chd1&gt;).
    LOOP AT lt_dfies ASSIGNING FIELD-SYMBOL(&lt;dfie&gt;).
      ASSIGN COMPONENT &lt;dfie&gt;-fieldname OF STRUCTURE &lt;chd1&gt; TO FIELD-SYMBOL(&lt;fhd1&gt;).
      MOVE: &lt;dfie&gt;-fieldname TO &lt;fhd1&gt;.
    ENDLOOP.
    &quot;..-line2: descriptions
    APPEND INITIAL LINE TO me-&gt;at_cdat ASSIGNING FIELD-SYMBOL(&lt;chd2&gt;).
    LOOP AT lt_dfies ASSIGNING &lt;dfie&gt;.
      ASSIGN COMPONENT &lt;dfie&gt;-fieldname OF STRUCTURE &lt;chd2&gt; TO FIELD-SYMBOL(&lt;fhd2&gt;).
      MOVE: &lt;dfie&gt;-fieldtext TO &lt;fhd2&gt;.
    ENDLOOP.

    &quot;§ populate items (if selected)
    LOOP AT me-&gt;at_rsel ASSIGNING FIELD-SYMBOL(&lt;rsel&gt;).
      READ TABLE me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;) INDEX &lt;rsel&gt;. CHECK sy-subrc = 0.
      APPEND INITIAL LINE TO me-&gt;at_cdat ASSIGNING FIELD-SYMBOL(&lt;cdat&gt;).
      &quot;move what we can, then handle specials
      MOVE-CORRESPONDING: &lt;sdat&gt; TO &lt;cdat&gt;.

      &quot;date from date-time to simple date (but iso-like)
      CONDENSE: &lt;cdat&gt;-price_valid_from,
                &lt;cdat&gt;-price_valid_to  .
      DATA(lv_date_fr) = CONV sy-datum( &lt;cdat&gt;-price_valid_from(8) ).
      DATA(lv_date_to) = CONV sy-datum( &lt;cdat&gt;-price_valid_to(8) ).
      &lt;cdat&gt;-price_valid_from = |{ lv_date_fr DATE = ISO }|.
      &lt;cdat&gt;-price_valid_to   = |{ lv_date_to DATE = ISO }|.

      &quot;UoMs
      CALL FUNCTION &apos;CONVERSION_EXIT_CUNIT_OUTPUT&apos;
        EXPORTING
          input  = &lt;cdat&gt;-unit
        IMPORTING
          output = &lt;cdat&gt;-unit.

***      &quot;item price
***      CONDENSE: &lt;cdat&gt;-itm_price,
***                &lt;cdat&gt;-itm_currency.
***      DATA(lv_netpr) = CONV ekpo-netpr( &lt;cdat&gt;-itm_price ).
***      DATA(lv_waers) = CONV ekko-waers( &lt;cdat&gt;-itm_currency ).
***      WRITE lv_netpr CURRENCY lv_waers TO &lt;cdat&gt;-itm_price.

      DATA: lv_wamnt TYPE wmto_s-amount.
      CALL FUNCTION &apos;CURRENCY_AMOUNT_SAP_TO_DISPLAY&apos;
        EXPORTING
          currency        = CONV tcurc-waers( &lt;cdat&gt;-itm_currency )
          amount_internal = CONV wmto_s-amount( &lt;cdat&gt;-itm_price )
        IMPORTING
          amount_display  = lv_wamnt.
      MOVE: lv_wamnt TO &lt;cdat&gt;-itm_price.
      &quot;(everything is shifted??)
      &lt;cdat&gt;-itm_price = &lt;cdat&gt;-itm_price * 100.

    ENDLOOP.


    &quot;§ ask for file
    DATA: lv_file TYPE string,
          lv_path TYPE string,
          lv_full TYPE string.
    CALL METHOD cl_gui_frontend_services=&gt;file_save_dialog
      EXPORTING
        window_title              = &apos;Catalog items download&apos;     &quot; Window Title
        default_extension         = &apos;&apos; &quot; Default Extension
        default_file_name         = &apos;CatalogItemData.xlsx&apos; &quot; Default File Name
        &quot;with_encoding             = with_encoding
        &quot;file_filter               = file_filter       &quot; File Type Filter Table
        &quot;initial_directory         = initial_directory &quot; Initial Directory
        &quot;prompt_on_overwrite       = &apos;X&apos;
      CHANGING
        filename                  = lv_file          &quot; File Name to Save
        path                      = lv_path              &quot; Path to File
        fullpath                  = lv_full          &quot; Path + File Name
        &quot;user_action               = user_action       &quot; User Action (C Class Const ACTION_OK, ACTION_OVERWRITE etc)
        &quot;file_encoding             = file_encoding
      EXCEPTIONS
        cntl_error                = 1                 &quot; Control error
        error_no_gui              = 2                 &quot; No GUI available
        not_supported_by_gui      = 3                 &quot; GUI does not support this
        invalid_default_file_name = 4                 &quot; Invalid default file name
        OTHERS                    = 5.
    IF sy-subrc &lt;&gt; 0. MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4. ENDIF.

    CHECK lv_full IS NOT INITIAL.

    &quot;§ file download
    CALL FUNCTION &apos;SAP_CONVERT_TO_XLS_FORMAT&apos;
      EXPORTING
        i_filename        = CONV rlgrap-filename( lv_full )
        i_appl_keep       = &apos;&apos;
      TABLES
        i_tab_sap_data    = me-&gt;at_cdat
      EXCEPTIONS
        conversion_failed = 1
        OTHERS            = 2.
    IF sy-subrc &lt;&gt; 0. MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4. ENDIF.
    IF sy-subrc =  0. MESSAGE s001(zvnmm_ctlg) WITH lv_full. ENDIF.







  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_CTLG_RUPL" CMPNAME="ON_UCOM_PUBLISH" VERSION="1" LANGU="E" DESCRIPT="call: publish" EXPOSURE="0" STATE="1" EDITORDER="25 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD on_ucom_publish.

    &quot;publish to catalogue

    DATA: lt_citm TYPE mmpur_cat_t_cat_itm,
          lt_desc TYPE mmpur_cat_t_desctxt,
          lt_ltxt TYPE mmpur_cat_t_longtxt,
          lt_strm TYPE mmpur_cat_t_srchtrm,
          lt_prcv TYPE mmpur_cat_t_prcconv,
          lt_attm TYPE mmpur_cat_t_att_main,
          lt_atch TYPE mmpur_cat_t_atchmnt.
    DATA: lx_inew TYPE xfeld.

    &quot;(based on class CL_MM_PUR_CTLG_EXTRACTOR ~EXTRACTOR_MATMASTER )
    CHECK me-&gt;at_rsel[] IS NOT INITIAL.

    &quot;per each item separately
    LOOP AT me-&gt;at_rsel ASSIGNING FIELD-SYMBOL(&lt;rsel&gt;).
      CLEAR: lt_citm[], lt_desc[], lt_ltxt[], lt_strm[], lt_prcv[],
             lx_inew.
      READ TABLE me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;) INDEX &lt;rsel&gt;. CHECK sy-subrc = 0.

      &quot;**********************************************************************
      &quot;prep: item MMPUR_CAT_ITM
      APPEND INITIAL LINE TO lt_citm ASSIGNING FIELD-SYMBOL(&lt;citm&gt;).
      MOVE-CORRESPONDING: &lt;sdat&gt; TO &lt;citm&gt;.
      &quot;key &amp; hash
      IF &lt;sdat&gt;-ctlg_itm_key IS INITIAL   .
        CALL METHOD cl_mm_pur_cat_data_trigger=&gt;get_sha1_key
          EXPORTING
            iv_catalog_itm_guid = CONV #( &lt;sdat&gt;-matnr )
          IMPORTING
            ev_sha1_hash_key    = DATA(lv_hash)
            ev_ctlg_itm_key     = DATA(lv_ikey).
        MOVE: lv_hash TO &lt;sdat&gt;-cat_itm_hash,
              lv_ikey TO &lt;sdat&gt;-ctlg_itm_key.
        MOVE: &apos;X&apos; TO lx_inew.
      ENDIF.
      MOVE: &lt;sdat&gt;-ctlg_itm_key TO &lt;citm&gt;-ctlg_itm_key,
            &lt;sdat&gt;-cat_itm_hash TO &lt;citm&gt;-cat_itm_hash.

      &quot;price key
      IF &lt;sdat&gt;-price_key IS INITIAL   .
        CALL FUNCTION &apos;NUMBER_GET_NEXT&apos;
          EXPORTING
            nr_range_nr             = &apos;01&apos;
            object                  = &apos;MMPUR_PC&apos;
          IMPORTING
            number                  = &lt;sdat&gt;-price_key
          EXCEPTIONS
            interval_not_found      = 1
            number_range_not_intern = 2
            object_not_found        = 3
            quantity_is_0           = 4
            quantity_is_not_1       = 5
            interval_overflow       = 6
            buffer_overflow         = 7
            OTHERS                  = 8.
        IF sy-subrc NE 0. MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4. ENDIF.
      ENDIF.
      MOVE: &lt;sdat&gt;-price_key TO &lt;citm&gt;-price_key.

      &quot;item: catalog name
      MOVE: &lt;sdat&gt;-ws_serv_name TO &lt;citm&gt;-ws_serv_name,
            &lt;sdat&gt;-ws_serv_name TO &lt;citm&gt;-catalog_name,
            &lt;sdat&gt;-ws_serv_name TO &lt;citm&gt;-intcatalogname.

      CONDENSE: &lt;citm&gt;-leadtime,
                &lt;citm&gt;-minorder_qty.
      IF &lt;citm&gt;-leadtime     = &apos;0&apos;. CLEAR &lt;citm&gt;-leadtime.     ENDIF.
      IF &lt;citm&gt;-minorder_qty = &apos;0&apos;. CLEAR &lt;citm&gt;-minorder_qty. ENDIF.

      &quot;item: created
      MOVE: sy-datum TO &lt;citm&gt;-created_on,
            sy-uname TO &lt;citm&gt;-changed_by.
      CONVERT DATE sy-datum TIME sy-uzeit INTO TIME STAMP &lt;citm&gt;-changed_at TIME ZONE &apos;UTC&apos;.


      &quot;**********************************************************************
      &quot;***descriptions en+vi

      IF &lt;sdat&gt;-description_en IS NOT INITIAL.
        APPEND INITIAL LINE TO lt_desc ASSIGNING FIELD-SYMBOL(&lt;desc_en&gt;).
        MOVE: &lt;citm&gt;-ctlg_itm_key   TO &lt;desc_en&gt;-ctlg_itm_key,
              &apos;E&apos;                   TO &lt;desc_en&gt;-language,
              &lt;sdat&gt;-description_en TO &lt;desc_en&gt;-description.
      ENDIF.
      IF &lt;sdat&gt;-description_vi IS NOT INITIAL.
        APPEND INITIAL LINE TO lt_desc ASSIGNING FIELD-SYMBOL(&lt;desc_vi&gt;).
        MOVE: &lt;citm&gt;-ctlg_itm_key   TO &lt;desc_vi&gt;-ctlg_itm_key,
              &apos;쁩&apos;                   TO &lt;desc_vi&gt;-language,
              &lt;sdat&gt;-description_vi   TO &lt;desc_vi&gt;-description.
      ENDIF.

      &quot;**********************************************************************
      &quot;***long text
      &quot;check text
      IF &lt;sdat&gt;-longtext_en IS NOT INITIAL.
        APPEND INITIAL LINE TO lt_ltxt ASSIGNING FIELD-SYMBOL(&lt;ltxt_en&gt;).
        MOVE: &lt;citm&gt;-ctlg_itm_key TO &lt;ltxt_en&gt;-ctlg_itm_key,
              &apos;E&apos;                 TO &lt;ltxt_en&gt;-language,
              &lt;sdat&gt;-longtext_en  TO &lt;ltxt_en&gt;-longtext.
      ENDIF.
      IF &lt;sdat&gt;-longtext_vi IS NOT INITIAL.
        APPEND INITIAL LINE TO lt_ltxt ASSIGNING FIELD-SYMBOL(&lt;ltxt_vi&gt;).
        MOVE: &lt;citm&gt;-ctlg_itm_key TO &lt;ltxt_vi&gt;-ctlg_itm_key,
              &apos;쁩&apos;                 TO &lt;ltxt_vi&gt;-language,
              &lt;sdat&gt;-longtext_vi  TO &lt;ltxt_vi&gt;-longtext.
      ENDIF.


      &quot;**********************************************************************
      &quot;***search term (??, at least keep it)
      CLEAR: lt_strm.
      SELECT * FROM mmpur_ct_itm_srh WHERE ctlg_itm_key = @&lt;sdat&gt;-ctlg_itm_key  INTO TABLE @lt_strm.

      &quot;**********************************************************************
      &quot;***price converions
      APPEND INITIAL LINE TO lt_prcv ASSIGNING FIELD-SYMBOL(&lt;prcv&gt;).
***      &lt;sdat&gt;-itm_price        = &lt;sdat&gt;-itm_price * 100.
***      &lt;sdat&gt;-price_comm_curr  = &lt;sdat&gt;-price_comm_curr * 100.
      MOVE: &lt;citm&gt;-price_key        TO &lt;prcv&gt;-price_key,
            &lt;sdat&gt;-itm_price        TO &lt;prcv&gt;-itm_price,
            &lt;sdat&gt;-itm_currency     TO &lt;prcv&gt;-itm_currency,
            &lt;sdat&gt;-price_comm_curr  TO &lt;prcv&gt;-price_comm_curr,
            &lt;sdat&gt;-common_currency  TO &lt;prcv&gt;-common_currency.

      &quot;(I have somehow messed up the prices with those convs via bapi-curr &amp; sap-curr..
      &quot; ..but no time to fix this; so ***TBD be it                                     )
      &lt;prcv&gt;-itm_price       = &lt;prcv&gt;-itm_price * 100.
      &lt;prcv&gt;-price_comm_curr = &lt;prcv&gt;-price_comm_curr * 100.

      &quot;keep MMPUR_CAT_T_ATCHMNT &amp; MMPUR_CAT_T_ATT_MAIN (to not loose images)
      CLEAR: lt_attm, lt_atch.
      IF &lt;sdat&gt;-image_url IS NOT INITIAL.
        APPEND INITIAL LINE TO lt_attm ASSIGNING FIELD-SYMBOL(&lt;attm&gt;).
        MOVE: &lt;sdat&gt;-ctlg_itm_key TO &lt;attm&gt;-ctlg_itm_key.
        IF &lt;sdat&gt;-image_key IS INITIAL.
          CALL FUNCTION &apos;NUMBER_GET_NEXT&apos;
            EXPORTING
              nr_range_nr             = &apos;01&apos;
              object                  = &apos;MMPUR_ATT&apos;
            IMPORTING
              number                  = &lt;sdat&gt;-image_key
            EXCEPTIONS
              interval_not_found      = 1
              number_range_not_intern = 2
              object_not_found        = 3
              quantity_is_0           = 4
              quantity_is_not_1       = 5
              interval_overflow       = 6
              buffer_overflow         = 7
              OTHERS                  = 8.
          IF sy-subrc NE 0. MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4. ENDIF.
          MOVE: &lt;sdat&gt;-image_key TO &lt;attm&gt;-main_image_key.
        ENDIF.
        APPEND INITIAL LINE TO lt_atch ASSIGNING FIELD-SYMBOL(&lt;atch&gt;).
        MOVE: &lt;attm&gt;-main_image_key   TO &lt;atch&gt;-image_key,
              &lt;attm&gt;-ctlg_itm_key     TO &lt;atch&gt;-ctlg_itm_key,
              &lt;sdat&gt;-image_name       TO &lt;atch&gt;-image_name,
              &lt;sdat&gt;-image_url        TO &lt;atch&gt;-image_url,
              &lt;sdat&gt;-mime_type        TO &lt;atch&gt;-mime_type,
              &lt;sdat&gt;-attachment_type  TO &lt;atch&gt;-attachment_type.
      ENDIF.


      &quot;**********************************************************************
      &quot;call update
      CALL FUNCTION &apos;SAVE_CATALOG_ITEM&apos;
        EXPORTING
          it_catalog_item       = lt_citm
          it_catalog_desc       = lt_desc
          it_catalog_lngtxt     = lt_ltxt
          it_search_term        = lt_strm
          it_catalog_price      = lt_prcv
          it_catalog_image      = lt_atch
          it_catalog_image_main = lt_attm
          iv_new_item           = lx_inew
*         IV_ACTIVATE           = IV_ACTIVATE
        EXCEPTIONS
          db_failure            = 1
          OTHERS                = 2.
      IF sy-subrc NE 0.
        MOVE: &apos;E&apos; TO &lt;sdat&gt;-xupl_statu. &quot;E= error
        MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4.
      ENDIF.
      IF sy-subrc EQ 0.
        CALL FUNCTION &apos;BAPI_TRANSACTION_COMMIT&apos; EXPORTING wait = &apos;X&apos;.
        MOVE: &apos;P&apos; TO &lt;sdat&gt;-xupl_statu. &quot;P= published to catalog
      ENDIF.



    ENDLOOP.

    me-&gt;salv_refr( ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_CTLG_RUPL" CMPNAME="ON_UCOM_UPLOAD" VERSION="1" LANGU="E" DESCRIPT="call: upload" EXPOSURE="0" STATE="1" EDITORDER="22 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD on_ucom_upload.

    &quot;upload from file
    CLEAR: me-&gt;at_cdat.

    &quot;§ ask for file
    DATA: lt_ftab TYPE filetable,
          lv_retc TYPE i.
    CALL METHOD cl_gui_frontend_services=&gt;file_open_dialog
      EXPORTING
        window_title            = &apos;Catalog items upload&apos;     &quot; Title Of File Open Dialog
        default_extension       = &apos;XLSX&apos; &quot; Default Extension
        &quot;default_filename        = default_filename  &quot; Default File Name
        &quot;file_filter             = file_filter       &quot; File Extension Filter String
        &quot;with_encoding           = with_encoding     &quot; File Encoding
        &quot;initial_directory       = initial_directory &quot; Initial Directory
        &quot;multiselection          = multiselection    &quot; Multiple selections poss.
      CHANGING
        file_table              = lt_ftab           &quot; Table Holding Selected Files
        rc                      = lv_retc           &quot; Return Code, Number of Files or -1 If Error Occurred
        &quot;user_action             = user_action       &quot; User Action (See Class Constants ACTION_OK, ACTION_CANCEL)
        &quot;file_encoding           = file_encoding
      EXCEPTIONS
        file_open_dialog_failed = 1                 &quot; &quot;Open File&quot; dialog failed
        cntl_error              = 2                 &quot; Control error
        error_no_gui            = 3                 &quot; No GUI available
        not_supported_by_gui    = 4                 &quot; GUI does not support this
        OTHERS                  = 5.
    IF sy-subrc &lt;&gt; 0. MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4. ENDIF.

    CHECK lt_ftab IS NOT INITIAL.
    READ TABLE lt_ftab ASSIGNING FIELD-SYMBOL(&lt;ftab&gt;) INDEX 1. CHECK sy-subrc = 0.

    &quot;§ load file to table
    DATA: lt_dummy TYPE truxs_t_text_data.
    CALL FUNCTION &apos;TEXT_CONVERT_XLS_TO_SAP&apos;
      EXPORTING
        i_tab_raw_data       = lt_dummy
        i_filename           = CONV rlgrap-filename( &lt;ftab&gt;-filename )
      TABLES
        i_tab_converted_data = me-&gt;at_cdat
      EXCEPTIONS
        conversion_failed    = 1
        OTHERS               = 2.
    IF sy-subrc &lt;&gt; 0. MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4. RETURN. ENDIF.

    &quot;§ pass file to salv

    DELETE me-&gt;at_cdat INDEX 2. &quot;header-descriptions
    DELETE me-&gt;at_cdat INDEX 1. &quot;header-technames

    me-&gt;on_ucom_upload_sdat( ).
    me-&gt;on_ucom_upload_enha( ).




  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_CTLG_RUPL" CMPNAME="ON_UCOM_UPLOAD_ENHA" VERSION="1" LANGU="E" DESCRIPT="call: upload: enhance SDat" EXPOSURE="0" STATE="1" EDITORDER="24 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD on_ucom_upload_enha.

    &quot;enhance SDAT from upload

    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      &quot;catalogue name
      SELECT SINGLE service_descript INTO @&lt;sdat&gt;-ws_serv_name FROM wsi_description WHERE service_id = @&lt;sdat&gt;-ws_service_id
                                                                                      AND spras      = @sy-langu            .

      &quot;vendor name
      SELECT SINGLE name1 INTO @&lt;sdat&gt;-vendor_name FROM lfa1 WHERE lifnr = @&lt;sdat&gt;-vendor.

      &quot;material group
      SELECT SINGLE matkl INTO @&lt;sdat&gt;-matgroup FROM mara WHERE matnr = @&lt;sdat&gt;-matnr.

      &quot;pass common
      MOVE: &lt;sdat&gt;-itm_currency TO &lt;sdat&gt;-common_currency,
            &lt;sdat&gt;-itm_price    TO &lt;sdat&gt;-price_comm_curr.

      &quot;attchmnt type
      IF &lt;sdat&gt;-image_url IS NOT INITIAL.
        MOVE: &apos;01&apos; TO &lt;sdat&gt;-attachment_type.
      ENDIF.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_CTLG_RUPL" CMPNAME="ON_UCOM_UPLOAD_SDAT" VERSION="1" LANGU="E" DESCRIPT="call: upload: pass CDat to SDat" EXPOSURE="0" STATE="1" EDITORDER="23 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD on_ucom_upload_sdat.

    &quot;pass c-like CDAT to salv SDAT

    CHECK: me-&gt;at_cdat IS NOT INITIAL.
    CLEAR: me-&gt;at_sdat.
    MOVE: &apos;X&apos; TO me-&gt;ax_file.

    LOOP AT me-&gt;at_cdat ASSIGNING FIELD-SYMBOL(&lt;cdat&gt;).

      APPEND INITIAL LINE TO me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      MOVE: &apos;L&apos; TO &lt;sdat&gt;-xupl_statu. &quot;L= loaded from file

      &quot;move item by item &amp; handle specials (no move-crsponding since c-like to others)
      MOVE: &lt;cdat&gt;-ctlg_itm_key   TO &lt;sdat&gt;-ctlg_itm_key,
            &lt;cdat&gt;-cat_itm_hash   TO &lt;sdat&gt;-cat_itm_hash,
            &lt;cdat&gt;-price_key      TO &lt;sdat&gt;-price_key,
            &lt;cdat&gt;-ws_service_id  TO &lt;sdat&gt;-ws_service_id,
            &lt;cdat&gt;-vendor         TO &lt;sdat&gt;-vendor,
            &lt;cdat&gt;-vendor_mat     TO &lt;sdat&gt;-vendor_mat.

      &quot;date from simple date to date-time
      &lt;sdat&gt;-price_valid_from = zvnmm_util=&gt;conv_date_excel( CONV char10( &lt;cdat&gt;-price_valid_from ) ) &amp;&amp; &apos;000000&apos;.
      &lt;sdat&gt;-price_valid_to   = zvnmm_util=&gt;conv_date_excel( CONV char10( &lt;cdat&gt;-price_valid_to   ) ) &amp;&amp; &apos;000000&apos;.


      MOVE: &lt;cdat&gt;-price_quantity TO &lt;sdat&gt;-price_quantity,
            &lt;cdat&gt;-matnr          TO &lt;sdat&gt;-matnr,
            &lt;cdat&gt;-manufactcode   TO &lt;sdat&gt;-manufactcode,
            &lt;cdat&gt;-manufactmat    TO &lt;sdat&gt;-manufactmat,
            &lt;cdat&gt;-leadtime       TO &lt;sdat&gt;-leadtime.

      &quot;uoms
      CALL FUNCTION &apos;CONVERSION_EXIT_CUNIT_INPUT&apos;
        EXPORTING
          input  = &lt;cdat&gt;-unit
        IMPORTING
          output = &lt;sdat&gt;-unit.

      MOVE: &lt;cdat&gt;-contract TO &lt;sdat&gt;-contract.
      &quot;oagr-item
      CALL FUNCTION &apos;CONVERSION_EXIT_ALPHA_INPUT&apos;
        EXPORTING
          input  = &lt;cdat&gt;-contract_item
        IMPORTING
          output = &lt;sdat&gt;-contract_item.
      &lt;sdat&gt;-contract_item = CONV ekpo-ebelp( &lt;sdat&gt;-contract_item ).

      MOVE: &lt;cdat&gt;-deletion_ind   TO &lt;sdat&gt;-deletion_ind,
            &lt;cdat&gt;-plant          TO &lt;sdat&gt;-plant,
            &lt;cdat&gt;-company_code   TO &lt;sdat&gt;-company_code,
            &lt;cdat&gt;-ekorg          TO &lt;sdat&gt;-ekorg,
            &lt;cdat&gt;-description_en TO &lt;sdat&gt;-description_en,
            &lt;cdat&gt;-description_vi TO &lt;sdat&gt;-description_vi.

      &quot;currency amoutn
      MOVE: &lt;cdat&gt;-itm_currency   TO &lt;sdat&gt;-itm_currency.

      DATA: lv_wamnt TYPE wmto_s-amount.
      CALL FUNCTION &apos;CURRENCY_AMOUNT_DISPLAY_TO_SAP&apos;
        EXPORTING
          currency        = CONV tcurc-waers( &lt;sdat&gt;-itm_currency )
          amount_display  = CONV wmto_s-amount( &lt;cdat&gt;-itm_price )
        IMPORTING
          amount_internal = lv_wamnt.
      &lt;sdat&gt;-itm_price = lv_wamnt.

      CALL FUNCTION &apos;BAPI_CURRENCY_CONV_TO_INTERNAL&apos;
        EXPORTING
          currency             = CONV waers( &lt;cdat&gt;-itm_currency )
          amount_external      = CONV bapicurr-bapicurr( &lt;cdat&gt;-itm_price )
          max_number_of_digits = 23
        IMPORTING
          amount_internal      = &lt;sdat&gt;-itm_price
          &quot;RETURN               = RETURN
        .

      &quot;..***
      &lt;sdat&gt;-itm_price       = &lt;sdat&gt;-itm_price       / 100.
      &lt;sdat&gt;-price_comm_curr = &lt;sdat&gt;-price_comm_curr / 100.

      MOVE: &lt;cdat&gt;-longtext_en      TO &lt;sdat&gt;-longtext_en,
            &lt;cdat&gt;-longtext_vi      TO &lt;sdat&gt;-longtext_vi,
            &lt;cdat&gt;-image_key        TO &lt;sdat&gt;-image_key,
            &lt;cdat&gt;-image_name       TO &lt;sdat&gt;-image_name,
            &lt;cdat&gt;-image_url        TO &lt;sdat&gt;-image_url,
            &lt;cdat&gt;-mime_type        TO &lt;sdat&gt;-mime_type.



    ENDLOOP.

  ENDMETHOD.</source>
  </method>
