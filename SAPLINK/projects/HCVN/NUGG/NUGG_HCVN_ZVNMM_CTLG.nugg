  <method CLSNAME="ZCL_VNMM_CTLG_RUPL" CMPNAME="SALV_REFR" VERSION="1" LANGU="E" DESCRIPT="salv-object: refresh" EXPOSURE="0" STATE="1" EDITORDER="18 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_refr.

    &quot;refresh SALV

    &quot;..re-read data

    me-&gt;sdat_selc( ). &quot;data select
    me-&gt;sdat_auth( ). &quot;data authorisation check
    me-&gt;sdat_enha( ). &quot;data enhance

    me-&gt;salv_init_ccel( ). &quot;re-colour cells (per row-line)

    &quot;..refresh salv
    me-&gt;ar_salv-&gt;refresh( ).






  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_CTLG_RUPL" CMPNAME="SDAT_AUTH" VERSION="1" LANGU="E" DESCRIPT="salv-data: check authorisation" EXPOSURE="0" STATE="1" EDITORDER="2 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SDAT_AUTH.

    &quot;authorithy check
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      AUTHORITY-CHECK OBJECT &apos;M_CTLG_PRP&apos; ID &apos;ACTVT&apos;      FIELD &apos;03&apos;                   &quot;03= Display
                                          ID &apos;WEB_SRV_ID&apos; FIELD &lt;sdat&gt;-ws_service_id.
      IF sy-subrc &lt;&gt; 0.
        CLEAR: &lt;sdat&gt;.
      ENDIF.
    ENDLOOP.
    DELETE me-&gt;at_sdat WHERE XUPL_STATU IS INITIAL.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_CTLG_RUPL" CMPNAME="SDAT_ENHA" VERSION="1" LANGU="E" DESCRIPT="***salv-data: enhance" EXPOSURE="0" STATE="1" EDITORDER="3 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha.

    &quot;enhance salv-data

    me-&gt;sdat_enha_changedby_tx( ).
    me-&gt;sdat_enha_statu_tx( ).
    me-&gt;sdat_enha_statu_ic( ).
    me-&gt;sdat_enha_amntc( ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_CTLG_RUPL" CMPNAME="SDAT_ENHA_AMNTC" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: currency amount (int»ext)" EXPOSURE="0" STATE="1" EDITORDER="7 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_amntc.

    &quot;currency handling: bapi-int » bapi-ext » sap-int (so curr in alv will work)
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      &quot;bapi-int » bapi-ext
      CALL FUNCTION &apos;BAPI_CURRENCY_CONV_TO_EXTERNAL&apos;
        EXPORTING
          currency        = &lt;sdat&gt;-itm_currency
          amount_internal = &lt;sdat&gt;-itm_price
        IMPORTING
          amount_external = &lt;sdat&gt;-itm_price.
      CALL FUNCTION &apos;BAPI_CURRENCY_CONV_TO_EXTERNAL&apos;
        EXPORTING
          currency        = &lt;sdat&gt;-common_currency
          amount_internal = &lt;sdat&gt;-price_comm_curr
        IMPORTING
          amount_external = &lt;sdat&gt;-price_comm_curr.


      &quot; bapi-ext » sap-int
      DATA: lv_wcurr TYPE wmto_s-amount.
      CALL FUNCTION &apos;CURRENCY_AMOUNT_DISPLAY_TO_SAP&apos;
        EXPORTING
          currency        = &lt;sdat&gt;-itm_currency
          amount_display  = CONV wmto_s-amount( &lt;sdat&gt;-itm_price )
        IMPORTING
          amount_internal = lv_wcurr.
      MOVE lv_wcurr TO &lt;sdat&gt;-itm_price.
      CALL FUNCTION &apos;CURRENCY_AMOUNT_DISPLAY_TO_SAP&apos;
        EXPORTING
          currency        = &lt;sdat&gt;-common_currency
          amount_display  = CONV wmto_s-amount( &lt;sdat&gt;-price_comm_curr )
        IMPORTING
          amount_internal = lv_wcurr.
      MOVE lv_wcurr TO &lt;sdat&gt;-price_comm_curr.





    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_CTLG_RUPL" CMPNAME="SDAT_ENHA_CHANGEDBY_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: changed by user" EXPOSURE="0" STATE="1" EDITORDER="4 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SDAT_ENHA_CHANGEDBY_TX.

    &quot;enha Changed By user
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      DATA: ls_addr TYPE          bapiaddr3,
            lt_ret  TYPE TABLE OF bapiret2.
      CALL FUNCTION &apos;BAPI_USER_GET_DETAIL&apos;
        EXPORTING
          username = &lt;sdat&gt;-changed_by
        IMPORTING
          address  = ls_addr
        TABLES
          return   = lt_ret.
      IF sy-subrc = 0. MOVE: ls_addr-fullname TO &lt;sdat&gt;-changed_by_tx. ENDIF.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_CTLG_RUPL" CMPNAME="SDAT_ENHA_STATU_IC" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: status icon" EXPOSURE="0" STATE="1" EDITORDER="6 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_statu_ic.

    &quot;enha Status icon
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      CASE &lt;sdat&gt;-xupl_statu.
        WHEN &apos;C&apos; OR   &quot;C= catalog item
             &apos;P&apos;   .  &quot;P= published to catalog
          CASE &lt;sdat&gt;-deletion_ind.
            WHEN &apos; &apos;. MOVE: icon_led_green  TO &lt;sdat&gt;-xupl_statu_ic.
            WHEN &apos;X&apos;. MOVE: icon_led_yellow TO &lt;sdat&gt;-xupl_statu_ic.
          ENDCASE.

        WHEN &apos;L&apos;. &quot;L=loaded from file
          MOVE: icon_led_inactive TO &lt;sdat&gt;-xupl_statu_ic.

        WHEN &apos;E&apos;.
          MOVE: icon_led_red TO &lt;sdat&gt;-xupl_statu_ic.

      ENDCASE.
    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_CTLG_RUPL" CMPNAME="SDAT_ENHA_STATU_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: status text" EXPOSURE="0" STATE="1" EDITORDER="5 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_statu_tx.

    &quot;enha Status text
    DATA(lo_dref) = CAST cl_abap_elemdescr( cl_abap_elemdescr=&gt;describe_by_name( p_name = &apos;ZVNMM_CTLG_STATU&apos; ) ).
    DATA(lt_vals) = lo_dref-&gt;get_ddic_fixed_values( ).

    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      READ TABLE lt_vals ASSIGNING FIELD-SYMBOL(&lt;vals&gt;) WITH KEY low = &lt;sdat&gt;-xupl_statu. CHECK sy-subrc = 0.

      MOVE: &lt;vals&gt;-ddtext TO &lt;sdat&gt;-xupl_statu_tx.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_CTLG_RUPL" CMPNAME="SDAT_SELC" VERSION="1" LANGU="E" DESCRIPT="salv-data: select data per keys (or item, from file)" EXPOSURE="0" STATE="1" EDITORDER="1 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_selc.

    &quot;keys: either init from sel-screen, or upd from file
    IF me-&gt;ax_file = &apos;X&apos;.
      CLEAR: me-&gt;at_keys.
      LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;) WHERE ctlg_itm_key IS NOT INITIAL.
        APPEND INITIAL LINE TO me-&gt;at_keys ASSIGNING FIELD-SYMBOL(&lt;key&gt;).
        MOVE: &lt;sdat&gt;-ctlg_itm_key TO &lt;key&gt;-ctlg_itm_key,
              &lt;sdat&gt;-cat_itm_hash TO &lt;key&gt;-cat_itm_hash.
      ENDLOOP.

      &quot;actually, no selections after upload
      &quot;(we are working with uploaded data now)
      RETURN.
    ELSE.
      CLEAR: me-&gt;at_sdat.
    ENDIF.



    &quot;get data
    IF me-&gt;at_keys IS NOT INITIAL.
      &quot;select..
      &quot;..item
      SELECT * FROM mmpur_cat_itm    FOR ALL ENTRIES IN @me-&gt;at_keys WHERE ctlg_itm_key = @me-&gt;at_keys-ctlg_itm_key INTO TABLE @DATA(lt_citm).
      &quot;..descriptions
      SELECT * FROM mmpur_cat_dsctxt FOR ALL ENTRIES IN @lt_citm WHERE ctlg_itm_key = @lt_citm-ctlg_itm_key INTO TABLE @DATA(lt_dtxt).
      &quot;..long texts
      SELECT * FROM mmpur_cat_lngtxt FOR ALL ENTRIES IN @lt_citm WHERE ctlg_itm_key = @lt_citm-ctlg_itm_key INTO TABLE @DATA(lt_ltxt) ##SELECT_FAE_WITH_LOB[LONGTEXT] .
      &quot;..prices
      SELECT * FROM mmpur_cat_prconv FOR ALL ENTRIES IN @lt_citm WHERE price_key = @lt_citm-price_key INTO TABLE @DATA(lt_pcnv).
      &quot;..attachments
      SELECT * FROM mmpur_cat_atmain FOR ALL ENTRIES IN @lt_citm WHERE ctlg_itm_key = @lt_citm-ctlg_itm_key INTO TABLE @DATA(lt_atmn).
      SELECT * FROM mmpur_cat_attchm FOR ALL ENTRIES IN @lt_citm WHERE ctlg_itm_key = @lt_citm-ctlg_itm_key INTO TABLE @DATA(lt_atch) ##SELECT_FAE_WITH_LOB[IMAGE_URL] .
      &quot;..search terms
      SELECT * FROM mmpur_ct_itm_srh FOR ALL ENTRIES IN @lt_citm WHERE ctlg_itm_key = @lt_citm-ctlg_itm_key INTO TABLE @DATA(lt_isrh).
    ENDIF.

    &quot;map together..
    &quot;..items
    LOOP AT lt_citm ASSIGNING FIELD-SYMBOL(&lt;citm&gt;).
      IF me-&gt;ax_file = &apos; &apos;.
        &quot;init sel from sel-screen: new items
        APPEND INITIAL LINE TO me-&gt;at_sdat ASSIGNING &lt;sdat&gt;.
        MOVE-CORRESPONDING: &lt;citm&gt; TO &lt;sdat&gt;.
      ENDIF.
      IF me-&gt;ax_file = &apos;X&apos;.
        &quot;form upload: only update items
        READ TABLE me-&gt;at_sdat ASSIGNING &lt;sdat&gt; WITH KEY ctlg_itm_key = &lt;citm&gt;-ctlg_itm_key
                                                         cat_itm_hash = &lt;citm&gt;-cat_itm_hash. CHECK sy-subrc = 0.
        MOVE-CORRESPONDING: &lt;citm&gt; TO &lt;sdat&gt;.
      ENDIF.
    ENDLOOP.
    &quot;..descriptions
    LOOP AT lt_dtxt ASSIGNING FIELD-SYMBOL(&lt;dtxt&gt;).
      READ TABLE me-&gt;at_sdat ASSIGNING &lt;sdat&gt; WITH KEY ctlg_itm_key = &lt;dtxt&gt;-ctlg_itm_key. CHECK sy-subrc = 0.
      CASE &lt;dtxt&gt;-language.
        WHEN &apos;E&apos;. MOVE: &lt;dtxt&gt;-description TO &lt;sdat&gt;-description_en.
        WHEN &apos;쁩&apos;. MOVE: &lt;dtxt&gt;-description TO &lt;sdat&gt;-description_vi.
      ENDCASE.
    ENDLOOP.
    &quot;..long texts
    LOOP AT lt_ltxt ASSIGNING FIELD-SYMBOL(&lt;ltxt&gt;).
      READ TABLE me-&gt;at_sdat ASSIGNING &lt;sdat&gt; WITH KEY ctlg_itm_key = &lt;ltxt&gt;-ctlg_itm_key. CHECK sy-subrc = 0.
      CASE &lt;ltxt&gt;-language.
        WHEN &apos;E&apos;. MOVE: &lt;ltxt&gt;-longtext TO &lt;sdat&gt;-longtext_en.
        WHEN &apos;쁩&apos;. MOVE: &lt;ltxt&gt;-longtext TO &lt;sdat&gt;-longtext_vi.
      ENDCASE.
    ENDLOOP.
    &quot;..prices
    LOOP AT lt_pcnv ASSIGNING FIELD-SYMBOL(&lt;pcnv&gt;).
      READ TABLE me-&gt;at_sdat ASSIGNING &lt;sdat&gt; WITH KEY price_key = &lt;pcnv&gt;-price_key. CHECK sy-subrc = 0.
      MOVE-CORRESPONDING: &lt;pcnv&gt; TO &lt;sdat&gt;.
      &quot;***TBD check what is wrong here
      &lt;sdat&gt;-itm_price       = &lt;sdat&gt;-itm_price       / 100.
      &lt;sdat&gt;-price_comm_curr = &lt;sdat&gt;-price_comm_curr / 100.
    ENDLOOP.




    &quot;..attachments
    LOOP AT lt_atmn ASSIGNING FIELD-SYMBOL(&lt;atmn&gt;).
      READ TABLE lt_atch ASSIGNING FIELD-SYMBOL(&lt;atch&gt;) WITH KEY ctlg_itm_key = &lt;atmn&gt;-ctlg_itm_key
                                                                 image_key    = &lt;atmn&gt;-main_image_key. CHECK sy-subrc = 0.
      READ TABLE me-&gt;at_sdat ASSIGNING &lt;sdat&gt; WITH KEY ctlg_itm_key = &lt;atch&gt;-ctlg_itm_key. CHECK sy-subrc = 0.
      MOVE-CORRESPONDING: &lt;atch&gt; TO &lt;sdat&gt;.
    ENDLOOP.
    &quot;..search terms
    &quot;***TBD

    &quot;default status (items from catalog, not file)
    IF me-&gt;ax_file IS INITIAL.
      LOOP AT me-&gt;at_sdat ASSIGNING &lt;sdat&gt;.
        MOVE: &apos;C&apos; TO &lt;sdat&gt;-xupl_statu.
      ENDLOOP.
    ENDIF.



    &quot;default sort (I never know whether this is good idea)
    SORT me-&gt;at_sdat BY ctlg_itm_key DESCENDING.

  ENDMETHOD.</source>
  </method>
