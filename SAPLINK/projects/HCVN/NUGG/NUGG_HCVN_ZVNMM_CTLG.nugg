  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_CTLG_RUPL" CMPNAME="SALV_INIT_CCOL" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: colours per columns" EXPOSURE="0" STATE="1" EDITORDER="13 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SALV_INIT_CCOL.

    &quot;init: column colours

    DATA: lr_col   TYPE REF TO cl_salv_column_list, &quot; object of column, allowing set_color
          ls_color TYPE        lvc_s_colo.          &quot; colours structure

    DATA(lr_cols) = me-&gt;ar_salv-&gt;get_columns( ).
    DATA(lt_cols) = lr_cols-&gt;get( ).

    LOOP AT lt_cols ASSIGNING FIELD-SYMBOL(&lt;col&gt;).
      TRY.
          lr_col ?= lr_cols-&gt;get_column( &lt;col&gt;-columnname ).
        CATCH cx_salv_not_found.
      ENDTRY.

      DATA(ls_c101) = VALUE lvc_s_colo( col = 1 inv = 0 int = 1 ).  &quot;blue+h
      DATA(ls_c100) = VALUE lvc_s_colo( col = 1 inv = 0 int = 0 ).  &quot;blue
      DATA(ls_c300) = VALUE lvc_s_colo( col = 3 inv = 0 int = 0 ).  &quot;yellow
      DATA(ls_c700) = VALUE lvc_s_colo( col = 7 inv = 0 int = 0 ).  &quot;orange

      &quot;setting colours
      CASE &lt;col&gt;-columnname.
        WHEN &apos;CTLG_ITM_KEY&apos;
          OR &apos;CAT_ITM_HASH&apos;     . lr_col-&gt;set_color( ls_c101 ). &quot;keys : blue+h
        WHEN &apos;WS_SERV_NAME&apos;
          OR &apos;VENDOR_NAME&apos;
          OR &apos;DESCRIPTION_EN&apos;
          OR &apos;DESCRIPTION_VI&apos;
          OR &apos;LONGTEXT_EN&apos;
          OR &apos;LONGTEXT_VI&apos;      . lr_col-&gt;set_color( ls_c300 ). &quot;descr: yellow
        WHEN  OTHERS            . lr_col-&gt;set_color( ls_c100 ). &quot;data : blue
      ENDCASE.

    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_CTLG_RUPL" CMPNAME="SALV_INIT_COLS" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: columns types (exc, icon, chkbox)" EXPOSURE="0" STATE="1" EDITORDER="10 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_cols.

    &quot;init columns type: exception, icon, checkbox, etc

***    &quot; set column for lights
***    DATA(lr_cols) = me-&gt;ar_salv-&gt;get_columns( ).
***    TRY.
***        lr_cols-&gt;set_exception_column( value = &apos;STATU_IC&apos; ).
***      CATCH cx_salv_data_error.
***    ENDTRY.

***    &quot;init cols: icon
***    TRY.
***        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;LOEKZ_IC &apos;) )-&gt;set_icon( &apos;X&apos; ).
***      CATCH cx_salv_data_error.
***      CATCH cx_salv_not_found.
***    ENDTRY.

    TRY.
        &quot;init cols: checkbox

***        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;XUPL_STATU&apos;) )-&gt;set_cell_type( if_salv_c_cell_type=&gt;checkbox ).

        &quot;init cols: centered
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;XUPL_STATU_IC&apos;) )-&gt;set_alignment( if_salv_c_alignment=&gt;centered ).

        &quot;init cols: no conv-output
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;CTLG_ITM_KEY&apos;) )-&gt;set_edit_mask( value = &apos;__________&apos; ). &quot;c10
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;PRICE_KEY&apos;) )-&gt;set_edit_mask( value = &apos;__________&apos; )   . &quot;c10

        &quot;uoms
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;UNIT&apos;) )-&gt;set_edit_mask( value = &apos;==CUNIT&apos; ) .
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;CONTENT_UNIT&apos;) )-&gt;set_edit_mask( value = &apos;==CUNIT&apos; ) .


        &quot;init cols: currency
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;ITM_PRICE&apos;) )-&gt;set_currency_column( &apos;ITM_CURRENCY&apos; ).
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;PRICE_COMM_CURR&apos;) )-&gt;set_currency_column( &apos;COMMON_CURRENCY&apos; ).

        &quot;init cols: decimals
**        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;CPAMT&apos;) )-&gt;set_decimals( value = &apos;0&apos; ).


      CATCH cx_salv_data_error.
      CATCH cx_salv_not_found.
    ENDTRY.

  ENDMETHOD.</source>
