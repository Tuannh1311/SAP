  <method CLSNAME="ZCL_VNMM_CTLG_RCAT" CMPNAME="ON_UCOM_DELETE" VERSION="1" LANGU="E" DESCRIPT="call: delete" EXPOSURE="0" STATE="1" EDITORDER="22 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD ON_UCOM_DELETE.

    &quot;full item deletion
    CHECK me-&gt;at_rsel IS NOT INITIAL.

    &quot;popup confirm
    DATA: lx_answ TYPE c.

    CALL FUNCTION &apos;POPUP_TO_CONFIRM&apos;
      EXPORTING
        titlebar              = &apos;Catalog item deletion&apos;(q01)
        text_question         = &apos;Full deletion from database?&apos;(q02)
        text_button_1         = &apos;Delete&apos;(q03)
        icon_button_1         = &apos;ICON_DELETE&apos;
        text_button_2         = &apos;no&apos;(q04)
        icon_button_2         = &apos;ICON_CANCEL&apos;
        default_button        = &apos;2&apos;
        display_cancel_button = &apos;&apos;
      IMPORTING
        answer                = lx_answ
      EXCEPTIONS
        text_not_found        = 1
        OTHERS                = 2.

    CHECK lx_answ = 1. &quot;1= delete

    &quot;
    &quot;(? via fm MMPUR_CATALOG_DATA_DELETE? but thats just same)
    LOOP AT me-&gt;at_rsel ASSIGNING FIELD-SYMBOL(&lt;rsel&gt;).
      READ TABLE me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;) INDEX &lt;rsel&gt;. CHECK sy-subrc = 0.

      DELETE FROM mmpur_cat_itm     WHERE ctlg_itm_key = &lt;sdat&gt;-ctlg_itm_key.
      DELETE FROM mmpur_cat_dsctxt  WHERE ctlg_itm_key = &lt;sdat&gt;-ctlg_itm_key.
      DELETE FROM mmpur_cat_lngtxt  WHERE ctlg_itm_key = &lt;sdat&gt;-ctlg_itm_key.
      DELETE FROM mmpur_cat_mgptxt  WHERE ctlg_itm_key = &lt;sdat&gt;-ctlg_itm_key.
      DELETE FROM mmpur_cat_attchm  WHERE ctlg_itm_key = &lt;sdat&gt;-ctlg_itm_key.
*      delete from MMPUR_CAT_ATHTXT  WHERE ctlg_itm_key = &lt;sdat&gt;-ctlg_itm_key.
      DELETE FROM mmpur_cat_atthmb  WHERE ctlg_itm_key = &lt;sdat&gt;-ctlg_itm_key.
      DELETE FROM mmpur_cat_atmain  WHERE ctlg_itm_key = &lt;sdat&gt;-ctlg_itm_key.
*      DELETE FROM mmpur_cat_prconv  WHERE ctlg_itm_key = &lt;sdat&gt;-ctlg_itm_key.
      DELETE FROM mmpur_cat_prcscl  WHERE ctlg_itm_key = &lt;sdat&gt;-ctlg_itm_key.
      DELETE FROM mmpur_cat_itmatr  WHERE ctlg_itm_key = &lt;sdat&gt;-ctlg_itm_key.
      DELETE FROM mmpur_ct_itm_srh  WHERE ctlg_itm_key = &lt;sdat&gt;-ctlg_itm_key.
      DELETE FROM mmpur_cat_my_fav  WHERE ctlg_itm_key = &lt;sdat&gt;-ctlg_itm_key.
      DELETE FROM mmpur_cat_rtgrvw  WHERE ctlg_itm_key = &lt;sdat&gt;-ctlg_itm_key.
      DELETE FROM mmpur_cat_prdrel  WHERE ctlg_itm_key = &lt;sdat&gt;-ctlg_itm_key.
      DELETE FROM mmpur_cat_vr      WHERE ctlg_itm_key = &lt;sdat&gt;-ctlg_itm_key.

    ENDLOOP.

    CALL FUNCTION &apos;BAPI_TRANSACTION_COMMIT&apos; EXPORTING wait = &apos;X&apos;.





  ENDMETHOD.</source>
  </method>
