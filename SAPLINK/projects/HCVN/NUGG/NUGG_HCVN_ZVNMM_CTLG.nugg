  </method>
  <method CLSNAME="ZCL_VNMM_CTLG_RUPL" CMPNAME="SALV_INIT_MLTS" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: mutli-selection" EXPOSURE="0" STATE="1" EDITORDER="9 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SALV_INIT_MLTS.

    &quot;set multiple-selection
    DATA(lr_sel) = me-&gt;ar_salv-&gt;get_selections( ).
