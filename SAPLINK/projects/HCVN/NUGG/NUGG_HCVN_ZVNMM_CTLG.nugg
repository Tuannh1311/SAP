  <method CLSNAME="ZCL_VNMM_CTLG_RCON" CMPNAME="ON_UCOM_PUBLISH" VERSION="1" LANGU="E" DESCRIPT="call: publish to catalog" EXPOSURE="0" STATE="1" EDITORDER="19 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD on_ucom_publish.
    DATA: lt_citm TYPE mmpur_cat_t_cat_itm,
          lt_desc TYPE mmpur_cat_t_desctxt,
          lt_ltxt TYPE mmpur_cat_t_longtxt,
          lt_strm TYPE mmpur_cat_t_srchtrm,
          lt_prcv TYPE mmpur_cat_t_prcconv,
          lt_attm TYPE mmpur_cat_t_att_main,
          lt_atch TYPE mmpur_cat_t_atchmnt.
    DATA: lx_inew TYPE xfeld.

    &quot;publish to catalogue
    &quot;(based on class CL_MM_PUR_CTLG_EXTRACTOR ~EXTRACTOR_MATMASTER )
    CHECK me-&gt;at_rsel[] IS NOT INITIAL.

    &quot;per each item separately
    LOOP AT me-&gt;at_rsel ASSIGNING FIELD-SYMBOL(&lt;rsel&gt;).
      CLEAR: lt_citm[], lt_desc[], lt_ltxt[], lt_strm[], lt_prcv[],
             lx_inew.
      READ TABLE me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;) INDEX &lt;rsel&gt;. CHECK sy-subrc = 0.

      SELECT SINGLE * FROM ekko WHERE ebeln = @&lt;sdat&gt;-ebeln INTO @DATA(ls_ekko).
      SELECT SINGLE * FROM ekpo WHERE ebeln = @&lt;sdat&gt;-ebeln
                                  AND ebelp = @&lt;sdat&gt;-ebelp INTO @DATA(ls_ekpo).

      &quot;**********************************************************************
      &quot;prep: item MMPUR_CAT_ITM

      APPEND INITIAL LINE TO lt_citm ASSIGNING FIELD-SYMBOL(&lt;citm&gt;).
      &quot;key &amp; hash
      IF me-&gt;px_ctupd        IS INITIAL OR
         &lt;sdat&gt;-ctlg_itm_key IS INITIAL   .
        CALL METHOD cl_mm_pur_cat_data_trigger=&gt;get_sha1_key
          EXPORTING
            iv_catalog_itm_guid = CONV #( &lt;sdat&gt;-matnr )
          IMPORTING
            ev_sha1_hash_key    = DATA(lv_hash)
            ev_ctlg_itm_key     = DATA(lv_ikey).
        MOVE: lv_hash TO &lt;sdat&gt;-cat_itm_hash,
              lv_ikey TO &lt;sdat&gt;-ctlg_itm_key.
        MOVE: &apos;X&apos; TO lx_inew.
      ENDIF.
      MOVE: &lt;sdat&gt;-ctlg_itm_key TO &lt;citm&gt;-ctlg_itm_key,
            &lt;sdat&gt;-cat_itm_hash TO &lt;citm&gt;-cat_itm_hash.

      &quot;price key
      IF me-&gt;px_ctupd     IS INITIAL OR
         &lt;sdat&gt;-price_key IS INITIAL   .
        CALL FUNCTION &apos;NUMBER_GET_NEXT&apos;
          EXPORTING
            nr_range_nr             = &apos;01&apos;
            object                  = &apos;MMPUR_PC&apos;
          IMPORTING
            number                  = &lt;sdat&gt;-price_key
          EXCEPTIONS
            interval_not_found      = 1
            number_range_not_intern = 2
            object_not_found        = 3
            quantity_is_0           = 4
            quantity_is_not_1       = 5
            interval_overflow       = 6
            buffer_overflow         = 7
            OTHERS                  = 8.
        IF sy-subrc NE 0. MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4. ENDIF.
      ENDIF.
      MOVE: &lt;sdat&gt;-price_key TO &lt;citm&gt;-price_key.

      &quot;item: catalog name
      MOVE: me-&gt;pa_ctnam TO &lt;citm&gt;-ws_service_id.
      SELECT SINGLE * FROM wsi_description WHERE spras      EQ @sy-langu
                                             AND service_id EQ @me-&gt;pa_ctnam INTO @DATA(ls_wsids).
      MOVE: ls_wsids-service_descript TO &lt;citm&gt;-ws_serv_name,
            ls_wsids-service_descript TO &lt;citm&gt;-catalog_name,
            ls_wsids-service_descript TO &lt;citm&gt;-intcatalogname.

      &quot;item: vendor name
      MOVE: &lt;sdat&gt;-lifnr TO &lt;citm&gt;-vendor.
      SELECT SINGLE name1 FROM lfa1 WHERE lifnr = @&lt;sdat&gt;-lifnr INTO @&lt;citm&gt;-vendor_name.

      &quot;item: VENDOR_MAT
      MOVE: ls_ekpo-idnlf TO &lt;citm&gt;-vendor_mat.

      &quot;item: price validity
      CONVERT DATE: ls_ekko-kdatb INTO TIME STAMP &lt;citm&gt;-price_valid_from TIME ZONE &apos;UTC&apos;,
                    ls_ekko-kdate INTO TIME STAMP &lt;citm&gt;-price_valid_to   TIME ZONE &apos;UTC&apos;.

      &quot;item: data
      MOVE: ls_ekpo-peinh TO &lt;citm&gt;-price_quantity,
            ls_ekpo-matnr TO &lt;citm&gt;-matnr,
            ls_ekpo-matkl TO &lt;citm&gt;-matgroup,
            ls_ekpo-plifz TO &lt;citm&gt;-leadtime,
            ls_ekpo-meins TO &lt;citm&gt;-unit,
            ls_ekpo-meins TO &lt;citm&gt;-content_unit,
            ls_ekpo-peinh TO &lt;citm&gt;-minorder_qty,
            ls_ekpo-ebeln TO &lt;citm&gt;-contract,
            ls_ekpo-ebelp TO &lt;citm&gt;-contract_item.
      CONDENSE: &lt;citm&gt;-leadtime,
                &lt;citm&gt;-minorder_qty.
      IF &lt;citm&gt;-leadtime     = &apos;0&apos;. CLEAR &lt;citm&gt;-leadtime.     ENDIF.
      IF &lt;citm&gt;-minorder_qty = &apos;0&apos;. CLEAR &lt;citm&gt;-minorder_qty. ENDIF.

      &quot;item: created
      MOVE: sy-datum TO &lt;citm&gt;-created_on,
            sy-uname TO &lt;citm&gt;-changed_by.
      CONVERT DATE sy-datum TIME sy-uzeit INTO TIME STAMP &lt;citm&gt;-changed_at TIME ZONE &apos;UTC&apos;.

      &quot;item: active/inactive
      MOVE: me-&gt;px_inact TO &lt;citm&gt;-deletion_ind.

      &quot;item: plant, company
      MOVE: ls_ekpo-werks TO &lt;citm&gt;-plant,
            ls_ekko-bukrs TO &lt;citm&gt;-company_code.
      &quot;item: porg
      IF me-&gt;pa_ekorg IS NOT INITIAL.
        MOVE: me-&gt;pa_ekorg  TO &lt;citm&gt;-ekorg.
      ELSE.
        MOVE: ls_ekko-ekorg TO &lt;citm&gt;-ekorg.
      ENDIF.

      &quot;**********************************************************************
      &quot;***descriptions en+vi
      SELECT SINGLE * FROM makt WHERE matnr = @ls_ekpo-matnr AND spras = &apos;E&apos; INTO @DATA(ls_makt_en).
      IF sy-subrc = 0.
        APPEND INITIAL LINE TO lt_desc ASSIGNING FIELD-SYMBOL(&lt;desc_en&gt;).
        MOVE: &lt;citm&gt;-ctlg_itm_key TO &lt;desc_en&gt;-ctlg_itm_key,
              ls_makt_en-spras    TO &lt;desc_en&gt;-language,
              ls_makt_en-maktx    TO &lt;desc_en&gt;-description.
      ENDIF.
      SELECT SINGLE * FROM makt WHERE matnr = @ls_ekpo-matnr AND spras = &apos;쁩&apos; INTO @DATA(ls_makt_vi).
      IF sy-subrc = 0.
        APPEND INITIAL LINE TO lt_desc ASSIGNING FIELD-SYMBOL(&lt;desc_vi&gt;).
        MOVE: &lt;citm&gt;-ctlg_itm_key TO &lt;desc_vi&gt;-ctlg_itm_key,
              ls_makt_vi-spras    TO &lt;desc_vi&gt;-language,
              ls_makt_vi-maktx    TO &lt;desc_vi&gt;-description.
      ENDIF.

      &quot;**********************************************************************
      &quot;***long text
      &quot;check text (and get language)
      DATA: lv_tdnam TYPE stxh-tdname.
      CLEAR lv_tdnam.
      CONCATENATE ls_ekpo-ebeln ls_ekpo-ebelp INTO lv_tdnam.
      SELECT SINGLE * FROM stxh INTO @DATA(ls_stxh) WHERE tdobject = &apos;EKPO&apos;
                                                      AND tdname   = @lv_tdnam
                                                      AND tdid     = &apos;K03&apos;     . &quot;K03= OAgr POtext
      IF sy-subrc = 0.
        &quot;get text
        DATA: lt_line TYPE TABLE OF tline.
        CALL FUNCTION &apos;READ_TEXT&apos;
          EXPORTING
            id                      = ls_stxh-tdid
            language                = ls_stxh-tdspras
            name                    = ls_stxh-tdname
            object                  = ls_stxh-tdobject
          TABLES
            lines                   = lt_line
          EXCEPTIONS
            id                      = 1
            language                = 2
            name                    = 3
            not_found               = 4
            object                  = 5
            reference_check         = 6
            wrong_access_to_archive = 7
            OTHERS                  = 8.
        IF sy-subrc = 0.
          &quot;convert to string2083
          DATA: lt_s2083 TYPE TABLE OF text2083.
          CALL FUNCTION &apos;CONVERT_ITF_TO_STREAM_TEXT&apos;
            EXPORTING
              language    = ls_stxh-tdspras
            TABLES
              itf_text    = lt_line
              text_stream = lt_s2083.
          READ TABLE lt_s2083 ASSIGNING FIELD-SYMBOL(&lt;s2083&gt;) INDEX 1.
          IF sy-subrc = 0.
            APPEND INITIAL LINE TO lt_ltxt ASSIGNING FIELD-SYMBOL(&lt;ltxt_en&gt;).
            MOVE: &lt;citm&gt;-ctlg_itm_key TO &lt;ltxt_en&gt;-ctlg_itm_key,
                  &apos;E&apos;                 TO &lt;ltxt_en&gt;-language,
                  &lt;s2083&gt;             TO &lt;ltxt_en&gt;-longtext.
            APPEND INITIAL LINE TO lt_ltxt ASSIGNING FIELD-SYMBOL(&lt;ltxt_vi&gt;).
            MOVE: &lt;citm&gt;-ctlg_itm_key TO &lt;ltxt_vi&gt;-ctlg_itm_key,
                  &apos;쁩&apos;                  TO &lt;ltxt_vi&gt;-language,
                  &lt;s2083&gt;             TO &lt;ltxt_vi&gt;-longtext.
          ENDIF.
        ENDIF.


      ENDIF.



      &quot;**********************************************************************
      &quot;***search term (??, at least keep it)
      CLEAR: lt_strm.
      SELECT * FROM mmpur_ct_itm_srh WHERE ctlg_itm_key = @&lt;sdat&gt;-ctlg_itm_key  INTO TABLE @lt_strm.

      &quot;**********************************************************************
      &quot;***price converions
      APPEND INITIAL LINE TO lt_prcv ASSIGNING FIELD-SYMBOL(&lt;prcv&gt;).
      MOVE: &lt;citm&gt;-price_key TO &lt;prcv&gt;-price_key,
            ls_ekpo-netpr    TO &lt;prcv&gt;-itm_price,
            ls_ekko-waers    TO &lt;prcv&gt;-itm_currency,
            &quot;(one would thing that here we should hade common currency VND..
            &quot; ..but this is what it done from manual maintenance from Fiori, so keep it)
            ls_ekpo-netpr    TO &lt;prcv&gt;-price_comm_curr,
            ls_ekko-waers    TO &lt;prcv&gt;-common_currency.


      &quot;keep MMPUR_CAT_T_ATCHMNT &amp; MMPUR_CAT_T_ATT_MAIN (to not loose images)
      CLEAR: lt_attm, lt_atch.
      SELECT * FROM mmpur_cat_atmain WHERE ctlg_itm_key = @&lt;sdat&gt;-ctlg_itm_key  INTO TABLE @lt_attm.
      SELECT * FROM mmpur_cat_attchm WHERE ctlg_itm_key = @&lt;sdat&gt;-ctlg_itm_key  INTO TABLE @lt_atch.


      &quot;**********************************************************************
      &quot;call update
      CALL FUNCTION &apos;SAVE_CATALOG_ITEM&apos;
        EXPORTING
          it_catalog_item       = lt_citm
          it_catalog_desc       = lt_desc
          it_catalog_lngtxt     = lt_ltxt
          it_search_term        = lt_strm
          it_catalog_price      = lt_prcv
          it_catalog_image      = lt_atch
          it_catalog_image_main = lt_attm
          iv_new_item           = lx_inew
*         IV_ACTIVATE           = IV_ACTIVATE
        EXCEPTIONS
          db_failure            = 1
          OTHERS                = 2.
      IF sy-subrc NE 0. MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4. ENDIF.
      IF sy-subrc EQ 0. CALL FUNCTION &apos;BAPI_TRANSACTION_COMMIT&apos; EXPORTING wait = &apos;X&apos;.                               ENDIF.



    ENDLOOP.



  ENDMETHOD.</source>
  </method>
