      SELECT * FROM mmpur_ct_itm_srh WHERE ctlg_itm_key = @&lt;sdat&gt;-ctlg_itm_key  INTO TABLE @lt_strm.
      SELECT * FROM mmpur_cat_atmain WHERE ctlg_itm_key = @&lt;sdat&gt;-ctlg_itm_key  INTO TABLE @lt_attm.
      SELECT * FROM mmpur_cat_attchm WHERE ctlg_itm_key = @&lt;sdat&gt;-ctlg_itm_key  INTO TABLE @lt_atch.

      CALL FUNCTION &apos;SAVE_CATALOG_ITEM&apos;
