   <parameter CLSNAME="ZCL_VNMM_CTLG_RCON" CMPNAME="ON_UCOM" SCONAME="E_SALV_FUNCTION" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="1" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="0"/>
   <source>METHOD on_ucom.

    &quot;default: get selected rows
    me-&gt;at_rsel = me-&gt;ar_salv-&gt;get_selections( )-&gt;get_selected_rows( ).


