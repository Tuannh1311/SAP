  <method CLSNAME="ZCL_VNMM_CTLG_RCAT" CMPNAME="ON_UCOM" VERSION="1" LANGU="E" DESCRIPT="handler: on user-command" EXPOSURE="0" STATE="1" EDITORDER="19 " DISPID="0 " MTDTYPE="1" MTDDECLTYP="0" REFCLSNAME="CL_SALV_EVENTS" REFCMPNAME="ADDED_FUNCTION" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNMM_CTLG_RCAT" CMPNAME="ON_UCOM" SCONAME="E_SALV_FUNCTION" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="1" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="0"/>
   <source>METHOD ON_UCOM.

    &quot;default: get selected rows
    me-&gt;at_rsel = me-&gt;ar_salv-&gt;get_selections( )-&gt;get_selected_rows( ).


    CASE e_salv_function.

      WHEN &apos;REFRESH&apos;. me-&gt;salv_refr( ).

      WHEN   &apos;ACTIVATE&apos;. me-&gt;on_ucom_activate( ).   me-&gt;salv_refr( ).
      WHEN &apos;DEACTIVATE&apos;. me-&gt;on_ucom_deactive( ).   me-&gt;salv_refr( ).

      WHEN &apos;DELETE&apos;.     me-&gt;on_ucom_delete( ).     me-&gt;salv_refr( ).

      WHEN &apos;CHGLOG&apos;.     me-&gt;on_ucom_chglog( ).

    ENDCASE.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_CTLG_RCAT" CMPNAME="ON_UCOM_ACTIVATE" VERSION="1" LANGU="E" DESCRIPT="call: activate" EXPOSURE="0" STATE="1" EDITORDER="20 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD on_ucom_activate.

    &quot;activate selected items

    CHECK me-&gt;at_rsel IS NOT INITIAL.

    &quot;item by item
    LOOP AT me-&gt;at_rsel  ASSIGNING FIELD-SYMBOL(&lt;rsel&gt;).

      DATA: lt_citm TYPE mmpur_cat_t_cat_itm.
      CLEAR lt_citm.

      READ TABLE me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;) INDEX &lt;rsel&gt;. CHECK sy-subrc = 0.
      SELECT SINGLE * FROM mmpur_cat_itm WHERE ctlg_itm_key = @&lt;sdat&gt;-ctlg_itm_key
                                           AND cat_itm_hash = @&lt;sdat&gt;-cat_itm_hash INTO @DATA(ls_citm). CHECK sy-subrc = 0.

      MOVE: &apos; &apos; TO ls_citm-deletion_ind.
      APPEND ls_citm TO lt_citm.
      &quot;(although the save_cat fm has citm as table, it cannot be called for multiple records.. weird)

      &quot;(descriptions disappearing just with activation? needs to pass current data..
      &quot; ..also all other data, for correct change logs                              )
      DATA: lt_desc TYPE mmpur_cat_t_desctxt,
            lt_prcv TYPE mmpur_cat_t_prcconv,
            lt_ltxt TYPE mmpur_cat_t_longtxt,
            lt_strm TYPE mmpur_cat_t_srchtrm,
            lt_attm TYPE mmpur_cat_t_att_main,
            lt_atch TYPE mmpur_cat_t_atchmnt.
      CLEAR: lt_desc, lt_prcv, lt_ltxt, lt_strm, lt_attm, lt_atch.
      SELECT * FROM mmpur_cat_dsctxt WHERE ctlg_itm_key = @&lt;sdat&gt;-ctlg_itm_key  INTO TABLE @lt_desc.
      SELECT * FROM mmpur_cat_prconv WHERE price_key    = @&lt;sdat&gt;-price_key     INTO TABLE @lt_prcv.
      SELECT * FROM mmpur_cat_lngtxt WHERE ctlg_itm_key = @&lt;sdat&gt;-ctlg_itm_key  INTO TABLE @lt_ltxt.
      SELECT * FROM mmpur_ct_itm_srh WHERE ctlg_itm_key = @&lt;sdat&gt;-ctlg_itm_key  INTO TABLE @lt_strm.
      SELECT * FROM mmpur_cat_atmain WHERE ctlg_itm_key = @&lt;sdat&gt;-ctlg_itm_key  INTO TABLE @lt_attm.
      SELECT * FROM mmpur_cat_attchm WHERE ctlg_itm_key = @&lt;sdat&gt;-ctlg_itm_key  INTO TABLE @lt_atch.

      &quot;save itself
      CALL FUNCTION &apos;SAVE_CATALOG_ITEM&apos;
        EXPORTING
          it_catalog_item       = lt_citm
          it_catalog_desc       = lt_desc
          it_catalog_lngtxt     = lt_ltxt
          it_search_term        = lt_strm
          it_catalog_price      = lt_prcv
          it_catalog_image      = lt_atch
          it_catalog_image_main = lt_attm
          iv_new_item           = &apos;&apos;
          &quot;IV_ACTIVATE           = IV_ACTIVATE
          &quot;(activate not as activate, since this bypass the change log..
          &quot; ..this is truly idiotic; brave new world of fiori apps      )
        EXCEPTIONS
          db_failure            = 1
          OTHERS                = 2.
      IF sy-subrc = 0.
        CALL FUNCTION &apos;BAPI_TRANSACTION_COMMIT&apos; EXPORTING wait = &apos;X&apos;.
      ELSE.
        MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4.
      ENDIF.



    ENDLOOP.








  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_CTLG_RCAT" CMPNAME="ON_UCOM_CHGLOG" VERSION="1" LANGU="E" DESCRIPT="call: change log" EXPOSURE="0" STATE="1" EDITORDER="23 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD ON_UCOM_CHGLOG.

    &quot;display change log (for multiple recs)

    CHECK me-&gt;at_rsel IS NOT INITIAL.

    &quot;prep ranges
    DATA: ra_objid TYPE RANGE OF cdhdr-objectid.

    LOOP AT me-&gt;at_rsel ASSIGNING FIELD-SYMBOL(&lt;rsel&gt;).
      READ TABLE me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;) INDEX &lt;rsel&gt;. CHECK sy-subrc = 0.

      APPEND INITIAL LINE TO ra_objid ASSIGNING FIELD-SYMBOL(&lt;objid&gt;).
      MOVE: &apos;I&apos;                 TO &lt;objid&gt;-sign,
            &apos;EQ&apos;                TO &lt;objid&gt;-option,
            &lt;sdat&gt;-ctlg_itm_key TO &lt;objid&gt;-low.

    ENDLOOP.

    CHECK ra_objid[] IS NOT INITIAL.

    SUBMIT rsscd_disp_web WITH s_object-low = &apos;MM_PUR_CATITM&apos;
                          WITH s_obid       IN ra_objid
                          WITH p_date       = &apos;19000101&apos;       AND RETURN .

  ENDMETHOD.</source>
  </method>
