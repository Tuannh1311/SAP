*        outmod  =
*        exprss  =
*        test    =
        idoctyp = &apos;FIDCCP02&apos;
*        cimtyp  = &apos;YFIDCCP02&apos;
        mestyp  = &apos;FIDCC2&apos;
*        mescod  =
*        mesfct  =
