DATA(lo_hsi) = NEW zcl_vnhsi( iv_domain = pa_pdoma
                              iv_code   = pa_pcode
                              &quot;is_ifln   = &lt;ifln&gt;
                              ).
lo_hsi-&gt;process( ).</source>
