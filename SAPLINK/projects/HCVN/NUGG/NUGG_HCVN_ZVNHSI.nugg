  <method CLSNAME="ZCL_VNHSI" CMPNAME="BAPI_EMAIL" VERSION="1" LANGU="E" DESCRIPT="CVI emai: call via bapi BAPI_ADDRESSORG_CHANGE" EXPOSURE="0" STATE="1" EDITORDER="20 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD bapi_email.

    &quot;
    CHECK: me-&gt;at_smtp    IS NOT INITIAL,
           me-&gt;av_partner IS NOT INITIAL.

    &quot;
    DATA: lt_bsmtp  TYPE STANDARD TABLE OF bapiadsmtp,
          lt_xsmtp  TYPE STANDARD TABLE OF bapiadsmtx,
          lt_return TYPE STANDARD TABLE OF bapiret2.

    &quot;map CVI to BAPI
    LOOP AT me-&gt;at_smtp ASSIGNING FIELD-SYMBOL(&lt;smtp&gt;).

      APPEND INITIAL LINE TO lt_bsmtp ASSIGNING FIELD-SYMBOL(&lt;bsmtp&gt;).
      APPEND INITIAL LINE TO lt_xsmtp ASSIGNING FIELD-SYMBOL(&lt;xsmtp&gt;).

      MOVE: &lt;smtp&gt;-contact-data  TO &lt;bsmtp&gt;,
            &lt;smtp&gt;-contact-datax TO &lt;xsmtp&gt;.

    ENDLOOP.

    &quot;call BAPI
    CALL FUNCTION &apos;BAPI_BUPA_ADDRESS_CHANGE&apos;
      EXPORTING
        businesspartner = me-&gt;av_partner
      TABLES
        bapiadsmtp      = lt_bsmtp
        bapiadsmt_x     = lt_xsmtp
        return          = lt_return.

    LOOP AT lt_return ASSIGNING FIELD-SYMBOL(&lt;ret&gt;).
      WRITE: / &apos;   #cl_md_bp_maintain=&gt;BAPI_EMAIL:&apos;,
            &lt;ret&gt;-id, &lt;ret&gt;-number, &lt;ret&gt;-message.
    ENDLOOP.



  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNHSI" CMPNAME="BUPA_GET" VERSION="1" LANGU="E" DESCRIPT="get Partner, if exists" EXPOSURE="0" STATE="1" EDITORDER="3 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" MTDNEWEXC="X" BCMTDCAT="00" BCMTDSYN="0">
   <exception CLSNAME="ZCL_VNHSI" CMPNAME="BUPA_GET" SCONAME="ZCX_VNHSI" VERSION="1" LANGU="E" DESCRIPT="CSI: DCS exception class" MTDTYPE="0" EDITORDER="1 "/>
   <source>METHOD bupa_get.


    CASE me-&gt;cs_c020-uqidty.

      WHEN &apos;IDNUMBER&apos;.
        &quot;get partner, by customized extID
        SELECT SINGLE partner FROM but0id INTO @me-&gt;av_partner WHERE type     = @me-&gt;cs_c020-idtyp
                                                                 AND idnumber = @me-&gt;av_pcode     .


      WHEN &apos;BPEXT&apos;.
        CASE me-&gt;av_domain.

          WHEN &apos;SALESMAN&apos;.
            &quot;get partner, by external IDnumber (Salesman)
            SELECT SINGLE partner FROM but000 INTO @me-&gt;av_partner WHERE bpext = @me-&gt;wss0-ident_number .

          WHEN &apos;PARTNER&apos;.
            &quot;get partner, by external IDnumber (Partner)
            SELECT SINGLE partner FROM but000 INTO @me-&gt;av_partner WHERE bpext = @me-&gt;wsp0-tax_identification_number.
        ENDCASE.

    ENDCASE.

    &quot;then guid
    IF me-&gt;av_partner IS NOT INITIAL.
      SELECT SINGLE partner_guid FROM but000 INTO @me-&gt;av_pguid WHERE partner = @me-&gt;av_partner.
    ENDIF.


    &quot;log-write
    IF me-&gt;av_partner IS NOT INITIAL.
      WRITE: / &apos; determined Partner=&apos;, me-&gt;av_partner.
    ENDIF.

  ENDMETHOD.</source>
  </method>
