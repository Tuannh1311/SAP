  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNHSI" CMPNAME="C02X_GET" VERSION="1" LANGU="E" DESCRIPT="get configuration C02* per Domain/PartnerType" EXPOSURE="0" STATE="1" EDITORDER="2 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" MTDNEWEXC="X" BCMTDCAT="00" BCMTDSYN="0">
   <exception CLSNAME="ZCL_VNHSI" CMPNAME="C02X_GET" SCONAME="ZCX_VNHSI" VERSION="1" LANGU="E" DESCRIPT="CSI: DCS exception class" MTDTYPE="0" EDITORDER="1 "/>
   <source>METHOD c02x_get.

    &quot;get customizing per Domain/PartnerType

    &quot;set PartnerType (and Salesman)
    CASE me-&gt;av_domain .
      WHEN &apos;PARTNER&apos; .
        CASE me-&gt;wsp0-partner_type.
          WHEN &apos;S&apos;.    MOVE: &apos;D&apos; TO me-&gt;av_ptype.  &quot;Seller   -&gt; Dealer
          WHEN &apos;PRD&apos;.  MOVE: &apos;P&apos; TO me-&gt;av_ptype.  &quot;Producer -&gt; Producer
          WHEN &apos;MER&apos;.  MOVE: &apos;M&apos; TO me-&gt;av_ptype.  &quot;Merchant -&gt; Merchant
          WHEN &apos;CCP&apos;.  MOVE: &apos;C&apos; TO me-&gt;av_ptype.  &quot;CrCashPr -&gt; CreditCashProvider &quot;++CBL-13741
          WHEN OTHERS.
            MESSAGE w001(zvnhsi) WITH me-&gt;av_pcode me-&gt;wsp0-partner_type INTO me-&gt;as_ifln-_msg-msgtx.
            RAISE EXCEPTION TYPE zcx_vnhsi MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4.
        ENDCASE.
      WHEN &apos;SALESMAN&apos;. MOVE: &apos;S&apos; TO me-&gt;av_ptype.  &quot;salesman
    ENDCASE.

    &quot;get config
    SELECT SINGLE * FROM zvnhsi_c020 INTO @me-&gt;cs_c020       WHERE pdoma = @me-&gt;av_domain  AND ptype = @me-&gt;av_ptype.
    SELECT SINGLE * FROM zvnhsi_c021 INTO @me-&gt;cs_c021       WHERE pdoma = @me-&gt;av_domain  AND ptype = @me-&gt;av_ptype.
    SELECT SINGLE * FROM zvnhsi_c022 INTO @me-&gt;cs_c022       WHERE pdoma = @me-&gt;av_domain  AND ptype = @me-&gt;av_ptype.
    SELECT SINGLE * FROM zvnhsi_c023 INTO @me-&gt;cs_c023       WHERE pdoma = @me-&gt;av_domain  AND ptype = @me-&gt;av_ptype.
    SELECT        * FROM zvnhsi_c024 INTO TABLE @me-&gt;ct_c024 WHERE pdoma = @me-&gt;av_domain  AND ptype = @me-&gt;av_ptype.



  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNHSI" CMPNAME="CONSTRUCTOR" VERSION="1" LANGU="E" EXPOSURE="2" STATE="1" EDITORDER="1 " DISPID="0 " MTDTYPE="2" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNHSI" CMPNAME="CONSTRUCTOR" SCONAME="IV_DOMAIN" VERSION="1" LANGU="E" DESCRIPT="HSI: HomeSIS Domain of Partner (PARTNER/SALESMAN)" CMPTYPE="1" MTDTYPE="2" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="ZVNHSI_DOMAIN"/>
   <parameter CLSNAME="ZCL_VNHSI" CMPNAME="CONSTRUCTOR" SCONAME="IV_CODE" VERSION="1" LANGU="E" DESCRIPT="HSI: HomeSIS Code of Partner/SalesMan" CMPTYPE="1" MTDTYPE="2" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="ZVNHSI_CODE"/>
