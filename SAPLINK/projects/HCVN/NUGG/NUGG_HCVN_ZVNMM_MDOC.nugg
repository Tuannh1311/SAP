  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_GREV" CMPNAME="SALV_INIT_CCOL" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: colours per columns" EXPOSURE="0" STATE="1" EDITORDER="12 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_ccol.

    &quot;init: column colours

    DATA: lr_col   TYPE REF TO cl_salv_column_list, &quot; object of column, allowing set_color
          ls_color TYPE        lvc_s_colo.          &quot; colours structure

    DATA(lr_cols) = me-&gt;ar_salv-&gt;get_columns( ).
    DATA(lt_cols) = lr_cols-&gt;get( ).

    LOOP AT lt_cols ASSIGNING FIELD-SYMBOL(&lt;col&gt;).
      TRY.
          lr_col ?= lr_cols-&gt;get_column( &lt;col&gt;-columnname ).
        CATCH cx_salv_not_found.
      ENDTRY.

      DATA(ls_c101) = VALUE lvc_s_colo( col = 1 inv = 0 int = 1 ).  &quot;blue+h
      DATA(ls_c100) = VALUE lvc_s_colo( col = 1 inv = 0 int = 0 ).  &quot;blue
      DATA(ls_c300) = VALUE lvc_s_colo( col = 3 inv = 0 int = 0 ).  &quot;yellow
      DATA(ls_c700) = VALUE lvc_s_colo( col = 7 inv = 0 int = 0 ).  &quot;orange
      DATA(ls_c500) = VALUE lvc_s_colo( col = 5 inv = 0 int = 0 ).  &quot;green
      DATA(ls_c501) = VALUE lvc_s_colo( col = 5 inv = 0 int = 1 ).  &quot;green+h

      &quot;setting colours
      &quot;..keys
      IF &lt;col&gt;-columnname EQ &apos;MBLNR&apos; OR
         &lt;col&gt;-columnname EQ &apos;MJAHR&apos; OR
         &lt;col&gt;-columnname EQ &apos;ZEILE&apos;    .
        lr_col-&gt;set_color( ls_c101 ). &quot;keys : blue+h (comid is kinda-key)
      ENDIF.
      &quot;..grec
      IF &lt;col&gt;-columnname CS &apos;_GR&apos;.
        lr_col-&gt;set_color( ls_c100 ). &quot;grec : blue
      ENDIF.
      &quot;..pord
      IF &lt;col&gt;-columnname CS &apos;_PO&apos;.
        IF &lt;col&gt;-columnname EQ &apos;EBELN_PO&apos; OR
           &lt;col&gt;-columnname EQ &apos;EBELP_PO&apos;   .
          lr_col-&gt;set_color( ls_c501 ). &quot;pord-keys : green+h
        ELSE.
          lr_col-&gt;set_color( ls_c500 ). &quot;pord-data : green
        ENDIF.
      ENDIF.
      &quot;..invo
      IF &lt;col&gt;-columnname CS &apos;_IV&apos;.
        lr_col-&gt;set_color( ls_c300 ). &quot;invo : yellow
      ENDIF.
      &quot;..diff
      IF &lt;col&gt;-columnname CS &apos;_DF&apos;.
        lr_col-&gt;set_color( ls_c700 ). &quot;diff : orange
      ENDIF.


    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_GREV" CMPNAME="SALV_INIT_COLS" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: columns types (exc, icon, chkbox)" EXPOSURE="0" STATE="1" EDITORDER="9 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_cols.

    &quot;init columns type: exception, icon, checkbox, etc

    TRY.

        &quot;init cols: F1 rollname
        DATA(lr_cols) = me-&gt;ar_salv-&gt;get_columns( ).
        DATA(lt_cols) = lr_cols-&gt;get( ).
        LOOP AT lt_cols ASSIGNING FIELD-SYMBOL(&lt;cols&gt;).
          &quot;update DDIC ref together with rollname
          &quot;(this will get normal F1 help pointing to our structure)
          DATA(ddic) = &lt;cols&gt;-r_column-&gt;get_ddic_reference( ).
          ddic-table = &apos;ZVNMM_MDOC_GREV_SDAT&apos;.
          &lt;cols&gt;-r_column-&gt;set_ddic_reference( value = ddic ).
          &lt;cols&gt;-r_column-&gt;set_f1_rollname( value = &apos;&apos; ).
        ENDLOOP.


        &quot;init cols: checkbox
        &quot;CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;XXXXX&apos;) )-&gt;set_cell_type( if_salv_c_cell_type=&gt;checkbox ).

        &quot;init cols: centered
        &quot;CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;set_exception_column( value = &apos;XXXXX&apos; ) ).

        &quot;init cols: lights
        &quot;CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;XXXXX&apos;) )-&gt;set_cell_type( if_salv_c_cell_type=&gt;checkbox ).

        &quot;init cols: icon
        &quot;CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;XXXXX &apos;) )-&gt;set_icon( &apos;X&apos; ).


        &quot;init cols: currency
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;WRBTR_IV&apos;) )-&gt;set_currency_column( &apos;WAERS_IV&apos; ).

        &quot;init cols: quantity
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;BPMNG_GR&apos;) )-&gt;set_quantity_column( &apos;BPRME_GR&apos; ).
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;MENGE_IV&apos;) )-&gt;set_quantity_column( &apos;BSTME_IV&apos; ).
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;BPMNG_DF&apos;) )-&gt;set_quantity_column( &apos;BPRME_GR&apos; ).

        &quot;init cols: decimals
        &quot;CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;CPAMT&apos;) )-&gt;set_decimals( value = &apos;0&apos; ).


      CATCH cx_salv_data_error.
      CATCH cx_salv_not_found.
    ENDTRY.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_GREV" CMPNAME="SALV_INIT_FUNC" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: functions" EXPOSURE="0" STATE="1" EDITORDER="10 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_func.

    &quot;salv all functions
    me-&gt;ar_salv-&gt;get_functions( )-&gt;set_all( abap_true ).

    &quot; layout save &amp; default (&apos;all functions&apos; above are not really all)
    DATA: lr_layout TYPE REF TO cl_salv_layout,
          ls_key    TYPE salv_s_layout_key.

    ls_key-report = me-&gt;lc_repid.
    lr_layout     = me-&gt;ar_salv-&gt;get_layout( ).

    lr_layout-&gt;set_key( ls_key ).         &quot;
    lr_layout-&gt;set_save_restriction( cl_salv_layout=&gt;restrict_none ). &quot;User can save
    lr_layout-&gt;set_default( abap_true ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_GREV" CMPNAME="SALV_INIT_HNDL" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: event handlers" EXPOSURE="0" STATE="1" EDITORDER="15 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_hndl.

    &quot;set handler
