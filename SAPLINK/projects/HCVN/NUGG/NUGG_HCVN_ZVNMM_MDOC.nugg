  <method CLSNAME="ZCL_VNMM_MDOC_GREV" CMPNAME="ON_DBCL" VERSION="1" LANGU="E" DESCRIPT="handler: on double-click" EXPOSURE="0" STATE="1" EDITORDER="19 " DISPID="0 " MTDTYPE="1" MTDDECLTYP="0" REFCLSNAME="CL_SALV_EVENTS_TABLE" REFCMPNAME="DOUBLE_CLICK" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNMM_MDOC_GREV" CMPNAME="ON_DBCL" SCONAME="ROW" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="1" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="0"/>
   <parameter CLSNAME="ZCL_VNMM_MDOC_GREV" CMPNAME="ON_DBCL" SCONAME="COLUMN" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="1" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="0"/>
   <source>METHOD on_dbcl.

    &quot;get double-clicked row
    READ TABLE me-&gt;at_sdat REFERENCE INTO DATA(lr_sdat) INDEX row.

    &quot;by column
    IF column CS &apos;_GR&apos;   OR
       column EQ &apos;MBLNR&apos; OR
       column EQ &apos;MJAHR&apos; OR
       column EQ &apos;ZEILE&apos;   .
      me-&gt;call_disp_mdoc( lr_sdat ).
    ELSEIF column CS &apos;_PO&apos;.
      me-&gt;call_disp_pord( lr_sdat ).
    ELSEIF column CS &apos;_DF&apos;.
      me-&gt;call_disp_mdoc2( lr_sdat ).
    ENDIF.



  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_GREV" CMPNAME="ON_UCOM" VERSION="1" LANGU="E" DESCRIPT="handler: on user-command" EXPOSURE="0" STATE="1" EDITORDER="20 " DISPID="0 " MTDTYPE="1" MTDDECLTYP="0" REFCLSNAME="CL_SALV_EVENTS" REFCMPNAME="ADDED_FUNCTION" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNMM_MDOC_GREV" CMPNAME="ON_UCOM" SCONAME="E_SALV_FUNCTION" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="1" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="0"/>
   <source>METHOD on_ucom.

    &quot;default: get selected rows
    me-&gt;at_rsel = me-&gt;ar_salv-&gt;get_selections( )-&gt;get_selected_rows( ).


    CASE e_salv_function.

      WHEN &apos;REFRESH&apos;.     me-&gt;salv_refr( ).

      WHEN &apos;REVERSE&apos;.     me-&gt;call_reverse( ).

    ENDCASE.

  ENDMETHOD.</source>
  </method>
