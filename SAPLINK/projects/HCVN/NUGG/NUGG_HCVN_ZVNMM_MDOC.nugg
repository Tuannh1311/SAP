  <method CLSNAME="ZCL_VNMM_MDOC_GREV" CMPNAME="CALL_DISP_MDOC" VERSION="1" LANGU="E" DESCRIPT="call: MIGO Goods Receipt display" EXPOSURE="0" STATE="1" EDITORDER="22 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
