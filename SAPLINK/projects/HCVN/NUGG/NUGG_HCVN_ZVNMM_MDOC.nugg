  <method CLSNAME="ZCL_VNMM_MDOC_RUPL" CMPNAME="SALV_INIT" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise" EXPOSURE="0" STATE="1" EDITORDER="8 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init.

    &quot; create SAVL table object
    TRY.
        cl_salv_table=&gt;factory( IMPORTING r_salv_table = me-&gt;ar_salv
                                CHANGING  t_table      = me-&gt;at_sdat ).
      CATCH cx_salv_msg.
    ENDTRY.


    me-&gt;salv_init_mlts( ).  &quot;set multi-sel
    me-&gt;salv_init_cols( ).  &quot;set columns
    me-&gt;salv_init_func( ).  &quot;set all grid-functions
    me-&gt;salv_init_pfst( ).  &quot;set own gui-status
    me-&gt;salv_init_ccol( ).  &quot;set colours per column
    me-&gt;salv_init_ccel( ).  &quot;set colours per cell(row)
    me-&gt;salv_init_invs( ).  &quot;set column invisible
    me-&gt;salv_init_hndl( ).  &quot;set event handlers





  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_RUPL" CMPNAME="SALV_INIT_CCEL" VERSION="1" LANGU="E" DESCRIPT="***salv-object: initialise: colours per cell (row)" EXPOSURE="0" STATE="1" EDITORDER="14 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_ccel.

***    &quot;set colours per cell/row
***
***    &quot;get field list (from columns)
***    DATA(lo_cols) =  me-&gt;ar_salv-&gt;get_columns( ).
***    DATA(lt_cols) = lo_cols-&gt;get( ).
***
***    &quot;set column SCOL
***    TRY. lo_cols-&gt;set_color_column( value = &apos;TSCOL&apos; ). CATCH cx_salv_data_error. ENDTRY.
***
***    &quot;for each line..
***    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).
***
***      CLEAR: &lt;sdat&gt;-tscol[].
***
***      &quot;..prepare dynamic colour per XXXX
***      DATA: ls_scol TYPE lvc_s_colo.
***      CASE &lt;sdat&gt;-msgty.
***        WHEN &apos;E&apos;. ls_scol-col = 7. &quot;orange
***      ENDCASE.
***
***      &quot;..for message column
***      LOOP AT lt_cols ASSIGNING FIELD-SYMBOL(&lt;cols&gt;) WHERE columnname = &apos;MSGTY&apos;
***                                                        OR columnname = &apos;MSGTX&apos;.
***        APPEND INITIAL LINE TO &lt;sdat&gt;-tscol ASSIGNING FIELD-SYMBOL(&lt;tscol&gt;).
***        MOVE: &lt;cols&gt;-columnname TO &lt;tscol&gt;-fname,
***              ls_scol           TO &lt;tscol&gt;-color,
***              1                 TO &lt;tscol&gt;-color-int.
***
***      ENDLOOP.
***
***
***    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_RUPL" CMPNAME="SALV_INIT_CCOL" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: colours per columns" EXPOSURE="0" STATE="1" EDITORDER="13 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_ccol.

    &quot;init: column colours

    DATA: lr_col   TYPE REF TO cl_salv_column_list, &quot; object of column, allowing set_color
          ls_color TYPE        lvc_s_colo.          &quot; colours structure

    DATA(lr_cols) = me-&gt;ar_salv-&gt;get_columns( ).
    DATA(lt_cols) = lr_cols-&gt;get( ).

    LOOP AT lt_cols ASSIGNING FIELD-SYMBOL(&lt;col&gt;).
      TRY.
          lr_col ?= lr_cols-&gt;get_column( &lt;col&gt;-columnname ).
        CATCH cx_salv_not_found.
      ENDTRY.

      DATA(ls_c101) = VALUE lvc_s_colo( col = 1 inv = 0 int = 1 ).  &quot;blue+h
      DATA(ls_c100) = VALUE lvc_s_colo( col = 1 inv = 0 int = 0 ).  &quot;blue
      DATA(ls_c300) = VALUE lvc_s_colo( col = 3 inv = 0 int = 0 ).  &quot;yellow
      DATA(ls_c700) = VALUE lvc_s_colo( col = 7 inv = 0 int = 0 ).  &quot;orange

      &quot;setting colours
      IF &lt;col&gt;-columnname EQ &apos;BANFN_PO&apos; OR
         &lt;col&gt;-columnname EQ &apos;BNFPO_PO&apos; OR
         &lt;col&gt;-columnname EQ &apos;EBELN_PO&apos; OR
         &lt;col&gt;-columnname EQ &apos;EBELP_PO&apos;   . lr_col-&gt;set_color( ls_c101 ). &quot;keys : blue+h
      ELSEIF &lt;col&gt;-columnname CS &apos;_PO&apos;  .
        lr_col-&gt;set_color( ls_c100 ). &quot;pord : blue
      ELSEIF &lt;col&gt;-columnname CS &apos;_GR&apos;  .
        lr_col-&gt;set_color( ls_c700 ). &quot;grec : orange
      ENDIF.

    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_RUPL" CMPNAME="SALV_INIT_COLS" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: columns types (exc, icon, chkbox)" EXPOSURE="0" STATE="1" EDITORDER="10 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_cols.

    &quot;init columns type: exception, icon, checkbox, etc

    TRY.

        &quot;init cols: F1 rollname
        DATA(lr_cols) = me-&gt;ar_salv-&gt;get_columns( ).
        DATA(lt_cols) = lr_cols-&gt;get( ).
        LOOP AT lt_cols ASSIGNING FIELD-SYMBOL(&lt;cols&gt;).
          &quot;update DDIC ref together with rollname
          &quot;(this will get normal F1 help pointing to our structure)
          DATA(ddic) = &lt;cols&gt;-r_column-&gt;get_ddic_reference( ).
          ddic-table = &apos;ZVNMM_MDOC_RUPL_SDAT&apos;.
          &lt;cols&gt;-r_column-&gt;set_ddic_reference( value = ddic ).
          &lt;cols&gt;-r_column-&gt;set_f1_rollname( value = &apos;&apos; ).
        ENDLOOP.


***        &quot;init cols: checkbox
***        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;XUPL_STATU&apos;) )-&gt;set_cell_type( if_salv_c_cell_type=&gt;checkbox ).

        &quot;init cols: centered
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;XUPL_STATU_IC&apos;) )-&gt;set_alignment( if_salv_c_alignment=&gt;centered ).
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;RETIC&apos;) )-&gt;set_alignment( if_salv_c_alignment=&gt;centered ).

        &quot;init cols: buttons
        &quot;shoot me, button is dumping.. why??
        &quot;CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;RET_B&apos;) )-&gt;set_cell_type( if_salv_c_cell_type=&gt;button ).

        &quot;init cols: no conv-output
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;BANFN&apos;) )-&gt;set_edit_mask( value = &apos;__________&apos; ). &quot;c10
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;BNFPO&apos;) )-&gt;set_edit_mask( value = &apos;_____&apos; ).      &quot;c5
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;EBELN&apos;) )-&gt;set_edit_mask( value = &apos;__________&apos; ). &quot;c10
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;EBELP&apos;) )-&gt;set_edit_mask( value = &apos;_____&apos; ).      &quot;c5

        &quot;uoms
*        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;MEINS&apos;) )-&gt;set_edit_mask( value = &apos;==CUNIT&apos; ) .
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;MENGE&apos;) )-&gt;set_quantity_column( value = &apos;MEINS&apos; ).


***        &quot;init cols: currency
***        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;PREIS&apos;) )-&gt;set_currency_column( &apos;WAERS&apos; ).

        &quot;init cols: decimals
**        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;CPAMT&apos;) )-&gt;set_decimals( value = &apos;0&apos; ).

        &quot;init cols: technical
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;XSEL&apos;) )-&gt;set_technical( value = &apos;X&apos;).


      CATCH cx_salv_data_error.
      CATCH cx_salv_not_found.
    ENDTRY.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_RUPL" CMPNAME="SALV_INIT_FUNC" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: functions" EXPOSURE="0" STATE="1" EDITORDER="11 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_func.

    &quot;salv all functions
    me-&gt;ar_salv-&gt;get_functions( )-&gt;set_all( abap_true ).

    &quot; layout save &amp; default (&apos;all functions&apos; above are not really all)
    DATA: lr_layout TYPE REF TO cl_salv_layout,
          ls_key    TYPE        salv_s_layout_key.

    ls_key-report = me-&gt;lc_repid.
    lr_layout     = me-&gt;ar_salv-&gt;get_layout( ).

    lr_layout-&gt;set_key( ls_key ).         &quot;
    lr_layout-&gt;set_save_restriction( cl_salv_layout=&gt;restrict_none ). &quot;User can save
    lr_layout-&gt;set_default( abap_true ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_RUPL" CMPNAME="SALV_INIT_HNDL" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: event handlers" EXPOSURE="0" STATE="1" EDITORDER="16 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_hndl.

    &quot;set handler
    SET HANDLER me-&gt;on_dbcl FOR me-&gt;ar_salv-&gt;get_event( ).
    SET HANDLER me-&gt;on_ucom FOR me-&gt;ar_salv-&gt;get_event( ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_RUPL" CMPNAME="SALV_INIT_INVS" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: invisible fields" EXPOSURE="0" STATE="1" EDITORDER="15 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_invs.

    &quot;set invisible fields

    DATA: lr_col   TYPE REF TO cl_salv_column_list. &quot; object of column, allowing set_color

    DATA(lr_cols) = me-&gt;ar_salv-&gt;get_columns( ).
    DATA(lt_cols) = lr_cols-&gt;get( ).

    &quot; remove MANDT
    LOOP AT lt_cols ASSIGNING FIELD-SYMBOL(&lt;col&gt;) WHERE columnname = &apos;MANDT&apos;
                                                     OR columnname = &apos;XSEL&apos;.
      TRY.
          lr_col ?= lr_cols-&gt;get_column( &lt;col&gt;-columnname ).
        CATCH cx_salv_not_found.
      ENDTRY.
      lr_col-&gt;set_technical( if_salv_c_bool_sap=&gt;true ).
    ENDLOOP.



  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_RUPL" CMPNAME="SALV_INIT_MLTS" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: mutli-selection" EXPOSURE="0" STATE="1" EDITORDER="9 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_mlts.

    &quot;set multiple-selection
    DATA(lr_sel) = me-&gt;ar_salv-&gt;get_selections( ).
    lr_sel-&gt;set_selection_mode( if_salv_c_selection_mode=&gt;row_column ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_RUPL" CMPNAME="SALV_INIT_PFST" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: gui status" EXPOSURE="0" STATE="1" EDITORDER="12 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_pfst.

    &quot;init: gui status from calling program

    &quot;set own function by own screen status (seriously, I hate SALV)
    me-&gt;ar_salv-&gt;set_screen_status(
                                    pfstatus      = me-&gt;lc_pfkey
                                    report        = me-&gt;lc_repid
                                    set_functions = me-&gt;ar_salv-&gt;c_functions_all ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_RUPL" CMPNAME="SALV_REFR" VERSION="1" LANGU="E" DESCRIPT="salv-object: refresh" EXPOSURE="0" STATE="1" EDITORDER="18 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_refr.

    &quot;refresh SALV

    &quot;..re-read data

    me-&gt;sdat_keys( ). &quot;keys select
    me-&gt;sdat_data( ). &quot;data select
    me-&gt;sdat_auth( ). &quot;data authorisation check
    me-&gt;sdat_enha( ). &quot;data enhance

    me-&gt;salv_init_ccel( ). &quot;re-colour cells (per row-line)

    &quot;..refresh salv
    me-&gt;ar_salv-&gt;refresh( ).






  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_RUPL" CMPNAME="SDAT_AUTH" VERSION="1" LANGU="E" DESCRIPT="salv-data: check authorisation" EXPOSURE="0" STATE="1" EDITORDER="3 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_auth.

    &quot;authorithy check (pord display)
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      AUTHORITY-CHECK OBJECT &apos;M_BANF_BSA&apos; ID &apos;ACTVT&apos; FIELD &apos;03&apos;
                                          ID &apos;BSART&apos; FIELD &lt;sdat&gt;-_pord-bsart.
      IF sy-subrc &lt;&gt; 0. CLEAR: &lt;sdat&gt;. ENDIF.

      AUTHORITY-CHECK OBJECT &apos;M_BEST_EKO&apos; ID &apos;ACTVT&apos; FIELD &apos;03&apos;
                                          ID &apos;EKORG&apos; FIELD &lt;sdat&gt;-_pord-ekorg.
      IF sy-subrc &lt;&gt; 0. CLEAR: &lt;sdat&gt;. ENDIF.

      AUTHORITY-CHECK OBJECT &apos;M_BEST_EKG&apos; ID &apos;ACTVT&apos; FIELD &apos;03&apos;
                                          ID &apos;EKGRP&apos; FIELD &lt;sdat&gt;-_pord-ekgrp.
      IF sy-subrc &lt;&gt; 0. CLEAR: &lt;sdat&gt;. ENDIF.

      AUTHORITY-CHECK OBJECT &apos;M_BEST_WRK&apos; ID &apos;ACTVT&apos; FIELD &apos;03&apos;
                                          ID &apos;WERKS&apos; FIELD &lt;sdat&gt;-_pord-werks.
      IF sy-subrc &lt;&gt; 0. CLEAR: &lt;sdat&gt;. ENDIF.

    ENDLOOP.
    DELETE me-&gt;at_sdat WHERE _pord-ebeln IS INITIAL.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_RUPL" CMPNAME="SDAT_DATA" VERSION="1" LANGU="E" DESCRIPT="salv-data: select data per keys (or item, from file)" EXPOSURE="0" STATE="1" EDITORDER="2 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_data.
    &quot;salv-data: select data per keys (or item, from file)

    CHECK: me-&gt;at_sdat[] IS NOT INITIAL.

    &quot;get data
    &quot;select..
    &quot;..pord-head
    SELECT * FROM ekko    FOR ALL ENTRIES IN @me-&gt;at_keys WHERE ebeln = @me-&gt;at_keys-ebeln INTO TABLE @DATA(lt_ekko).
    &quot;..pord-item
    SELECT * FROM ekpo    FOR ALL ENTRIES IN @me-&gt;at_keys WHERE ebeln = @me-&gt;at_keys-ebeln
                                                            AND ebelp = @me-&gt;at_keys-ebelp INTO TABLE @DATA(lt_ekpo).
    &quot;..pord-accn
    SELECT * FROM ekkn    FOR ALL ENTRIES IN @me-&gt;at_keys WHERE ebeln = @me-&gt;at_keys-ebeln
                                                            AND ebelp = @me-&gt;at_keys-ebelp
                                                            AND zekkn = &apos;01&apos;               INTO TABLE @DATA(lt_ekkn).
    &quot;..pord-sche
    SELECT * FROM eket    FOR ALL ENTRIES IN @me-&gt;at_keys WHERE ebeln = @me-&gt;at_keys-ebeln
                                                            AND ebelp = @me-&gt;at_keys-ebelp
                                                            AND etenr = &apos;0001&apos;             INTO TABLE @DATA(lt_eket).

    &quot;map together..
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      &quot;..pord-head
      READ TABLE lt_ekko ASSIGNING FIELD-SYMBOL(&lt;ekko&gt;) WITH KEY ebeln = &lt;sdat&gt;-_pord-ebeln.
      IF sy-subrc = 0.
        MOVE: &lt;ekko&gt;-lifnr TO &lt;sdat&gt;-_pord-lifnr,
              &lt;ekko&gt;-bsart TO &lt;sdat&gt;-_pord-bsart,
              &lt;ekko&gt;-ekorg TO &lt;sdat&gt;-_pord-ekorg,
              &lt;ekko&gt;-ekgrp TO &lt;sdat&gt;-_pord-ekgrp.
      ENDIF.

      &quot;..pord-item
      READ TABLE lt_ekpo ASSIGNING FIELD-SYMBOL(&lt;ekpo&gt;) WITH KEY ebeln = &lt;sdat&gt;-_pord-ebeln
                                                                 ebelp = &lt;sdat&gt;-_pord-ebelp.
      IF sy-subrc = 0.
        MOVE-CORRESPONDING: &lt;ekpo&gt; TO &lt;sdat&gt;-_pord.
      ENDIF.

      &quot;..pord-accn
      READ TABLE lt_ekkn ASSIGNING FIELD-SYMBOL(&lt;ekkn&gt;) WITH KEY ebeln = &lt;sdat&gt;-_pord-ebeln
                                                                 ebelp = &lt;sdat&gt;-_pord-ebelp.
      IF sy-subrc = 0.
        MOVE: &lt;ekkn&gt;-kostl TO &lt;sdat&gt;-_pord-kostl,
              &lt;ekkn&gt;-aufnr TO &lt;sdat&gt;-_pord-aufnr.
      ENDIF.

      &quot;..pord-sche
      READ TABLE lt_eket ASSIGNING FIELD-SYMBOL(&lt;eket&gt;) WITH KEY ebeln = &lt;sdat&gt;-_pord-ebeln
                                                                 ebelp = &lt;sdat&gt;-_pord-ebelp.
      IF sy-subrc = 0.
        MOVE: &lt;eket&gt;-eindt TO &lt;sdat&gt;-_pord-eindt.
      ENDIF.

    ENDLOOP.

    &quot;UoM from POrd to GRec
    LOOP AT me-&gt;at_sdat ASSIGNING &lt;sdat&gt;.
      MOVE: &lt;sdat&gt;-_pord-meins TO &lt;sdat&gt;-_grec-meins.
    ENDLOOP.


    &quot;default status (items from preq, template)
    IF me-&gt;ax_file IS INITIAL.
      LOOP AT me-&gt;at_sdat ASSIGNING &lt;sdat&gt;.
        MOVE: &apos;T&apos; TO &lt;sdat&gt;-xupl_statu.
      ENDLOOP.
    ENDIF.


    &quot;default sort (I never know whether this is good idea)
    SORT me-&gt;at_sdat BY _pord-ebeln DESCENDING
                        _pord-ebelp ASCENDING.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_RUPL" CMPNAME="SDAT_ENHA" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance" EXPOSURE="0" STATE="1" EDITORDER="4 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha.

    &quot;enhance salv-data

    me-&gt;sdat_enha_statu_tx( ).
    me-&gt;sdat_enha_statu_ic( ).
    me-&gt;sdat_enha_field_tx( ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_RUPL" CMPNAME="SDAT_ENHA_FIELD_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: various descriptions" EXPOSURE="0" STATE="1" EDITORDER="7 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_field_tx.

    CHECK: me-&gt;at_sdat IS NOT INITIAL.

    &quot;ekgrp_tx: purch-grp description
    SELECT * FROM t024 INTO TABLE @DATA(lt_t024) FOR ALL ENTRIES IN @me-&gt;at_sdat WHERE ekgrp = @me-&gt;at_sdat-_pord-ekgrp.

    &quot;afnam_tx: requestor name
    &quot;(not here, as FAE needs same data types (pernr-vs-afnam) )

    &quot;kostl_tx: cost centre description
    SELECT * FROM cskt INTO TABLE @DATA(lt_cskt) FOR ALL ENTRIES IN @me-&gt;at_sdat WHERE spras =  @sy-langu
                                                                                   AND kostl =  @me-&gt;at_sdat-_pord-kostl
                                                                                   AND datbi &gt;= @sy-datum.

    &quot;aufnr_tx: order description
    SELECT * FROM aufk INTO TABLE @DATA(lt_aufk) FOR ALL ENTRIES IN @me-&gt;at_sdat WHERE aufnr = @me-&gt;at_sdat-_pord-aufnr.

    &quot;lifnr_tx+flief_tx: vendor name
    SELECT * FROM lfa1 INTO TABLE @DATA(lt_lfa1) FOR ALL ENTRIES IN @me-&gt;at_sdat WHERE lifnr = @me-&gt;at_sdat-_pord-lifnr.

    &quot;map together
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      &quot;ekgrp_tx: purch-grp description
      READ TABLE lt_t024 ASSIGNING FIELD-SYMBOL(&lt;t024&gt;) WITH KEY ekgrp = &lt;sdat&gt;-_pord-ekgrp.
      &quot;IF sy-subrc = 0. MOVE: &lt;t024&gt;-eknam TO &lt;sdat&gt;-ekgrp_tx. ENDIF.

      &quot;afnam_tx: requestor name
      &quot;(single select, as FAE needs same data types (pernr-vs-afnam) )
      SELECT SINGLE ename FROM pa0001 INTO @&lt;sdat&gt;-_pord-afnam_tx WHERE pernr = @&lt;sdat&gt;-_pord-afnam
                                                              AND begda &lt;= @sy-datum
                                                              AND endda &gt;= @sy-datum.

      &quot;kostl_tx: cost centre description
      READ TABLE lt_cskt ASSIGNING FIELD-SYMBOL(&lt;cskt&gt;) WITH KEY kostl = &lt;sdat&gt;-_pord-kostl.
      IF sy-subrc = 0. MOVE: &lt;cskt&gt;-ltext TO &lt;sdat&gt;-_pord-kostl_tx. ENDIF.

      &quot;aufnr_tx: order description
      READ TABLE lt_aufk ASSIGNING FIELD-SYMBOL(&lt;aufk&gt;) WITH KEY aufnr = &lt;sdat&gt;-_pord-aufnr.
      IF sy-subrc = 0. MOVE: &lt;aufk&gt;-ktext TO &lt;sdat&gt;-_pord-aufnr_tx. ENDIF.

      &quot;lifnr_tx: vendor name
      READ TABLE lt_lfa1 ASSIGNING FIELD-SYMBOL(&lt;lfa1&gt;) WITH KEY lifnr = &lt;sdat&gt;-_pord-lifnr.
      IF sy-subrc = 0. MOVE: &lt;lfa1&gt;-name1 TO &lt;sdat&gt;-_pord-lifnr_tx. ENDIF.


    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_RUPL" CMPNAME="SDAT_ENHA_STATU_IC" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: status icon" EXPOSURE="0" STATE="1" EDITORDER="6 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_statu_ic.

    &quot;enha Status icon
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      CASE &lt;sdat&gt;-xupl_statu.
        WHEN &apos;T&apos;.  &quot;T= template (existing item)
          MOVE: icon_led_inactive TO &lt;sdat&gt;-xupl_statu_ic.

        WHEN &apos;L&apos;. &quot;L= loaded from file
          MOVE: icon_led_yellow   TO &lt;sdat&gt;-xupl_statu_ic.

        WHEN &apos;S&apos;. &quot;S= tested successfully
          MOVE: icon_checked      TO &lt;sdat&gt;-xupl_statu_ic.

        WHEN &apos;P&apos;. &quot;P= posted to Goods Receipt
          MOVE: icon_led_green    TO &lt;sdat&gt;-xupl_statu_ic.

        WHEN &apos;E&apos;.
          MOVE: icon_led_red      TO &lt;sdat&gt;-xupl_statu_ic.

      ENDCASE.

      &quot;icon messages
      IF &lt;sdat&gt;-ret_t[] IS INITIAL.
        CLEAR: &lt;sdat&gt;-retic.
      ELSEIF &lt;sdat&gt;-ret_t[] IS NOT INITIAL.
        MOVE: icon_history TO &lt;sdat&gt;-retic.
      ENDIF.


    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_RUPL" CMPNAME="SDAT_ENHA_STATU_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: status text" EXPOSURE="0" STATE="1" EDITORDER="5 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_statu_tx.

    &quot;enha Status text
    DATA(lo_dref) = CAST cl_abap_elemdescr( cl_abap_elemdescr=&gt;describe_by_name( p_name = &apos;ZVNMM_MDOC_RUPL_STATU&apos; ) ).
    DATA(lt_vals) = lo_dref-&gt;get_ddic_fixed_values( ).

    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      READ TABLE lt_vals ASSIGNING FIELD-SYMBOL(&lt;vals&gt;) WITH KEY low = &lt;sdat&gt;-xupl_statu. CHECK sy-subrc = 0.

      MOVE: &lt;vals&gt;-ddtext TO &lt;sdat&gt;-xupl_statu_tx.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_RUPL" CMPNAME="SDAT_KEYS" VERSION="1" LANGU="E" DESCRIPT="salv-data: select keys" EXPOSURE="0" STATE="1" EDITORDER="1 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_keys.
    &quot;salv-data: select keys

    &quot;keys: either init from sel-screen, or upd from file
    IF me-&gt;ax_file = &apos;X&apos;.
      CLEAR: me-&gt;at_keys.
      LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;) WHERE _pord-ebeln IS NOT INITIAL
                                                           AND _pord-ebelp IS NOT INITIAL.
        APPEND INITIAL LINE TO me-&gt;at_keys ASSIGNING FIELD-SYMBOL(&lt;key&gt;).
        MOVE: &lt;sdat&gt;-_pord-ebeln TO &lt;key&gt;-ebeln,
              &lt;sdat&gt;-_pord-ebelp TO &lt;key&gt;-ebelp.
      ENDLOOP.

      &quot;actually, no selections after upload
      &quot;(we are working with uploaded data now)
      RETURN.
    ELSE.
      CLEAR: me-&gt;at_sdat.
    ENDIF.

    &quot;pass keys
    LOOP AT me-&gt;at_keys ASSIGNING FIELD-SYMBOL(&lt;keys&gt;).
      APPEND INITIAL LINE TO me-&gt;at_sdat ASSIGNING &lt;sdat&gt;.
      MOVE: &lt;keys&gt;-ebeln TO &lt;sdat&gt;-_pord-ebeln,
            &lt;keys&gt;-ebelp TO &lt;sdat&gt;-_pord-ebelp.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
