  <method CLSNAME="ZCL_VNMM_MDOC_GREV" CMPNAME="SALV_AUTO" VERSION="1" LANGU="E" DESCRIPT="salv-object: background posting" EXPOSURE="0" STATE="1" EDITORDER="16 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_auto.
    &quot;salv-object: background posting

    CHECK me-&gt;ax_auto = &apos;X&apos;.
    &quot;auto posting: mark all and post
    DATA: lt_rows TYPE salv_t_row.

    &quot;select all rows
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).
      APPEND INITIAL LINE TO lt_rows ASSIGNING FIELD-SYMBOL(&lt;row&gt;).
      MOVE: sy-tabix TO &lt;row&gt;.
    ENDLOOP.
    me-&gt;ar_salv-&gt;get_selections( )-&gt;set_selected_rows( lt_rows ).

    &quot;call reverse
    me-&gt;call_reverse( ).



  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_GREV" CMPNAME="SALV_DISP" VERSION="1" LANGU="E" DESCRIPT="salv-object: display" EXPOSURE="0" STATE="1" EDITORDER="17 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_disp.

**    &quot; optimise columns
**    me-&gt;ar_salv-&gt;get_columns( )-&gt;set_optimize( abap_true ).

    &quot;sort
    SORT me-&gt;at_sdat BY mjahr ASCENDING
                        mblnr ASCENDING
                        zeile ASCENDING.


    &quot; and finally display SALV table as full screen
    me-&gt;ar_salv-&gt;display( ).


  ENDMETHOD.</source>
