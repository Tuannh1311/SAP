SELECTION-SCREEN: BEGIN OF BLOCK s00 WITH FRAME TITLE TEXT-s00. &quot;block: processing
  PARAMETERS: px_read TYPE xfeld  DEFAULT &apos;X&apos; RADIOBUTTON GROUP r1,
              px_xlsx TYPE xfeld              RADIOBUTTON GROUP r1,
              px_down TYPE xfeld              RADIOBUTTON GROUP r1.

SELECTION-SCREEN: END   OF BLOCK s00.




********************************************************************** &quot;
*&amp; data selection


**********************************************************************
*** file open
DATA: rc      TYPE sy-subrc,
      lt_file TYPE filetable.
cl_gui_frontend_services=&gt;file_open_dialog(
*    EXPORTING
*      window_title            = window_title      &quot; Title Of File Open Dialog
*      default_extension       = default_extension &quot; Default Extension
*      default_filename        = default_filename  &quot; Default File Name
*      file_filter             = file_filter       &quot; File Extension Filter String
*      with_encoding           = with_encoding     &quot; File Encoding
*      initial_directory       = initial_directory &quot; Initial Directory
*      multiselection          = multiselection    &quot; Multiple selections poss.
  CHANGING
    file_table              = lt_file       &quot; Table Holding Selected Files
    rc                      = rc                &quot; Return Code, Number of Files or -1 If Error Occurred
*      user_action             = user_action       &quot; User Action (See Class Constants ACTION_OK, ACTION_CANCEL)
*      file_encoding           = file_encoding
  EXCEPTIONS
    file_open_dialog_failed = 1                 &quot; &quot;Open File&quot; dialog failed
    cntl_error              = 2                 &quot; Control error
    error_no_gui            = 3                 &quot; No GUI available
    not_supported_by_gui    = 4                 &quot; GUI does not support this
    OTHERS                  = 5
).
IF sy-subrc &lt;&gt; 0. WRITE: &apos;error sy-subrc=&apos;, sy-subrc. RETURN. ENDIF.

READ TABLE lt_file ASSIGNING FIELD-SYMBOL(&lt;file&gt;) INDEX 1.
WRITE: / &apos;file= &apos;, &lt;file&gt;.

IF px_read = &apos;X&apos;.

**********************************************************************
*** file read
  TYPES: dta_type_bin(240) TYPE x.
  DATA: &quot;lt_data TYPE TABLE OF string,
    lt_data TYPE TABLE OF dta_type_bin,
    lv_flen TYPE i.
  DATA: lv_strg TYPE xstring.

  cl_gui_frontend_services=&gt;gui_upload(
  EXPORTING
      filename                = CONV string( &lt;file&gt; )             &quot; Name of file
    filetype                = &apos;BIN&apos;              &quot; File Type (ASCII, Binary)
*    has_field_separator     = space              &quot; Columns Separated by Tabs in Case of ASCII Upload
*    header_length           = 0                  &quot; Length of Header for Binary Data
*    read_by_line            = &apos;X&apos;                &quot; File Written Line-By-Line to the Internal Table
*    dat_mode                = space              &quot; Numeric and date fields are in DAT format in WS_DOWNLOAD
*    codepage                = codepage           &quot; Character Representation for Output
*    ignore_cerr             = abap_true          &quot; Ignore character set conversion errors?
*    replacement             = &apos;#&apos;                &quot; Replacement Character for Non-Convertible Characters
*    virus_scan_profile      = virus_scan_profile &quot; Virus Scan Profile
  IMPORTING
    filelength              = lv_flen         &quot; File Length
    header                  = lv_strg            &quot; File Header in Case of Binary Upload
    CHANGING
      data_tab                = lt_data          &quot; Transfer table for file contents
*    isscanperformed         = space              &quot; File already scanned
    EXCEPTIONS
      file_open_error         = 1                  &quot; File does not exist and cannot be opened
      file_read_error         = 2                  &quot; Error when reading file
      no_batch                = 3                  &quot; Cannot execute front-end function in background
      gui_refuse_filetransfer = 4                  &quot; Incorrect front end or error on front end
      invalid_type            = 5                  &quot; Incorrect parameter FILETYPE
      no_authority            = 6                  &quot; No upload authorization
      unknown_error           = 7                  &quot; Unknown error
      bad_data_format         = 8                  &quot; Cannot Interpret Data in File
      header_not_allowed      = 9                  &quot; Invalid header
      separator_not_allowed   = 10                 &quot; Invalid separator
      header_too_long         = 11                 &quot; Header information currently restricted to 1023 bytes
      unknown_dp_error        = 12                 &quot; Error when calling data provider
      access_denied           = 13                 &quot; Access to file denied.
      dp_out_of_memory        = 14                 &quot; Not enough memory in data provider
      disk_full               = 15                 &quot; Storage medium is full.
      dp_timeout              = 16                 &quot; Data provider timeout
      not_supported_by_gui    = 17                 &quot; GUI does not support this
      error_no_gui            = 18                 &quot; GUI not available
      OTHERS                  = 19
  ).
  IF sy-subrc &lt;&gt; 0.
    WRITE: &apos;error sy-subrc=&apos;, sy-subrc.
  ENDIF.

***  IF lt_data[] IS NOT INITIAL.
***    WRITE: / &apos;file data=&apos;.
***    LOOP AT lt_data ASSIGNING FIELD-SYMBOL(&lt;data&gt;).
***      WRITE: / &lt;data&gt;.
***    ENDLOOP.
***  ENDIF.


**********************************************************************
*** excel convert string

  CALL FUNCTION &apos;SCMS_BINARY_TO_XSTRING&apos;
    EXPORTING
      input_length = lv_flen
    IMPORTING
      buffer       = lv_strg
    TABLES
      binary_tab   = lt_data
    EXCEPTIONS
      failed       = 1
      OTHERS       = 2.

**********************************************************************
*** excel class
  DATA : lo_excel_ref TYPE REF TO cl_fdt_xl_spreadsheet .
  TRY .
      lo_excel_ref = NEW cl_fdt_xl_spreadsheet(
                              document_name = CONV string( &lt;file&gt; )
                              xdocument     = lv_strg ) .
    CATCH cx_fdt_excel_core.
      &quot;Implement suitable error handling here
  ENDTRY .

  &quot;Get List of Worksheets
  lo_excel_ref-&gt;if_fdt_doc_spreadsheet~get_worksheet_names(
    IMPORTING
      worksheet_names = DATA(lt_worksheets) ).

  IF NOT lt_worksheets IS INITIAL.
    READ TABLE lt_worksheets INTO DATA(lv_woksheetname) INDEX 1.

    DATA(lo_data_ref) = lo_excel_ref-&gt;if_fdt_doc_spreadsheet~get_itab_from_worksheet(
                                             lv_woksheetname ).
    &quot;now you have excel work sheet data in dyanmic internal table
    FIELD-SYMBOLS: &lt;gt_data&gt; TYPE ANY TABLE.
    ASSIGN lo_data_ref-&gt;* TO &lt;gt_data&gt;.

    LOOP AT &lt;gt_data&gt; ASSIGNING FIELD-SYMBOL(&lt;data&gt;).
      DO 8 TIMES.
        ASSIGN COMPONENT sy-index OF STRUCTURE &lt;data&gt; TO FIELD-SYMBOL(&lt;cell&gt;).
        WRITE: / &lt;cell&gt;.

      ENDDO.
    ENDLOOP.

  ENDIF.

ENDIF.


**********************************************************************
*** load via
IF px_xlsx = &apos;X&apos;.

  DATA(xlsxhandler) = cl_ehfnd_xlsx=&gt;get_instance( ).
  DATA(xstring_excel) = cl_openxml_helper=&gt;load_local_file( CONV string( &lt;file&gt; ) ).
  DATA(xlsxdocument) = xlsxhandler-&gt;load_doc( iv_file_data = xstring_excel ).
  DATA(firstsheet) = xlsxdocument-&gt;get_sheet_by_id( iv_sheet_id = 1 ).
  &quot;IF_EHFND_XLSX_SHEET
  DATA(last_row) = firstsheet-&gt;get_last_row_number( ).
  DO last_row TIMES.
    DATA(lv_rowix) = sy-index.
    DO 8 TIMES.
      DATA(lv_colix) = sy-index.
      DATA(lv_content) = firstsheet-&gt;get_cell_content( EXPORTING iv_row     = lv_rowix
                                                                 iv_column  = lv_colix      ).
      WRITE: / &apos;rowix= &apos;, lv_rowix, &apos; colix= &apos;, lv_colix, &apos; content= &apos;, lv_content.
    ENDDO.
  ENDDO.

ENDIF.

**********************************************************************
*** down
IF px_down = &apos;X&apos;.

  xlsxhandler = cl_ehfnd_xlsx=&gt;get_instance( ).
  xlsxdocument = xlsxhandler-&gt;create_doc( ).
  DATA(lt_sheet) = xlsxdocument-&gt;get_sheets( ).
  READ TABLE lt_sheet INDEX 1 ASSIGNING FIELD-SYMBOL(&lt;sheet&gt;).
*  DATA(newsheet) = xlsxdocument-&gt;add_new_sheet( iv_sheet_name = &apos;exportsheet&apos; ).
  DATA(onesheet) = xlsxdocument-&gt;get_sheet_by_id( iv_sheet_id = &lt;sheet&gt;-sheet_id ).
  DO 4 TIMES.
    DATA(lv_nroix) = sy-index.
    DO 8 TIMES.
      DATA(lv_newix) = sy-index.
*      newsheet-&gt;set_cell_content(
