*       I_NO_AUTH_CHECK     = I_NO_AUTH_CHECK
*       I_SKIP_FIRST_SCREEN = &apos;X&apos;
*       I_DEADEND           = &apos;X&apos;
*       I_OKCODE            = &apos;OK_GO&apos;
*       I_LEAVE_AFTER_POST  = I_LEAVE_AFTER_POST
*       I_NEW_ROLLAREA      = &apos;X&apos;
*       I_SYTCODE           = I_SYTCODE
*       I_EBELN             = I_EBELN
*       I_EBELP             = I_EBELP
        i_mblnr             = ir_sdat-&gt;*-mblnr
        i_mjahr             = ir_sdat-&gt;*-mjahr
        i_zeile             = ir_sdat-&gt;*-zeile
*       I_TRANSPORT         = I_TRANSPORT
*       I_ORDER_NUMBER      = I_ORDER_NUMBER
*       I_ORDER_ITEM        = I_ORDER_ITEM
*       I_TRANSPORT_MEANS   = I_TRANSPORT_MEANS
*       I_TRANSPORTIDENT    = I_TRANSPORTIDENT
*       I_INBOUND_DELIV     = I_INBOUND_DELIV
*       I_OUTBOUND_DELIV    = I_OUTBOUND_DELIV
*       I_RESERVATION_NUMB  = I_RESERVATION_NUMB
*       I_RESERVATION_ITEM  = I_RESERVATION_ITEM
*       EXT                 = EXT
*       I_MOVE_TYPE         = I_MOVE_TYPE
*       I_SPEC_STOCK        = I_SPEC_STOCK
*       I_PSTNG_DATE        = I_PSTNG_DATE
*       I_DOC_DATE          = I_DOC_DATE
*       I_REF_DOC_NO        = I_REF_DOC_NO
*       I_HEADER_TXT        = I_HEADER_TXT
      EXCEPTIONS
        illegal_combination = 1
        OTHERS              = 2.
    IF sy-subrc &lt;&gt; 0. MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4. ENDIF.



  ENDMETHOD.</source>
