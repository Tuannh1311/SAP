  <method CLSNAME="ZCL_VNMM_MDOC_GREV" CMPNAME="SALV_REFR" VERSION="1" LANGU="E" DESCRIPT="salv-object: refresh" EXPOSURE="0" STATE="1" EDITORDER="18 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_refr.

    &quot;refresh SALV

    &quot;..re-read data
    CLEAR: me-&gt;at_sdat.

    me-&gt;sdat_selc( ). &quot;data select
    me-&gt;sdat_auth( ). &quot;data authorisation check
    me-&gt;sdat_enha( ). &quot;data enhance

    me-&gt;salv_init_ccel( ). &quot;re-colour cells (per row-line)

    &quot;..refresh salv
    me-&gt;ar_salv-&gt;refresh( ).







  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_GREV" CMPNAME="SDAT_AUTH" VERSION="1" LANGU="E" DESCRIPT="salv-data: check authorisation" EXPOSURE="0" STATE="1" EDITORDER="2 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_auth.

    &quot;authorithy check
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      &quot;Material Documents: Plant
      AUTHORITY-CHECK OBJECT &apos;M_MSEG_WMB&apos;  ID &apos;ACTVT&apos; FIELD &apos;03&apos;
                                           ID &apos;WERKS&apos; FIELD &lt;sdat&gt;-_grec-werks.
      IF sy-subrc &lt;&gt; 0. CLEAR &lt;sdat&gt;. ENDIF.

      &quot;Material Documents: Movement Type
      AUTHORITY-CHECK OBJECT &apos;M_MSEG_BMB&apos; ID &apos;ACTVT&apos; FIELD &apos;03&apos;
                                          ID &apos;BWART&apos; FIELD &lt;sdat&gt;-_grec-bwart.
      IF sy-subrc &lt;&gt; 0. CLEAR &lt;sdat&gt;. ENDIF.

    ENDLOOP.
    DELETE me-&gt;at_sdat WHERE mblnr IS INITIAL. &quot;clear cleared


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_GREV" CMPNAME="SDAT_ENHA" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance" EXPOSURE="0" STATE="1" EDITORDER="3 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha.

    &quot;enhance salv-data
    me-&gt;sdat_enha_bsart_tx( ).
    me-&gt;sdat_enha_lifnr_tx( ).
    me-&gt;sdat_enha_grund_tx( ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_GREV" CMPNAME="SDAT_ENHA_BSART_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: document type" EXPOSURE="0" STATE="1" EDITORDER="4 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_bsart_tx.
    &quot;salv-data: enhance: document type

    SELECT * FROM t161t INTO TABLE @DATA(lt_t161t) WHERE spras = @sy-langu
                                                     AND bstyp = &apos;F&apos;.

    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).
      READ TABLE lt_t161t ASSIGNING FIELD-SYMBOL(&lt;t161t&gt;) WITH KEY bsart = &lt;sdat&gt;-bsart_po.
      IF sy-subrc = 0.
        MOVE: &lt;t161t&gt;-batxt TO &lt;sdat&gt;-bsart_tx_po.
      ENDIF.
    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_GREV" CMPNAME="SDAT_ENHA_GRUND_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: reason for movement" EXPOSURE="0" STATE="1" EDITORDER="6 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_grund_tx.
    &quot;salv-data: enhance: reason for movement

    CHECK me-&gt;at_sdat[] IS NOT INITIAL.

    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).
      SELECT SINGLE grtxt INTO @&lt;sdat&gt;-_grec-grund_tx FROM t157e WHERE spras = @sy-langu
                                                                   AND bwart = @&lt;sdat&gt;-_grec-bwart
                                                                   AND grund = @&lt;sdat&gt;-_grec-grund.
    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_GREV" CMPNAME="SDAT_ENHA_LIFNR_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: vendor name" EXPOSURE="0" STATE="1" EDITORDER="5 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_lifnr_tx.

    &quot;enha Vendor&apos;s descriptions
    CHECK me-&gt;at_sdat[] IS NOT INITIAL.

    SELECT lfa1~lifnr,
           lfa1~adrnr,
           adrc~addrnumber,
           adrc~name1,
           adrc~name2  INTO TABLE @DATA(lt_lfnm)
      FROM            lfa1 AS lfa1
           INNER JOIN adrc AS adrc ON adrc~addrnumber = lfa1~adrnr
      FOR ALL ENTRIES IN @me-&gt;at_sdat
      WHERE lfa1~lifnr = @me-&gt;at_sdat-lifnr_po.

    &quot;pass to items
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      READ TABLE lt_lfnm ASSIGNING FIELD-SYMBOL(&lt;lfnm&gt;) WITH KEY lifnr = &lt;sdat&gt;-lifnr_po. CHECK sy-subrc = 0.
      MOVE: &lt;lfnm&gt;-name1 TO &lt;sdat&gt;-lifnr_tx1_po,
            &lt;lfnm&gt;-name2 TO &lt;sdat&gt;-lifnr_tx2_po.
    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_GREV" CMPNAME="SDAT_SELC" VERSION="1" LANGU="E" DESCRIPT="salv-data: select data per keys +calc-diffs" EXPOSURE="0" STATE="1" EDITORDER="1 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_selc.
    &quot;salv-data: select data per keys +calc-diffs

    CHECK me-&gt;at_keys[] IS NOT INITIAL.

    &quot;select base data
    SELECT * FROM mkpf INTO TABLE @DATA(lt_mkpf) FOR ALL ENTRIES IN @me-&gt;at_keys WHERE mblnr = @me-&gt;at_keys-mblnr
                                                                                   AND mjahr = @me-&gt;at_keys-mjahr.
    SELECT * FROM mseg INTO TABLE @DATA(lt_mseg) FOR ALL ENTRIES IN @me-&gt;at_keys WHERE mblnr = @me-&gt;at_keys-mblnr
                                                                                   AND mjahr = @me-&gt;at_keys-mjahr
                                                                                   AND zeile = @me-&gt;at_keys-zeile.
    SELECT * FROM ekko INTO TABLE @DATA(lt_ekko) FOR ALL ENTRIES IN @lt_mseg     WHERE ebeln = @lt_mseg-ebeln.
    SELECT * FROM ekpo INTO TABLE @DATA(lt_ekpo) FOR ALL ENTRIES IN @lt_mseg     WHERE ebeln = @lt_mseg-ebeln
                                                                                   AND ebelp = @lt_mseg-ebelp.
    SELECT * FROM eket INTO TABLE @DATA(lt_eket) FOR ALL ENTRIES IN @lt_mseg     WHERE ebeln = @lt_mseg-ebeln
                                                                                   AND ebelp = @lt_mseg-ebelp.
    SELECT * FROM ekbe INTO TABLE @DATA(lt_ekbe) FOR ALL ENTRIES IN @lt_mseg     WHERE ebeln = @lt_mseg-ebeln
                                                                                   AND ebelp = @lt_mseg-ebelp.
    SELECT * FROM rseg INTO TABLE @DATA(lt_rseg) FOR ALL ENTRIES IN @lt_mseg     WHERE lfbnr = @lt_mseg-mblnr
                                                                                   AND lfgja = @lt_mseg-mjahr
                                                                                   AND lfpos = @lt_mseg-zeile.

    &quot;(special)mdoc: clear reversed grecs
    &quot;..sel ref, but not self-ref
    SELECT * FROM mseg INTO TABLE @DATA(lt_mref) FOR ALL ENTRIES IN @lt_mseg     WHERE lfbja = @lt_mseg-mjahr
                                                                                   AND lfbnr = @lt_mseg-mblnr
                                                                                   AND lfpos = @lt_mseg-zeile
                                                                             AND NOT ( mjahr = @lt_mseg-mjahr
                                                                                   AND mblnr = @lt_mseg-mblnr
                                                                                   AND zeile = @lt_mseg-zeile ).
    LOOP AT lt_mref ASSIGNING FIELD-SYMBOL(&lt;mref&gt;).
      READ TABLE lt_mseg ASSIGNING FIELD-SYMBOL(&lt;mseg&gt;) WITH KEY mjahr = &lt;mref&gt;-lfbja
                                                                 mblnr = &lt;mref&gt;-lfbnr
                                                                 zeile = &lt;mref&gt;-lfpos. CHECK sy-subrc = 0.
      CASE &lt;mref&gt;-shkzg.
        WHEN &apos;S&apos;. ADD      &lt;mref&gt;-erfmg TO   &lt;mseg&gt;-erfmg.
        WHEN &apos;H&apos;. SUBTRACT &lt;mref&gt;-erfmg FROM &lt;mseg&gt;-erfmg.
      ENDCASE.
    ENDLOOP.
    DELETE lt_mseg WHERE erfmg LE 0.

    &quot;map together
    LOOP AT lt_mseg ASSIGNING &lt;mseg&gt;.

      APPEND INITIAL LINE TO me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      &quot;keys
      MOVE: &lt;mseg&gt;-mblnr TO &lt;sdat&gt;-mblnr,
            &lt;mseg&gt;-mjahr TO &lt;sdat&gt;-mjahr,
            &lt;mseg&gt;-zeile TO &lt;sdat&gt;-zeile.

      &quot;grec-head
      READ TABLE lt_mkpf ASSIGNING FIELD-SYMBOL(&lt;mkpf&gt;) WITH KEY mblnr = &lt;sdat&gt;-mblnr
                                                                 mjahr = &lt;sdat&gt;-mjahr.
      IF sy-subrc = 0.
        MOVE: &lt;mkpf&gt;-vgart TO &lt;sdat&gt;-_grec-vgart,
              &lt;mkpf&gt;-blart TO &lt;sdat&gt;-_grec-blart,
              &lt;mkpf&gt;-bldat TO &lt;sdat&gt;-_grec-bldat,
              &lt;mkpf&gt;-budat TO &lt;sdat&gt;-_grec-budat,
              &lt;mkpf&gt;-usnam TO &lt;sdat&gt;-_grec-usnam,
              &lt;mkpf&gt;-xblnr TO &lt;sdat&gt;-_grec-xblnr,
              &lt;mkpf&gt;-bktxt TO &lt;sdat&gt;-_grec-bktxt.
      ENDIF.

      &quot;grec-item
      MOVE: &lt;mseg&gt;-bwart TO &lt;sdat&gt;-_grec-bwart,
            &lt;mseg&gt;-matnr TO &lt;sdat&gt;-_grec-matnr,
            &lt;mseg&gt;-werks TO &lt;sdat&gt;-_grec-werks,
            &lt;mseg&gt;-lgort TO &lt;sdat&gt;-_grec-lgort,
            &lt;mseg&gt;-erfmg TO &lt;sdat&gt;-_grec-qgrec,
            &lt;mseg&gt;-erfme TO &lt;sdat&gt;-_grec-ugrec,
            &lt;mseg&gt;-kokrs TO &lt;sdat&gt;-_grec-kokrs,
            &lt;mseg&gt;-kostl TO &lt;sdat&gt;-_grec-kostl,
            &lt;mseg&gt;-aufnr TO &lt;sdat&gt;-_grec-aufnr,
            &lt;mseg&gt;-kzbew TO &lt;sdat&gt;-_grec-kzbew,
            &lt;mseg&gt;-kzvbr TO &lt;sdat&gt;-_grec-kzvbr,
            &lt;mseg&gt;-weunb TO &lt;sdat&gt;-_grec-weunb,
            &lt;mseg&gt;-grund TO &lt;sdat&gt;-_grec-grund.

      &quot;pord
      MOVE: &lt;mseg&gt;-ebeln TO &lt;sdat&gt;-_pord-ebeln,
            &lt;mseg&gt;-ebelp TO &lt;sdat&gt;-_pord-ebelp.
      &quot;pord-head
      READ TABLE lt_ekko ASSIGNING FIELD-SYMBOL(&lt;ekko&gt;) WITH KEY ebeln = &lt;mseg&gt;-ebeln.
      IF sy-subrc = 0.
        MOVE: &lt;ekko&gt;-lifnr TO &lt;sdat&gt;-_pord-lifnr,
              &lt;ekko&gt;-bsart TO &lt;sdat&gt;-_pord-bsart,
              &lt;ekko&gt;-bedat TO &lt;sdat&gt;-_pord-bedat.
      ENDIF.
      &quot;pord-item
      READ TABLE lt_ekpo ASSIGNING FIELD-SYMBOL(&lt;ekpo&gt;) WITH KEY ebeln = &lt;mseg&gt;-ebeln
                                                                 ebelp = &lt;mseg&gt;-ebelp.
      IF sy-subrc = 0.
        MOVE: &lt;ekpo&gt;-txz01            TO &lt;sdat&gt;-_pord-txz01,
              &lt;ekpo&gt;-afnam            TO &lt;sdat&gt;-_pord-afnam,
              &lt;ekpo&gt;-zzekpo_afnam_tx  TO &lt;sdat&gt;-_pord-afnam_tx,
              &lt;ekpo&gt;-menge            TO &lt;sdat&gt;-_pord-qpord,
              &lt;ekpo&gt;-meins            TO &lt;sdat&gt;-_pord-upord.
      ENDIF.
      &quot;pord-schedule (assume one)
      READ TABLE lt_eket ASSIGNING FIELD-SYMBOL(&lt;eket&gt;) WITH KEY ebeln = &lt;mseg&gt;-ebeln
                                                                 ebelp = &lt;mseg&gt;-ebelp.
      IF sy-subrc = 0.
        MOVE: &lt;eket&gt;-eindt TO &lt;sdat&gt;-eindt_po.
      ENDIF.
      &quot;pord-quantities
      &quot;(summ qtties grec/invo on level of pord, not grec)
      LOOP AT lt_ekbe ASSIGNING FIELD-SYMBOL(&lt;ekbe&gt;) WHERE ebeln = &lt;mseg&gt;-ebeln
                                                       AND ebelp = &lt;mseg&gt;-ebelp.
        CASE &lt;ekbe&gt;-vgabe.
          WHEN &apos;1&apos;.     &quot;1= Goods Receipt
            CASE &lt;ekbe&gt;-shkzg.
              WHEN &apos;S&apos;. ADD      &lt;ekbe&gt;-menge TO   &lt;sdat&gt;-_pord-qgrec.
              WHEN &apos;H&apos;. SUBTRACT &lt;ekbe&gt;-menge FROM &lt;sdat&gt;-_pord-qgrec.
            ENDCASE.
          WHEN &apos;2&apos; OR   &quot;2= Invoice Receipt
               &apos;P&apos;   .  &quot;P= Invoice Parking
            CASE &lt;ekbe&gt;-shkzg.
              WHEN &apos;S&apos;. ADD      &lt;ekbe&gt;-menge TO   &lt;sdat&gt;-_pord-qinvo.
              WHEN &apos;H&apos;. SUBTRACT &lt;ekbe&gt;-menge FROM &lt;sdat&gt;-_pord-qinvo.
            ENDCASE.
        ENDCASE.
      ENDLOOP.
      &quot;diff: pord grec-invo
      &lt;sdat&gt;-_pord-qdiff = &lt;sdat&gt;-_pord-qgrec - &lt;sdat&gt;-_pord-qinvo.

      &quot;invoice: summ gr-qtty
      LOOP AT lt_rseg ASSIGNING FIELD-SYMBOL(&lt;rseg&gt;) WHERE lfbnr = &lt;mseg&gt;-mblnr
                                                       AND lfgja = &lt;mseg&gt;-mjahr
                                                       AND lfpos = &lt;mseg&gt;-zeile.
        MOVE: &lt;rseg&gt;-bstme TO &lt;sdat&gt;-_invo-uinvo.
        CASE &lt;rseg&gt;-shkzg.
          WHEN &apos;S&apos;. ADD      &lt;rseg&gt;-menge TO   &lt;sdat&gt;-_invo-qinvo.
          WHEN &apos;H&apos;. SUBTRACT &lt;rseg&gt;-menge FROM &lt;sdat&gt;-_invo-qinvo.
        ENDCASE.
      ENDLOOP.

    ENDLOOP.

    &quot;calc-diff: grec - invo
    &quot;(only on grec/invo level)
    LOOP AT me-&gt;at_sdat ASSIGNING &lt;sdat&gt;.
      &lt;sdat&gt;-_diff-qdiff = &lt;sdat&gt;-_grec-qgrec - &lt;sdat&gt;-_invo-qinvo.
      &lt;sdat&gt;-_diff-udiff = &lt;sdat&gt;-_grec-ugrec.
    ENDLOOP.


    &quot;keep only pos-diffs
    DELETE me-&gt;at_sdat WHERE _diff-qdiff LE 0.


  ENDMETHOD.</source>
  </method>
 </CLAS>
