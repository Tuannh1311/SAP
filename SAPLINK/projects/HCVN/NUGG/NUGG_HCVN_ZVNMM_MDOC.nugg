  <method CLSNAME="ZCL_VNMM_MDOC_RUPL" CMPNAME="ON_DBCL" VERSION="1" LANGU="E" DESCRIPT="handler: on double-click" EXPOSURE="0" STATE="1" EDITORDER="19 " DISPID="0 " MTDTYPE="1" MTDDECLTYP="0" REFCLSNAME="CL_SALV_EVENTS_TABLE" REFCMPNAME="DOUBLE_CLICK" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNMM_MDOC_RUPL" CMPNAME="ON_DBCL" SCONAME="ROW" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="1" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="0"/>
   <parameter CLSNAME="ZCL_VNMM_MDOC_RUPL" CMPNAME="ON_DBCL" SCONAME="COLUMN" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="1" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="0"/>
   <source>METHOD on_dbcl.

    &quot;get double-clicked row
    READ TABLE me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;) INDEX row.

    &quot;by column
    IF column CS &apos;_PO&apos;.
      &quot;POrd: display
      CALL FUNCTION &apos;ME_DISPLAY_PURCHASE_DOCUMENT&apos;
        EXPORTING
          i_ebeln              = &lt;sdat&gt;-_pord-ebeln
          i_ebelp              = &lt;sdat&gt;-_pord-ebelp
        EXCEPTIONS
          not_found            = 1
          no_authority         = 2
          invalid_call         = 3
          preview_not_possible = 4
          OTHERS               = 5.
      IF sy-subrc &lt;&gt; 0. MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4. ENDIF.


    ELSEIF column CS &apos;_GR&apos;.
      &quot;GRec: display
      CALL FUNCTION &apos;MIGO_DIALOG&apos;
        EXPORTING
          i_action            = &apos;A04&apos; &quot;A04= Display
          i_refdoc            = &apos;R02&apos; &quot;R02= Material Document
          i_mblnr             = &lt;sdat&gt;-_grec-mblnr
          i_mjahr             = &lt;sdat&gt;-_grec-mjahr
        EXCEPTIONS
          illegal_combination = 1
          OTHERS              = 2.
      IF sy-subrc &lt;&gt; 0. MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4. ENDIF.

    ELSEIF column EQ &apos;RETIC&apos;.
      &quot;messages: popup with msg-list
      CALL FUNCTION &apos;C14ALD_BAPIRET2_SHOW&apos;
        TABLES
          i_bapiret2_tab = &lt;sdat&gt;-ret_t.
    ENDIF.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_RUPL" CMPNAME="ON_UCOM" VERSION="1" LANGU="E" DESCRIPT="handler: on user-command" EXPOSURE="0" STATE="1" EDITORDER="20 " DISPID="0 " MTDTYPE="1" MTDDECLTYP="0" REFCLSNAME="CL_SALV_EVENTS" REFCMPNAME="ADDED_FUNCTION" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNMM_MDOC_RUPL" CMPNAME="ON_UCOM" SCONAME="E_SALV_FUNCTION" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="1" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="0"/>
   <source>METHOD on_ucom.

    &quot;default: get selected rows
    me-&gt;at_rsel = me-&gt;ar_salv-&gt;get_selections( )-&gt;get_selected_rows( ).
    &quot;default: set selections
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;). CLEAR: &lt;sdat&gt;-xsel. ENDLOOP.
    LOOP AT me-&gt;at_rsel ASSIGNING FIELD-SYMBOL(&lt;rsel&gt;).
      READ TABLE me-&gt;at_sdat ASSIGNING &lt;sdat&gt; INDEX &lt;rsel&gt;. CHECK sy-subrc = 0.
      MOVE: &apos;X&apos; TO &lt;sdat&gt;-xsel.
    ENDLOOP.


    CASE e_salv_function.

      WHEN &apos;REFRESH&apos;. me-&gt;salv_refr( ).

      WHEN &apos;DOWNLOAD&apos;. me-&gt;on_ucom_download( )            .
      WHEN &apos;UPLOAD&apos;  . me-&gt;on_ucom_upload( )              .   me-&gt;salv_refr( ).
      WHEN &apos;TEST&apos;    . me-&gt;on_ucom_bapi( ix_test = &apos;X&apos;)   .   me-&gt;salv_refr( ).
      WHEN &apos;POST&apos;    . me-&gt;on_ucom_bapi( ix_test = &apos; &apos;)   .   me-&gt;salv_refr( ).


    ENDCASE.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_RUPL" CMPNAME="ON_UCOM_BAPI" VERSION="1" LANGU="E" DESCRIPT="call: bapi BAPI_GOODSMVT_CREATE" EXPOSURE="0" STATE="1" EDITORDER="25 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNMM_MDOC_RUPL" CMPNAME="ON_UCOM_BAPI" SCONAME="IX_TEST" VERSION="1" LANGU="E" DESCRIPT="text run" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="XFELD" PAROPTIONL="X"/>
   <source>METHOD on_ucom_bapi.

    &quot;check any selected
    IF me-&gt;at_rsel[] IS INITIAL.
      MESSAGE w007(zvnmm_preq). &quot;msg{Select lines for processing.}
      RETURN.
    ENDIF.


    &quot;per-item
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;) WHERE xsel        = &apos;X&apos;.   &quot;per selected

      DATA: ls_head TYPE                   bapi2017_gm_head_01,
            ls_hret TYPE                   bapi2017_gm_head_ret,
            lt_item TYPE STANDARD TABLE OF bapi2017_gm_item_create,
            lt_ret  TYPE STANDARD TABLE OF bapiret2.
      CLEAR: ls_head,   ls_hret,
             lt_item[], lt_ret[].

      &quot;map: header
      MOVE: sy-datum            TO ls_head-pstng_date,
            &lt;sdat&gt;-_pord-ebeln  TO ls_head-ref_doc_no,
            &lt;sdat&gt;-_grec-bktxt  TO ls_head-header_txt.

      APPEND INITIAL LINE TO lt_item ASSIGNING FIELD-SYMBOL(&lt;item&gt;).
      MOVE: me-&gt;lc_bwart_101    TO &lt;item&gt;-move_type,  &quot;101= Goods Receipt
            &lt;sdat&gt;-_pord-ebeln  TO &lt;item&gt;-po_number,
            &lt;sdat&gt;-_pord-ebelp  TO &lt;item&gt;-po_item,
            &apos;B&apos;                 TO &lt;item&gt;-mvt_ind,    &quot;B= Goods movement for purchase order
            &lt;sdat&gt;-_grec-menge  TO &lt;item&gt;-entry_qnt,
            &lt;sdat&gt;-_grec-meins  TO &lt;item&gt;-entry_uom,
            &lt;sdat&gt;-_grec-grund  TO &lt;item&gt;-move_reas.


      &quot;post Goods Receipt
      CALL FUNCTION &apos;BAPI_GOODSMVT_CREATE&apos;
        EXPORTING
          goodsmvt_header  = ls_head
          goodsmvt_code    = &apos;01&apos;   &quot;01= Goods receipt for purchase order
          testrun          = ix_test
        IMPORTING
          goodsmvt_headret = ls_hret
        TABLES
          goodsmvt_item    = lt_item
          return           = lt_ret.


      &quot;pass return back to processed items
      MOVE:  lt_ret TO &lt;sdat&gt;-ret_t.
      MOVE: ls_hret-mat_doc   TO &lt;sdat&gt;-_grec-mblnr,
            ls_hret-doc_year  TO &lt;sdat&gt;-_grec-mjahr.

      &quot;set status/message/grec-num from bapiret
      &quot;any error message?
      LOOP AT &lt;sdat&gt;-ret_t ASSIGNING FIELD-SYMBOL(&lt;rett&gt;) WHERE type = &apos;E&apos;
                                                             OR type = &apos;A&apos;.
        &quot;..set error
        MOVE: &apos;E&apos; TO &lt;sdat&gt;-xupl_statu. &quot;E= &apos;error&apos;
      ENDLOOP.
      IF sy-subrc NE 0.
        &quot;no error message:
        CASE ix_test.
          WHEN &apos;X&apos;. MOVE: &apos;S&apos; TO &lt;sdat&gt;-xupl_statu. &quot;S= &apos;tested successfully&apos;
          WHEN &apos; &apos;. MOVE: &apos;P&apos; TO &lt;sdat&gt;-xupl_statu. &quot;P= &apos;created PReq&apos;
        ENDCASE.
      ENDIF.


      &quot;commit
      IF ix_test NE &apos;X&apos;.
        CALL FUNCTION &apos;BAPI_TRANSACTION_COMMIT&apos; EXPORTING wait = &apos;X&apos;.
      ENDIF.

      &quot;rollback
      IF ix_test         EQ &apos;X&apos; OR
         ls_hret-mat_doc IS INITIAL.
        CALL FUNCTION &apos;BAPI_TRANSACTION_ROLLBACK&apos;.
      ENDIF.


    ENDLOOP.


    &quot;refresh salv
    me-&gt;salv_refr( ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_RUPL" CMPNAME="ON_UCOM_DOWNLOAD" VERSION="1" LANGU="E" DESCRIPT="call: download" EXPOSURE="0" STATE="1" EDITORDER="21 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD on_ucom_download.
    &quot;call: download2
    &quot;via class CL_EHFND_XLSX &apos;XLSX Handling&apos; &amp; CL_OPENXML_HELPER &apos;OpenXML Helper Class&apos;..
    &quot;..to allow even for Fiori WebGUI calls

    &quot;§1 prepare xml-excel
    TRY.
        DATA(lo_xhndl) = cl_ehfnd_xlsx=&gt;get_instance( ).
        DATA(lo_xdocu) = lo_xhndl-&gt;create_doc( ).
        DATA(lt_sheet) =  lo_xdocu-&gt;get_sheets( ).
        READ TABLE lt_sheet INDEX 1 ASSIGNING FIELD-SYMBOL(&lt;sheet&gt;).
        DATA(onesheet) = lo_xdocu-&gt;get_sheet_by_id( iv_sheet_id = &lt;sheet&gt;-sheet_id ).
        onesheet-&gt;change_sheet_name( iv_new_name = &apos;GoodsReceiptUpload&apos; ).
      CATCH cx_dynamic_check.
        MESSAGE e002(zvnmm_mdoc) WITH &apos;cx_dynamic_check&apos;   . &quot; Packaging Error - Invalid Content &quot; Exceptions with Dynamic Check Only of the RAISING Clause
      CATCH cx_openxml_format.
        MESSAGE e002(zvnmm_mdoc) WITH &apos;cx_openxml_format&apos;   . &quot; Packaging Error - Invalid Content
      CATCH cx_openxml_not_found.
        MESSAGE e002(zvnmm_mdoc) WITH &apos;cx_openxml_not_found&apos;. &quot; Part not found
    ENDTRY.

    &quot;§2   prepare c-like data
    &quot;§2.1 prepare header..
    CLEAR: me-&gt;at_cdat[].
    DATA: lt_dfies TYPE STANDARD TABLE OF dfies.
    CALL FUNCTION &apos;DDIF_FIELDINFO_GET&apos;
      EXPORTING
        tabname   = &apos;ZVNMM_MDOC_RUPL_CDAT&apos;
      TABLES
        dfies_tab = lt_dfies.
    &quot;..-line1: technical names
    APPEND INITIAL LINE TO me-&gt;at_cdat ASSIGNING FIELD-SYMBOL(&lt;chd1&gt;).
    LOOP AT lt_dfies ASSIGNING FIELD-SYMBOL(&lt;dfie&gt;).
      ASSIGN COMPONENT &lt;dfie&gt;-fieldname OF STRUCTURE &lt;chd1&gt; TO FIELD-SYMBOL(&lt;fhd1&gt;).
      MOVE: &lt;dfie&gt;-fieldname TO &lt;fhd1&gt;.
    ENDLOOP.
    &quot;..-line2: descriptions
    APPEND INITIAL LINE TO me-&gt;at_cdat ASSIGNING FIELD-SYMBOL(&lt;chd2&gt;).
    LOOP AT lt_dfies ASSIGNING &lt;dfie&gt;.
      ASSIGN COMPONENT &lt;dfie&gt;-fieldname OF STRUCTURE &lt;chd2&gt; TO FIELD-SYMBOL(&lt;fhd2&gt;).
      MOVE: &lt;dfie&gt;-fieldtext TO &lt;fhd2&gt;.
    ENDLOOP.
    &quot;§2.2 populate items (if selected)
    LOOP AT me-&gt;at_rsel ASSIGNING FIELD-SYMBOL(&lt;rsel&gt;).
      READ TABLE me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;) INDEX &lt;rsel&gt;. CHECK sy-subrc = 0.
      APPEND INITIAL LINE TO me-&gt;at_cdat ASSIGNING FIELD-SYMBOL(&lt;cdat&gt;).
      &quot;pord/preq keys
      MOVE: &lt;sdat&gt;-_pord-banfn TO &lt;cdat&gt;-banfn,
            &lt;sdat&gt;-_pord-bnfpo TO &lt;cdat&gt;-bnfpo,
            &lt;sdat&gt;-_pord-ebeln TO &lt;cdat&gt;-ebeln,
            &lt;sdat&gt;-_pord-ebelp TO &lt;cdat&gt;-ebelp.
      MOVE: &lt;sdat&gt;-_pord-txz01 TO &lt;cdat&gt;-txz01.
      IF &lt;cdat&gt;-bnfpo EQ &apos;00000&apos;. CLEAR &lt;cdat&gt;-bnfpo. ENDIF.
      &quot;qtty &amp; uom
      IF me-&gt;ax_qtxt = &apos;X&apos;. WRITE: &lt;sdat&gt;-_pord-menge UNIT &lt;sdat&gt;-_pord-meins TO &lt;cdat&gt;-menge. ENDIF.
      IF me-&gt;ax_qnum = &apos;X&apos;. MOVE : &lt;sdat&gt;-_pord-menge                         TO &lt;cdat&gt;-menge. ENDIF.
      WRITE:        &lt;sdat&gt;-_pord-meins TO &lt;cdat&gt;-meins.
      CONDENSE: &lt;cdat&gt;-menge.
      MOVE: &lt;sdat&gt;-_pord-kostl    TO &lt;cdat&gt;-kostl_po,
            &lt;sdat&gt;-_pord-kostl_tx TO &lt;cdat&gt;-kostl_tx_po.
    ENDLOOP.

    &quot;§3 pass c-date to xml-excel
    LOOP AT me-&gt;at_cdat ASSIGNING &lt;cdat&gt;.
      DATA(lv_rowix) = sy-tabix.
      LOOP AT lt_dfies ASSIGNING &lt;dfie&gt;.
        DATA(lv_colix) = sy-tabix.
        ASSIGN COMPONENT &lt;dfie&gt;-fieldname OF STRUCTURE &lt;cdat&gt; TO FIELD-SYMBOL(&lt;field&gt;).

        DATA(lv_inpty) = if_ehfnd_xlsx_sheet=&gt;gc_celltype-charlike.
***        CASE &lt;dfie&gt;-fieldname.
***          WHEN &apos;BANFN&apos;
***            OR &apos;BNFPO&apos;
***            OR &apos;EBELN&apos;
***            OR &apos;EBELP&apos;. lv_inpty = if_ehfnd_xlsx_sheet=&gt;gc_celltype-numericchar.
***          WHEN &apos;TXZ01&apos;. lv_inpty = if_ehfnd_xlsx_sheet=&gt;gc_celltype-charlike.
***          WHEN &apos;TXZ01&apos;
***            OR &apos;BKTXT&apos;. lv_inpty = if_ehfnd_xlsx_sheet=&gt;gc_celltype-charlike.
***          WHEN &apos;MENGE&apos;. lv_inpty = if_ehfnd_xlsx_sheet=&gt;gc_celltype-float.
***          WHEN &apos;MEINS&apos;. lv_inpty = if_ehfnd_xlsx_sheet=&gt;gc_celltype-charlike.
***        ENDCASE.

        onesheet-&gt;set_cell_content( EXPORTING iv_row          = lv_rowix
                                              iv_column       = lv_colix
                                              iv_value        = &lt;field&gt;
                                              iv_input_type   = lv_inpty
                                             &quot;iv_force_string = abap_false
        ).
      ENDLOOP.
    ENDLOOP.

    &quot;§4 save file
    DATA(lv_fnam) = cl_openxml_helper=&gt;browse_local_file_save( EXPORTING iv_title      = &apos;Goods Receipt Upload Template&apos;
                                                                         iv_filename   = &apos;GoodsReceiptUpload.xlsx&apos;
                                                                         iv_extpattern = &apos;&apos;                               ).

    TRY.
        DATA(lv_xstr) = lo_xdocu-&gt;save( ).
        cl_openxml_helper=&gt;store_local_file( EXPORTING im_file_name = lv_fnam
                                                       im_data      = lv_xstr      ).
      CATCH cx_openxml_format.
        MESSAGE e002(zvnmm_mdoc) WITH &apos;cx_openxml_format&apos;       . &quot; Packaging Error - Invalid Content
      CATCH cx_openxml_not_found.
        MESSAGE e002(zvnmm_mdoc) WITH &apos;cx_openxml_not_found&apos;    . &quot; Part not found
      CATCH cx_openxml_not_allowed.
        MESSAGE e002(zvnmm_mdoc) WITH &apos;cx_openxml_not_allowed&apos;  . &quot; Action is not allowed
      CATCH cx_dynamic_check.
        MESSAGE e002(zvnmm_mdoc) WITH &apos;cx_dynamic_check&apos;        . &quot; Exceptions with Dynamic Check Only of the RAISING Clause
    ENDTRY.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_RUPL" CMPNAME="ON_UCOM_UPLOAD" VERSION="1" LANGU="E" DESCRIPT="call: upload" EXPOSURE="0" STATE="1" EDITORDER="22 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD on_ucom_upload.
    &quot;call: upload
    CLEAR: me-&gt;at_cdat.

    &quot;§1 get file
    TRY.
        DATA(lo_xhndl) = cl_ehfnd_xlsx=&gt;get_instance( ).
        DATA(lv_fname) = cl_openxml_helper=&gt;browse_local_file_open(
                           iv_title      = &apos;Goods Receipt Upload&apos;
                           iv_filename   = &apos;GoodsReceiptUpload.xlsx&apos;
                           iv_extpattern = &apos;*.xlsx&apos;
                         ).
        DATA(ls_xstrg) = cl_openxml_helper=&gt;load_local_file( lv_fname ).
        DATA(lo_xdocu) = lo_xhndl-&gt;load_doc( iv_file_data = ls_xstrg ).
        DATA(lo_sheet) = lo_xdocu-&gt;get_sheet_by_id( iv_sheet_id = 1 ).

      CATCH cx_openxml_format.
        MESSAGE e002(zvnmm_mdoc) WITH &apos;cx_openxml_format&apos;.
      CATCH cx_openxml_not_allowed.
        MESSAGE e002(zvnmm_mdoc) WITH &apos;cx_openxml_not_allowed&apos;.
      CATCH cx_dynamic_check.
        MESSAGE e002(zvnmm_mdoc) WITH &apos;cx_dynamic_check&apos;.
      CATCH cx_openxml_not_found.
        MESSAGE e002(zvnmm_mdoc) WITH &apos;cx_openxml_not_found&apos;.
    ENDTRY.

    &quot;§2 get data
    DATA: lt_dfies TYPE STANDARD TABLE OF dfies.
    CALL FUNCTION &apos;DDIF_FIELDINFO_GET&apos;
      EXPORTING
        tabname   = &apos;ZVNMM_MDOC_RUPL_CDAT&apos;
      TABLES
        dfies_tab = lt_dfies.
    DATA(lv_rows) = lo_sheet-&gt;get_last_row_number( ).
    DO lv_rows TIMES.
      DATA(lv_rowix) = sy-index.
      CHECK lv_rowix NE 1.  &quot;skip head-name
      CHECK lv_rowix NE 2.  &quot;skip-head-descr

      APPEND INITIAL LINE TO me-&gt;at_cdat ASSIGNING FIELD-SYMBOL(&lt;cdat&gt;).

****      &quot;*&amp; &quot;SAP-3941 SAP MM upload GR dump
****      &quot; this is extremely stupid..
****      &quot; ..but for the love of god I cannot get the enh public meth ZZ_GET_CELL_STYLE accessible
****      FIELD-SYMBOLS: &lt;gv_zz_cell_style&gt; TYPE i.
****      ASSIGN (&apos;(ZVNMM_MDOC_RUPL)GV_ZZ_CELL_STYLE&apos;) TO &lt;gv_zz_cell_style&gt;.

      LOOP AT lt_dfies ASSIGNING FIELD-SYMBOL(&lt;dfie&gt;).
        DATA(lv_colix) = sy-tabix.
        ASSIGN COMPONENT lv_colix OF STRUCTURE &lt;cdat&gt; TO FIELD-SYMBOL(&lt;field&gt;).
        DATA(lv_content) = lo_sheet-&gt;get_cell_content( EXPORTING iv_row     = lv_rowix
                                                                 iv_column  = lv_colix      ).

        MOVE: lv_content TO &lt;field&gt;.

****        &quot;*&amp; &quot;SAP-3941 SAP MM upload GR dump
****        &quot;special treatment: MENGE with style
****        &quot;*DATA(lv_style)   = lo_sheet-&gt;zz_get_cell_style( EXPORTING iv_row     = lv_rowix
****        &quot;*                                                          iv_column  = lv_colix      ).
****        IF &lt;dfie&gt;-fieldname = &apos;MENGE&apos; AND
****           &lt;gv_zz_cell_style&gt; IS ASSIGNED.
****          CASE &lt;gv_zz_cell_style&gt;.
****            WHEN 0. &quot;0= general
****              MOVE lv_content TO &lt;field&gt;.
****            WHEN 1. &quot;1= general
****              MOVE lv_content TO &lt;field&gt;.
****            WHEN 2. &quot;2= text
****              CALL FUNCTION &apos;RSDS_CONVERT_NUMBER&apos;
****                EXPORTING
****                  input  = lv_content
****                IMPORTING
****                  output = &lt;field&gt;.
****            WHEN 3. &quot;3= number
****              MOVE lv_content TO &lt;field&gt;.
****            WHEN OTHERS.
****              MOVE lv_content TO &lt;field&gt;.
****          ENDCASE.
****        ENDIF.

      ENDLOOP.
    ENDDO.

    &quot;§3 pass data
    me-&gt;on_ucom_upload_sdat( ).
    me-&gt;on_ucom_upload_enha( ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_RUPL" CMPNAME="ON_UCOM_UPLOAD_ENHA" VERSION="1" LANGU="E" DESCRIPT="call: upload: enhance SDat" EXPOSURE="0" STATE="1" EDITORDER="24 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD on_ucom_upload_enha.
    &quot;call: upload: enhance SDat


    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      &quot;status: default after upload
      MOVE: &apos;L&apos; TO &lt;sdat&gt;-xupl_statu. &quot;L= loaded from file

      &quot;pord-from-preq
      IF &lt;sdat&gt;-_pord-banfn IS NOT INITIAL AND
         &lt;sdat&gt;-_pord-bnfpo IS NOT INITIAL AND
         &lt;sdat&gt;-_pord-ebeln IS     INITIAL    .

        SELECT * FROM ekpo INTO TABLE @DATA(lt_ekpo) WHERE banfn = @&lt;sdat&gt;-_pord-banfn
                                                       AND bnfpo = @&lt;sdat&gt;-_pord-bnfpo
                                                       AND loekz = &apos;&apos;.
        DESCRIBE TABLE lt_ekpo LINES DATA(lv_lines).
        IF     lv_lines = 0.

        ELSEIF lv_lines = 1.
          READ TABLE lt_ekpo ASSIGNING FIELD-SYMBOL(&lt;ekpo&gt;) INDEX 1. CHECK sy-subrc = 0.
          MOVE: &lt;ekpo&gt;-ebeln TO &lt;sdat&gt;-_pord-ebeln,
                &lt;ekpo&gt;-ebelp TO &lt;sdat&gt;-_pord-ebelp.

        ELSEIF lv_lines &gt; 1.

        ENDIF.

      ENDIF.

      &quot;Reason for Movement: description
      IF &lt;sdat&gt;-_grec-grund IS NOT INITIAL.
        SELECT SINGLE grtxt INTO @&lt;sdat&gt;-_grec-grund_tx FROM t157e WHERE spras = @sy-langu
                                                                     AND bwart = @me-&gt;lc_bwart_101
                                                                     AND grund = @&lt;sdat&gt;-_grec-grund.
      ENDIF.

    ENDLOOP.



  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_RUPL" CMPNAME="ON_UCOM_UPLOAD_SDAT" VERSION="1" LANGU="E" DESCRIPT="call: upload: pass CDat to SDat" EXPOSURE="0" STATE="1" EDITORDER="23 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD on_ucom_upload_sdat.

    &quot;pass c-like CDAT to salv SDAT

    CHECK: me-&gt;at_cdat IS NOT INITIAL.
    CLEAR: me-&gt;at_sdat.
    MOVE: &apos;X&apos; TO me-&gt;ax_file.

    LOOP AT me-&gt;at_cdat ASSIGNING FIELD-SYMBOL(&lt;cdat&gt;).

      APPEND INITIAL LINE TO me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      &quot;move item by item &amp; handle specials (no move-crsponding since c-like to others)
      MOVE: &lt;cdat&gt;-banfn    TO &lt;sdat&gt;-_pord-banfn,
            &lt;cdat&gt;-bnfpo    TO &lt;sdat&gt;-_pord-bnfpo,
            &lt;cdat&gt;-ebeln    TO &lt;sdat&gt;-_pord-ebeln,
            &lt;cdat&gt;-ebelp    TO &lt;sdat&gt;-_pord-ebelp.
      MOVE: &lt;cdat&gt;-bktxt    TO &lt;sdat&gt;-_grec-bktxt.
      MOVE: &lt;cdat&gt;-grund    TO &lt;sdat&gt;-_grec-grund.

      &quot;menge/meins: qtty &amp; uom
      IF me-&gt;ax_qnum = &apos;X&apos;.
        MOVE: &lt;cdat&gt;-menge TO &lt;sdat&gt;-_grec-menge.
      ENDIF.
      IF me-&gt;ax_qtxt = &apos;X&apos;.
        CALL FUNCTION &apos;RSDS_CONVERT_NUMBER&apos;
          EXPORTING
            input  = &lt;cdat&gt;-menge
          IMPORTING
            output = &lt;sdat&gt;-_grec-menge.
      ENDIF.

      CALL FUNCTION &apos;CONVERSION_EXIT_CUNIT_INPUT&apos;
        EXPORTING
          input  = &lt;cdat&gt;-meins
        IMPORTING
          output = &lt;sdat&gt;-_grec-meins.

    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_RUPL" CMPNAME="REPORT" VERSION="1" LANGU="E" DESCRIPT="main report" EXPOSURE="2" STATE="1" EDITORDER="2 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD report.

    me-&gt;sdat_keys( ). &quot;keys select
    me-&gt;sdat_data( ). &quot;data select
    me-&gt;sdat_auth( ). &quot;data authorisation check

    me-&gt;sdat_enha( ). &quot;data enhance

    me-&gt;salv_init( ). &quot;salv initialisation
    me-&gt;salv_disp( ). &quot;salv display

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MDOC_RUPL" CMPNAME="SALV_DISP" VERSION="1" LANGU="E" DESCRIPT="salv-object: display" EXPOSURE="0" STATE="1" EDITORDER="17 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_disp.

***    &quot; optimise columns
***    me-&gt;ar_salv-&gt;get_columns( )-&gt;set_optimize( abap_true ).

    &quot; and finally display SALV table as full screen
    me-&gt;ar_salv-&gt;display( ).


