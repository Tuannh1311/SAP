  <method CLSNAME="ZCL_VNMM_MDOC_GREV" CMPNAME="CALL_REVERSE" VERSION="1" LANGU="E" DESCRIPT="call: Reverse Goods Receipt" EXPOSURE="0" STATE="1" EDITORDER="24 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD call_reverse.
    &quot;call: Reverse Goods Receipt

    DATA(lt_rows) = me-&gt;ar_salv-&gt;get_selections( )-&gt;get_selected_rows( ).
    CHECK lt_rows[] IS NOT INITIAL.

    &quot;popup confirm
    IF me-&gt;ax_auto NE &apos;X&apos;. &quot;(not for background posting)
      DATA: lx_answ TYPE c.
      CALL FUNCTION &apos;POPUP_TO_CONFIRM&apos;
        EXPORTING
          titlebar              = &apos;Reverse Goods Receips&apos;(q01)
          text_question         = &apos;Reverse selected Goods Receipt quantities?&apos;(q02)
          text_button_1         = &apos;Reverse&apos;(q03)
          icon_button_1         = &apos;ICON_STORNO&apos;
          text_button_2         = &apos;back&apos;(q04)
          icon_button_2         = &apos;ICON_SYSTEM_UNDO&apos;
          default_button        = &apos;2&apos;
          display_cancel_button = &apos;&apos;
        IMPORTING
          answer                = lx_answ
        EXCEPTIONS
          text_not_found        = 1
          OTHERS                = 2.
      CHECK lx_answ = 1. &quot;1= delete
    ENDIF.


    &quot;reverse
    LOOP AT lt_rows ASSIGNING FIELD-SYMBOL(&lt;row&gt;).

      READ TABLE me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;) INDEX &lt;row&gt;. CHECK sy-subrc = 0.

      CHECK: &lt;sdat&gt;-_diff-mblnr IS INITIAL,
             &lt;sdat&gt;-_diff-mjahr IS INITIAL. &quot;(could be already posted)

      CLEAR &lt;sdat&gt;-_diff-msg. &quot;(no previous message)

      &quot;check full qtty
      &quot;(note: bapi _cancel always reversed full item qtty)
      IF &lt;sdat&gt;-_diff-qdiff NE &lt;sdat&gt;-_grec-qgrec.
        MESSAGE w001(zvnmm_mdoc) INTO &lt;sdat&gt;-_diff-msg.
        CONTINUE.
      ENDIF.

      &quot;
      DATA: ls_head TYPE                   bapi2017_gm_head_01,
            lt_item TYPE STANDARD TABLE OF bapi2017_gm_item_04,
            lt_retu TYPE STANDARD TABLE OF bapiret2.
      CLEAR: ls_head, lt_item[], lt_retu[].

      &quot;prepare header
      MOVE: sy-datum           TO ls_head-pstng_date,
            &lt;sdat&gt;-_grec-xblnr TO ls_head-ref_doc_no.
      &quot;prepare items
      APPEND INITIAL LINE TO lt_item ASSIGNING FIELD-SYMBOL(&lt;item&gt;).
      MOVE: &lt;sdat&gt;-zeile TO &lt;item&gt;-matdoc_item.

      DATA: ls_hret TYPE bapi2017_gm_head_ret.
      CALL FUNCTION &apos;BAPI_GOODSMVT_CANCEL&apos;
        EXPORTING
          materialdocument    = &lt;sdat&gt;-mblnr
          matdocumentyear     = &lt;sdat&gt;-mjahr
          goodsmvt_pstng_date = sy-datum
          &quot;GOODSMVT_PR_UNAME   = GOODSMVT_PR_UNAME
          &quot;DOCUMENTHEADER_TEXT = DOCUMENTHEADER_TEXT
        IMPORTING
          goodsmvt_headret    = ls_hret
        TABLES
          return              = lt_retu
          goodsmvt_matdocitem = lt_item.


      IF ls_hret-mat_doc IS NOT INITIAL. &quot;»posted
        &quot;pass result
        MOVE: ls_hret-mat_doc  TO &lt;sdat&gt;-_diff-mblnr,
              ls_hret-doc_year TO &lt;sdat&gt;-_diff-mjahr.
        &quot;commit
        CALL FUNCTION &apos;BAPI_TRANSACTION_COMMIT&apos; EXPORTING wait = &apos;X&apos;.

      ELSE. &quot;»fail, not posted
        &quot;pass message
        LOOP AT lt_retu ASSIGNING FIELD-SYMBOL(&lt;retu&gt;) WHERE type = &apos;E&apos;.
          MOVE: &lt;retu&gt;-message TO &lt;sdat&gt;-_diff-msg.
        ENDLOOP.
        &quot;rollback
        CALL FUNCTION &apos;BAPI_TRANSACTION_ROLLBACK&apos;.

      ENDIF.

    ENDLOOP.

    &quot;..refresh salv
    me-&gt;ar_salv-&gt;refresh( ).


  ENDMETHOD.</source>
  </method>
