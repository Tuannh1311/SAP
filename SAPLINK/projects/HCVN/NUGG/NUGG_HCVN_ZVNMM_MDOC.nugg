   <parameter CLSNAME="ZCL_VNMM_MDOC_GREV" CMPNAME="CONSTRUCTOR" SCONAME="IX_AUTO" VERSION="1" LANGU="E" DESCRIPT="automatic posting" CMPTYPE="1" MTDTYPE="2" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="XFELD"/>
   <source>METHOD constructor.
    &quot;&amp; &quot;CBL-12887(HQ_TTa) mass: mass Goods Receipt Reversal
    &quot;&amp;   context:
    &quot;&amp;            due POord accruals, GRs needs to be posted at e-o-month..
    &quot;&amp;            ..and reversed with mass tool then
