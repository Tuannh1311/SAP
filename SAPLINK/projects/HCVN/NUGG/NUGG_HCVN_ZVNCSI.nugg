  <method CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="CHK_MULTI" VERSION="1" LANGU="E" DESCRIPT="check multi-mix of partner (on batch per one partner &amp; year)" EXPOSURE="0" STATE="1" EDITORDER="1 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" MTDNEWEXC="X" BCMTDCAT="00" BCMTDSYN="0">
   <exception CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="CHK_MULTI" SCONAME="ZCX_VNCSI_DCS" VERSION="1" LANGU="E" DESCRIPT="CSI: DCS exception class" MTDTYPE="0" EDITORDER="1 "/>
   <source>METHOD CHK_MULTI.

    &quot;check mix of Vendor(Dealer/SaleMan) &amp; Year (one POrd = one Vendor within one Year)

    DATA(lt_iflc) = me-&gt;at_iflc[].

    SORT lt_iflc BY dealr
                    slman
                    cdate.

    DELETE ADJACENT DUPLICATES FROM lt_iflc COMPARING dealr
                                                      slman
                                                      cdate(4). &quot;YYYY(-MM-DD)

    DESCRIBE TABLE lt_iflc LINES DATA(lv_iflc_lines).

    IF lv_iflc_lines &gt; 1.
      RAISE EXCEPTION TYPE zcx_vncsi_dcs MESSAGE e008(zvncsi). &quot;msg{Class IFLC: processing mix of record Vendor/Year!}
    ENDIF.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="CONSTRUCTOR" VERSION="1" LANGU="E" DESCRIPT="CONSTRUCTOR" EXPOSURE="2" STATE="1" EDITORDER="1 " DISPID="0 " MTDTYPE="2" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="CONSTRUCTOR" SCONAME="IT_IFLC" VERSION="1" LANGU="E" DESCRIPT="CSI: DCS Commission log &amp; staging table (tty)" CMPTYPE="1" MTDTYPE="2" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="ZVNCSI_DCS_IFLC_T"/>
   <parameter CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="CONSTRUCTOR" SCONAME="IX_TEST" VERSION="1" LANGU="E" DESCRIPT="CSI: test mode (no commit, no ws-call)" CMPTYPE="1" MTDTYPE="2" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="ZVNCSI_XTEST"/>
   <source>METHOD CONSTRUCTOR.

    me-&gt;at_iflc = it_iflc[].
    me-&gt;ax_test = ix_test.

    &quot;clear status and messages
    LOOP AT me-&gt;at_iflc ASSIGNING FIELD-SYMBOL(&lt;iflc&gt;).
      CLEAR: &lt;iflc&gt;-statu,
             &lt;iflc&gt;-msgid,
             &lt;iflc&gt;-msgno,
             &lt;iflc&gt;-msgty,
             &lt;iflc&gt;-msgtx.
    ENDLOOP.

    &quot;get IFLC1 records
    SELECT * FROM zvncsi_dcs_iflc1 INTO TABLE @me-&gt;at_ifl1
      FOR ALL ENTRIES IN @me-&gt;at_iflc
     WHERE rdate = @me-&gt;at_iflc-rdate
       AND rtime = @me-&gt;at_iflc-rtime
       AND dealr = @me-&gt;at_iflc-dealr
       AND slman = @me-&gt;at_iflc-slman
       AND comid = @me-&gt;at_iflc-comid
       and comm_type = @me-&gt;at_iflc-comm_type. &quot;hq_dvy CBL CBL-12796


    &quot;first record
    READ TABLE it_iflc INDEX 1 REFERENCE INTO me-&gt;ar_iflc.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="DTB_COM" VERSION="1" LANGU="E" DESCRIPT="db commit" EXPOSURE="0" STATE="1" EDITORDER="26 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD dtb_com.

    &quot;any error
    READ TABLE me-&gt;et_ret TRANSPORTING NO FIELDS WITH KEY type = &apos;E&apos;.
    IF sy-subrc = 0. DATA(lx_error) = &apos;X&apos;. ENDIF.

    &quot;perform commit
    IF me-&gt;ax_test = &apos; &apos; AND
       lx_error    = &apos; &apos;    .
      CALL FUNCTION &apos;BAPI_TRANSACTION_COMMIT&apos;   EXPORTING wait = &apos;X&apos;.
    ELSE.
      CALL FUNCTION &apos;BAPI_TRANSACTION_ROLLBACK&apos;.
    ENDIF.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="DTB_DEQ" VERSION="1" LANGU="E" DESCRIPT="db IFLC un-lock" EXPOSURE="0" STATE="1" EDITORDER="24 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD DTB_DEQ.

    &quot;unlock db recs IFLC


    CALL FUNCTION &apos;DEQUEUE_EZVNCSI_IFLC&apos;
      EXPORTING
        mode_zvncsi_dcs_iflc = &apos;E&apos;
        mandt                = sy-mandt
        dealr                = me-&gt;ar_iflc-&gt;*-dealr
        slman                = me-&gt;ar_iflc-&gt;*-slman
*       X_DEALR              = &apos; &apos;
*       X_SLMAN              = &apos; &apos;
*       _SCOPE               = &apos;3&apos;
*       _SYNCHRON            = &apos; &apos;
*       _COLLECT             = &apos; &apos;
      .


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="DTB_ENQ" VERSION="1" LANGU="E" DESCRIPT="db IFLC lock" EXPOSURE="0" STATE="1" EDITORDER="23 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" MTDNEWEXC="X" BCMTDCAT="00" BCMTDSYN="0">
   <exception CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="DTB_ENQ" SCONAME="ZCX_VNCSI_DCS" VERSION="1" LANGU="E" DESCRIPT="CSI: DCS exception class" MTDTYPE="0" EDITORDER="1 "/>
   <source>METHOD DTB_ENQ.

    &quot;lock db records: all for given Dealer/SaleMan

    CHECK me-&gt;ar_iflc IS BOUND.

    CALL FUNCTION &apos;ENQUEUE_EZVNCSI_IFLC&apos;
      EXPORTING
        mode_zvncsi_dcs_iflc = &apos;E&apos;
        mandt                = sy-mandt
        dealr                = me-&gt;ar_iflc-&gt;*-dealr
        slman                = me-&gt;ar_iflc-&gt;*-slman
*       x_dealr              = &apos; &apos;
*       x_slman              = &apos; &apos;
*       _SCOPE               = &apos;2&apos;
*       _WAIT                = &apos; &apos;
*       _COLLECT             = &apos; &apos;
      EXCEPTIONS
        foreign_lock         = 1
        system_failure       = 2
        OTHERS               = 3.
    IF sy-subrc &lt;&gt; 0.

      RAISE EXCEPTION TYPE zcx_vncsi_dcs MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4.

    ENDIF.





  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="DTB_MOD" VERSION="1" LANGU="E" DESCRIPT="db IFLC modify (+iflc1, +iflck)" EXPOSURE="0" STATE="1" EDITORDER="25 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD DTB_MOD.

    &quot;save to db IFLC
    MODIFY zvncsi_dcs_iflc FROM TABLE me-&gt;at_iflc.
    ASSERT sy-subrc = 0.

    &quot;save to db IFLC1
    MODIFY zvncsi_dcs_iflc1 FROM TABLE me-&gt;at_ifl1.
    ASSERT sy-subrc = 0.

    &quot;save to db IFLCK
    MODIFY zvncsi_dcs_iflck FROM TABLE me-&gt;bt_iflck.
    ASSERT sy-subrc = 0.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="MSG_LOG_ALL" VERSION="1" LANGU="E" DESCRIPT="expand exception error into all IFLC records" EXPOSURE="0" STATE="1" EDITORDER="8 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD MSG_LOG_ALL.

    CHECK me-&gt;ar_exc IS BOUND.

    DATA(lv_msgtx) = me-&gt;ar_exc-&gt;get_longtext( ).

    LOOP AT me-&gt;at_iflc ASSIGNING FIELD-SYMBOL(&lt;iflc&gt;).

      &quot;expand message
      MOVE: me-&gt;ar_exc-&gt;if_t100_message~t100key-msgid TO &lt;iflc&gt;-_msg-msgid,
            me-&gt;ar_exc-&gt;if_t100_message~t100key-msgno TO &lt;iflc&gt;-_msg-msgno,
            me-&gt;ar_exc-&gt;if_t100_dyn_msg~msgty         TO &lt;iflc&gt;-_msg-msgty,
            lv_msgtx                                  TO &lt;iflc&gt;-_msg-msgtx.

      &quot;status fail
      MOVE: zif_vncsi_gc=&gt;statu_f                   TO &lt;iflc&gt;-_sta-statu.

    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="MSG_LOG_ONE" VERSION="1" LANGU="E" DESCRIPT="expand message into one IFLC record" EXPOSURE="0" STATE="1" EDITORDER="9 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="1" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="MSG_LOG_ONE" SCONAME="CS_IFLC" VERSION="1" LANGU="E" DESCRIPT="CSI: DCS Commission log &amp; staging table" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="2" PARPASSTYP="1" TYPTYPE="1" TYPE="ZVNCSI_DCS_IFLC"/>
   <source>METHOD MSG_LOG_ONE.

    &quot;expand message
    MOVE: sy-msgid TO cs_iflc-_msg-msgid,
          sy-msgno TO cs_iflc-_msg-msgno,
          sy-msgty TO cs_iflc-_msg-msgty.

    &quot;status fail/processed
    CASE cs_iflc-_msg-msgty.
      WHEN &apos;E&apos;.     MOVE zif_vncsi_gc=&gt;statu_f TO cs_iflc-_sta-statu.
      WHEN OTHERS.  MOVE zif_vncsi_gc=&gt;statu_p TO cs_iflc-_sta-statu.
    ENDCASE.



  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="PRE_ACCA" VERSION="1" LANGU="E" DESCRIPT="prepare bapi data: Account Assignment (~IFLCK, para EKKN)" EXPOSURE="0" STATE="1" EDITORDER="17 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" MTDNEWEXC="X" BCMTDCAT="00" BCMTDSYN="0">
   <exception CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="PRE_ACCA" SCONAME="ZCX_VNCSI_DCS" VERSION="1" LANGU="E" DESCRIPT="CSI: DCS exception class" MTDTYPE="0" EDITORDER="1 "/>
   <source>METHOD pre_acca.

    &quot;prepare Account Assignment: for each Item..
    LOOP AT me-&gt;bt_item ASSIGNING FIELD-SYMBOL(&lt;item&gt;).

      &quot;..for each Product Profile (within item)
      READ TABLE me-&gt;at_iflc ASSIGNING FIELD-SYMBOL(&lt;iflc&gt;) WITH KEY ebelp = &lt;item&gt;-po_item. ASSERT sy-subrc = 0.
      LOOP AT me-&gt;at_ifl1 ASSIGNING FIELD-SYMBOL(&lt;ifl1&gt;) WHERE ebelp = &lt;iflc&gt;-_sta-ebelp.

        &quot;item: get config (per VendorBatch from item)
        READ TABLE me-&gt;at_c002 ASSIGNING FIELD-SYMBOL(&lt;c002&gt;) WITH KEY party = me-&gt;av_party
                                                                       cpprf = &lt;ifl1&gt;-cpprf.
        IF sy-subrc NE 0.
          RAISE EXCEPTION TYPE zcx_vncsi_dcs MESSAGE e016(zvncsi) WITH &lt;ifl1&gt;-cpprf. &quot;msg{Customizing C002 for Product Profile &amp; not found!}
        ENDIF.

        APPEND INITIAL LINE TO me-&gt;bt_acca ASSIGNING FIELD-SYMBOL(&lt;acca&gt;).
        MOVE: &lt;item&gt;-po_item    TO &lt;acca&gt;-po_item,
              &lt;ifl1&gt;-_key-itmno TO &lt;acca&gt;-serial_no,
              &lt;c002&gt;-sakto      TO &lt;acca&gt;-gl_account,
              &lt;c002&gt;-kostl      TO &lt;acca&gt;-costcenter,
              &lt;c002&gt;-aufnr      TO &lt;acca&gt;-orderid,
              &lt;ifl1&gt;-_dat-cpprf TO &lt;acca&gt;-unload_pt.    &quot;(just ref, no technical feature)
***              &lt;ifl1&gt;-_dat-cpamt TO &lt;acca&gt;-quantity,
***              &lt;ifl1&gt;-_dat-cpamt TO &lt;acca&gt;-net_value.

        APPEND INITIAL LINE TO me-&gt;bx_acca ASSIGNING FIELD-SYMBOL(&lt;xacc&gt;).
        MOVE: &lt;acca&gt;-po_item    TO &lt;xacc&gt;-po_item,
              &lt;ifl1&gt;-_key-itmno TO &lt;xacc&gt;-serial_no,
              &apos;X&apos;               TO &lt;xacc&gt;-po_itemx,
              &apos;X&apos;               TO &lt;xacc&gt;-serial_nox,
              &apos;X&apos;               TO &lt;xacc&gt;-gl_account,
              &apos;&apos;                TO &lt;xacc&gt;-costcenter,
              &apos;&apos;                TO &lt;xacc&gt;-orderid,
              &apos;X&apos;               TO &lt;xacc&gt;-unload_pt,
              &apos;X&apos;               TO &lt;xacc&gt;-quantity,
              &apos;X&apos;               TO &lt;xacc&gt;-net_value.

        &quot;unit of measure: VND= 1VND, or VN3= 1000VND
        CASE &lt;item&gt;-po_unit.
          WHEN &apos;VND&apos;.
            &lt;acca&gt;-quantity  = &lt;ifl1&gt;-_dat-cpamt.
            &lt;acca&gt;-net_value = &lt;ifl1&gt;-_dat-cpamt .
          WHEN &apos;VN3&apos;.
            &lt;acca&gt;-quantity  = &lt;ifl1&gt;-_dat-cpamt / 1000.
            &lt;acca&gt;-net_value = &lt;ifl1&gt;-_dat-cpamt       .
        ENDCASE.

        CASE me-&gt;as_c000-knttp. &quot; Cost Centre vs Order
          WHEN &apos;K&apos;.
            MOVE: &apos;X&apos; TO &lt;xacc&gt;-costcenter.
          WHEN &apos;F&apos; OR
               &apos;Z&apos;   .
            MOVE: &apos;X&apos; TO &lt;xacc&gt;-costcenter,
                  &apos;X&apos; TO &lt;xacc&gt;-orderid.
          WHEN OTHERS. ASSERT 1 = 2.
        ENDCASE.

        &quot;backfill ILFC1
        MOVE: &lt;acca&gt;-serial_no TO &lt;ifl1&gt;-_sta-zekkn.

        &quot;prepare IFLCK
        APPEND INITIAL LINE TO me-&gt;bt_iflck ASSIGNING FIELD-SYMBOL(&lt;iflck&gt;).
        MOVE: &lt;iflc&gt;-_sta-ebeln TO &lt;iflck&gt;-ebeln,
              &lt;acca&gt;-po_item    TO &lt;iflck&gt;-ebelp,
              &lt;acca&gt;-serial_no  TO &lt;iflck&gt;-zekkn,
              &lt;ifl1&gt;-_dat       TO &lt;iflck&gt;-_dat.

      ENDLOOP.

    ENDLOOP.



  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="PRE_EXTI" VERSION="1" LANGU="E" DESCRIPT="prepare bapi data: extension data (item)" EXPOSURE="0" STATE="1" EDITORDER="18 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD pre_exti.

    &quot;prepare extension data
    DATA: ls_bapi_te_mepoitem  TYPE bapi_te_mepoitem,
          ls_bapi_te_mepoitemx TYPE bapi_te_mepoitemx.

    &quot;for each POrd item, create extension data-struc &amp; x-struc
    LOOP AT me-&gt;bt_item ASSIGNING FIELD-SYMBOL(&lt;item&gt;).

      READ TABLE me-&gt;at_iflc ASSIGNING FIELD-SYMBOL(&lt;iflc&gt;) WITH KEY comid = &lt;item&gt;-trackingno. CHECK sy-subrc = 0.

      CLEAR: ls_bapi_te_mepoitem ,
             ls_bapi_te_mepoitem.

      MOVE: &lt;item&gt;-po_item TO ls_bapi_te_mepoitem-po_item,
            &apos;&apos;             TO ls_bapi_te_mepoitem-_csi-zvncsi_scsta. &quot;(slmn-statu below)

      &quot;summarize SalesMan amounts form AccAsgn zEKKN items
      LOOP AT me-&gt;at_ifl1 ASSIGNING FIELD-SYMBOL(&lt;ifl1&gt;) WHERE _sta-ebeln = &lt;iflc&gt;-_sta-ebeln
                                                           AND _sta-ebelp = &lt;iflc&gt;-_sta-ebelp.
        ADD: &lt;ifl1&gt;-_dat-smgrs TO ls_bapi_te_mepoitem-_csi-zvncsi_smgrs,
             &lt;ifl1&gt;-_dat-smpit TO ls_bapi_te_mepoitem-_csi-zvncsi_smpit,
             &lt;ifl1&gt;-_dat-smded TO ls_bapi_te_mepoitem-_csi-zvncsi_smded,
             &lt;ifl1&gt;-_dat-smadj TO ls_bapi_te_mepoitem-_csi-zvncsi_smadj,
             &lt;ifl1&gt;-_dat-smdcn TO ls_bapi_te_mepoitem-_csi-zvncsi_smdcn.
      ENDLOOP.

      &quot;commission status: only for SalesMan Commissions (not Dealers)
      IF me-&gt;av_party = zif_vncsi_gc=&gt;party_s.
        MOVE zif_vncsi_gc=&gt;scsta_0i TO ls_bapi_te_mepoitem-_csi-zvncsi_scsta.
      ENDIF.

      MOVE: &lt;item&gt;-po_item TO ls_bapi_te_mepoitemx-po_item,
            &apos;X&apos;            TO ls_bapi_te_mepoitemx-_csi-zvncsi_scsta,
            &apos;X&apos;            TO ls_bapi_te_mepoitemx-_csi-zvncsi_smgrs,
            &apos;X&apos;            TO ls_bapi_te_mepoitemx-_csi-zvncsi_smpit,
            &apos;X&apos;            TO ls_bapi_te_mepoitemx-_csi-zvncsi_smded,
            &apos;X&apos;            TO ls_bapi_te_mepoitemx-_csi-zvncsi_smadj,
            &apos;X&apos;            TO ls_bapi_te_mepoitemx-_csi-zvncsi_smdcn.

      DATA: lv_valuepart1 TYPE bapiparex-valuepart1.
      CONCATENATE ls_bapi_te_mepoitem-po_item
                  ls_bapi_te_mepoitem-_csi    INTO lv_valuepart1.

      APPEND INITIAL LINE TO me-&gt;bt_exti ASSIGNING FIELD-SYMBOL(&lt;exti&gt;).
      &lt;exti&gt;-structure  =               &apos;BAPI_TE_MEPOITEM&apos;    .
      &lt;exti&gt;-valuepart1 = CONV string( lv_valuepart1  ).

      APPEND INITIAL LINE TO me-&gt;bt_exti ASSIGNING              &lt;exti&gt;.
      &lt;exti&gt;-structure  =             &apos;BAPI_TE_MEPOITEMX&apos;    .
      &lt;exti&gt;-valuepart1 = CONV string( ls_bapi_te_mepoitemx  ).


    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="PRE_HEAD" VERSION="1" LANGU="E" DESCRIPT="prepare bapi data: Header" EXPOSURE="0" STATE="1" EDITORDER="10 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD pre_head.

    &quot;prepare Header Data
    MOVE: me-&gt;av_ebeln        TO me-&gt;bs_head-po_number,
          me-&gt;as_c000-bukrs   TO me-&gt;bs_head-comp_code,
          me-&gt;as_c000-bsart   TO me-&gt;bs_head-doc_type,
          me-&gt;av_lifnr        TO me-&gt;bs_head-vendor,
          &apos;&apos;                  TO me-&gt;bs_head-item_intvl,
          &quot;me-&gt;as_c000-zterm   TO me-&gt;bs_head-pmnttrms,
          me-&gt;as_c000-ekorg   TO me-&gt;bs_head-purch_org,
          me-&gt;as_c000-ekgrp   TO me-&gt;bs_head-pur_group,
          me-&gt;as_c000-waers   TO me-&gt;bs_head-currency.

    MOVE: &apos;X&apos;                 TO me-&gt;bx_head-po_number,
          &apos;X&apos;                 TO me-&gt;bx_head-comp_code,
          &apos;X&apos;                 TO me-&gt;bx_head-doc_type,
          &apos;X&apos;                 TO me-&gt;bx_head-vendor,
          &apos;X&apos;                 TO me-&gt;bx_head-item_intvl,
          &quot;&apos;X&apos;                 TO me-&gt;bx_head-pmnttrms,
          &apos;X&apos;                 TO me-&gt;bx_head-purch_org,
          &apos;X&apos;                 TO me-&gt;bx_head-pur_group,
          &apos;X&apos;                 TO me-&gt;bx_head-currency.

    me-&gt;pre_head_pterm( ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="PRE_HEAD_PTERM" VERSION="1" LANGU="E" DESCRIPT="prepare bapi data: Header: Payment Terms" EXPOSURE="0" STATE="1" EDITORDER="11 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD pre_head_pterm.

    &quot;prepare Header Data: Payment Terms

    &quot;1st: from Vendor Master
    SELECT SINGLE zterm FROM lfm1 INTO @DATA(lv_lfm1_zterm) WHERE lifnr = @me-&gt;bs_head-vendor
                                                              AND ekorg = @me-&gt;bs_head-purch_org.
    IF sy-subrc      = 0            AND
       lv_lfm1_zterm IS NOT INITIAL    .
      MOVE: lv_lfm1_zterm TO me-&gt;bs_head-pmnttrms.
    ELSE.
      &quot;2nd: if misssing, from Config
      MOVE: me-&gt;as_c000-zterm   TO me-&gt;bs_head-pmnttrms.
    ENDIF.

    MOVE: &apos;X&apos;                 TO me-&gt;bx_head-pmnttrms.



  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="PRE_ITEM" VERSION="1" LANGU="E" DESCRIPT="prepare bapi data: Item" EXPOSURE="0" STATE="1" EDITORDER="12 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD pre_item.

    &quot;indicate duplicities
    me-&gt;pre_item_dupl( ).

    &quot;prepare Item Data
    LOOP AT me-&gt;at_iflc ASSIGNING FIELD-SYMBOL(&lt;iflc&gt;) WHERE statu IS INITIAL. &quot;(statu is used for skipped records, see PRE_ITEM_DUPL above)
      MOVE &lt;iflc&gt;-comm_type TO me-&gt;av_comm_type. &quot;hq_dvy CBL CBL-12796
      me-&gt;set_c00x( ). &quot;hq_dvy CBL CBL-12796 - call again for item specific commoditz type

      APPEND INITIAL LINE TO me-&gt;bt_item ASSIGNING FIELD-SYMBOL(&lt;item&gt;).

      me-&gt;pre_item_numb( CHANGING cs_iflc = &lt;iflc&gt;
                                  cs_item = &lt;item&gt; ). &quot;(note: also backfilling ebeln to iflc)

      &quot;~CBL-12415 DCS waive-action: process block before delete (otherwise block overrides delete)
      IF &lt;iflc&gt;-cstat = &apos;B&apos;.                    MOVE: &apos;S&apos; TO &lt;item&gt;-delete_ind.  ENDIF. &quot;special: status open/block
      IF &lt;iflc&gt;-cactv = zif_vncsi_gc=&gt;cactv_d.  MOVE: &apos;L&apos; TO &lt;item&gt;-delete_ind.  ENDIF. &quot;special: delete

      MOVE: &lt;iflc&gt;-comid          TO &lt;item&gt;-trackingno,
            &lt;iflc&gt;-comid          TO &lt;item&gt;-vend_mat,
            &quot;&lt;iflc&gt;-ctamt          TO &lt;item&gt;-quantity,
            me-&gt;as_c000-bstme     TO &lt;item&gt;-po_unit,
            &quot;1                     TO &lt;item&gt;-net_price,
            &apos; &apos;                   TO &lt;item&gt;-gr_ind,
            &apos; &apos;                   TO &lt;item&gt;-gr_non_val.

      &quot;unit of measure: VND= 1VND, or VN3= 1000VND
      CASE &lt;item&gt;-po_unit.
        WHEN &apos;VND&apos;.
          &lt;item&gt;-quantity   = &lt;iflc&gt;-ctamt .
          &lt;item&gt;-net_price  = 1            .
        WHEN &apos;VN3&apos;.
          &lt;item&gt;-quantity   = &lt;iflc&gt;-ctamt / 1000.
          &lt;item&gt;-net_price  = 1000.
      ENDCASE.

      &quot;+CBL-12415 DCS waive-action: Lock + BlockReason, if cannot be Deleted due Parked Invoices
      &quot;(note: after qtty, as needed)
      me-&gt;pre_item_dlck( CHANGING cs_item = &lt;item&gt; ).

      &quot;count acc-assignment
      DATA(lv_aacnt) = 0.
      LOOP AT me-&gt;at_ifl1 TRANSPORTING NO FIELDS WHERE ebelp = &lt;iflc&gt;-ebelp.
        ADD 1 TO lv_aacnt.
        CHECK lv_aacnt &gt; 1.
        MOVE: 1   TO &lt;item&gt;-distrib,     &quot;VRTKZ= 1 Distribution on Quantity basis
              2   TO &lt;item&gt;-part_inv.    &quot;TWRKZ= 2 apportion proportionately
        EXIT.
      ENDLOOP.



      &quot;item short text: &lt;cust-text&gt; mm-yyyy &quot;&lt;comm-id&gt;
      &quot;&lt;item&gt;-short_text = |{ me-&gt;as_c000-txz01 } { &lt;iflc&gt;-cdate+5(2) }-{ &lt;iflc&gt;-cdate(4) } { &lt;iflc&gt;-comid }|.
      &lt;item&gt;-short_text = |{ me-&gt;as_c000-txz01 } { &lt;iflc&gt;-cdate+5(2) }-{ &lt;iflc&gt;-cdate(4) } |.

      &quot;item from config
      MOVE: me-&gt;as_c000-knttp TO &lt;item&gt;-acctasscat,
            me-&gt;as_c000-matnr TO &lt;item&gt;-material,
            me-&gt;as_c000-werks TO &lt;item&gt;-plant,
            me-&gt;as_c000-mwskz TO &lt;item&gt;-tax_code.

      &quot;x-struc per item
      APPEND INITIAL LINE TO me-&gt;bx_item ASSIGNING FIELD-SYMBOL(&lt;xitm&gt;).
      MOVE: &lt;item&gt;-po_item  TO &lt;xitm&gt;-po_item,
            &apos;X&apos;             TO &lt;xitm&gt;-po_itemx,
            &apos;X&apos;             TO &lt;xitm&gt;-delete_ind,
            &apos;X&apos;             TO &lt;xitm&gt;-short_text,
            &apos;X&apos;             TO &lt;xitm&gt;-trackingno,
            &apos;X&apos;             TO &lt;xitm&gt;-vend_mat,
            &apos;X&apos;             TO &lt;xitm&gt;-quantity,
            &apos;X&apos;             TO &lt;xitm&gt;-po_unit,
            &apos;X&apos;             TO &lt;xitm&gt;-net_price,
            &apos;X&apos;             TO &lt;xitm&gt;-gr_ind,
            &apos;X&apos;             TO &lt;xitm&gt;-gr_non_val,
            &apos;X&apos;             TO &lt;xitm&gt;-block_reason_id.
      &quot;..from config
      MOVE: &apos;X&apos;             TO &lt;xitm&gt;-vendrbatch,
            &apos;X&apos;             TO &lt;xitm&gt;-material,
            &apos;X&apos;             TO &lt;xitm&gt;-plant,
            &apos;X&apos;             TO &lt;xitm&gt;-acctasscat,
            &apos;X&apos;             TO &lt;xitm&gt;-tax_code.
      &quot;..defaults
      MOVE: &apos;X&apos;             TO &lt;xitm&gt;-distrib,
            &apos;X&apos;             TO &lt;xitm&gt;-part_inv.

    ENDLOOP.

    DELETE: me-&gt;bt_item WHERE po_item IS INITIAL, &quot;delete skipped items
            me-&gt;bx_item WHERE po_item IS INITIAL. &quot;(as result of me-&gt;pre_item_numb)

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="PRE_ITEM_DLCK" VERSION="1" LANGU="E" DESCRIPT="prepare bapi data: Item Lock+BlockReason (if cannot Delete)" EXPOSURE="0" STATE="1" EDITORDER="15 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="PRE_ITEM_DLCK" SCONAME="CS_ITEM" VERSION="1" LANGU="E" DESCRIPT="ref to bapi Item" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="2" PARPASSTYP="1" TYPTYPE="1" TYPE="BAPIMEPOITEM"/>
   <source>METHOD pre_item_dlck.
    &quot;+CBL-12415 DCS waive-action: Lock + BlockReason, if cannot be Deleted due Parked Invoices

    CHECK me-&gt;av_ebeln IS NOT INITIAL. &quot;(only for existing POrds)
    CHECK cs_item-delete_ind = &apos;L&apos;.     &quot;(only for deletions)


    &quot;§0 get Parked/Poster Invoices
    SELECT * FROM ekbe INTO TABLE @DATA(lt_ekbe) WHERE ebeln = @me-&gt;av_ebeln
                                                   AND ebelp = @cs_item-po_item.

    &quot;§1 Parked Invoices -&gt; change Delete to Lock, add BlockReason
    READ TABLE lt_ekbe WITH KEY vgabe = &apos;P&apos; TRANSPORTING NO FIELDS. &quot;P= Invoice Parking
    IF sy-subrc = 0.
      cs_item-delete_ind      = &apos;S&apos;.
      cs_item-block_reason_id = me-&gt;as_c000-blk_delp. &quot;block: cannot be deleted due parked invoices
      RETURN.
    ENDIF.

    &quot;§2 summ of Invoice &lt;&gt; POrd -&gt; change Delete to Lock, add BlockReason
    DATA: lv_summ TYPE ekbe-bpmng.
    LOOP AT lt_ekbe ASSIGNING FIELD-SYMBOL(&lt;ekbe&gt;) WHERE vgabe = &apos;2&apos;. &quot;2= Invoice Receipt (also Credit Memo, actually)
      CASE &lt;ekbe&gt;-shkzg.
        WHEN &apos;S&apos;. ADD      &lt;ekbe&gt;-bpmng TO   lv_summ.
        WHEN &apos;H&apos;. SUBTRACT &lt;ekbe&gt;-bpmng FROM lv_summ.
      ENDCASE.
    ENDLOOP.
    IF lv_summ NE 0                AND
       lv_summ NE cs_item-quantity    .
      cs_item-delete_ind      = &apos;S&apos;.
      cs_item-block_reason_id = me-&gt;as_c000-blk_delq. &quot;block: cannot be deleted due open invo qtty
      RETURN.
    ENDIF.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="PRE_ITEM_DUPL" VERSION="1" LANGU="E" DESCRIPT="prepare bapi data: Item duplicities" EXPOSURE="0" STATE="1" EDITORDER="13 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD PRE_ITEM_DUPL.

    &quot;identify duplicities per CommID, keep later records
    LOOP AT me-&gt;at_iflc ASSIGNING FIELD-SYMBOL(&lt;iflc&gt;) WHERE statu IS INITIAL.

      &quot;get later record (later date or same date + later time)
      LOOP AT me-&gt;at_iflc ASSIGNING FIELD-SYMBOL(&lt;later&gt;) WHERE comid = &lt;iflc&gt;-comid
                                                            AND ( ( rdate &gt; &lt;iflc&gt;-rdate      ) OR
                                                                  ( rdate = &lt;iflc&gt;-rdate AND
                                                                    rtime &gt; &lt;iflc&gt;-rtime    )     ).

        &quot;message (note: msg also setting status)
        MESSAGE s012(zvncsi) WITH &lt;later&gt;-rdate &lt;later&gt;-rtime INTO &lt;iflc&gt;-msgtx. &quot;msg{Skipping record, processed with later one &amp; &amp;.}
        me-&gt;msg_log_one( CHANGING cs_iflc = &lt;iflc&gt; ).

      ENDLOOP.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="PRE_ITEM_NUMB" VERSION="1" LANGU="E" DESCRIPT="prepare bapi data: Item numbering (also into iflc)" EXPOSURE="0" STATE="1" EDITORDER="14 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="PRE_ITEM_NUMB" SCONAME="CS_IFLC" VERSION="1" LANGU="E" DESCRIPT="ref to IFLC" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="2" PARPASSTYP="1" TYPTYPE="1" TYPE="ZVNCSI_DCS_IFLC"/>
   <parameter CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="PRE_ITEM_NUMB" SCONAME="CS_ITEM" VERSION="1" LANGU="E" DESCRIPT="ref to bapi Item" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="2" PARPASSTYP="1" TYPTYPE="1" TYPE="BAPIMEPOITEM"/>
   <source>METHOD PRE_ITEM_NUMB.

    &quot;handle item numbering: together with create/update/delete

    &quot;default: check if item exists
    SELECT SINGLE ebelp FROM ekpo INTO @cs_item-po_item WHERE ebeln = @me-&gt;av_ebeln
                                                          AND bednr = @cs_iflc-comid.

    &quot;item: create new / update-delete existing
    CASE cs_iflc-cactv.

      WHEN zif_vncsi_gc=&gt;cactv_c.  &quot;CREATE

        IF cs_item-po_item IS NOT INITIAL.
          &quot;create but item exist: treat like update
        ELSE.
          ADD 10 TO me-&gt;av_ebelp.
          cs_item-po_item = me-&gt;av_ebelp.
        ENDIF.

      WHEN zif_vncsi_gc=&gt;cactv_u.  &quot;UPDATE

        IF cs_item-po_item IS NOT INITIAL.
          &quot;derived above, nothing needed
        ELSE.
          &quot;update but item missing: treat like creation
          ADD 10 TO me-&gt;av_ebelp.
          cs_item-po_item = me-&gt;av_ebelp.
        ENDIF.

      WHEN zif_vncsi_gc=&gt;cactv_d.  &quot;DELETE

        IF cs_item-po_item IS NOT INITIAL.
          &quot;derived above, nothing needed
        ELSE.
          &quot;delete but item missing: skip item
          MESSAGE w009(zvncsi) INTO cs_iflc-msgtx. &quot;msg{Activity D= Delete, but no item found; record skipped.}
          me-&gt;msg_log_one( CHANGING cs_iflc = cs_iflc ).
          CLEAR cs_item-po_item.
        ENDIF.

    ENDCASE.

    &quot;backfill: IFLC
    MOVE: cs_item-po_item TO cs_iflc-ebelp.

    &quot;backfill: IFLC1
    LOOP AT me-&gt;at_ifl1 ASSIGNING FIELD-SYMBOL(&lt;ifl1&gt;) WHERE rdate = cs_iflc-rdate
                                                         AND rtime = cs_iflc-rtime
                                                         AND dealr = cs_iflc-dealr
                                                         AND slman = cs_iflc-slman
                                                         AND comid = cs_iflc-comid
                                                         and comm_type = cs_iflc-comm_type.
      MOVE: cs_iflc-ebelp TO &lt;ifl1&gt;-_sta-ebelp.
    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="PRE_SCHE" VERSION="1" LANGU="E" DESCRIPT="prepare bapi data: Delivery Schedule (=item delivery date)" EXPOSURE="0" STATE="1" EDITORDER="16 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD pre_sche.

    &quot;prepare Schedule Data
    &quot;(based on item data)
    LOOP AT me-&gt;bt_item ASSIGNING FIELD-SYMBOL(&lt;item&gt;).

      READ TABLE me-&gt;at_iflc ASSIGNING FIELD-SYMBOL(&lt;iflc&gt;) WITH KEY ebelp = &lt;item&gt;-po_item.
      CHECK sy-subrc = 0.

      APPEND INITIAL LINE TO bt_sche ASSIGNING FIELD-SYMBOL(&lt;sche&gt;).
      MOVE: &lt;item&gt;-po_item  TO &lt;sche&gt;-po_item,
            0001            TO &lt;sche&gt;-sched_line,
           &lt;iflc&gt;-cdate     TO &lt;sche&gt;-delivery_date,
           &lt;item&gt;-quantity  TO &lt;sche&gt;-quantity.

      APPEND INITIAL LINE TO bx_sche ASSIGNING FIELD-SYMBOL(&lt;xsch&gt;).
      MOVE: &lt;sche&gt;-po_item    TO &lt;xsch&gt;-po_item,
            &lt;sche&gt;-sched_line TO &lt;xsch&gt;-sched_line,
            &apos;X&apos;               TO &lt;xsch&gt;-po_itemx,
            &apos;X&apos;               TO &lt;xsch&gt;-sched_linex,
            &apos;X&apos;               TO &lt;xsch&gt;-delivery_date,
            &apos;X&apos;               TO &lt;xsch&gt;-quantity.



    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="PROCESS" VERSION="1" LANGU="E" DESCRIPT="process commission IFLC" EXPOSURE="2" STATE="1" EDITORDER="2 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD PROCESS.


    TRY.

        me-&gt;chk_multi( ).     &quot;check mix of Vendor/Year
        me-&gt;dtb_enq( ).       &quot;lock db records
        me-&gt;set_party( ).     &quot;get Partner Type
        me-&gt;set_parid( ).     &quot;get Partner ID
        me-&gt;set_c00x( ).      &quot;get customizing c001/c002

        me-&gt;set_lifnr( ).     &quot;get Vendor
        me-&gt;set_ebeln( ).     &quot;get Purchase Order (implies create/update)
        me-&gt;set_ebelp( ).     &quot;get last POrd Item (for count next)

        me-&gt;pre_head( ).      &quot;prepare bapi header
        me-&gt;pre_item( ).      &quot;prepare bapi item
        me-&gt;pre_sche( ).      &quot;prepare bapi schedule
        me-&gt;pre_acca( ).      &quot;prepare bapi account assignment
        me-&gt;pre_exti( ).      &quot;prepare bapi extension(item)

        me-&gt;pro_pord( ).      &quot;process pord create/change

        me-&gt;pro_pord_log( ).  &quot;process pord log



        me-&gt;dtb_mod( ).       &quot;modify db iflc
        me-&gt;dtb_com( ).       &quot;commit (or rollback)
        me-&gt;dtb_deq( ).       &quot;unlock db records




      CATCH zcx_vncsi_dcs INTO me-&gt;ar_exc.
        me-&gt;msg_log_all( ).   &quot;expand msg into all records
        me-&gt;dtb_mod( ).
        me-&gt;dtb_com( ).
        me-&gt;dtb_deq( ).


    ENDTRY.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="PRO_PORD" VERSION="1" LANGU="E" DESCRIPT="call POrd" EXPOSURE="0" STATE="1" EDITORDER="19 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD PRO_PORD.

    &quot;decide create/change based on determined POrd number (exist, non-exist)

    IF me-&gt;av_ebeln IS     INITIAL.
      me-&gt;pro_pord_create( ).
    ELSE.
      me-&gt;pro_pord_change( ).
    ENDIF.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="PRO_PORD_CHANGE" VERSION="1" LANGU="E" DESCRIPT="call POrd: change" EXPOSURE="0" STATE="1" EDITORDER="21 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD PRO_PORD_CHANGE.

    CALL FUNCTION &apos;BAPI_PO_CHANGE&apos;
      EXPORTING
        purchaseorder = me-&gt;av_ebeln
        poheader      = me-&gt;bs_head
        poheaderx     = me-&gt;bx_head
*       POADDRVENDOR  = POADDRVENDOR
        testrun       = me-&gt;ax_test
*       MEMORY_UNCOMPLETE = MEMORY_UNCOMPLETE
*       MEMORY_COMPLETE  = MEMORY_COMPLETE
*       POEXPIMPHEADER   = POEXPIMPHEADER
*       POEXPIMPHEADERX  = POEXPIMPHEADERX
*       VERSIONS      = VERSIONS
*       NO_MESSAGING  = NO_MESSAGING
*       NO_MESSAGE_REQ   = NO_MESSAGE_REQ
*       NO_AUTHORITY  = NO_AUTHORITY
*       NO_PRICE_FROM_PO = NO_PRICE_FROM_PO
*       PARK_COMPLETE = PARK_COMPLETE
*       PARK_UNCOMPLETE  = PARK_UNCOMPLETE
      TABLES
        return        = me-&gt;bt_ret
        poitem        = me-&gt;bt_item
        poitemx       = me-&gt;bx_item
        poschedule    = me-&gt;bt_sche
        poschedulex   = me-&gt;bx_sche
        poaccount     = me-&gt;bt_acca
        poaccountx    = me-&gt;bx_acca
        extensionin   = me-&gt;bt_exti
        extensionout  = me-&gt;bt_exti.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="PRO_PORD_CREATE" VERSION="1" LANGU="E" DESCRIPT="call POrd: creation" EXPOSURE="0" STATE="1" EDITORDER="20 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD PRO_PORD_CREATE.

    CALL FUNCTION &apos;BAPI_PO_CREATE1&apos;
      EXPORTING
        poheader         = me-&gt;bs_head
        poheaderx        = me-&gt;bx_head
*       POADDRVENDOR     = POADDRVENDOR
        testrun          = me-&gt;ax_test
*       MEMORY_UNCOMPLETE = MEMORY_UNCOMPLETE
*       MEMORY_COMPLETE  = MEMORY_COMPLETE
*       POEXPIMPHEADER   = POEXPIMPHEADER
*       POEXPIMPHEADERX  = POEXPIMPHEADERX
*       VERSIONS         = VERSIONS
*       NO_MESSAGING     = NO_MESSAGING
*       NO_MESSAGE_REQ   = NO_MESSAGE_REQ
*       NO_AUTHORITY     = NO_AUTHORITY
*       NO_PRICE_FROM_PO = NO_PRICE_FROM_PO
*       PARK_COMPLETE    = PARK_COMPLETE
*       PARK_UNCOMPLETE  = PARK_UNCOMPLETE
      IMPORTING
        exppurchaseorder = me-&gt;av_ebeln
      TABLES
        return           = me-&gt;bt_ret
        poitem           = me-&gt;bt_item
        poitemx          = me-&gt;bx_item
        poschedule       = me-&gt;bt_sche
        poschedulex      = me-&gt;bx_sche
        poaccount        = me-&gt;bt_acca
        poaccountx       = me-&gt;bx_acca
        extensionin      = me-&gt;bt_exti
        extensionout     = me-&gt;bt_exti.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="PRO_PORD_LOG" VERSION="1" LANGU="E" DESCRIPT="call POrd: log results" EXPOSURE="0" STATE="1" EDITORDER="22 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD pro_pord_log.

    &quot;POrd number back to log
    LOOP AT me-&gt;at_iflc ASSIGNING FIELD-SYMBOL(&lt;iflc&gt;).
      MOVE: me-&gt;av_ebeln TO &lt;iflc&gt;-ebeln.
      &quot;(item is already there from item numbering)
    ENDLOOP.
    &quot;..also iflc1
    LOOP AT me-&gt;at_ifl1 ASSIGNING FIELD-SYMBOL(&lt;ifl1&gt;).
      MOVE: me-&gt;av_ebeln TO &lt;ifl1&gt;-ebeln.
    ENDLOOP.
    &quot;..also iflck (+2020-01-29 SAP-3078 DCS PIT posting missing)
    LOOP AT me-&gt;bt_iflck ASSIGNING FIELD-SYMBOL(&lt;iflck&gt;).
      MOVE: me-&gt;av_ebeln TO &lt;iflck&gt;-ebeln.
    ENDLOOP.

    &quot;get messages from bapi ret, and keep only errors
    DATA(lt_eret) = me-&gt;bt_ret.
    DELETE lt_eret WHERE type &lt;&gt; &apos;E&apos;.

    IF ax_test IS INITIAL.
      ASSERT NOT ( lt_eret[]    IS INITIAL AND
                   me-&gt;av_ebeln IS INITIAL     ). &quot;this cannot happen, let it dump
    ENDIF.

    IF lt_eret[] IS INITIAL.

      &quot;no error messages, must be success..
      LOOP AT me-&gt;at_iflc ASSIGNING &lt;iflc&gt;.
        MESSAGE s010(zvncsi) WITH me-&gt;av_ebeln INTO &lt;iflc&gt;-msgtx. &quot;msg{Commission(s) processed into Purchase Order &amp;.}
        me-&gt;msg_log_one( CHANGING cs_iflc = &lt;iflc&gt; ).
      ENDLOOP.

    ELSE.

      &quot;error, lets take first one and pass to log..
      &quot;..well, not first one, clear the useless ones
      DELETE lt_eret WHERE ( id = &apos;MEPO&apos; AND number = 002 )  &quot;MEPO002{PO header data still faulty}
                        OR ( id = &apos;BAPI&apos; AND number = 001 ). &quot;BAPI001{No instance of object type &amp; has been created}

      READ TABLE lt_eret INDEX 1 ASSIGNING FIELD-SYMBOL(&lt;eret&gt;).
      LOOP AT me-&gt;at_iflc ASSIGNING &lt;iflc&gt;.
        MESSAGE ID     &lt;eret&gt;-id
                TYPE   &lt;eret&gt;-type
                NUMBER &lt;eret&gt;-number
                WITH   &lt;eret&gt;-message_v1
                       &lt;eret&gt;-message_v2
                       &lt;eret&gt;-message_v3
                       &lt;eret&gt;-message_v4 INTO &lt;iflc&gt;-msgtx.
        me-&gt;msg_log_one( CHANGING cs_iflc = &lt;iflc&gt; ).
      ENDLOOP.

    ENDIF.


    &quot;publish ret-log full
    me-&gt;et_ret = me-&gt;bt_ret.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="SET_C00X" VERSION="1" LANGU="E" DESCRIPT="get customizing c000/c002/c003" EXPOSURE="0" STATE="1" EDITORDER="2 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" MTDNEWEXC="X" BCMTDCAT="00" BCMTDSYN="0">
   <exception CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="SET_C00X" SCONAME="ZCX_VNCSI_DCS" VERSION="1" LANGU="E" DESCRIPT="CSI: exception class" MTDTYPE="0" EDITORDER="1 "/>
   <source>METHOD set_c00x.

    SELECT SINGLE * FROM zvncsi_dcs_c000 INTO @me-&gt;as_c000 WHERE party = @me-&gt;av_party
                                                             AND comm_type = @me-&gt;av_comm_type. &quot;hq_dvy CBL CBL-12796
    IF sy-subrc NE 0. RAISE EXCEPTION TYPE zcx_vncsi_dcs MESSAGE e007(zvncsi) WITH &apos;ZVNCSI_DCS_C000&apos;. ENDIF. &quot;msg{Customizing &amp; not found.}

    SELECT * FROM zvncsi_dcs_c002 INTO TABLE @me-&gt;at_c002 WHERE party = @me-&gt;av_party.
    IF sy-subrc NE 0. RAISE EXCEPTION TYPE zcx_vncsi_dcs MESSAGE e007(zvncsi) WITH &apos;ZVNCSI_DCS_C002&apos;. ENDIF. &quot;msg{Customizing &amp; not found.}

    SELECT * FROM zvncsi_dcs_c003 INTO TABLE @me-&gt;at_c003 WHERE party = @me-&gt;av_party.
    IF sy-subrc NE 0. RAISE EXCEPTION TYPE zcx_vncsi_dcs MESSAGE e007(zvncsi) WITH &apos;ZVNCSI_DCS_C003&apos;. ENDIF. &quot;msg{Customizing &amp; not found.}

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="SET_EBELN" VERSION="1" LANGU="E" DESCRIPT="determine Purchase Order (indicate Create/Update)" EXPOSURE="0" STATE="1" EDITORDER="6 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SET_EBELN.

    &quot;prepare date from/to from year
    DATA(lv_eindt_fr) = CONV eindt( me-&gt;ar_iflc-&gt;*-cdate(4) &amp;&amp; &apos;0101&apos; ). &quot;YYYY0101
    DATA(lv_eindt_to) = CONV eindt( me-&gt;ar_iflc-&gt;*-cdate(4) &amp;&amp; &apos;1231&apos; ). &quot;YYYY1231


    &quot;get Purchase Order for Vendor within Year
    SELECT SINGLE ekko~ebeln
      INTO @me-&gt;av_ebeln
      FROM            ekko AS ekko
           INNER JOIN ekpo AS ekpo ON ekpo~ebeln = ekko~ebeln
           INNER JOIN eket AS eket ON eket~ebeln = ekpo~ebeln AND
                                      eket~ebelp = ekpo~ebelp
     WHERE ekko~lifnr =  @me-&gt;av_lifnr
       AND ekko~loekz =  &apos;&apos;
       AND eket~eindt &gt;= @lv_eindt_fr
       AND eket~eindt &lt;= @lv_eindt_to.

    &quot;backfill: IFLC
    LOOP AT me-&gt;at_iflc ASSIGNING FIELD-SYMBOL(&lt;iflc&gt;).
      MOVE: me-&gt;av_ebeln TO &lt;iflc&gt;-_sta-ebeln.
    ENDLOOP.

    &quot;backfill: IFLC1
    LOOP AT me-&gt;at_ifl1 ASSIGNING FIELD-SYMBOL(&lt;ifl1&gt;).
      MOVE: me-&gt;av_ebeln TO &lt;ifl1&gt;-_sta-ebeln.
    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="SET_EBELP" VERSION="1" LANGU="E" DESCRIPT="determine last Purchase Order item (for count next)" EXPOSURE="0" STATE="1" EDITORDER="7 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SET_EBELP.

    CHECK me-&gt;av_ebeln IS NOT INITIAL.

    SELECT SINGLE MAX( ebelp ) INTO @me-&gt;av_ebelp FROM ekpo WHERE ebeln = @me-&gt;av_ebeln.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="SET_LIFNR" VERSION="1" LANGU="E" DESCRIPT="determine Vendor" EXPOSURE="0" STATE="1" EDITORDER="5 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" MTDNEWEXC="X" BCMTDCAT="00" BCMTDSYN="0">
   <exception CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="SET_LIFNR" SCONAME="ZCX_VNCSI_DCS" VERSION="1" LANGU="E" MTDTYPE="0" EDITORDER="1 "/>
   <source>METHOD set_lifnr.

    &quot;determine Vendor from extID of Dealer/SaleMan

    &quot;~CBL-13741 CreditCashProvider (C003 for multiple extID to single PartnerType)
    SELECT SINGLE vend~vendor INTO @me-&gt;av_lifnr
      FROM            zvncsi_exid_vend AS vend
           INNER JOIN zvncsi_dcs_c003  AS c003 ON c003~idtyp = vend~type
     WHERE vend~idnumber  = @me-&gt;av_parid
       AND c003~party     = @me-&gt;as_c000-party.

    IF sy-subrc NE 0.
      RAISE EXCEPTION TYPE zcx_vncsi_dcs MESSAGE e005(zvncsi) WITH me-&gt;av_parid. &quot;msg{No Vendor found for Dealer/SaleMan &amp; &amp;!}
    ENDIF.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="SET_PARID" VERSION="1" LANGU="E" DESCRIPT="determine Partner ID (Dealer/SaleMan)" EXPOSURE="0" STATE="1" EDITORDER="4 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" MTDNEWEXC="X" BCMTDCAT="00" BCMTDSYN="0">
   <exception CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="SET_PARID" SCONAME="ZCX_VNCSI_DCS" VERSION="1" LANGU="E" MTDTYPE="0" EDITORDER="1 "/>
   <source>METHOD SET_PARID.

    &quot;set ParID: Dealer/SaleMan into one field
    IF me-&gt;ar_iflc-&gt;*-dealr IS NOT INITIAL. me-&gt;av_parid = me-&gt;ar_iflc-&gt;*-dealr. ENDIF.
    IF me-&gt;ar_iflc-&gt;*-slman IS NOT INITIAL. me-&gt;av_parid = me-&gt;ar_iflc-&gt;*-slman. ENDIF.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="SET_PARTY" VERSION="1" LANGU="E" DESCRIPT="determine Partner Type" EXPOSURE="0" STATE="1" EDITORDER="3 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" MTDNEWEXC="X" BCMTDCAT="00" BCMTDSYN="0">
   <exception CLSNAME="ZCL_VNCSI_DCS_IFPC" CMPNAME="SET_PARTY" SCONAME="ZCX_VNCSI_DCS" VERSION="1" LANGU="E" MTDTYPE="0" EDITORDER="1 "/>
   <source>METHOD set_party.

    &quot;determine Partner Type: Dealer vs SaleMan
    IF me-&gt;ar_iflc-&gt;*-dealr IS NOT INITIAL.
      me-&gt;av_party = zif_vncsi_gc=&gt;party_d.
* begin of hq_dvy CBL-CBL-12796
      IF me-&gt;ar_iflc-&gt;*-comm_type IS NOT INITIAL.
        me-&gt;av_comm_type = me-&gt;ar_iflc-&gt;*-comm_type.
      ENDIF.
    ENDIF.
* end of hq_dvy CBL-CBL-12796
    IF me-&gt;ar_iflc-&gt;*-slman IS NOT INITIAL. me-&gt;av_party = zif_vncsi_gc=&gt;party_s. ENDIF.

  ENDMETHOD.</source>
  </method>
