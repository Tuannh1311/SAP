  <method CLSNAME="ZCL_VNCSI_SUB_IFPP" CMPNAME="PREP_IFLP" VERSION="1" LANGU="E" DESCRIPT="prepare IFLP-log from response" EXPOSURE="0" STATE="1" EDITORDER="4 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD prep_iflp.

    &quot;prepare IFLP log from ws-response

    LOOP AT me-&gt;at_rsp  ASSIGNING FIELD-SYMBOL(&lt;rsp&gt;).

      APPEND INITIAL LINE TO me-&gt;at_iflp ASSIGNING FIELD-SYMBOL(&lt;iflp&gt;).

      MOVE: sy-datum            TO &lt;iflp&gt;-rdate,
            sy-uzeit            TO &lt;iflp&gt;-rtime,
            &lt;rsp&gt;-partner_code  TO &lt;iflp&gt;-partner_code,
            &lt;rsp&gt;-partner_type  TO &lt;iflp&gt;-partner_type,
            &apos;VN01&apos;              TO &lt;iflp&gt;-bukrs,        &quot;*TBD
            &lt;rsp&gt;-pmnt_docn     TO &lt;iflp&gt;-pmnt_docn,
            &lt;rsp&gt;-pmnt_year     TO &lt;iflp&gt;-pmnt_year,
            &lt;rsp&gt;-posted        TO &lt;iflp&gt;-statu,
            &lt;rsp&gt;-msg_text      TO &lt;iflp&gt;-msgtx.

    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_IFPP" CMPNAME="PREP_REQ" VERSION="1" LANGU="E" DESCRIPT="select Payments, prepare request" EXPOSURE="0" STATE="1" EDITORDER="2 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD prep_req.

    &quot;***NOTE: here we are working with already selected Payment Documents, from repo ZVNCSI_SUB_IFPP

    &quot;select Payments
    &quot; per documents in AT_PSEL, prepare request data AT_REQ

    CHECK me-&gt;at_psel IS NOT INITIAL.

    &quot;**********************************************************************
    &quot;§0 data preparation

    SELECT
           &quot;BKPF IHDR inv-header
           ihdr~bukrs         AS ihdr_bukrs,
           ihdr~belnr         AS ihdr_belnr,
           ihdr~gjahr         AS ihdr_gjahr,
           &quot;BSEG ICUS item with customer
           icst~buzei         AS icst_buzei,
           icst~kunnr         AS icst_kunnr,
           icst~augbl         AS icst_augbl,
           icst~auggj         AS icst_auggj,
           &quot;view for Vendor-externalIDs (Dealer, SalesMan)
           vcus~customer      AS vcus_kunnr,
           vcus~idnumber      AS vcus_idnum,
           &quot;customizing per partner type
           c000~party         AS c000_party,
           c000~idtyp         AS c000_idtyp,
           &quot;paydoc header
           pdoc~bukrs         AS pdoc_bukrs,
           pdoc~belnr         AS pdoc_belnr,
           pdoc~gjahr         AS pdoc_gjahr,
           pdoc~waers         AS pdoc_waers,
           pdoc~bldat         AS pdoc_bldat,
           pdoc~budat         AS pdoc_budat,
           &quot;paydoc item
           pitm~buzei         AS pitm_buzei,
           pitm~wrbtr         AS pitm_wrbtr

      INTO TABLE @DATA(lt_ipay)
      FROM            bkpf                  AS ihdr
           INNER JOIN bseg                  AS icst ON icst~bukrs         =  ihdr~bukrs     AND
                                                       icst~belnr         =  ihdr~belnr     AND
                                                       icst~gjahr         =  ihdr~gjahr     AND
                                                       icst~koart         = &apos;D&apos;                 &quot;only customer item

           INNER JOIN zvncsi_exid_cust      AS vcus ON vcus~customer      =  icst~kunnr
           INNER JOIN zvncsi_sub_c000       AS c000 ON c000~idtyp         =  vcus~type
           INNER JOIN bkpf                  AS pdoc ON pdoc~blart         = &apos;DZ&apos;            AND &quot;DZ= Customer Payment
                                                       pdoc~bukrs         = ihdr~bukrs      AND
                                                       pdoc~belnr         = icst~augbl      AND
                                                       pdoc~gjahr         = icst~auggj
           INNER JOIN bseg                  AS pitm ON pitm~bukrs         = pdoc~bukrs      AND
                                                       pitm~belnr         = pdoc~belnr      AND
                                                       pitm~gjahr         = pdoc~gjahr      AND
                                                       pitm~bschl         = &apos;15&apos;            AND &quot;15= Incoming payment
                                                       pitm~koart         = &apos;D&apos;

      FOR ALL ENTRIES IN @me-&gt;at_psel
      WHERE icst~kunnr = @me-&gt;at_psel-kunnr
        AND pdoc~bukrs = @me-&gt;at_psel-bukrs
        AND pdoc~belnr = @me-&gt;at_psel-belnr
        AND pdoc~gjahr = @me-&gt;at_psel-gjahr
        AND ( ihdr~blart = c000~dtycm OR
              ihdr~blart = c000~dtyiv   ).



    &quot;table of payments only
    DATA(lt_ppay) = lt_ipay[].
    SORT lt_ppay BY icst_kunnr
                    pdoc_bukrs
                    pdoc_belnr
                    pdoc_gjahr.
    DELETE ADJACENT DUPLICATES FROM lt_ppay COMPARING icst_kunnr
                                                      pdoc_bukrs
                                                      pdoc_belnr
                                                      pdoc_gjahr.


    &quot;**********************************************************************
    &quot;§1 map to request
    LOOP AT lt_ppay ASSIGNING FIELD-SYMBOL(&lt;ppay&gt;).
      APPEND INITIAL LINE TO me-&gt;at_req ASSIGNING FIELD-SYMBOL(&lt;req&gt;).

      MOVE: &lt;ppay&gt;-vcus_idnum   TO &lt;req&gt;-partner_code,
            &lt;ppay&gt;-c000_party   TO &lt;req&gt;-partner_type,
            &lt;ppay&gt;-pdoc_belnr   TO &lt;req&gt;-pmnt_docn,
            &lt;ppay&gt;-pdoc_gjahr   TO &lt;req&gt;-pmnt_year,
            &apos;&apos;                  TO &lt;req&gt;-pmnt_amnt.   &quot;below: curr conv
      &lt;req&gt;-pmnt_amnt = me-&gt;conv_amnt( iv_amnt = &lt;ppay&gt;-pitm_wrbtr
                                       iv_curr = &lt;ppay&gt;-pdoc_waers ).

      &quot;payment date
      &lt;req&gt;-pmnt_date = me-&gt;conv_date( iv_date = &lt;ppay&gt;-pdoc_bldat ).

      &quot;invoices within payment
      LOOP AT lt_ipay ASSIGNING FIELD-SYMBOL(&lt;ipay&gt;) WHERE pdoc_bukrs = &lt;ppay&gt;-pdoc_bukrs
                                                       AND pdoc_belnr = &lt;ppay&gt;-pdoc_belnr
                                                       AND pdoc_gjahr = &lt;ppay&gt;-pdoc_gjahr.
        APPEND INITIAL LINE TO &lt;req&gt;-struc_inv ASSIGNING FIELD-SYMBOL(&lt;rit&gt;).

        MOVE: &lt;ipay&gt;-ihdr_belnr TO &lt;rit&gt;-inv_docn,
              &lt;ipay&gt;-ihdr_gjahr TO &lt;rit&gt;-inv_year.

      ENDLOOP.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_IFPP" CMPNAME="PROCESS" VERSION="1" LANGU="E" DESCRIPT="process payment IFLP" EXPOSURE="2" STATE="1" EDITORDER="2 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD PROCESS.

    me-&gt;read_c000( ).    &quot;read customizing

    me-&gt;prep_req( ).     &quot;prepare ws-request (from PSEL to REQ)

    me-&gt;proc_wscall( ).  &quot;call ws

    me-&gt;prep_iflp( ).    &quot;prepare iflp-log from ws-response
    me-&gt;save_iflp( ).    &quot;save iflp-log to database

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_IFPP" CMPNAME="PROC_WSCALL" VERSION="1" LANGU="E" DESCRIPT="process into ws-call" EXPOSURE="0" STATE="1" EDITORDER="3 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD proc_wscall.

    &quot;take prepared request, call ws

    CHECK me-&gt;ax_test IS INITIAL. &quot;(no ws-call in test mode)

    CALL FUNCTION &apos;ZVNCSI_CS_SUB_PAY&apos;
      EXPORTING
        iv_port = me-&gt;av_port
        it_req  = me-&gt;at_req
      IMPORTING
        et_rsp  = me-&gt;at_rsp.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_IFPP" CMPNAME="READ_C000" VERSION="1" LANGU="E" DESCRIPT="get config" EXPOSURE="0" STATE="1" EDITORDER="1 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD read_c000.

    &quot;get config
    SELECT * FROM: zvncsi_sub_c000 INTO TABLE @me-&gt;at_c000 .


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_IFPP" CMPNAME="SAVE_IFLP" VERSION="1" LANGU="E" DESCRIPT="save IFLP-log to database" EXPOSURE="0" STATE="1" EDITORDER="5 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SAVE_IFLP.

    CHECK: me-&gt;ax_test IS     INITIAL,
           me-&gt;at_iflp IS NOT INITIAL.

    INSERT zvncsi_sub_iflp FROM TABLE me-&gt;at_iflp.

    CALL FUNCTION &apos;BAPI_TRANSACTION_COMMIT&apos;. &quot;(default commit, see check of test mode above)



  ENDMETHOD.</source>
  </method>
 </CLAS>
