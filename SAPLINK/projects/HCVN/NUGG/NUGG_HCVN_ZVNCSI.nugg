  WRITE: /,/ &apos;*** Selection parameters:&apos;(ts0).

  LOOP AT so_dealr ASSIGNING FIELD-SYMBOL(&lt;dealr&gt;).
    WRITE: / &apos;  SO_DEALR Dealer                  =&apos;, &lt;dealr&gt;-sign, &lt;dealr&gt;-option, &lt;dealr&gt;-low, &lt;dealr&gt;-high.
  ENDLOOP.

  LOOP AT so_slman ASSIGNING FIELD-SYMBOL(&lt;slman&gt;).
    WRITE: / &apos;  SO_SLMAN SalesMa                 =&apos;, &lt;slman&gt;-sign, &lt;slman&gt;-option, &lt;slman&gt;-low, &lt;slman&gt;-high.
  ENDLOOP.

  LOOP AT so_belnr ASSIGNING FIELD-SYMBOL(&lt;belnr&gt;).
    WRITE: / &apos;  SO_BELNR Document number         =&apos;, &lt;belnr&gt;-sign, &lt;belnr&gt;-option, &lt;belnr&gt;-low, &lt;belnr&gt;-high.
  ENDLOOP.

  LOOP AT so_gjahr ASSIGNING FIELD-SYMBOL(&lt;gjahr&gt;).
    WRITE: / &apos;  SO_GJAHR Fiscal Year             =&apos;, &lt;gjahr&gt;-sign, &lt;gjahr&gt;-option, &lt;gjahr&gt;-low, &lt;gjahr&gt;-high.
  ENDLOOP.

  LOOP AT so_budat ASSIGNING FIELD-SYMBOL(&lt;budat&gt;).
    WRITE: / &apos;  SO_BUDAT Posting Date            =&apos;, &lt;budat&gt;-sign, &lt;budat&gt;-option, &lt;budat&gt;-low, &lt;budat&gt;-high.
  ENDLOOP.

  LOOP AT so_cpudt ASSIGNING FIELD-SYMBOL(&lt;cpudt&gt;).
    WRITE: / &apos;  SO_CPUDT Entry Date              =&apos;, &lt;cpudt&gt;-sign, &lt;cpudt&gt;-option, &lt;cpudt&gt;-low, &lt;cpudt&gt;-high.
  ENDLOOP.



  WRITE:   / &apos;  PA_RERR  re-process errors      =&apos;, pa_rerr,
           / &apos;  PA_WISL  write selected docs    =&apos;, pa_wisl,
           / &apos;  PA_WIFL  write log IFLI         =&apos;, pa_wifl,
           / &apos;  PA_WREQ  write full ws-Request  =&apos;, pa_wreq,
           / &apos;  PA_WRSP  write full ws-Response =&apos;, pa_wrsp,
           / &apos;  PA_WSEP  write sel-parameters   =&apos;, pa_wsep,
           / &apos;  PA_PORT  logical port           =&apos;, pa_port.

  WRITE: /.


ENDFORM.


*&amp;---------------------------------------------------------------------*
*&amp; Form data_select
*&amp;---------------------------------------------------------------------*
*&amp; select data from db per sel-screen
*&amp;---------------------------------------------------------------------*
FORM data_select .

  &quot;customizing: Vendor externalIDs
  SELECT SINGLE * FROM: zvncsi_dcs_c000 INTO @DATA(ls_c00d) WHERE party = @zif_vncsi_gc=&gt;party_d, &quot;PartnTyp D= Dealer
                        zvncsi_dcs_c000 INTO @DATA(ls_c00s) WHERE party = @zif_vncsi_gc=&gt;party_s. &quot;PartnTyp S= SalesMan

  SELECT
         &quot;RBKP mm-Invoice
         rbkp~belnr       AS rbkp_belnr,
         rbkp~gjahr       AS rbkp_gjahr,
         rbkp~budat       AS rbkp_budat,
         rbkp~cpudt       AS rbkp_cpudt,
         rbkp~stblg       AS rbkp_stblg,
         rbkp~stjah       AS rbkp_stjah,
         rbkp~ivtyp       AS rbkp_ivtyp,
         &quot;view for Vendor-externalIDs (Dealer, SalesMan)
         vdlr~vendor      AS vdlr_lifnr,
         vdlr~idnumber    AS vdlr_dlrid,
         vslm~vendor      AS vslm_lifnr,
         vslm~idnumber    AS vslm_slmid,
         &quot;IFLI invoice interface log
         ifli~rdate       AS ifli_rdate,
         ifli~rtime       AS ifli_rtime,
         ifli~dealr       AS ifli_dealr,
         ifli~slman       AS ifli_slman,
         ifli~inv_doc_no  AS ifli_rbkp,
         ifli~inv_doc_yr  AS ifli_gjahr,
         ifli~statu       AS ifli_statu
    INTO TABLE @DATA(lt_dsel)
    FROM            rbkp                  AS rbkp
         LEFT OUTER JOIN zvncsi_dcs_c003  AS c003d ON c003d~party     =  @ls_c00d-party
         LEFT OUTER JOIN zvncsi_exid_vend AS vdlr  ON vdlr~type       =  c003d~idtyp AND
                                                      vdlr~vendor     =  rbkp~lifnr
         LEFT OUTER JOIN zvncsi_dcs_c003  AS c003s ON c003s~party     =  @ls_c00s-party
         LEFT OUTER JOIN zvncsi_exid_vend AS vslm  ON vslm~type       =  c003s~idtyp AND
                                                      vslm~vendor     =  rbkp~lifnr
         LEFT OUTER JOIN zvncsi_dcs_ifli  AS ifli ON ifli~inv_doc_no  =  rbkp~belnr AND
                                                     ifli~inv_doc_yr  =  rbkp~gjahr
    WHERE vdlr~idnumber IN @so_dealr
      AND vslm~idnumber IN @so_slman
      AND rbkp~belnr    IN @so_belnr
      AND rbkp~gjahr    IN @so_gjahr
      AND rbkp~budat    IN @so_budat
      AND rbkp~cpudt    IN @so_cpudt
      AND rbkp~rbstat   EQ &apos;5&apos;       &quot;5= Posted
      AND ( vdlr~vendor NE &apos;&apos; OR
            vslm~vendor NE &apos;&apos; ). &quot;(too many left joins?)

  &quot;+CBL-13926(HQ_TTa) &quot;remove Errors when Success&quot;
  IF pa_succ = &apos;X&apos;.
    LOOP AT lt_dsel ASSIGNING FIELD-SYMBOL(&lt;succ&gt;) WHERE ifli_statu = &apos;S&apos;.
      DELETE lt_dsel WHERE rbkp_belnr = &lt;succ&gt;-rbkp_belnr
                       AND rbkp_gjahr = &lt;succ&gt;-rbkp_gjahr
                       AND ifli_statu = &apos;E&apos;.
    ENDLOOP.
  ENDIF.

  &quot;keep only last record from IFLI (prevent duplicates when re-sending errors)
  SORT lt_dsel BY rbkp_belnr DESCENDING
                  rbkp_gjahr DESCENDING
                  ifli_rdate DESCENDING
                  ifli_rtime DESCENDING.
  DELETE ADJACENT DUPLICATES FROM lt_dsel COMPARING rbkp_belnr
                                                    rbkp_gjahr
                                                    ifli_rdate
                                                    ifli_rtime.

  &quot;do not selecte invoice+cancelation together
  &quot;(1st it makes no sense, send invoice and immediatelly/together cancellation..
  &quot; ..and 2nd it messes up with IFLI log, since cancellations are sent under orig-invo number)
  LOOP AT lt_dsel ASSIGNING FIELD-SYMBOL(&lt;dseli&gt;) WHERE rbkp_stblg IS NOT INITIAL
                                                    AND rbkp_stjah IS NOT INITIAL
                                                    AND rbkp_ivtyp NE &apos;5&apos;.                       &quot;&lt;dseli&gt;= invoice, cancelled
    READ TABLE lt_dsel ASSIGNING FIELD-SYMBOL(&lt;dselc&gt;) WITH KEY rbkp_belnr = &lt;dseli&gt;-rbkp_stblg
                                                                rbkp_gjahr = &lt;dseli&gt;-rbkp_stjah
                                                                rbkp_ivtyp = &apos;5&apos;.                 &quot;&lt;dselc&gt;= cancellation
    IF sy-subrc = 0.
      CLEAR: &lt;dseli&gt;,
             &lt;dselc&gt;.
    ENDIF.
  ENDLOOP.
  DELETE lt_dsel WHERE rbkp_belnr IS INITIAL.

  &quot;remove not for Commission (by Dealer/SalesMan)
  DELETE lt_dsel WHERE vdlr_dlrid IS INITIAL AND
                       vslm_slmid IS INITIAL    .

  &quot;remove not for Commission (by Commission POrd)
  IF lt_dsel[] IS NOT INITIAL.
    SELECT rseg~belnr,
           rseg~gjahr,
           rseg~buzei,
           rseg~ebeln,
           ekko~bsart,
           ekpo~ebelp,
           ekpo~bednr
      INTO TABLE @DATA(lt_ditm)
      FROM            rseg AS rseg
           INNER JOIN ekko AS ekko ON ekko~ebeln = rseg~ebeln
           INNER JOIN ekpo AS ekpo ON ekpo~ebeln = rseg~ebeln AND
                                      ekpo~ebelp = rseg~ebelp
      FOR ALL ENTRIES IN @lt_dsel
      WHERE   rseg~belnr = @lt_dsel-rbkp_belnr
        AND   rseg~gjahr = @lt_dsel-rbkp_gjahr
        AND ( ekko~bsart = @ls_c00d-bsart OR    &quot;POrd type: commission Dealer/SalesMan
              ekko~bsart = @ls_c00s-bsart   )
        AND   ekpo~bednr &lt;&gt; &apos;&apos;.                 &quot;POrd item TracingNo: contains ref to CommID

    &quot;for each selected invoice..
    LOOP AT lt_dsel ASSIGNING FIELD-SYMBOL(&lt;dsel&gt;).
      &quot;..check if item selected (so filtered by POrd DocType &amp; ref to CommID)
      READ TABLE lt_ditm WITH KEY belnr = &lt;dsel&gt;-rbkp_belnr
                                  gjahr = &lt;dsel&gt;-rbkp_gjahr TRANSPORTING NO FIELDS.
      IF sy-subrc &lt;&gt; 0.
        &quot;..if missing, remove
        CLEAR: &lt;dsel&gt;.
      ENDIF.
    ENDLOOP.
    DELETE lt_dsel WHERE rbkp_belnr IS INITIAL. &quot;delete deleted


  ENDIF.

  &quot;remove already sent with Success
  DELETE lt_dsel WHERE ifli_statu = zif_vncsi_gc=&gt;statu_s.

  &quot;remove already sent with Errors
  IF pa_rerr NE &apos;X&apos;.
    DELETE lt_dsel WHERE ifli_statu = zif_vncsi_gc=&gt;statu_e.
  ENDIF.

  &quot;pass to sel-tab
  LOOP AT lt_dsel ASSIGNING &lt;dsel&gt;.
    APPEND INITIAL LINE TO gt_isel ASSIGNING FIELD-SYMBOL(&lt;isel&gt;).
    MOVE: &lt;dsel&gt;-rbkp_belnr TO &lt;isel&gt;-belnr,
          &lt;dsel&gt;-rbkp_gjahr TO &lt;isel&gt;-gjahr.
  ENDLOOP.
  SORT gt_isel.
  DELETE ADJACENT DUPLICATES FROM gt_isel.


  &quot;write count
  DESCRIBE TABLE gt_isel LINES DATA(lv_lines).
  WRITE: /,/ &apos;*** Data selection:&apos;.
  WRITE: / &apos; Selected records for processing: &apos;, lv_lines.

  &quot;check anything to process
  IF lv_lines = 0.
    WRITE: / &apos; No data for processing, leaving report.&apos;.
    STOP.
  ENDIF.

ENDFORM.

*&amp;---------------------------------------------------------------------*
*&amp; Form write_wisl
*&amp;---------------------------------------------------------------------*
*&amp; write selected documents
*&amp;---------------------------------------------------------------------*
FORM write_wisl .

  CHECK pa_wisl = &apos;X&apos;.

  WRITE: /,/ &apos;*** Selected invoices:&apos;.

  LOOP AT gt_isel ASSIGNING FIELD-SYMBOL(&lt;isel&gt;).
    WRITE: / &apos;                      &apos;, &lt;isel&gt;-belnr, &lt;isel&gt;-gjahr.

  ENDLOOP.


ENDFORM.


*&amp;---------------------------------------------------------------------*
*&amp; Form data_process
*&amp;---------------------------------------------------------------------*
*&amp; process selected data
*&amp;---------------------------------------------------------------------*
FORM data_process .

  CALL FUNCTION &apos;ZVNCSI_IF_DCS_INV&apos;
    EXPORTING
      iv_port = pa_port
      it_isel = gt_isel
      ix_test = pa_test
    IMPORTING
      et_ifli = gt_ifli
      et_req  = gt_req
      et_rsp  = gt_rsp.

ENDFORM.


*&amp;---------------------------------------------------------------------*
*&amp; Form write_wifl
*&amp;---------------------------------------------------------------------*
*&amp; write IFLI log
*&amp;---------------------------------------------------------------------*
FORM write_wifl .

  CHECK pa_wifl = &apos;X&apos;.

  WRITE: /,/ &apos;*** IFLI log:&apos;.

  LOOP AT gt_ifli ASSIGNING FIELD-SYMBOL(&lt;ifli&gt;).
    WRITE: / &apos;                      &apos;, &lt;ifli&gt;-rdate,
                                       &lt;ifli&gt;-rtime,
                                       &lt;ifli&gt;-dealr,
                                       &lt;ifli&gt;-slman,
                                       &lt;ifli&gt;-inv_doc_no,
                                       &lt;ifli&gt;-inv_doc_yr,
                                       &lt;ifli&gt;-statu,
                                       &lt;ifli&gt;-msgid,
                                       &lt;ifli&gt;-msgno,
                                       &lt;ifli&gt;-msgty,
                                       &lt;ifli&gt;-msgtx.

  ENDLOOP.

ENDFORM.


*&amp;---------------------------------------------------------------------*
*&amp; Form write_wreq
*&amp;---------------------------------------------------------------------*
*&amp; write ws-Request
*&amp;---------------------------------------------------------------------*
FORM write_wreq .

  CHECK pa_wreq = &apos;X&apos;.

  WRITE: /,/ &apos;*** ws-Request data:&apos;.

  LOOP AT gt_req ASSIGNING FIELD-SYMBOL(&lt;req&gt;).

    WRITE: / &apos;                      &apos;, &apos;DEALER_CODE :&apos;, &lt;req&gt;-dealer_code,
           / &apos;                      &apos;, &apos;SMAN_CODE   :&apos;, &lt;req&gt;-sman_code,
           / &apos;                      &apos;, &apos;INV_DOC_NO  :&apos;, &lt;req&gt;-inv_doc_no,
           / &apos;                      &apos;, &apos;INV_DOC_YR  :&apos;, &lt;req&gt;-inv_doc_yr,
           / &apos;                      &apos;, &apos;POST_DATE   :&apos;, &lt;req&gt;-post_date,
           / &apos;                      &apos;, &apos;REFERENCE   :&apos;, &lt;req&gt;-reference,
           / &apos;                      &apos;, &apos;SERIAL      :&apos;, &lt;req&gt;-serial,
           / &apos;                      &apos;, &apos;STATUS      :&apos;, &lt;req&gt;-status,
           / &apos;                      &apos;, &apos;AMNT_TOT    :&apos;, &lt;req&gt;-amnt_tot,
           / &apos;                      &apos;, &apos;CURRENCY    :&apos;, &lt;req&gt;-currency,
           / &apos;                      &apos;, &apos;INV_TYPE    :&apos;, &lt;req&gt;-inv_type,
           / &apos;                      &apos;, &apos;VAT_RATE    :&apos;, &lt;req&gt;-vat_rate.

    LOOP AT &lt;req&gt;-st_inv_req_itm ASSIGNING FIELD-SYMBOL(&lt;rit&gt;).
      WRITE: / &apos;                               &apos;, &apos;COMM_ID   :&apos;, &lt;rit&gt;-comm_id,
             / &apos;                               &apos;, &apos;AMNT_ITM  :&apos;, &lt;rit&gt;-amnt_itm.
    ENDLOOP.

  ENDLOOP.

ENDFORM.

*&amp;---------------------------------------------------------------------*
*&amp; Form write_wrsp
*&amp;---------------------------------------------------------------------*
*&amp; write ws-Response
*&amp;---------------------------------------------------------------------*
FORM write_wrsp .

  CHECK pa_wrsp = &apos;X&apos;.

  WRITE: /,/ &apos;*** ws-Response data:&apos;.

  LOOP AT gt_rsp ASSIGNING FIELD-SYMBOL(&lt;rsp&gt;).

    WRITE: / &apos;                      &apos;, &apos;DEALER_CODE :&apos;, &lt;rsp&gt;-dealer_code,
           / &apos;                      &apos;, &apos;SMAN_CODE   :&apos;, &lt;rsp&gt;-sman_code,
           / &apos;                      &apos;, &apos;INV_DOC_NO  :&apos;, &lt;rsp&gt;-inv_doc_no,
           / &apos;                      &apos;, &apos;INV_DOC_YR  :&apos;, &lt;rsp&gt;-inv_doc_yr,
           / &apos;                      &apos;, &apos;POSTED      :&apos;, &lt;rsp&gt;-posted,
           / &apos;                      &apos;, &apos;MSG_TEXT    :&apos;, &lt;rsp&gt;-msg_text.



  ENDLOOP.

ENDFORM.</source>
