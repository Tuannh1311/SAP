  <method CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="BAPI_PARKPOST" VERSION="1" LANGU="E" DESCRIPT="func: post Parked" EXPOSURE="0" STATE="1" EDITORDER="41 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="BAPI_PARKPOST" SCONAME="IV_BUKRS" VERSION="1" LANGU="E" DESCRIPT="Company Code" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="BUKRS"/>
   <parameter CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="BAPI_PARKPOST" SCONAME="IV_BELNR" VERSION="1" LANGU="E" DESCRIPT="Accounting Document Number" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="BELNR_D"/>
   <parameter CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="BAPI_PARKPOST" SCONAME="IV_GJAHR" VERSION="1" LANGU="E" DESCRIPT="Fiscal Year" CMPTYPE="1" MTDTYPE="0" EDITORDER="3 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="GJAHR"/>
   <parameter CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="BAPI_PARKPOST" SCONAME="EX_POST" VERSION="1" LANGU="E" DESCRIPT="X= ok, _= fail" CMPTYPE="1" MTDTYPE="0" EDITORDER="4 " DISPID="0 " PARDECLTYP="1" PARPASSTYP="1" TYPTYPE="1" TYPE="XFELD"/>
   <source>METHOD bapi_parkpost.

    CLEAR: ex_post.

    &quot;call bapi: Post Parked document
    &quot;..wait, there is NO bapi to post parked?
    &quot;see note: 2092366 - Parking with BAPI_ACC_DOCUMENT_POST
    &quot;&gt;&gt;How can I continue to edit the documents parked using the BAPI?
    &quot;&gt;&gt;There are no BAPIs that you can use to post, edit or delete a parked document.
    &quot;&gt;&gt;You must therefore carry out any further processing of the document using transaction FBV0 or FBV2.

    &quot;anyway, logic/function taken from workflow/bor-object FIPP &quot;Parked Document&quot;

    DATA: lt_vbkpf TYPE TABLE OF vbkpf,
          lt_msgt  TYPE TABLE OF msg_tab_line.

    SELECT SINGLE * FROM vbkpf INTO @DATA(ls_vbkpf) WHERE bukrs = @iv_bukrs
                                                      AND belnr = @iv_belnr
                                                      AND gjahr = @iv_gjahr . CHECK sy-subrc = 0.
    APPEND ls_vbkpf TO lt_vbkpf.

    CALL FUNCTION &apos;PRELIMINARY_POSTING_POST_ALL&apos;
      EXPORTING
*       BUPBI     = &apos; &apos;
        nomsg     = &apos;X&apos;
*       SYNCH     = &apos; &apos;
*       NOCHECK   = &apos; &apos;
*       APPL_LOG  = &apos; &apos;
*       I_NO_AUTH_CHECK          = &apos; &apos;
*     IMPORTING
*       E_LOG_HANDLE             = E_LOG_HANDLE
*       ES_DISPLAY_PROFILE       = ES_DISPLAY_PROFILE
      TABLES
        t_vbkpf   = lt_vbkpf
*       T_MSG     = T_MSG
        t_msg_tab = lt_msgt.

    &quot;check errors
    LOOP AT lt_msgt ASSIGNING FIELD-SYMBOL(&lt;msgt&gt;) WHERE msgty = &apos;E&apos;.

      &quot;message to user
      MESSAGE ID &lt;msgt&gt;-msgid TYPE &apos;I&apos; NUMBER &lt;msgt&gt;-msgno WITH &lt;msgt&gt;-msgv1 &lt;msgt&gt;-msgv2 &lt;msgt&gt;-msgv3 &lt;msgt&gt;-msgv4 DISPLAY LIKE &apos;E&apos;.

    ENDLOOP.

    &quot;success IFF explicit message (yeah, I&apos;m quite desperate here)
    LOOP AT lt_msgt ASSIGNING &lt;msgt&gt; WHERE msgid = &apos;F5&apos;
                                       AND msgno = &apos;312&apos;. &quot;F5-312= msg{Document &amp; was posted in company code &amp;}
      MOVE: &apos;X&apos; TO ex_post.
    ENDLOOP.

    &quot;no commit needed, as the fm above has its own

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="BAPI_POSTREVE" VERSION="1" LANGU="E" DESCRIPT="func: reverse Posted" EXPOSURE="0" STATE="1" EDITORDER="42 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="BAPI_POSTREVE" SCONAME="IV_BUKRS" VERSION="1" LANGU="E" DESCRIPT="Company Code" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="BUKRS"/>
   <parameter CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="BAPI_POSTREVE" SCONAME="IV_BELNR" VERSION="1" LANGU="E" DESCRIPT="Accounting Document Number" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="BELNR_D"/>
   <parameter CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="BAPI_POSTREVE" SCONAME="IV_GJAHR" VERSION="1" LANGU="E" DESCRIPT="Fiscal Year" CMPTYPE="1" MTDTYPE="0" EDITORDER="3 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="GJAHR"/>
   <parameter CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="BAPI_POSTREVE" SCONAME="IV_STCNC" VERSION="1" LANGU="E" DESCRIPT="CSI: Subvention Cancellation Reason (char255)" CMPTYPE="1" MTDTYPE="0" EDITORDER="4 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="ZVNCSI_SUBV_TCANCEL" PAROPTIONL="X"/>
   <parameter CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="BAPI_POSTREVE" SCONAME="EX_POST" VERSION="1" LANGU="E" DESCRIPT="X= ok, _= fail" CMPTYPE="1" MTDTYPE="0" EDITORDER="5 " DISPID="0 " PARDECLTYP="1" PARPASSTYP="1" TYPTYPE="1" TYPE="XFELD"/>
   <parameter CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="BAPI_POSTREVE" SCONAME="EV_BUKRS" VERSION="1" LANGU="E" DESCRIPT="Company Code" CMPTYPE="1" MTDTYPE="0" EDITORDER="6 " DISPID="0 " PARDECLTYP="1" PARPASSTYP="1" TYPTYPE="1" TYPE="BUKRS"/>
   <parameter CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="BAPI_POSTREVE" SCONAME="EV_BELNR" VERSION="1" LANGU="E" DESCRIPT="Accounting Document Number" CMPTYPE="1" MTDTYPE="0" EDITORDER="7 " DISPID="0 " PARDECLTYP="1" PARPASSTYP="1" TYPTYPE="1" TYPE="BELNR_D"/>
   <parameter CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="BAPI_POSTREVE" SCONAME="EV_GJARH" VERSION="1" LANGU="E" DESCRIPT="Fiscal Year" CMPTYPE="1" MTDTYPE="0" EDITORDER="8 " DISPID="0 " PARDECLTYP="1" PARPASSTYP="1" TYPTYPE="1" TYPE="GJAHR"/>
   <source>METHOD bapi_postreve.

    CLEAR: ex_post.

    &quot;
    DATA: ls_rev   TYPE bapiacrev,
          lt_ret   TYPE TABLE OF bapiret2,
          lv_otype TYPE bapiacrev-obj_type,
          lv_okey  TYPE bapiacrev-obj_key,
          lv_osys  TYPE bapiacrev-obj_sys.

    &quot;get original posting date
    SELECT SINGLE budat INTO @DATA(lv_budat) FROM bkpf WHERE bukrs = @iv_bukrs
                                                         AND belnr = @iv_belnr
                                                         AND gjahr = @iv_gjahr. ASSERT sy-subrc = 0.

    MOVE: &apos;BKPFF&apos;   TO ls_rev-obj_type,
          iv_bukrs  TO ls_rev-comp_code,
          &apos;01&apos;      TO ls_rev-reason_rev,
          lv_budat  TO ls_rev-pstng_date.
    CONCATENATE iv_belnr iv_bukrs iv_gjahr INTO ls_rev-obj_key.
    CONCATENATE iv_belnr iv_bukrs iv_gjahr INTO ls_rev-obj_key_r.

    CALL FUNCTION &apos;BAPI_ACC_DOCUMENT_REV_POST&apos;
      EXPORTING
        reversal = ls_rev
        bus_act  = &apos;RFBU&apos;
      IMPORTING
        obj_type = lv_otype
        obj_key  = lv_okey
        obj_sys  = lv_osys
      TABLES
        return   = lt_ret.

    &quot;..errors
    LOOP AT lt_ret ASSIGNING FIELD-SYMBOL(&lt;ret&gt;) WHERE type = &apos;E&apos;.
      CALL FUNCTION &apos;BAPI_TRANSACTION_ROLLBACK&apos;.
      MESSAGE ID &lt;ret&gt;-id TYPE &apos;I&apos; NUMBER &lt;ret&gt;-number WITH &lt;ret&gt;-message_v1
                                                            &lt;ret&gt;-message_v2
                                                            &lt;ret&gt;-message_v3
                                                            &lt;ret&gt;-message_v4 DISPLAY LIKE &apos;E&apos;. &quot;(note: msgty= I to make it popup &amp; wait)
    ENDLOOP.
    &quot;..commit when no errors
    IF sy-subrc NE 0.

      CALL FUNCTION &apos;BAPI_TRANSACTION_COMMIT&apos; EXPORTING wait = &apos;X&apos;.
      MOVE: &apos;X&apos; TO ex_post.
      ev_belnr = lv_okey(10).
      ev_bukrs = lv_okey+10(4).
      ev_gjarh = lv_okey+14(4).


      &quot;add text adj-reason = cancel-reason
      IF iv_stcnc IS NOT INITIAL.

        &quot;..prep text table
        DATA: lt_t500 TYPE TABLE OF zvncsi_text500.
        APPEND INITIAL LINE TO lt_t500 ASSIGNING FIELD-SYMBOL(&lt;t500&gt;).
        MOVE: iv_stcnc TO &lt;t500&gt;.
        DATA: lt_tlin TYPE TABLE OF tline.
        CALL FUNCTION &apos;CONVERT_STREAM_TO_ITF_TEXT&apos;
          TABLES
            text_stream = lt_t500
            itf_text    = lt_tlin.

        &quot;..create text
        DATA(lv_tdnam) = CONV thead-tdname( ev_bukrs &amp;&amp; ev_belnr &amp;&amp; ev_gjarh ).

        CALL FUNCTION &apos;CREATE_TEXT&apos;
          EXPORTING
            fid       = &apos;VN01&apos;      &quot;VN01= Adjustment reason (here used as Cancelation Reason)
            flanguage = sy-langu
            fname     = lv_tdnam
            fobject   = &apos;BELEG&apos;
*           SAVE_DIRECT       = &apos;X&apos;
*           FFORMAT   = &apos;*&apos;
          TABLES
            flines    = lt_tlin
          EXCEPTIONS
            no_init   = 1
            no_save   = 2
            OTHERS    = 3.
        IF sy-subrc &lt;&gt; 0.
          &quot;text to LT_RET?
        ENDIF.

        CALL FUNCTION &apos;COMMIT_TEXT&apos;.

      ENDIF.

    ENDIF.

    &quot;dequeue after posting (bapi keeps hanging lock)
    CALL FUNCTION &apos;DEQUEUE_EFBKPF&apos;
      EXPORTING
*       MODE_BKPF       = &apos;E&apos;
*       MANDT = SY-MANDT
        bukrs = iv_bukrs
        belnr = iv_belnr
        gjahr = iv_gjahr
*       X_BUKRS         = &apos; &apos;
*       X_BELNR         = &apos; &apos;
*       X_GJAHR         = &apos; &apos;
*       _SCOPE          = &apos;3&apos;
*       _SYNCHRON       = &apos; &apos;
*       _COLLECT        = &apos; &apos;
      .



  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="CALL_CDDO_DIS" VERSION="1" LANGU="E" DESCRIPT="call: report change documents" EXPOSURE="0" STATE="1" EDITORDER="38 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="CALL_CDDO_DIS" SCONAME="IR_SDAT" VERSION="1" LANGU="E" DESCRIPT="CSI: Subvention report (salv)" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="TS_SDAT"/>
   <source>METHOD call_cddo_dis.

    CHECK: ir_sdat IS BOUND.

    DATA(lv_objid) = CONV cdhdr-objectid( ir_sdat-&gt;*-_key ).


    SUBMIT rsscd100 WITH objekt   = &apos;ZVNCSI_IFLS&apos;
                    WITH objektid = lv_objid      AND RETURN.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="CALL_FB03_DOC" VERSION="1" LANGU="E" DESCRIPT="call: FIdoc display: main document" EXPOSURE="0" STATE="1" EDITORDER="36 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="CALL_FB03_DOC" SCONAME="IR_SDAT" VERSION="1" LANGU="E" DESCRIPT="CSI: DCS Commission-SalesMan report (salv)" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="TS_SDAT"/>
   <source>METHOD call_fb03_doc.

    &quot;call FB03 display FIdon
    CHECK: ir_sdat IS BOUND.

    CHECK: ir_sdat-&gt;*-belnr IS NOT INITIAL,
           ir_sdat-&gt;*-bukrs IS NOT INITIAL,
           ir_sdat-&gt;*-gjahr IS NOT INITIAL.


    SET PARAMETER ID &apos;BLN&apos; FIELD ir_sdat-&gt;*-belnr.
    SET PARAMETER ID &apos;BUK&apos; FIELD ir_sdat-&gt;*-bukrs.
    SET PARAMETER ID &apos;GJR&apos; FIELD ir_sdat-&gt;*-gjahr.

    TRY.
        CALL TRANSACTION &apos;FB03&apos; WITH AUTHORITY-CHECK AND SKIP FIRST SCREEN.
      CATCH cx_sy_authorization_error.
        MESSAGE i023(zvncsi) DISPLAY LIKE &apos;E&apos;. &quot;msg{Authorisation missing (see SU53)!}
    ENDTRY.



  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="CALL_FB03_REF" VERSION="1" LANGU="E" DESCRIPT="call: FIdoc display: reference document" EXPOSURE="0" STATE="1" EDITORDER="37 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="CALL_FB03_REF" SCONAME="IR_SDAT" VERSION="1" LANGU="E" DESCRIPT="CSI: DCS Commission-SalesMan report (salv)" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="TS_SDAT"/>
   <source>METHOD call_fb03_ref.

    &quot;call FB03 display FIdon
    CHECK: ir_sdat IS BOUND.

    CHECK: ir_sdat-&gt;*-ref_belnr IS NOT INITIAL,
           ir_sdat-&gt;*-ref_bukrs IS NOT INITIAL,
           ir_sdat-&gt;*-ref_gjahr IS NOT INITIAL.

    SET PARAMETER ID &apos;BLN&apos; FIELD ir_sdat-&gt;*-ref_belnr.
    SET PARAMETER ID &apos;BUK&apos; FIELD ir_sdat-&gt;*-ref_bukrs.
    SET PARAMETER ID &apos;GJR&apos; FIELD ir_sdat-&gt;*-ref_gjahr.

    TRY.
        CALL TRANSACTION &apos;FB03&apos; WITH AUTHORITY-CHECK AND SKIP FIRST SCREEN.
      CATCH cx_sy_authorization_error.
        MESSAGE i023(zvncsi) DISPLAY LIKE &apos;E&apos;. &quot;msg{Authorisation missing (see SU53)!}
    ENDTRY.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="CONSTRUCTOR" VERSION="1" LANGU="E" DESCRIPT="constuctor" EXPOSURE="2" STATE="1" EDITORDER="1 " DISPID="0 " MTDTYPE="2" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="CONSTRUCTOR" SCONAME="IT_KEYS" VERSION="1" LANGU="E" DESCRIPT="ifls keys" CMPTYPE="1" MTDTYPE="2" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="TT_KEYS"/>
   <parameter CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="CONSTRUCTOR" SCONAME="IX_AUTO" VERSION="1" LANGU="E" DESCRIPT="auto-posting" CMPTYPE="1" MTDTYPE="2" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="XFELD"/>
   <source>METHOD constructor.

    me-&gt;at_keys = it_keys.
    me-&gt;ax_auto = ix_auto.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="CONV_AMNT" VERSION="1" LANGU="E" DESCRIPT="convert amount" EXPOSURE="0" STATE="1" EDITORDER="39 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="1" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="CONV_AMNT" SCONAME="IV_WRBTR" VERSION="1" LANGU="E" DESCRIPT="Amount in Document Currency" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="EKBE-WRBTR"/>
   <parameter CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="CONV_AMNT" SCONAME="IV_WAERS" VERSION="1" LANGU="E" DESCRIPT="Currency Key" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="TCURC-WAERS"/>
   <parameter CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="CONV_AMNT" SCONAME="RT_AMNT" VERSION="1" LANGU="E" DESCRIPT="Numeric fifteen-digit value" CMPTYPE="1" MTDTYPE="0" EDITORDER="3 " DISPID="0 " PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="NUM15"/>
   <source>METHOD conv_amnt.

    &quot;convert currency field to numc15

    DATA: lv_amnt1 TYPE wmto_s-amount,
          lv_amnt2 TYPE wmto_s-amount.

    TRY.
        lv_amnt1 = CONV #( iv_wrbtr ).

        CALL FUNCTION &apos;CURRENCY_AMOUNT_SAP_TO_DISPLAY&apos;
          EXPORTING
            currency        = iv_waers
            amount_internal = lv_amnt1
          IMPORTING
            amount_display  = lv_amnt2
          EXCEPTIONS
            internal_error  = 1
            OTHERS          = 2.
        IF sy-subrc &lt;&gt; 0. MESSAGE ID sy-msgid TYPE &apos;I&apos; NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4 DISPLAY LIKE &apos;E&apos;. ENDIF.

        rt_amnt = CONV #( lv_amnt2 ).

      CATCH cx_root.    &quot;+SAP-3631 dump overflow
        IF iv_waers = &apos;VND&apos;.
          rt_amnt = lv_amnt1 * 100.
        ENDIF.
    ENDTRY.



  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="IFLS_SETSTAT" VERSION="1" LANGU="E" DESCRIPT="ifls: set status" EXPOSURE="0" STATE="1" EDITORDER="40 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="1" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="IFLS_SETSTAT" SCONAME="IV_SNEW" VERSION="1" LANGU="E" DESCRIPT="CSI: Subvention status" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="ZVNCSI_SBSTA"/>
   <parameter CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="IFLS_SETSTAT" SCONAME="IS_SKEY" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="ZVNCSI_SUB_IFLS_KEY"/>
   <source>METHOD ifls_setstat.

    &quot;**set IFLS new-status

    &quot;..lock record(s, cross all party/subventions)
    CALL FUNCTION &apos;ENQUEUE_EZVNCSI_IFLS&apos;
      EXPORTING
        parid          = is_skey-parid
        party          = is_skey-party
        sbvid          = is_skey-sbvid
      EXCEPTIONS
        foreign_lock   = 1
        system_failure = 2
        OTHERS         = 3.
    IF sy-subrc &lt;&gt; 0.
      MESSAGE ID sy-msgid TYPE &apos;I&apos; NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4 DISPLAY LIKE &apos;E&apos;. &quot;(note: msgty= I to make it popup &amp; wait)
    ENDIF.

    &quot;..change db record
    SELECT SINGLE * FROM zvncsi_sub_ifls INTO @DATA(ls_ifls) WHERE rdate = @is_skey-rdate
                                                               AND rtime = @is_skey-rtime
                                                               AND parid = @is_skey-parid
                                                               AND party = @is_skey-party
                                                               AND sbvid = @is_skey-sbvid. ASSERT sy-subrc = 0.
    DATA(ls_ifls_old) = ls_ifls.
    DATA(ls_ifls_new) = ls_ifls.

    MOVE: iv_snew TO ls_ifls_new-sbsta.

    MODIFY zvncsi_sub_ifls FROM ls_ifls_new.

    &quot;..change document
    CALL FUNCTION &apos;ZVNCSI_IFLS_WRITE_DOCUMENT&apos;
      EXPORTING
        objectid            = CONV cdhdr-objectid( ls_ifls_old-_key )
        tcode               = sy-tcode
        utime               = sy-uzeit
        udate               = sy-datum
        username            = sy-uname
        &quot;PLANNED_CHANGE_NUMBER         = &apos; &apos;
        &quot;OBJECT_CHANGE_INDICATOR       = &apos;U&apos;
        &quot;PLANNED_OR_REAL_CHANGES       = &apos; &apos;
        &quot;NO_CHANGE_POINTERS  = &apos; &apos;
        n_zvncsi_sub_ifls   = ls_ifls_new
        o_zvncsi_sub_ifls   = ls_ifls_old
        upd_zvncsi_sub_ifls = &apos;U&apos;.


    &quot;..commit
    CALL FUNCTION &apos;BAPI_TRANSACTION_COMMIT&apos; EXPORTING wait = &apos;X&apos;.


    &quot;..ulock
    CALL FUNCTION &apos;DEQUEUE_EZVNCSI_IFLS&apos;
      EXPORTING
        parid = is_skey-parid
        party = is_skey-party
        sbvid = is_skey-sbvid.




  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="ON_AUTO" VERSION="1" LANGU="E" DESCRIPT="func: auto-posting (from sel-screen)" EXPOSURE="0" STATE="1" EDITORDER="27 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD on_auto.
    &quot;func: auto-posting (from sel-screen)
    &quot;+CBL-13606(HQ_TTa) Subvention auto-posting

    CHECK me-&gt;ax_auto = &apos;X&apos;.

    &quot;mark all rows as selected
    DATA: lt_rsel TYPE salv_t_row.
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).
      APPEND INITIAL LINE TO lt_rsel ASSIGNING FIELD-SYMBOL(&lt;rsel&gt;).
      MOVE: sy-tabix TO &lt;rsel&gt;.
    ENDLOOP.
    me-&gt;ar_salv-&gt;get_selections( )-&gt;set_selected_rows( lt_rsel ).
    me-&gt;at_rsel = me-&gt;ar_salv-&gt;get_selections( )-&gt;get_selected_rows( ).

    &quot;call posting
    me-&gt;on_ucom_parkpost( ).

    &quot;refresh
    me-&gt;salv_refr( ).


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="ON_DBCL" VERSION="1" LANGU="E" DESCRIPT="handler: on double-click" EXPOSURE="0" STATE="1" EDITORDER="26 " DISPID="0 " MTDTYPE="1" MTDDECLTYP="0" REFCLSNAME="CL_SALV_EVENTS_TABLE" REFCMPNAME="DOUBLE_CLICK" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="ON_DBCL" SCONAME="ROW" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="1" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="0"/>
   <parameter CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="ON_DBCL" SCONAME="COLUMN" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="1" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="0"/>
   <source>METHOD on_dbcl.

    &quot;get double-clicked row
    READ TABLE me-&gt;at_sdat REFERENCE INTO DATA(lr_sdat) INDEX row.
    CHECK sy-subrc = 0.

    CASE column.

      WHEN &apos;REF_BUKRS&apos;
        OR &apos;REF_BELNR&apos;
        OR &apos;REF_GJAHR&apos;.
        &quot;call display ref-doc
        me-&gt;call_fb03_ref( ir_sdat = lr_sdat ).

      WHEN OTHERS.

        &quot;special case: process RPOST &apos;reverse posted&apos; (no parked document)
        IF lr_sdat-&gt;*-proct =  zif_vncsi_gc=&gt;proct_rpost AND
           lr_sdat-&gt;*-belnr IS INITIAL                      . &quot;(but only before processing)

          MESSAGE i022(zvncsi). &quot;msg{Request to reverse posted document: original document displayed.}

          &quot;call display ref-doc
          me-&gt;call_fb03_ref( ir_sdat = lr_sdat ).
          RETURN.

        ENDIF.

        &quot;normal case:
        &quot;call display main-doc
        me-&gt;call_fb03_doc( ir_sdat = lr_sdat ).

    ENDCASE.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="ON_UCOM" VERSION="1" LANGU="E" DESCRIPT="handler: on user-command" EXPOSURE="0" STATE="1" EDITORDER="28 " DISPID="0 " MTDTYPE="1" MTDDECLTYP="0" REFCLSNAME="CL_SALV_EVENTS" REFCMPNAME="ADDED_FUNCTION" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="ON_UCOM" SCONAME="E_SALV_FUNCTION" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="1" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="0"/>
   <source>METHOD on_ucom.

    &quot;default: get selected rows
    me-&gt;at_rsel = me-&gt;ar_salv-&gt;get_selections( )-&gt;get_selected_rows( ).


    &quot;user-command
    CASE e_salv_function.

      WHEN &apos;REFRESH&apos;. me-&gt;salv_refr( ).

      WHEN &apos;SETREV&apos;.  me-&gt;on_ucom_setrev( ).
      WHEN &apos;REVREV&apos;.  me-&gt;on_ucom_revrev( ).

      WHEN &apos;SETAPP&apos;.  me-&gt;on_ucom_setapp( ).
      WHEN &apos;REVAPP&apos;.  me-&gt;on_ucom_revapp( ).


      WHEN &apos;INVPOST&apos;. me-&gt;on_ucom_parkpost( ).
      WHEN &apos;REVPOST&apos;. me-&gt;on_ucom_postreve( ).

      WHEN &apos;CDLOG&apos;.   me-&gt;on_ucom_cdlog( ).

    ENDCASE.

    &quot;refresh after each ucom (show actual/change data to user)
    me-&gt;salv_refr( ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="ON_UCOM_CDLOG" VERSION="1" LANGU="E" DESCRIPT="func: call change log" EXPOSURE="0" STATE="1" EDITORDER="35 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD on_ucom_cdlog.

    &quot;call change log
    DATA(lr_sdat) =  me-&gt;salv_line_get( ).

    CHECK lr_sdat IS BOUND.
    me-&gt;call_cddo_dis( ir_sdat = lr_sdat ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="ON_UCOM_PARKPOST" VERSION="1" LANGU="E" DESCRIPT="func: post Parked" EXPOSURE="0" STATE="1" EDITORDER="33 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD on_ucom_parkpost.

    &quot;set status Approved

    &quot;..check auth: status 5P= Posted
    AUTHORITY-CHECK OBJECT &apos;ZCSISBSTA&apos; ID &apos;ZCSISBSTA&apos; FIELD zif_vncsi_gc=&gt;sbsta_5p.
    IF sy-subrc &lt;&gt; 0.
      MESSAGE w015(zvncsi) WITH zif_vncsi_gc=&gt;sbsta_5p DISPLAY LIKE &apos;E&apos;. &quot;msg{Missing authorisation for status &amp; (see SU53).}
      RETURN.
    ENDIF.


    &quot;..check: any lines selected
    IF me-&gt;at_rsel IS INITIAL.
      MESSAGE w013(zvncsi) DISPLAY LIKE &apos;E&apos;. &quot;msg{No lines selected for processing.}
      RETURN.
    ENDIF.

    &quot;+CBL-13606(HQ_TTa) Subvention auto-posting
    IF me-&gt;ax_auto NE &apos;X&apos;.
      &quot;..check: only lines with status 2A= approved
      LOOP AT me-&gt;at_rsel ASSIGNING FIELD-SYMBOL(&lt;rsel&gt;).
        READ TABLE me-&gt;at_sdat INDEX &lt;rsel&gt; ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).
        IF &lt;sdat&gt;-sbsta NE zif_vncsi_gc=&gt;sbsta_2a.
          MESSAGE w014(zvncsi) WITH zif_vncsi_gc=&gt;sbsta_2a DISPLAY LIKE &apos;E&apos;. &quot;msg{Select only lines with status &amp;}
          RETURN.
        ENDIF.
      ENDLOOP.
    ENDIF.


    &quot;..call bapi ParkPost
    LOOP AT me-&gt;at_rsel ASSIGNING &lt;rsel&gt;.
      READ TABLE me-&gt;at_sdat INDEX &lt;rsel&gt; ASSIGNING &lt;sdat&gt;. ASSERT sy-subrc = 0.

      IF &lt;sdat&gt;-proct = zif_vncsi_gc=&gt;proct_rpost.
        &quot;special case: process RPOST &apos;reverse posted&apos; (no parked document)
        me-&gt;bapi_postreve( EXPORTING iv_bukrs = &lt;sdat&gt;-ref_bukrs
                                     iv_belnr = &lt;sdat&gt;-ref_belnr
                                     iv_gjahr = &lt;sdat&gt;-ref_gjahr
                                     iv_stcnc = &lt;sdat&gt;-stcnc
                           IMPORTING ex_post  = DATA(ex_post)
                                     ev_bukrs = &lt;sdat&gt;-bukrs
                                     ev_belnr = &lt;sdat&gt;-belnr
                                     ev_gjarh = &lt;sdat&gt;-gjahr   ).
        &quot;+special update ifls, to be ready for select in setstatus
        &quot;(this is truly dumb, should be redesigned)
        UPDATE zvncsi_sub_ifls SET bukrs = &lt;sdat&gt;-bukrs
                                   belnr = &lt;sdat&gt;-belnr
                                   gjahr = &lt;sdat&gt;-gjahr
                               WHERE rdate = &lt;sdat&gt;-rdate
                                 AND rtime = &lt;sdat&gt;-rtime
                                 AND parid = &lt;sdat&gt;-parid
                                 AND party = &lt;sdat&gt;-party
                                 AND sbvid = &lt;sdat&gt;-sbvid.

        &quot;set status 5P Posted
        IF ex_post = &apos;X&apos;.
          me-&gt;ifls_setstat( iv_snew = zif_vncsi_gc=&gt;sbsta_5p
                            is_skey = &lt;sdat&gt;-_key            ).
        ENDIF.
        RETURN.
      ENDIF.

      &quot;normal case:
      &quot;bapi ParkPost
      me-&gt;bapi_parkpost( EXPORTING iv_bukrs = &lt;sdat&gt;-bukrs
                                   iv_belnr = &lt;sdat&gt;-belnr
                                   iv_gjahr = &lt;sdat&gt;-gjahr
                         IMPORTING ex_post  = ex_post   ).

      IF ex_post = &apos;X&apos;.
        &quot;set status 5P Posted: when posted
        me-&gt;ifls_setstat( iv_snew = zif_vncsi_gc=&gt;sbsta_5p
                          is_skey = &lt;sdat&gt;-_key            ).
      ENDIF.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="ON_UCOM_POSTREVE" VERSION="1" LANGU="E" DESCRIPT="***func: reverse Posted (??? cannot go back to parked)" EXPOSURE="0" STATE="1" EDITORDER="34 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD on_ucom_postreve.

    &quot;*** THIS CANNOT BE DONE ***

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="ON_UCOM_REVAPP" VERSION="1" LANGU="E" DESCRIPT="func: reverse status Approved" EXPOSURE="0" STATE="1" EDITORDER="32 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD on_ucom_revapp.

    &quot;reverse status 2A Approved

    &quot;..check auth: status 2A= Approved
    AUTHORITY-CHECK OBJECT &apos;ZCSISBSTA&apos; ID &apos;ZCSISBSTA&apos; FIELD zif_vncsi_gc=&gt;sbsta_2a.
    IF sy-subrc &lt;&gt; 0.
      MESSAGE w015(zvncsi) WITH zif_vncsi_gc=&gt;sbsta_2a DISPLAY LIKE &apos;E&apos;. &quot;msg{Missing authorisation for status &amp; (see SU53).}
      RETURN.
    ENDIF.


    &quot;..check: any lines selected
    IF me-&gt;at_rsel IS INITIAL.
      MESSAGE w013(zvncsi) DISPLAY LIKE &apos;E&apos;. &quot;msg{No lines selected for processing.}
      RETURN.
    ENDIF.

    &quot;..check: only lines with status 2A= Approved
    LOOP AT me-&gt;at_rsel ASSIGNING FIELD-SYMBOL(&lt;rsel&gt;).
      READ TABLE me-&gt;at_sdat INDEX &lt;rsel&gt; ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).
      IF &lt;sdat&gt;-sbsta NE zif_vncsi_gc=&gt;sbsta_2a.
        MESSAGE w014(zvncsi) WITH zif_vncsi_gc=&gt;sbsta_2a DISPLAY LIKE &apos;E&apos;. &quot;msg{Select only lines with status &amp;}
        RETURN.
      ENDIF.
    ENDLOOP.

    &quot;..call IFLS set-status: 1R Reviewed
    LOOP AT me-&gt;at_rsel ASSIGNING &lt;rsel&gt;.
      READ TABLE me-&gt;at_sdat INDEX &lt;rsel&gt; REFERENCE INTO DATA(lr_sdat) .
      me-&gt;ifls_setstat( EXPORTING iv_snew = zif_vncsi_gc=&gt;sbsta_1r
