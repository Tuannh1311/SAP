  <method CLSNAME="ZCL_VNCSI_DCS_IFPI" CMPNAME="CONV_AMNT" VERSION="1" LANGU="E" DESCRIPT="conv: amount per currency" EXPOSURE="0" STATE="1" EDITORDER="6 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="1" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_DCS_IFPI" CMPNAME="CONV_AMNT" SCONAME="IV_AMNT" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="RSEG-WRBTR"/>
   <parameter CLSNAME="ZCL_VNCSI_DCS_IFPI" CMPNAME="CONV_AMNT" SCONAME="IV_CURR" VERSION="1" LANGU="E" DESCRIPT="Currency Key" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="TCURC-WAERS"/>
   <parameter CLSNAME="ZCL_VNCSI_DCS_IFPI" CMPNAME="CONV_AMNT" SCONAME="RT_AMNT" VERSION="1" LANGU="E" DESCRIPT="dec13_2" CMPTYPE="1" MTDTYPE="0" EDITORDER="3 " DISPID="0 " PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="ZVNCSI_DEC13_2"/>
   <source>METHOD conv_amnt.

    &quot;convert currency

    DATA: lv_amnt1 TYPE wmto_s-amount,
          lv_amnt2 TYPE wmto_s-amount.

    TRY.

        lv_amnt1 = CONV #( iv_amnt ).

        CALL FUNCTION &apos;CURRENCY_AMOUNT_SAP_TO_DISPLAY&apos;
          EXPORTING
            currency        = iv_curr
            amount_internal = lv_amnt1
          IMPORTING
            amount_display  = lv_amnt2
          EXCEPTIONS
            internal_error  = 1
            OTHERS          = 2.
        IF sy-subrc &lt;&gt; 0. ENDIF.

        rt_amnt = CONV #( lv_amnt2 ).

      CATCH cx_root.    &quot;+SAP-3631 dump overflow
        IF iv_curr = &apos;VND&apos;.
          rt_amnt = lv_amnt1 * 100.
        ENDIF.
    ENDTRY.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPI" CMPNAME="CONV_VATR" VERSION="1" LANGU="E" DESCRIPT="conv: tax code to tax rate" EXPOSURE="0" STATE="1" EDITORDER="7 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="1" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_DCS_IFPI" CMPNAME="CONV_VATR" SCONAME="IV_BUKRS" VERSION="1" LANGU="E" DESCRIPT="Company Code" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="BUKRS"/>
   <parameter CLSNAME="ZCL_VNCSI_DCS_IFPI" CMPNAME="CONV_VATR" SCONAME="IV_MWSKZ" VERSION="1" LANGU="E" DESCRIPT="Tax on sales/purchases code" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="MWSKZ"/>
   <parameter CLSNAME="ZCL_VNCSI_DCS_IFPI" CMPNAME="CONV_VATR" SCONAME="IV_WAERS" VERSION="1" LANGU="E" DESCRIPT="Currency Key" CMPTYPE="1" MTDTYPE="0" EDITORDER="3 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="WAERS"/>
   <parameter CLSNAME="ZCL_VNCSI_DCS_IFPI" CMPNAME="CONV_VATR" SCONAME="RV_TAXRT" VERSION="1" LANGU="E" DESCRIPT="Tax Amount in Document Currency" CMPTYPE="1" MTDTYPE="0" EDITORDER="4 " DISPID="0 " PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="ZVNCSI_TXRT"/>
   <source>METHOD conv_vatr.


    DATA: lv_fwste TYPE fwste.

    CALL FUNCTION &apos;RECP_FI_TAX_CALCULATE&apos;
      EXPORTING
        ic_bukrs    = iv_bukrs
        ic_mwskz    = iv_mwskz
        ic_waers    = iv_waers
      IMPORTING
        ep_tax_rate = lv_fwste
      EXCEPTIONS
        not_found   = 1
        OTHERS      = 2.
    IF sy-subrc &lt;&gt; 0.      ENDIF.

    DATA: lv_char5 TYPE char5.

    MOVE: lv_fwste TO lv_char5,   &quot;(yes, this is silly..
          lv_char5 TO rv_taxrt.   &quot; ..I must be missing somethig obvious)

    REPLACE &apos;.&apos; IN rv_taxrt WITH space.
    CONDENSE rv_taxrt.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPI" CMPNAME="PREP_IFLI" VERSION="1" LANGU="E" DESCRIPT="prepare IFLI-log from response" EXPOSURE="0" STATE="1" EDITORDER="4 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD prep_ifli.

    &quot;prepare IFLI log from ws-response

    LOOP AT me-&gt;at_rsp  ASSIGNING FIELD-SYMBOL(&lt;rsp&gt;).

      APPEND INITIAL LINE TO me-&gt;at_ifli ASSIGNING FIELD-SYMBOL(&lt;ifli&gt;).

      MOVE: sy-datum            TO &lt;ifli&gt;-rdate,
            sy-uzeit            TO &lt;ifli&gt;-rtime,
            &lt;rsp&gt;-dealer_code   TO &lt;ifli&gt;-dealr,
            &lt;rsp&gt;-sman_code     TO &lt;ifli&gt;-slman,
            &lt;rsp&gt;-inv_doc_no    TO &lt;ifli&gt;-inv_doc_no,
            &lt;rsp&gt;-inv_doc_yr    TO &lt;ifli&gt;-inv_doc_yr,
            &lt;rsp&gt;-posted        TO &lt;ifli&gt;-statu,
            &lt;rsp&gt;-msg_text      TO &lt;ifli&gt;-msgtx.

      &quot;special: map back reversed documents
      READ TABLE me-&gt;at_req ASSIGNING FIELD-SYMBOL(&lt;req&gt;) WITH KEY dealer_code = &lt;rsp&gt;-dealer_code
                                                                   sman_code   = &lt;rsp&gt;-sman_code
                                                                   inv_doc_no  = &lt;rsp&gt;-inv_doc_no
                                                                   inv_doc_yr  = &lt;rsp&gt;-inv_doc_yr
                                                                   status      = &apos;R&apos;.
      IF sy-subrc = 0.
        READ TABLE me-&gt;at_revd ASSIGNING FIELD-SYMBOL(&lt;revd&gt;) WITH KEY stblg = &lt;ifli&gt;-inv_doc_no
                                                                       stjah = &lt;ifli&gt;-inv_doc_yr.
        IF sy-subrc = 0.
          &lt;ifli&gt;-inv_doc_no = &lt;revd&gt;-belnr.
          &lt;ifli&gt;-inv_doc_yr = &lt;revd&gt;-gjahr.
        ENDIF.
      ENDIF.

    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPI" CMPNAME="PREP_REQ" VERSION="1" LANGU="E" DESCRIPT="select Invoices, prepare request" EXPOSURE="0" STATE="1" EDITORDER="2 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD prep_req.

    &quot;select invoices
    &quot; per documents in AT_ISEL, prepare request data AT_REQ

    CHECK me-&gt;at_isel IS NOT INITIAL.

    &quot;* invoices per RBKP Header
    SELECT vdlr~vendor   AS vdlr_lifnr,
           vdlr~idnumber AS vdlr_dlrid,
           vslm~vendor   AS vslm_lifnr,
           vslm~idnumber AS vslm_slmid,
           rbkp~lifnr,
           rbkp~belnr,
           rbkp~gjahr,
           rbkp~budat,
           rbkp~xblnr,
           rbkp~bktxt,       &quot;note: BkTxt is form**serial
           rbkp~ivtyp,       &quot;note: 5= Cancellation
           rbkp~stblg,       &quot;reversed by (on both sides, reversed &amp; reversed-by)
           rbkp~stjah,
           rbkp~rbstat,      &quot;5= Posted
           rbkp~rmwwr,
           rbkp~waers,
           rbkp~xrech,
           rbkp~mwskz1,
           rbkp~bukrs
      INTO TABLE @DATA(lt_invo)
      FROM                 rbkp             AS rbkp
           LEFT OUTER JOIN zvncsi_dcs_c003  AS c003d ON c003d~party    =  @me-&gt;as_c00d-party
           LEFT OUTER JOIN zvncsi_exid_vend AS vdlr  ON vdlr~type      =  c003d~idtyp         AND &quot;++CBL-13741 CreditCashProvider
                                                        vdlr~vendor    =  rbkp~lifnr
           LEFT OUTER JOIN zvncsi_dcs_c003  AS c003s ON c003s~party    =  @me-&gt;as_c00s-party
           LEFT OUTER JOIN zvncsi_exid_vend AS vslm  ON vslm~type      =  c003s~idtyp     AND
                                                        vslm~vendor    =  rbkp~lifnr
      FOR ALL ENTRIES IN @me-&gt;at_isel
      WHERE rbkp~belnr = @me-&gt;at_isel-belnr
        AND rbkp~gjahr = @me-&gt;at_isel-gjahr
        AND rbkp~rbstat = &apos;5&apos;                &quot;5= Posted, to exclude parked/held/etc
        AND vdlr~vendor NE &apos;&apos;. &quot;(too many left joins?)


    CHECK lt_invo[] IS NOT INITIAL.

    &quot;special: get reversed documents (for later mapping back to IFLI)
    SELECT * FROM rbkp AS rbkp INTO TABLE @me-&gt;at_revd FOR ALL ENTRIES IN @me-&gt;at_isel
     WHERE rbkp~belnr = @me-&gt;at_isel-belnr
       AND rbkp~gjahr = @me-&gt;at_isel-gjahr
       AND rbkp~stblg NE &apos;&apos;
       AND rbkp~stjah NE &apos;&apos;
       AND rbkp~ivtyp EQ &apos;5&apos;. &quot;not the cancelled document, the cancellation one

    &quot;* invoices per RSEG item
    SELECT rseg~belnr,
           rseg~gjahr,
           rseg~buzei,
           rseg~ebeln,
           rseg~ebelp,
           ekpo~bednr,
           rseg~wrbtr
      INTO TABLE @DATA(lt_item)
      FROM            rseg AS rseg
           INNER JOIN ekpo AS ekpo ON ekpo~ebeln = rseg~ebeln AND
                                      ekpo~ebelp = rseg~ebelp
      FOR ALL ENTRIES IN @lt_invo
      WHERE rseg~belnr = @lt_invo-belnr
        AND rseg~gjahr = @lt_invo-gjahr.

    CHECK lt_item[] IS NOT INITIAL.


    &quot;* prepare request (map SAP data to ws-request)
    LOOP AT lt_invo ASSIGNING FIELD-SYMBOL(&lt;invo&gt;).

      APPEND INITIAL LINE TO me-&gt;at_req ASSIGNING FIELD-SYMBOL(&lt;req&gt;).

      &quot;keys: invoice header
      MOVE: &lt;invo&gt;-vdlr_dlrid TO &lt;req&gt;-dealer_code,
            &lt;invo&gt;-vslm_slmid TO &lt;req&gt;-sman_code,
            &lt;invo&gt;-belnr      TO &lt;req&gt;-inv_doc_no,
            &lt;invo&gt;-gjahr      TO &lt;req&gt;-inv_doc_yr.

      &quot;data: invoice header
      DATA(lv_diso) = |{ &lt;invo&gt;-budat DATE = ISO }|.
      MOVE: lv_diso       TO &lt;req&gt;-post_date,
            &lt;invo&gt;-xblnr  TO &lt;req&gt;-reference.

      &quot;..serial: extracted from concat form**serial
      MOVE: &lt;invo&gt;-bktxt+13(8) TO &lt;req&gt;-serial.

      &quot;..status: P= posted, R= reverted
      IF &lt;invo&gt;-ivtyp NE &apos;5&apos;. MOVE: &apos;P&apos; TO &lt;req&gt;-status. ENDIF. &quot;P= posted
      IF &lt;invo&gt;-ivtyp EQ &apos;5&apos;. MOVE: &apos;R&apos; TO &lt;req&gt;-status. ENDIF. &quot;R= reverted


      &quot;..special: reversal to be reported under original document, not as separate document
      IF &lt;invo&gt;-ivtyp = &apos;5&apos;. &quot;5= Cancellation
        MOVE: &lt;invo&gt;-stblg TO &lt;req&gt;-inv_doc_no,
              &lt;invo&gt;-stjah TO &lt;req&gt;-inv_doc_yr.
      ENDIF.



      MOVE: &lt;invo&gt;-waers TO &lt;req&gt;-currency.
      &lt;req&gt;-amnt_tot = me-&gt;conv_amnt( iv_amnt = &lt;invo&gt;-rmwwr
                                      iv_curr = &lt;invo&gt;-waers ).



      &quot;..invoice type: I= invoice, C= credit memo
      IF &lt;invo&gt;-xrech = &apos;X&apos;. MOVE: &apos;I&apos; TO &lt;req&gt;-inv_type. ENDIF.
      IF &lt;invo&gt;-xrech = &apos; &apos;. MOVE: &apos;C&apos; TO &lt;req&gt;-inv_type. ENDIF.

      &quot;..vat rate
      &lt;req&gt;-vat_rate = me-&gt;conv_vatr( iv_bukrs = &lt;invo&gt;-bukrs
                                      iv_mwskz = &lt;invo&gt;-mwskz1
                                      iv_waers = &lt;invo&gt;-waers  ).


      &quot;subtable: invoice items
      LOOP AT lt_item ASSIGNING FIELD-SYMBOL(&lt;item&gt;) WHERE belnr = &lt;invo&gt;-belnr
                                                       AND gjahr = &lt;invo&gt;-gjahr.

        APPEND INITIAL LINE TO &lt;req&gt;-st_inv_req_itm ASSIGNING FIELD-SYMBOL(&lt;rit&gt;).

        MOVE: &lt;item&gt;-bednr TO &lt;rit&gt;-comm_id.
        &lt;rit&gt;-amnt_itm = me-&gt;conv_amnt( iv_amnt = &lt;item&gt;-wrbtr
                                        iv_curr = &lt;invo&gt;-waers ).

      ENDLOOP.


    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPI" CMPNAME="PROCESS" VERSION="1" LANGU="E" DESCRIPT="process invoice IFLI" EXPOSURE="2" STATE="1" EDITORDER="2 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD PROCESS.

    me-&gt;read_c000( ).    &quot;read customizing

    me-&gt;prep_req( ).     &quot;prepare ws-request (from ISEL to REQ)

    me-&gt;proc_wscall( ).  &quot;call ws

    me-&gt;prep_ifli( ).    &quot;prepare ifli-log from ws-response
    me-&gt;save_ifli( ).    &quot;save ifli-log to database

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPI" CMPNAME="PROC_WSCALL" VERSION="1" LANGU="E" DESCRIPT="process into ws-call" EXPOSURE="0" STATE="1" EDITORDER="3 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD PROC_WSCALL.

    &quot;take prepared request, call ws

    CHECK me-&gt;ax_test IS INITIAL. &quot;(no ws-call in test mode)

    CALL FUNCTION &apos;ZVNCSI_CS_DCS_INV&apos;
      EXPORTING
        iv_port = me-&gt;av_port
        it_req  = me-&gt;at_req
      IMPORTING
        et_rsp  = me-&gt;at_rsp.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPI" CMPNAME="READ_C000" VERSION="1" LANGU="E" DESCRIPT="get config for Dealer/SalesMan" EXPOSURE="0" STATE="1" EDITORDER="1 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD READ_C000.

    &quot;get config for Dealer/SalesMan
    SELECT SINGLE * FROM: zvncsi_dcs_c000 INTO @me-&gt;as_c00d WHERE party = @zif_vncsi_gc=&gt;party_d, &quot;PartnTyp D= Dealer
                          zvncsi_dcs_c000 INTO @me-&gt;as_c00s WHERE party = @zif_vncsi_gc=&gt;party_s. &quot;PartnTyp S= SalesMan

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPI" CMPNAME="SAVE_IFLI" VERSION="1" LANGU="E" DESCRIPT="save IFLI-log to database" EXPOSURE="0" STATE="1" EDITORDER="5 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SAVE_IFLI.

    CHECK: me-&gt;ax_test IS     INITIAL,
           me-&gt;at_ifli IS NOT INITIAL.

    INSERT zvncsi_dcs_ifli FROM TABLE me-&gt;at_ifli.

    CALL FUNCTION &apos;BAPI_TRANSACTION_COMMIT&apos;. &quot;(default commit, see check of test mode above)



  ENDMETHOD.</source>
  </method>
