  <method CLSNAME="ZCL_VNCSI_SUB_IFPP" CMPNAME="CONV_AMNT" VERSION="1" LANGU="E" DESCRIPT="conv: amount per currency" EXPOSURE="0" STATE="1" EDITORDER="6 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="1" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_SUB_IFPP" CMPNAME="CONV_AMNT" SCONAME="IV_AMNT" VERSION="1" LANGU="E" DESCRIPT="Amount in document currency" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="BSEG-WRBTR"/>
   <parameter CLSNAME="ZCL_VNCSI_SUB_IFPP" CMPNAME="CONV_AMNT" SCONAME="IV_CURR" VERSION="1" LANGU="E" DESCRIPT="Currency Key" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="TCURC-WAERS"/>
   <parameter CLSNAME="ZCL_VNCSI_SUB_IFPP" CMPNAME="CONV_AMNT" SCONAME="RT_AMNT" VERSION="1" LANGU="E" DESCRIPT="dec13_2" CMPTYPE="1" MTDTYPE="0" EDITORDER="3 " DISPID="0 " PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="ZVNCSI_DEC13_2"/>
   <source>METHOD conv_amnt.

    &quot;convert currency

    DATA: lv_amnt1 TYPE wmto_s-amount,
          lv_amnt2 TYPE wmto_s-amount.
    TRY.
        lv_amnt1 = CONV #( iv_amnt ).

        CALL FUNCTION &apos;CURRENCY_AMOUNT_SAP_TO_DISPLAY&apos;
          EXPORTING
            currency        = iv_curr
            amount_internal = lv_amnt1
          IMPORTING
            amount_display  = lv_amnt2
          EXCEPTIONS
            internal_error  = 1
            OTHERS          = 2.
        IF sy-subrc &lt;&gt; 0. ENDIF.

        rt_amnt = CONV #( lv_amnt2 ).

      CATCH cx_root.    &quot;+SAP-3631 dump overflow
        IF iv_curr = &apos;VND&apos;.
          rt_amnt = lv_amnt1 * 100.
        ENDIF.
    ENDTRY.

  ENDMETHOD.</source>
  </method>
