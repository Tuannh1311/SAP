      ENDLOOP.
    ENDIF.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="SDAT_ENHA_BLKRS_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: blocking reason" EXPOSURE="0" STATE="1" EDITORDER="13 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_blkrs_tx.
    &quot;salv-data: enhance: blocking reason

    &quot;+CBL-12415 DCS waive-action: add reason text from customizing, if missing
    SELECT * FROM blk_reason_tx INTO TABLE @DATA(lt_brtx).
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;) WHERE blk_reason_id  IS NOT INITIAL
                                                         AND blk_reason_txt IS     INITIAL.

      READ TABLE lt_brtx ASSIGNING FIELD-SYMBOL(&lt;brtx&gt;) WITH KEY spras         = sy-langu
                                                                 blk_reason_id = &lt;sdat&gt;-blk_reason_id. CHECK sy-subrc = 0.

      MOVE: &lt;brtx&gt;-blk_reason_txt TO &lt;sdat&gt;-blk_reason_txt.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="SDAT_ENHA_CPPRF_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: product profile" EXPOSURE="0" STATE="1" EDITORDER="12 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_cpprf_tx.

    &quot;enha product-profile (=segment) descriptions
    SELECT fagl_segmt~segment,
           fagl_segmt~name    INTO TABLE @DATA(lt_segm) FROM fagl_segmt
      FOR ALL ENTRIES IN @me-&gt;at_sdat
      WHERE segment = @me-&gt;at_sdat-cpprf.

    &quot;pass to items
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;) WHERE cpprf IS NOT INITIAL.

      READ TABLE lt_segm ASSIGNING FIELD-SYMBOL(&lt;segm&gt;) WITH KEY segment = &lt;sdat&gt;-cpprf. CHECK sy-subrc = 0.
      MOVE: &lt;segm&gt;-name TO &lt;sdat&gt;-cpprf_tx.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="SDAT_ENHA_IAMNT" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: calculate invoiced amount" EXPOSURE="0" STATE="1" EDITORDER="5 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_iamnt.

    &quot;calculate invoiced amount

    &quot;..calc per item: invoice
    SELECT      ekbe~ebeln,
                ekbe~ebelp,
                ekbe~zekkn,
                ekbe~vgabe,
                ekbe~gjahr,
                ekbe~belnr,
                ekbe~buzei,
                ekbe~wrbtr,
                ekbe~waers,
                ekbe~shkzg
      FROM ekbe INTO TABLE @DATA(lt_ekbe)  FOR ALL ENTRIES IN @me-&gt;at_sdat
     WHERE ekbe~ebeln = @me-&gt;at_sdat-ebeln
       AND ekbe~ebelp = @me-&gt;at_sdat-ebelp
       AND ( ekbe~vgabe = 2 OR              &quot;2= Invoice Receipt
             ekbe~vgabe = 3   ).            &quot;3= Subseq. Debit/Credit

    &quot;..sum per item
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).
      LOOP AT lt_ekbe ASSIGNING FIELD-SYMBOL(&lt;ekbe&gt;) WHERE ebeln = &lt;sdat&gt;-ebeln
                                                       AND ebelp = &lt;sdat&gt;-ebelp.

        &quot;***TBD: curr conversion (needed for DCS at all?)

        CASE &lt;ekbe&gt;-shkzg.
          WHEN &apos;S&apos;. &lt;sdat&gt;-iamnt = &lt;sdat&gt;-iamnt + &lt;ekbe&gt;-wrbtr.
          WHEN &apos;H&apos;. &lt;sdat&gt;-iamnt = &lt;sdat&gt;-iamnt - &lt;ekbe&gt;-wrbtr.
        ENDCASE.

      ENDLOOP.
    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="SDAT_ENHA_KOSTL_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: descr cost centre" EXPOSURE="0" STATE="1" EDITORDER="10 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_kostl_tx.

    &quot;enha cost centre descriptions
    SELECT cskt~kostl,
           cskt~ltext INTO TABLE @DATA(lt_cskt)
      FROM            tka02 AS tka02
           INNER JOIN cskt  AS cskt ON cskt~spras = @sy-langu AND
                                       cskt~kokrs = tka02~kokrs
      FOR ALL ENTRIES IN @me-&gt;at_sdat
      WHERE cskt~kostl = @me-&gt;at_sdat-kostl
        AND tka02~bukrs = @me-&gt;at_sdat-bukrs.

    &quot;pass to items
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;) WHERE kostl IS NOT INITIAL.

      READ TABLE lt_cskt ASSIGNING FIELD-SYMBOL(&lt;cskt&gt;) WITH KEY kostl = &lt;sdat&gt;-kostl. CHECK sy-subrc = 0.
      MOVE: &lt;cskt&gt;-ltext TO &lt;sdat&gt;-kostl_tx.

    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="SDAT_ENHA_LOEKZ" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: deletion icon" EXPOSURE="0" STATE="1" EDITORDER="4 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_loekz.

    &quot;set icon del/lock
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).
      CASE &lt;sdat&gt;-loekz.
        WHEN &apos;L&apos;. &lt;sdat&gt;-loekz_ic = icon_delete .
        WHEN &apos;S&apos;. &lt;sdat&gt;-loekz_ic = icon_locked.
      ENDCASE.
    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="SDAT_ENHA_MWSKZ_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: tax-code descr" EXPOSURE="0" STATE="1" EDITORDER="9 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_mwskz_tx.

    &quot;enha tax-code description

    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      SELECT SINGLE t007s~text1 INTO @&lt;sdat&gt;-mwskz_tx
        FROM            t001  AS t001
             INNER JOIN t005  AS t005  ON t005~land1 = t001~land1
             INNER JOIN t007a AS t007a ON t007a~kalsm = t005~kalsm
             INNER JOIN t007s AS t007s ON t007s~kalsm = t007a~kalsm AND
                                          t007s~mwskz = t007a~mwskz
       WHERE t001~bukrs  = @&lt;sdat&gt;-bukrs
         AND t007a~mwskz = @&lt;sdat&gt;-mwskz
         AND t007s~spras = @sy-langu.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="SDAT_ENHA_SAMNT" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: salesman NUMC to CURR amounts" EXPOSURE="0" STATE="1" EDITORDER="6 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_samnt.

    &quot; pass amt-numc to amt-curr
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).
      MOVE: &lt;sdat&gt;-ekpo_smgrs TO &lt;sdat&gt;-ekpo_smgrs_cur,
            &lt;sdat&gt;-ekpo_smpit TO &lt;sdat&gt;-ekpo_smpit_cur,
            &lt;sdat&gt;-ekpo_smded TO &lt;sdat&gt;-ekpo_smded_cur,
            &lt;sdat&gt;-ekpo_smadj TO &lt;sdat&gt;-ekpo_smadj_cur,
            &lt;sdat&gt;-ekpo_smdcn TO &lt;sdat&gt;-ekpo_smdcn_cur,
            &lt;sdat&gt;-flck_smgrs TO &lt;sdat&gt;-flck_smgrs_cur,
            &lt;sdat&gt;-flck_smpit TO &lt;sdat&gt;-flck_smpit_cur,
            &lt;sdat&gt;-flck_smded TO &lt;sdat&gt;-flck_smded_cur,
            &lt;sdat&gt;-flck_smadj TO &lt;sdat&gt;-flck_smadj_cur,
            &lt;sdat&gt;-flck_smdcn TO &lt;sdat&gt;-flck_smdcn_cur.

      IF &lt;sdat&gt;-waers = &apos;VND&apos;.
        DIVIDE: &lt;sdat&gt;-ekpo_smgrs_cur BY 100,
                &lt;sdat&gt;-ekpo_smpit_cur BY 100,
                &lt;sdat&gt;-ekpo_smded_cur BY 100,
                &lt;sdat&gt;-ekpo_smadj_cur BY 100,
                &lt;sdat&gt;-ekpo_smdcn_cur BY 100,
                &lt;sdat&gt;-flck_smgrs_cur BY 100,
                &lt;sdat&gt;-flck_smpit_cur BY 100,
                &lt;sdat&gt;-flck_smded_cur BY 100,
                &lt;sdat&gt;-flck_smadj_cur BY 100,
                &lt;sdat&gt;-flck_smdcn_cur BY 100.
      ENDIF.
    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="SDAT_ENHA_SCSTA_IC" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: status icon" EXPOSURE="0" STATE="1" EDITORDER="8 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_scsta_ic.

    &quot;ehna status icon
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      CASE &lt;sdat&gt;-scsta.
        WHEN zif_vncsi_gc=&gt;scsta_xx. &lt;sdat&gt;-scsta_ic = &apos;&apos;                          . &quot;status: __ not relevant
        WHEN zif_vncsi_gc=&gt;scsta_0i. &lt;sdat&gt;-scsta_ic = icon_status_open            . &quot;status: 0I interfaced
        WHEN zif_vncsi_gc=&gt;scsta_1r. &lt;sdat&gt;-scsta_ic = icon_status_booked          . &quot;status: 1R reviewed
        WHEN zif_vncsi_gc=&gt;scsta_2a. &lt;sdat&gt;-scsta_ic = icon_status_partly_booked   . &quot;status: 2A approved
        WHEN zif_vncsi_gc=&gt;scsta_5p. &lt;sdat&gt;-scsta_ic = icon_closed_folder_uptodate . &quot;status: 5P posted
      ENDCASE.

    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="SDAT_ENHA_SCSTA_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: status descr" EXPOSURE="0" STATE="1" EDITORDER="7 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_scsta_tx.

    &quot;enha status description
    DATA(lo_dref) = CAST cl_abap_elemdescr( cl_abap_elemdescr=&gt;describe_by_name( p_name = &apos;ZVNCSI_SLSM_SCSTA&apos; ) ).
    DATA(lt_vals) = lo_dref-&gt;get_ddic_fixed_values( ).

    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      READ TABLE lt_vals ASSIGNING FIELD-SYMBOL(&lt;vals&gt;) WITH KEY low = &lt;sdat&gt;-scsta. CHECK sy-subrc = 0.

      MOVE: &lt;vals&gt;-ddtext TO &lt;sdat&gt;-scsta_tx.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="SDAT_SELC" VERSION="1" LANGU="E" DESCRIPT="salv-data: select data per keys" EXPOSURE="0" STATE="1" EDITORDER="1 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_selc.

    &quot;get Commission data as per keys
    CHECK  me-&gt;at_keys[] IS NOT INITIAL.

    SELECT ekko~ebeln,
           ekpo~ebelp,
           ekpo~zvncsi_scsta AS scsta,
           ekko~bsart,
           ekko~bukrs,
           ekko~ekorg,
           ekko~ekgrp,
           ekko~lifnr,
           exid~idnumber      AS dealr,
           exid~bpext,
           lfa1~name1         AS lifnr_tx,
           ekpo~loekz,
           ekpo~werks,
           ekpo~bednr         AS comid,
           ekpo~creationdate  AS crdat,
           ekpo~matnr,
           ekpo~txz01,
           ekpo~netwr         AS ekpo_netwr,
           ekpo~menge,
           ekpo~meins,
           ekpo~peinh,
           ekpo~netpr,
           ekko~waers,
           eket~eindt,
           ekpo~erekz,
           ekpo~mwskz,
           ekpo~blk_reason_id,    &quot;+CBL-12415 DCS waive-action: block reason
           ekpo~blk_reason_txt,
           ekpo~zvncsi_smgrs  AS ekpo_smgrs,
           ekpo~zvncsi_smpit  AS ekpo_smpit,
           ekpo~zvncsi_smded  AS ekpo_smded,
           ekpo~zvncsi_smadj  AS ekpo_smadj,
           ekpo~zvncsi_smdcn  AS ekpo_smdcn,
           ekkn~zekkn,
           ekkn~netwr         AS ekkn_netwr,
           ekkn~menge         AS ekkn_menge,
           ekkn~sakto,
           ekkn~kostl,
           ekkn~aufnr,
           skat~txt50         AS sakto_tx,
           flck~cpprf,
           flck~cpamt,
           flck~smgrs         AS flck_smgrs,
           flck~smpit         AS flck_smpit,
           flck~smded         AS flck_smded,
           flck~smadj         AS flck_smadj,
           flck~smdcn         AS flck_smdcn

      INTO CORRESPONDING FIELDS OF TABLE @me-&gt;at_sdat
      FROM            ekko             AS ekko
           INNER JOIN ekpo             AS ekpo ON ekpo~ebeln  = ekko~ebeln
           INNER JOIN lfa1             AS lfa1 ON lfa1~lifnr  = ekko~lifnr
           INNER JOIN zvncsi_exid_vend AS exid ON exid~vendor = ekko~lifnr
           INNER JOIN eket             AS eket ON eket~ebeln  = ekpo~ebeln AND
                                                  eket~ebelp  = ekpo~ebelp
           INNER JOIN ekkn             AS ekkn ON ekkn~ebeln  = ekpo~ebeln AND
                                                  ekkn~ebelp  = ekpo~ebelp
      LEFT OUTER JOIN zvncsi_dcs_iflck AS flck ON flck~ebeln  = ekkn~ebeln AND    &quot;(lef outer for demoes)
                                                  flck~ebelp  = ekkn~ebelp AND
                                                  flck~zekkn  = ekkn~zekkn
           INNER JOIN t001             AS t001 ON t001~bukrs  = ekko~bukrs
           INNER JOIN skat             AS skat ON skat~spras  = @sy-langu  AND
                                                  skat~ktopl  = t001~ktopl AND
                                                  skat~saknr  = ekkn~sakto
      FOR ALL ENTRIES IN @me-&gt;at_keys
     WHERE ekko~ebeln = @me-&gt;at_keys-ebeln
       AND ekpo~ebelp = @me-&gt;at_keys-ebelp.


    &quot; default sort
    SORT me-&gt;at_sdat BY ebeln DESCENDING
                        ebelp ASCENDING.

    &quot; recalc EKKN-NETWR (is zero when single AccAsg, :( grrr )
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;) WHERE ekkn_netwr = 0.

      &lt;sdat&gt;-ekkn_netwr = &lt;sdat&gt;-ekkn_menge * &lt;sdat&gt;-netpr / &lt;sdat&gt;-peinh .

    ENDLOOP.

    &quot; remove account assignment, if not requested
    IF me-&gt;ax_ekkn = &apos; &apos;.
      DELETE ADJACENT DUPLICATES FROM me-&gt;at_sdat COMPARING ebeln
                                                            ebelp.
    ENDIF.

    &quot;for item(ekpo)-level report, clear account(ekkn)-level data
    IF me-&gt;ax_ekkn = &apos; &apos;.
      LOOP AT me-&gt;at_sdat ASSIGNING &lt;sdat&gt;.
        CLEAR:  &lt;sdat&gt;-flck_smgrs,
                &lt;sdat&gt;-flck_smpit,
                &lt;sdat&gt;-flck_smded,
                &lt;sdat&gt;-flck_smadj,
                &lt;sdat&gt;-flck_smdcn.
      ENDLOOP.
    ENDIF.


  ENDMETHOD.</source>
  </method>
 </CLAS>
