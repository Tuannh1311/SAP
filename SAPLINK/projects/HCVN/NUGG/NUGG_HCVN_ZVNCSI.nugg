  <method CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="CONV_AMNT" VERSION="1" LANGU="E" DESCRIPT="convert amount" EXPOSURE="0" STATE="1" EDITORDER="42 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="1" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="CONV_AMNT" SCONAME="IV_WRBTR" VERSION="1" LANGU="E" DESCRIPT="Amount in Document Currency" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="EKBE-WRBTR"/>
   <parameter CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="CONV_AMNT" SCONAME="IV_WAERS" VERSION="1" LANGU="E" DESCRIPT="Currency Key" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="TCURC-WAERS"/>
   <parameter CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="CONV_AMNT" SCONAME="RT_AMNT" VERSION="1" LANGU="E" DESCRIPT="dec13_2" CMPTYPE="1" MTDTYPE="0" EDITORDER="3 " DISPID="0 " PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="ZVNCSI_DEC13_2"/>
   <source>METHOD conv_amnt.

    &quot;convert currency field to numc15

    DATA: lv_amnt1 TYPE wmto_s-amount,
          lv_amnt2 TYPE wmto_s-amount.
    TRY.
        lv_amnt1 = CONV #( iv_wrbtr ).

        CALL FUNCTION &apos;CURRENCY_AMOUNT_SAP_TO_DISPLAY&apos;
          EXPORTING
            currency        = iv_waers
            amount_internal = lv_amnt1
          IMPORTING
            amount_display  = lv_amnt2
          EXCEPTIONS
            internal_error  = 1
            OTHERS          = 2.
        IF sy-subrc &lt;&gt; 0. ENDIF.

        rt_amnt = CONV #( lv_amnt2 ).

      CATCH cx_root.    &quot;+SAP-3631 dump overflow
        IF iv_waers = &apos;VND&apos;.
          rt_amnt = lv_amnt1 * 100.
        ENDIF.
    ENDTRY.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="ON_DBCL" VERSION="1" LANGU="E" DESCRIPT="handler: on double-click" EXPOSURE="0" STATE="1" EDITORDER="30 " DISPID="0 " MTDTYPE="1" MTDDECLTYP="0" REFCLSNAME="CL_SALV_EVENTS_TABLE" REFCMPNAME="DOUBLE_CLICK" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="ON_DBCL" SCONAME="ROW" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="1" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="0"/>
   <parameter CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="ON_DBCL" SCONAME="COLUMN" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="1" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="0"/>
   <source>METHOD on_dbcl.

    &quot;get double-clicked row
    READ TABLE me-&gt;at_sdat REFERENCE INTO DATA(lr_sdat) INDEX row.
    CHECK sy-subrc = 0.

    &quot;by column
    CASE column.
      WHEN &apos;BELNR&apos;
        OR &apos;GJAHR&apos;. me-&gt;call_mir4( lr_sdat ). &quot;call MMInvo display
      WHEN OTHERS.  me-&gt;call_pord( lr_sdat ). &quot;call POrd display
    ENDCASE.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="ON_UCOM" VERSION="1" LANGU="E" DESCRIPT="handler: on user-command" EXPOSURE="0" STATE="1" EDITORDER="31 " DISPID="0 " MTDTYPE="1" MTDDECLTYP="0" REFCLSNAME="CL_SALV_EVENTS" REFCMPNAME="ADDED_FUNCTION" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="ON_UCOM" SCONAME="E_SALV_FUNCTION" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="1" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="0"/>
   <source>METHOD on_ucom.

    &quot;default: get selected rows
    me-&gt;at_rsel = me-&gt;ar_salv-&gt;get_selections( )-&gt;get_selected_rows( ).


    &quot;user-command
    CASE e_salv_function.

      WHEN &apos;REFRESH&apos;. me-&gt;salv_refr( ).

      WHEN &apos;SETREV&apos;.  me-&gt;on_ucom_setrev( ).
      WHEN &apos;REVREV&apos;.  me-&gt;on_ucom_revrev( ).

      WHEN &apos;SETAPP&apos;.  me-&gt;on_ucom_setapp( ).
      WHEN &apos;REVAPP&apos;.  me-&gt;on_ucom_revapp( ).


      WHEN &apos;INVPOST&apos;. me-&gt;on_ucom_invpost( ).
      WHEN &apos;REVPOST&apos;. me-&gt;on_ucom_revpost( ).

      WHEN &apos;CDLOG&apos;  . me-&gt;on_ucom_cdlog( ).   &quot;++SAP-3574  SAP DCS Salesman commision Change status log

    ENDCASE.

    &quot;..dequeue: all
    me-&gt;salv_lsel_dequeue_all( ).

    &quot;refresh after each ucom (show actual/change data to user)
    me-&gt;salv_refr( ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="ON_UCOM_CDLOG" VERSION="1" LANGU="E" DESCRIPT="func: call change log" EXPOSURE="0" STATE="1" EDITORDER="38 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD on_ucom_cdlog.

    &quot;++SAP-3574  SAP DCS Salesman commision Change status log

    &quot;call change log
    DATA(lr_sdat) =  me-&gt;salv_line_get( ).
    CHECK lr_sdat IS BOUND.
    me-&gt;call_cddo_dis( ir_sdat = lr_sdat ).


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="ON_UCOM_INVPOST" VERSION="1" LANGU="E" DESCRIPT="func: Post Invoice" EXPOSURE="0" STATE="1" EDITORDER="36 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD on_ucom_invpost.

    &quot;set status Approved

    &quot;..check auth: status 5P= Posted
    AUTHORITY-CHECK OBJECT &apos;ZCSISCSTA&apos; ID &apos;ZCSISCSTA&apos; FIELD zif_vncsi_gc=&gt;scsta_5p.
    IF sy-subrc &lt;&gt; 0.
      MESSAGE w015(zvncsi) WITH zif_vncsi_gc=&gt;scsta_5p DISPLAY LIKE &apos;E&apos;. &quot;msg{Missing authorisation for status &amp; (see SU53).}
      RETURN.
    ENDIF.


    &quot;..check: any lines selected
    IF me-&gt;at_rsel IS INITIAL.
      MESSAGE w013(zvncsi) DISPLAY LIKE &apos;E&apos;. &quot;msg{No lines selected for processing.}
      RETURN.
    ENDIF.

    &quot;..check: only lines with status 2A= approved
    LOOP AT me-&gt;at_rsel ASSIGNING FIELD-SYMBOL(&lt;rsel&gt;).
      READ TABLE me-&gt;at_sdat INDEX &lt;rsel&gt; ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).
      IF &lt;sdat&gt;-scsta NE zif_vncsi_gc=&gt;scsta_2a.
        MESSAGE w014(zvncsi) WITH zif_vncsi_gc=&gt;scsta_2a DISPLAY LIKE &apos;E&apos;. &quot;msg{Select only lines with status &amp;}
        RETURN.
      ENDIF.
    ENDLOOP.

    &quot;..enqueu: selected lines
    me-&gt;salv_lsel_enqueue( ).


    &quot;..call MIRO invoice post
    LOOP AT me-&gt;at_rsel ASSIGNING &lt;rsel&gt;.
      READ TABLE me-&gt;at_sdat INDEX &lt;rsel&gt; ASSIGNING &lt;sdat&gt;. ASSERT sy-subrc = 0.
      me-&gt;bapi_invo_slman( EXPORTING iv_ebeln = &lt;sdat&gt;-ebeln    &quot; Purchasing Document Number
                                     iv_ebelp = &lt;sdat&gt;-ebelp    &quot; Item Number of Purchasing Document
                           IMPORTING ev_belnr = &lt;sdat&gt;-belnr    &quot; Document Number of an Invoice Document
                                     ev_gjahr = &lt;sdat&gt;-gjahr ). &quot; Fiscal Year

      &quot;  ..call POrd set-status (when invoice posted)
      IF &lt;sdat&gt;-belnr IS NOT INITIAL AND
         &lt;sdat&gt;-gjahr IS NOT INITIAL    .
        me-&gt;bapi_pord_scsta( EXPORTING iv_scsta = zif_vncsi_gc=&gt;scsta_5p
                                       iv_ebeln = &lt;sdat&gt;-ebeln
                                       iv_ebelp = &lt;sdat&gt;-ebelp ).
      ENDIF.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="ON_UCOM_REVAPP" VERSION="1" LANGU="E" DESCRIPT="func: reverse status Approved" EXPOSURE="0" STATE="1" EDITORDER="35 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD on_ucom_revapp.

    &quot;reverse status Approved

    &quot;..check auth: status 2A= Approved
    AUTHORITY-CHECK OBJECT &apos;ZCSISCSTA&apos; ID &apos;ZCSISCSTA&apos; FIELD zif_vncsi_gc=&gt;scsta_2a.
    IF sy-subrc &lt;&gt; 0.
      MESSAGE w015(zvncsi) WITH zif_vncsi_gc=&gt;scsta_2a DISPLAY LIKE &apos;E&apos;. &quot;msg{Missing authorisation for status &amp; (see SU53).}
      RETURN.
    ENDIF.


    &quot;..check: any lines selected
    IF me-&gt;at_rsel IS INITIAL.
      MESSAGE w013(zvncsi) DISPLAY LIKE &apos;E&apos;. &quot;msg{No lines selected for processing.}
      RETURN.
    ENDIF.

    &quot;..check: only lines with status 2A= Approved
    LOOP AT me-&gt;at_rsel ASSIGNING FIELD-SYMBOL(&lt;rsel&gt;).
      READ TABLE me-&gt;at_sdat INDEX &lt;rsel&gt; ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).
      IF &lt;sdat&gt;-scsta NE zif_vncsi_gc=&gt;scsta_2a.
        MESSAGE w014(zvncsi) WITH zif_vncsi_gc=&gt;scsta_2a DISPLAY LIKE &apos;E&apos;. &quot;msg{Select only lines with status &amp;}
        RETURN.
      ENDIF.
    ENDLOOP.

    &quot;..enqueu: selected lines
    me-&gt;salv_lsel_enqueue( ).

    &quot;..call POrd set-status
    LOOP AT me-&gt;at_rsel ASSIGNING &lt;rsel&gt;.
      READ TABLE me-&gt;at_sdat INDEX &lt;rsel&gt; ASSIGNING &lt;sdat&gt;.
      me-&gt;bapi_pord_scsta( EXPORTING iv_scsta = zif_vncsi_gc=&gt;scsta_1r
                                     iv_ebeln = &lt;sdat&gt;-ebeln
                                     iv_ebelp = &lt;sdat&gt;-ebelp ).
    ENDLOOP.



  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="ON_UCOM_REVPOST" VERSION="1" LANGU="E" DESCRIPT="func: Reverse Invoice" EXPOSURE="0" STATE="1" EDITORDER="37 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD on_ucom_revpost.

    &quot;set status Approved

    &quot;..check auth: status 5P= Posted
    AUTHORITY-CHECK OBJECT &apos;ZCSISCSTA&apos; ID &apos;ZCSISCSTA&apos; FIELD zif_vncsi_gc=&gt;scsta_5p.
    IF sy-subrc &lt;&gt; 0.
      MESSAGE w015(zvncsi) WITH zif_vncsi_gc=&gt;scsta_5p DISPLAY LIKE &apos;E&apos;. &quot;msg{Missing authorisation for status &amp; (see SU53).}
      RETURN.
    ENDIF.


    &quot;..check: any lines selected
    IF me-&gt;at_rsel IS INITIAL.
      MESSAGE w013(zvncsi) DISPLAY LIKE &apos;E&apos;. &quot;msg{No lines selected for processing.}
      RETURN.
    ENDIF.

    &quot;..check: only lines with status 5P= posted
    LOOP AT me-&gt;at_rsel ASSIGNING FIELD-SYMBOL(&lt;rsel&gt;).
      READ TABLE me-&gt;at_sdat INDEX &lt;rsel&gt; ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).
      IF &lt;sdat&gt;-scsta NE zif_vncsi_gc=&gt;scsta_5p.
        MESSAGE w014(zvncsi) WITH zif_vncsi_gc=&gt;scsta_5p DISPLAY LIKE &apos;E&apos;. &quot;msg{Select only lines with status &amp;}
        RETURN.
      ENDIF.
    ENDLOOP.

    &quot;..enqueu: selected lines
    me-&gt;salv_lsel_enqueue( ).


    &quot;..call MR8M invoice reversal
    LOOP AT me-&gt;at_rsel ASSIGNING &lt;rsel&gt;.
      READ TABLE me-&gt;at_sdat INDEX &lt;rsel&gt; ASSIGNING &lt;sdat&gt;. ASSERT sy-subrc = 0.
      me-&gt;bapi_invo_rever( EXPORTING iv_belnr = &lt;sdat&gt;-belnr        &quot; Document Number of an Invoice Document
                                     iv_gjahr = &lt;sdat&gt;-gjahr        &quot; Fiscal Year
                           IMPORTING ev_belnr = DATA(lv_rev_belnr)  &quot; rev:: Document Number of an Invoice Document
                                     ev_gjahr = DATA(lv_rev_gjahr)  &quot; rev::Fiscal Year
                                     ex_revok = DATA(lx_revok)    ).

      &quot;  ..call POrd set-status (when invoice reversed)
      IF lx_revok = &apos;X&apos;.
        me-&gt;bapi_pord_scsta( EXPORTING iv_scsta = zif_vncsi_gc=&gt;scsta_2a
                                       iv_ebeln = &lt;sdat&gt;-ebeln
                                       iv_ebelp = &lt;sdat&gt;-ebelp ).
        MOVE: lv_rev_belnr TO &lt;sdat&gt;-belnr,
              lv_rev_gjahr TO &lt;sdat&gt;-gjahr.
      ENDIF.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="ON_UCOM_REVREV" VERSION="1" LANGU="E" DESCRIPT="func: reverse status Reviewed" EXPOSURE="0" STATE="1" EDITORDER="33 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
