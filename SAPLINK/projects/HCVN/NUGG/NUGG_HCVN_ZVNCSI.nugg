  <method CLSNAME="ZCL_VNCSI_DCS_IFPP" CMPNAME="CONV_AMNT" VERSION="1" LANGU="E" DESCRIPT="conv: amount per currency" EXPOSURE="0" STATE="1" EDITORDER="6 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="1" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_DCS_IFPP" CMPNAME="CONV_AMNT" SCONAME="IV_AMNT" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="RSEG-WRBTR"/>
   <parameter CLSNAME="ZCL_VNCSI_DCS_IFPP" CMPNAME="CONV_AMNT" SCONAME="IV_CURR" VERSION="1" LANGU="E" DESCRIPT="Currency Key" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="TCURC-WAERS"/>
   <parameter CLSNAME="ZCL_VNCSI_DCS_IFPP" CMPNAME="CONV_AMNT" SCONAME="RT_AMNT" VERSION="1" LANGU="E" DESCRIPT="dec13_2" CMPTYPE="1" MTDTYPE="0" EDITORDER="3 " DISPID="0 " PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="ZVNCSI_DEC13_2"/>
   <source>METHOD conv_amnt.

    &quot;convert currency

    DATA: lv_amnt1 TYPE wmto_s-amount,
          lv_amnt2 TYPE wmto_s-amount.

    TRY.

        lv_amnt1 = CONV #( iv_amnt ).

        CALL FUNCTION &apos;CURRENCY_AMOUNT_SAP_TO_DISPLAY&apos;
          EXPORTING
            currency        = iv_curr
            amount_internal = lv_amnt1
          IMPORTING
            amount_display  = lv_amnt2
          EXCEPTIONS
            internal_error  = 1
            OTHERS          = 2.
        IF sy-subrc &lt;&gt; 0. ENDIF.

        rt_amnt = CONV #( lv_amnt2 ).

      CATCH cx_root.    &quot;+SAP-3631 dump overflow
        IF iv_curr = &apos;VND&apos;.
          rt_amnt = lv_amnt1 * 100.
        ENDIF.
    ENDTRY.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPP" CMPNAME="CONV_AMNT_RWBTR" VERSION="1" LANGU="E" DESCRIPT="conv: amount per currency (RWBTR)" EXPOSURE="0" STATE="1" EDITORDER="7 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="1" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_DCS_IFPP" CMPNAME="CONV_AMNT_RWBTR" SCONAME="IV_AMNT" VERSION="1" LANGU="E" DESCRIPT="Amount Paid in the Payment Currency" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="REGUH-RWBTR"/>
   <parameter CLSNAME="ZCL_VNCSI_DCS_IFPP" CMPNAME="CONV_AMNT_RWBTR" SCONAME="IV_CURR" VERSION="1" LANGU="E" DESCRIPT="Currency Key" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="REGUH-WAERS"/>
   <parameter CLSNAME="ZCL_VNCSI_DCS_IFPP" CMPNAME="CONV_AMNT_RWBTR" SCONAME="RT_AMNT" VERSION="1" LANGU="E" DESCRIPT="dec13_2" CMPTYPE="1" MTDTYPE="0" EDITORDER="3 " DISPID="0 " PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="ZVNCSI_DEC13_2"/>
   <source>METHOD conv_amnt_rwbtr.

    &quot;convert currency

    DATA: lv_amnt1 TYPE wmto_s-amount,
          lv_amnt2 TYPE wmto_s-amount.
    TRY.
        lv_amnt1 = CONV #( iv_amnt ).

        CALL FUNCTION &apos;CURRENCY_AMOUNT_SAP_TO_DISPLAY&apos;
          EXPORTING
            currency        = iv_curr
            amount_internal = lv_amnt1
          IMPORTING
            amount_display  = lv_amnt2
          EXCEPTIONS
            internal_error  = 1
            OTHERS          = 2.
        IF sy-subrc &lt;&gt; 0. ENDIF.

        rt_amnt = CONV #( lv_amnt2 ).

      CATCH cx_root.    &quot;+SAP-3631 dump overflow
        IF iv_curr = &apos;VND&apos;.
          rt_amnt = lv_amnt1 * 100.
        ENDIF.
    ENDTRY.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFPP" CMPNAME="CONV_DATE" VERSION="1" LANGU="E" DESCRIPT="conv: date to ISO format (yyyy-mm-dd)" EXPOSURE="0" STATE="1" EDITORDER="8 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="1" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_DCS_IFPP" CMPNAME="CONV_DATE" SCONAME="IV_DATE" VERSION="1" LANGU="E" DESCRIPT="abap date (yyyymmdd)" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="SY-DATUM"/>
   <parameter CLSNAME="ZCL_VNCSI_DCS_IFPP" CMPNAME="CONV_DATE" SCONAME="EV_DISO" VERSION="1" LANGU="E" DESCRIPT="iso date (yyyy-mm-dd)" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="CHAR10"/>
