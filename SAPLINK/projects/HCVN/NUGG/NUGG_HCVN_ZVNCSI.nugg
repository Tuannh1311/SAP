   <parameter CLSNAME="ZCL_VNCSI_SUB_IFPP" CMPNAME="CONV_DATE" SCONAME="IV_DATE" VERSION="1" LANGU="E" DESCRIPT="abap date (yyyymmdd)" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="SY-DATUM"/>
