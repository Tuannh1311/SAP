  <method CLSNAME="ZCL_VNCSI_DCS_RCOD" CMPNAME="SALV_INIT" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise" EXPOSURE="0" STATE="1" EDITORDER="13 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init.

