  <method CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="SALV_INIT_CCOL" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: colours per columns" EXPOSURE="0" STATE="1" EDITORDER="17 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_ccol.

    &quot;init: column colours

    DATA: lr_col   TYPE REF TO cl_salv_column_list, &quot; object of column, allowing set_color
          ls_color TYPE        lvc_s_colo.          &quot; colours structure

    DATA(lr_cols) = me-&gt;ar_salv-&gt;get_columns( ).
    DATA(lt_cols) = lr_cols-&gt;get( ).

    LOOP AT lt_cols ASSIGNING FIELD-SYMBOL(&lt;col&gt;).
      TRY.
          lr_col ?= lr_cols-&gt;get_column( &lt;col&gt;-columnname ).
        CATCH cx_salv_not_found.
      ENDTRY.

      DATA(ls_c101) = VALUE lvc_s_colo( col = 1 inv = 0 int = 1 ).  &quot;blue+h
      DATA(ls_c100) = VALUE lvc_s_colo( col = 1 inv = 0 int = 0 ).  &quot;blue
      DATA(ls_c300) = VALUE lvc_s_colo( col = 3 inv = 0 int = 0 ).  &quot;yellow
      DATA(ls_c700) = VALUE lvc_s_colo( col = 7 inv = 0 int = 0 ).  &quot;orange
      DATA(ls_c701) = VALUE lvc_s_colo( col = 7 inv = 0 int = 1 ).  &quot;orange+h
      DATA(ls_cx00) = VALUE lvc_s_colo( col = 9 inv = 0 int = 0 ).  &quot;gray

      &quot;setting colours
      CASE &lt;col&gt;-columnname.
        WHEN &apos;RDATE&apos;
          OR &apos;RTIME&apos;
          OR &apos;PARID&apos;
          OR &apos;PARTY&apos;
          OR &apos;BUKRS&apos;
          OR &apos;BELNR&apos;
          OR &apos;GJAHR&apos;
          OR &apos;SBVID&apos;    . lr_col-&gt;set_color( ls_c101 ). &quot;keys : blue+h (comid is kinda-key)
        WHEN &apos;KUNNR_TX&apos;
          OR &apos;PROCT_TX&apos;
          OR &apos;SBSTA_TX&apos; . lr_col-&gt;set_color( ls_c300 ). &quot;descr: yellow
        WHEN &apos;SXADJ&apos;
          OR &apos;SDADJ&apos;
          OR &apos;SYADJ&apos;
          OR &apos;SDCTP&apos;
          OR &apos;SXCNC&apos;
          OR &apos;STCNC&apos;
          OR &apos;SREDI&apos;
          OR &apos;SDATE&apos;
          OR &apos;SCURR&apos;    . lr_col-&gt;set_color( ls_c700 ). &quot;sman : orange
        WHEN  OTHERS    .
          IF &lt;col&gt;-columnname CS &apos;_LE&apos;.   &quot;data from eInvoice log ZVNEINV_IFLE
            lr_col-&gt;set_color( ls_cx00 ). &quot;ifle : blue-gray
          ELSE.
            lr_col-&gt;set_color( ls_c100 ). &quot;data: blue
          ENDIF.
      ENDCASE.



    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="SALV_INIT_COLS" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: columns types (exc, icon, chkbox)" EXPOSURE="0" STATE="1" EDITORDER="14 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_cols.

    &quot;init columns type: exception, icon, checkbox, etc

***    &quot; set column for lights
***    DATA(lr_cols) = me-&gt;ar_salv-&gt;get_columns( ).
***    TRY.
***        lr_cols-&gt;set_exception_column( value = &apos;STATU_IC&apos; ).
***      CATCH cx_salv_data_error.
***    ENDTRY.

***    &quot;init cols: icon
***    TRY.
***        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;LOEKZ_IC &apos;) )-&gt;set_icon( &apos;X&apos; ).
***      CATCH cx_salv_data_error.
***      CATCH cx_salv_not_found.
***    ENDTRY.

    TRY.

        &quot;init cols: F1 rollname
        DATA(lr_cols) = me-&gt;ar_salv-&gt;get_columns( ).
        DATA(lt_cols) = lr_cols-&gt;get( ).
        LOOP AT lt_cols ASSIGNING FIELD-SYMBOL(&lt;cols&gt;).
          &quot;update DDIC ref together with rollname
          &quot;(this will get normal F1 help pointing to our structure)
          DATA(ddic) = &lt;cols&gt;-r_column-&gt;get_ddic_reference( ).
          ddic-table = &apos;ZVNCSI_SUB_RSUB_SDAT&apos;.
          &lt;cols&gt;-r_column-&gt;set_ddic_reference( value = ddic ).
          &lt;cols&gt;-r_column-&gt;set_f1_rollname( value = &apos;&apos; ).
        ENDLOOP.

        &quot;init cols: checkbox
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;SXADJ&apos;) )-&gt;set_cell_type( 1 ).
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;SXCNC&apos;) )-&gt;set_cell_type( 1 ).

        &quot;init cols: centered
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;SBSTA_IC &apos;) )-&gt;set_alignment( if_salv_c_alignment=&gt;centered ).

        &quot;init cols: currency
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;ADOC_WRBTR&apos;) )-&gt;set_currency_column( &apos;ADOC_WAERS&apos; ).

        &quot;init cols: decimals
***        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;CPAMT&apos;) )-&gt;set_decimals( value = &apos;0&apos; ).

      CATCH cx_salv_data_error.
      CATCH cx_salv_not_found.
    ENDTRY.



  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="SALV_INIT_FUNC" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: functions" EXPOSURE="0" STATE="1" EDITORDER="15 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SALV_INIT_FUNC.

    &quot;salv all functions
    me-&gt;ar_salv-&gt;get_functions( )-&gt;set_all( abap_true ).

    &quot; layout save &amp; default (&apos;all functions&apos; above are not really all)
    DATA: lr_layout TYPE REF TO cl_salv_layout,
          ls_key    TYPE salv_s_layout_key.

    ls_key-report = me-&gt;lc_repid.

    lr_layout     = me-&gt;ar_salv-&gt;get_layout( ).
    lr_layout-&gt;set_key( ls_key ).         &quot;
    lr_layout-&gt;set_save_restriction( cl_salv_layout=&gt;restrict_none ). &quot;User can save
    lr_layout-&gt;set_default( abap_true ).

  ENDMETHOD.</source>
