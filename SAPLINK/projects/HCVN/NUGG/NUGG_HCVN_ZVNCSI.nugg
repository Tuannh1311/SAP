  <method CLSNAME="ZCL_VNCSI_SUB_IFPI" CMPNAME="CONV_AMNT" VERSION="1" LANGU="E" DESCRIPT="conv: amount per currency" EXPOSURE="0" STATE="1" EDITORDER="6 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="1" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_SUB_IFPI" CMPNAME="CONV_AMNT" SCONAME="IV_AMNT" VERSION="1" LANGU="E" DESCRIPT="Amount in document currency" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="BSEG-WRBTR"/>
   <parameter CLSNAME="ZCL_VNCSI_SUB_IFPI" CMPNAME="CONV_AMNT" SCONAME="IV_CURR" VERSION="1" LANGU="E" DESCRIPT="Currency Key" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="TCURC-WAERS"/>
   <parameter CLSNAME="ZCL_VNCSI_SUB_IFPI" CMPNAME="CONV_AMNT" SCONAME="RT_AMNT" VERSION="1" LANGU="E" DESCRIPT="dec13_2" CMPTYPE="1" MTDTYPE="0" EDITORDER="3 " DISPID="0 " PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="ZVNCSI_DEC13_2"/>
   <source>METHOD conv_amnt.

    &quot;convert currency

    DATA: lv_amnt1 TYPE wmto_s-amount,
          lv_amnt2 TYPE wmto_s-amount.

    TRY.

        lv_amnt1 = CONV #( iv_amnt ).

        CALL FUNCTION &apos;CURRENCY_AMOUNT_SAP_TO_DISPLAY&apos;
          EXPORTING
            currency        = iv_curr
            amount_internal = lv_amnt1
          IMPORTING
            amount_display  = lv_amnt2
          EXCEPTIONS
            internal_error  = 1
            OTHERS          = 2.
        IF sy-subrc &lt;&gt; 0. ENDIF.

        rt_amnt = CONV #( lv_amnt2 ).

      CATCH cx_root.    &quot;+SAP-3631 dump overflow
        IF iv_curr = &apos;VND&apos;.
          rt_amnt = lv_amnt1 * 100.
        ENDIF.

    ENDTRY.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_IFPI" CMPNAME="PREP_IFLI" VERSION="1" LANGU="E" DESCRIPT="prepare IFLI-log from response" EXPOSURE="0" STATE="1" EDITORDER="4 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD prep_ifli.

    &quot;prepare IFLI log from ws-response

    LOOP AT me-&gt;at_rsp  ASSIGNING FIELD-SYMBOL(&lt;rsp&gt;).

      APPEND INITIAL LINE TO me-&gt;at_ifli ASSIGNING FIELD-SYMBOL(&lt;ifli&gt;).

      &quot;backfill from request
      READ TABLE me-&gt;at_req ASSIGNING FIELD-SYMBOL(&lt;req&gt;) WITH KEY inv_docn = &lt;rsp&gt;-inv_docn
                                                                   inv_year = &lt;rsp&gt;-inv_year. CHECK sy-subrc = 0.
      MOVE: sy-datum            TO &lt;ifli&gt;-rdate,
            sy-uzeit            TO &lt;ifli&gt;-rtime,
            &lt;req&gt;-partner_code  TO &lt;ifli&gt;-partner_code,
            &lt;req&gt;-partner_type  TO &lt;ifli&gt;-partner_type,
            zif_vncsi_gc=&gt;bukrs TO &lt;ifli&gt;-bukrs.
      CASE &lt;req&gt;-struc_inv_dat-inv_status.
        WHEN &apos;P&apos;. MOVE &apos; &apos;      TO &lt;ifli&gt;-xreversed.
        WHEN &apos;R&apos;. MOVE &apos;X&apos;      TO &lt;ifli&gt;-xreversed.
      ENDCASE.
      &quot;response itself
      MOVE: &lt;rsp&gt;-inv_docn      TO &lt;ifli&gt;-belnr,
            &lt;rsp&gt;-inv_year      TO &lt;ifli&gt;-gjahr,
            &lt;rsp&gt;-posted        TO &lt;ifli&gt;-statu,
            &lt;rsp&gt;-msg_text      TO &lt;ifli&gt;-msgtx.

    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_IFPI" CMPNAME="PREP_REQ" VERSION="1" LANGU="E" DESCRIPT="select Invoices, prepare request" EXPOSURE="0" STATE="1" EDITORDER="2 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD prep_req.

    &quot;select invoices
    &quot; per documents in AT_ISEL, prepare request data AT_REQ

    CHECK me-&gt;at_isel IS NOT INITIAL.

    &quot;select invoices
    SELECT
             &quot;BKPF FIdoc-header
             bkpf~bukrs         AS bkpf_bukrs,
             bkpf~belnr         AS bkpf_belnr,
             bkpf~gjahr         AS bkpf_gjahr,
             bkpf~budat         AS bkpf_budat,
             bkpf~cpudt         AS bkpf_cpudt,
             bkpf~xblnr         AS bkpf_xblnr, &quot;=RedInvoNo
             bkpf~stblg         AS bkpf_stbgl, &quot; reversed with
             bkpf~stjah         AS bkpf_stjah,
             bkpf~bktxt         AS bkpf_bktxt, &quot;=form**serial
             bkpf~waers         AS bkpf_waers,
             bkpf~xreversal     AS bkpf_xreve, &quot;1= reversed, 2=reversal
             bkpf~xreversing    AS bkpf_xrsig,
             bkpf~xreversed     AS bkpf_xrsed,

             &quot;BSEG Customer item
             bseg~buzei         AS bseg_buzei,
             bseg~kunnr         AS bseg_kunnr,
             bseg~shkzg         AS bseg_shkzg,
             bseg~wrbtr         AS bseg_wrbtr,
             bseg~sgtxt         AS bseg_sgtxt,
             bseg~zuonr         AS bseg_zuonr,  &quot;=subvID

             &quot;view for Vendor-externalIDs (Dealer, SalesMan)
             vcus~customer      AS vcus_kunnr,
             vcus~idnumber      AS vcus_idnum,

             &quot;customizing per partner type
             c000~party         AS c000_party,
             c000~idtyp         AS c000_idtyp,

             &quot;IFLS subvention interface log
             ifls~rdate         AS ifls_rdate,
             ifls~rtime         AS ifls_rtime,
             ifls~parid         AS ifls_parid,
             ifls~party         AS ifls_party,
             ifls~sbvid         AS ifls_sbvid,
             ifls~bukrs         AS ifls_bukrs,
             ifls~belnr         AS ifls_belnr,
             ifls~gjahr         AS ifls_gjahr,
             ifls~statu         AS ifls_statu

        FROM            bkpf                  AS bkpf
             INNER JOIN bseg                  AS bseg ON bseg~bukrs       =  bkpf~bukrs     AND
                                                         bseg~belnr       =  bkpf~belnr     AND
                                                         bseg~gjahr       =  bkpf~gjahr     AND
                                                         bseg~koart       = &apos;D&apos;                 &quot;only customer item
             INNER JOIN zvncsi_exid_cust      AS vcus ON vcus~customer    =  bseg~kunnr
             INNER JOIN zvncsi_sub_c000       AS c000 ON c000~idtyp       =  vcus~type
             LEFT OUTER JOIN zvncsi_sub_ifls  AS ifls ON ifls~bukrs       =  bkpf~bukrs     AND
                                                         ifls~belnr       =  bkpf~belnr     AND
                                                         ifls~gjahr       =  bkpf~gjahr
         FOR ALL ENTRIES IN @me-&gt;at_isel
        WHERE bkpf~bukrs = @me-&gt;at_isel-bukrs
          AND bkpf~belnr = @me-&gt;at_isel-belnr
          AND bkpf~gjahr = @me-&gt;at_isel-gjahr
          AND ( bkpf~xreversal = &apos; &apos; OR   &quot;_= not reversed
                bkpf~xreversal = &apos;1&apos;   )  &quot;1=     reversed (but no 2= reversal)
      INTO TABLE @DATA(lt_invo).

    &quot;select items (for non-Subv invoices)
    SELECT bseg~bukrs,
           bseg~belnr,
           bseg~gjahr,
           bseg~buzei,
           bseg~wrbtr,
           bseg~sgtxt,
           bseg~menge,
           bseg~meins,
           bseg~mwskz

       FROM bseg AS bseg
      INTO TABLE @DATA(lt_item) FOR ALL ENTRIES IN @lt_invo
      WHERE bukrs = @lt_invo-bkpf_bukrs
        AND belnr = @lt_invo-bkpf_belnr
        AND gjahr = @lt_invo-bkpf_gjahr
        AND buzid = &apos;&apos;                  &quot;no: tax lines
        AND koart = &apos;S&apos;                .&quot;GL items

    &quot;select eInvoice log
    SELECT * FROM zvneinv_ifle INTO TABLE @DATA(lt_ifle) FOR ALL ENTRIES IN @lt_invo
     WHERE bukrs = @lt_invo-bkpf_bukrs
       AND belnr = @lt_invo-bkpf_belnr
       AND gjahr = @lt_invo-bkpf_gjahr
       AND wssta =  &apos;S&apos;               . &quot;S= interface: success
    &quot;keep only last rec per each invoice
    SORT lt_ifle BY bukrs ASCENDING
                    gjahr ASCENDING
                    belnr ASCENDING
                    rdate DESCENDING
                    rtime DESCENDING.
    DELETE ADJACENT DUPLICATES FROM lt_ifle COMPARING bukrs gjahr belnr.


    &quot;* prepare request (map SAP data to ws-request)
    LOOP AT lt_invo ASSIGNING FIELD-SYMBOL(&lt;invo&gt;).

      APPEND INITIAL LINE TO me-&gt;at_req ASSIGNING FIELD-SYMBOL(&lt;req&gt;).

      &quot;§1 keys: partner, document
      MOVE: &lt;invo&gt;-vcus_idnum     TO &lt;req&gt;-partner_code,
            &lt;invo&gt;-c000_party     TO &lt;req&gt;-partner_type,
            &lt;invo&gt;-bkpf_belnr     TO &lt;req&gt;-inv_docn,
            &lt;invo&gt;-bkpf_gjahr     TO &lt;req&gt;-inv_year.

      &quot;§2 data: subvention invoice
      &quot;(yeah, it&apos;s table, but we have one invo for one subv)
      IF &lt;invo&gt;-ifls_sbvid IS NOT INITIAL.

        &quot;APPEND INITIAL LINE TO &lt;req&gt;-struc_inv_dat ASSIGNING FIELD-SYMBOL(&lt;dat&gt;).
        ASSIGN &lt;req&gt;-struc_inv_dat TO FIELD-SYMBOL(&lt;dat&gt;).

        MOVE: &lt;invo&gt;-ifls_sbvid       TO &lt;dat&gt;-subv_id,
              &apos;S&apos;                     TO &lt;dat&gt;-subv_type,         &quot;S= subvention (as opposed to manual)
              &apos;P&apos;                     TO &lt;dat&gt;-inv_status,        &quot;below: P= Posted, R= Reverted
              &lt;invo&gt;-bkpf_bktxt(11)   TO &lt;dat&gt;-inv_form,          &quot;{01GTKT2/002**AA/17Txy} -&gt; {01GTKT2/002}
              &lt;invo&gt;-bkpf_bktxt+13(8) TO &lt;dat&gt;-inv_serial,        &quot;{01GTKT2/002**AA/17Txy} -&gt; {AA/17Txy}
              &lt;invo&gt;-bkpf_xblnr       TO &lt;dat&gt;-inv_rino,
              &apos;&apos;                      TO &lt;dat&gt;-inv_printeddate,   &quot;below: from e-invo:CyberLotus
              &apos;&apos;                      TO &lt;dat&gt;-inv_printeruser,   &quot;below: from e-invo CyberLotus
              &apos;&apos;                      TO &lt;dat&gt;-inv_amount,        &quot;below: curr conv
              &lt;invo&gt;-bkpf_waers       TO &lt;dat&gt;-inv_currency.

        &quot;..invo status
        CASE &lt;invo&gt;-bkpf_xrsed.
          WHEN &apos; &apos;. MOVE: &apos;P&apos; TO &lt;dat&gt;-inv_status.
          WHEN &apos;X&apos;. MOVE: &apos;R&apos; TO &lt;dat&gt;-inv_status.
        ENDCASE.

        &quot;..curr-conv
        &lt;dat&gt;-inv_amount = me-&gt;conv_amnt( iv_amnt = &lt;invo&gt;-bseg_wrbtr
                                          iv_curr = &lt;invo&gt;-bkpf_waers ).

      ENDIF.

      &quot;add data from eInvoice IFLE
      READ TABLE lt_ifle ASSIGNING FIELD-SYMBOL(&lt;ifle&gt;) WITH KEY bukrs = &lt;invo&gt;-bkpf_bukrs
                                                                 belnr = &lt;invo&gt;-bkpf_belnr
                                                                 gjahr = &lt;invo&gt;-bkpf_gjahr.
      IF sy-subrc = 0.
        MOVE: &lt;ifle&gt;-_sta-isprs     TO &lt;dat&gt;-inv_printeruser.
        &quot;e-invo is giving us the data as DD/MM/YYYY, so let change it to YYYY-MM-DD
        &lt;dat&gt;-inv_printeddate = |{ &lt;ifle&gt;-_rsp-signd+6(4) }-{ &lt;ifle&gt;-_rsp-signd+3(2) }-{ &lt;ifle&gt;-_rsp-signd(2) }|.
      ENDIF.

      &quot;§3 manual: manual invoice (non-subv)
      IF &lt;invo&gt;-ifls_sbvid IS INITIAL.

        &quot;&lt;&lt;&lt;header
        ASSIGN &lt;req&gt;-struc_inv_dat TO &lt;dat&gt;.
        MOVE: &lt;invo&gt;-ifls_sbvid       TO &lt;dat&gt;-subv_id,
              &apos;M&apos;                     TO &lt;dat&gt;-subv_type,         &quot;S= subvention (as opposed to manual)
              &apos;&apos;                      TO &lt;dat&gt;-inv_status,        &quot;below: P= Posted, R= Reverted
              &lt;invo&gt;-bkpf_bktxt(11)   TO &lt;dat&gt;-inv_form,          &quot;{01GTKT2/002**AA/17Txy} -&gt; {01GTKT2/002}
              &lt;invo&gt;-bkpf_bktxt+13(8) TO &lt;dat&gt;-inv_serial,        &quot;{01GTKT2/002**AA/17Txy} -&gt; {AA/17Txy}
              &lt;invo&gt;-bkpf_xblnr       TO &lt;dat&gt;-inv_rino,
              &apos;&apos;                      TO &lt;dat&gt;-inv_printeddate,   &quot;? from e-invo:CyberLotus
              &apos;&apos;                      TO &lt;dat&gt;-inv_printeruser,   &quot;? from e-invo:CyberLotus
              &apos;&apos;                      TO &lt;dat&gt;-inv_amount,        &quot;below: curr conv
              &lt;invo&gt;-bkpf_waers       TO &lt;dat&gt;-inv_currency.
        &quot;&gt;&gt;&gt;header


        LOOP AT lt_item ASSIGNING FIELD-SYMBOL(&lt;item&gt;) WHERE bukrs = &lt;invo&gt;-bkpf_bukrs
                                                         AND belnr = &lt;invo&gt;-bkpf_belnr
                                                         AND gjahr = &lt;invo&gt;-bkpf_gjahr.
          APPEND INITIAL LINE TO &lt;req&gt;-st_inv_manual ASSIGNING FIELD-SYMBOL(&lt;man&gt;).

          MOVE: &lt;item&gt;-buzei          TO &lt;man&gt;-itm_no,
                &lt;item&gt;-menge          TO &lt;man&gt;-itm_quantity,
                &apos;&apos;                    TO &lt;man&gt;-itm_unitprice,   &quot;below: faked
                &apos;&apos;                    TO &lt;man&gt;-itm_amount,      &quot;below: curr conv
                &apos;&apos;                    TO &lt;man&gt;-itm_taxrate,     &quot;below: conv
                &lt;item&gt;-sgtxt          TO &lt;man&gt;-itm_text.        &quot;+below: add long text

          &quot;..curr-conv
          &lt;man&gt;-itm_amount = me-&gt;conv_amnt( iv_amnt = &lt;item&gt;-wrbtr
                                            iv_curr = &lt;invo&gt;-bkpf_waers ).

          &quot;..fake qtty
          IF &lt;man&gt;-itm_quantity = 0. &lt;man&gt;-itm_quantity = 1. ENDIF.
          &quot;..fake unit-price
          &lt;man&gt;-itm_unitprice = &lt;man&gt;-itm_amount /  &lt;man&gt;-itm_quantity.

          &quot;..add long text
          DATA(lv_name) = CONV thead-tdname( &lt;item&gt;-bukrs &amp;&amp; &lt;item&gt;-belnr &amp;&amp; &lt;item&gt;-gjahr &amp;&amp; &lt;item&gt;-buzei ).
          &quot;..check &amp; get langu
          SELECT SINGLE * FROM stxh INTO @DATA(ls_stxh) WHERE tdobject = &apos;DOC_ITEM&apos;
                                                          AND tdname   = @lv_name
                                                          AND tdid     = &apos;0001&apos;   .
          IF sy-subrc = 0.
            DATA: lt_line TYPE TABLE OF tline.
            &quot;..read text
            CALL FUNCTION &apos;READ_TEXT&apos;
              EXPORTING
                id       = ls_stxh-tdid
                language = ls_stxh-tdspras
                name     = ls_stxh-tdname
                object   = ls_stxh-tdobject
              TABLES
                lines    = lt_line.
            &quot;..conv to string
            DATA: lt_c360 TYPE TABLE OF ms_rfc3600.
            CALL FUNCTION &apos;CONVERT_ITF_TO_STREAM_TEXT&apos;
              EXPORTING
                language    = ls_stxh-tdspras
              TABLES
                itf_text    = lt_line
                text_stream = lt_c360.
            READ TABLE lt_c360 ASSIGNING FIELD-SYMBOL(&lt;c360&gt;) INDEX 1.
            CONCATENATE &lt;item&gt;-sgtxt &lt;c360&gt; INTO &lt;c360&gt;.
            MOVE: &lt;c360&gt; TO &lt;man&gt;-itm_text.
          ENDIF.

          &quot;         string newline handling
          &quot;context: sap returs SOAP, OSB converts to REST JSON passed to Subvention
          &quot;feature: replace sap newline chars &lt;&amp;#xD;&amp;#xA;&gt; with &lt;\n&gt; for json string
          REPLACE ALL OCCURRENCES OF cl_abap_char_utilities=&gt;cr_lf   IN &lt;man&gt;-itm_text WITH &apos;\n&apos;.


          &quot;..tax rate from tax code
          READ TABLE me-&gt;at_c002 ASSIGNING FIELD-SYMBOL(&lt;c002&gt;) WITH KEY mwskz = &lt;item&gt;-mwskz.
          IF sy-subrc = 0.
            MOVE: &lt;c002&gt;-stxrt TO &lt;man&gt;-itm_taxrate.
          ENDIF.
          IF &lt;item&gt;-mwskz = &apos;&apos;.
            MOVE: &apos;0&apos; TO &lt;man&gt;-itm_taxrate.
          ENDIF.


        ENDLOOP.

        &quot;..invo status
        CASE &lt;invo&gt;-bkpf_xrsed.
          WHEN &apos; &apos;. MOVE: &apos;P&apos; TO &lt;dat&gt;-inv_status.
          WHEN &apos;X&apos;. MOVE: &apos;R&apos; TO &lt;dat&gt;-inv_status.
        ENDCASE.

        &quot;..curr-conv
        &lt;dat&gt;-inv_amount = me-&gt;conv_amnt( iv_amnt = &lt;invo&gt;-bseg_wrbtr
                                          iv_curr = &lt;invo&gt;-bkpf_waers ).



      ENDIF.

    ENDLOOP.

  ENDMETHOD.</source>
  </method>
