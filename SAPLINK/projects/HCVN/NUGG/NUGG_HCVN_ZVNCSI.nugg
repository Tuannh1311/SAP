 <PROG NAME="ZVNCSI_DCS_IFPI_SELSC" VARCL="X" SUBC="I" LEVL="754" RMAND="700" RLOAD="E" UCCHECK="X">
  <textPool>
   <language SPRAS="E">
    <textElement ID="R" ENTRY="ZVNCSI_DCS_IFPI selection screen" LENGTH="32 "/>
   </language>
  </textPool>
  <source>*&amp;---------------------------------------------------------------------*
*&amp; Include          ZVNCSI_DCS_IFPI_SELSC
