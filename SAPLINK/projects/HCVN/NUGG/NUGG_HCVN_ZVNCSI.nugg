  <method CLSNAME="ZCL_VNCSI_DCS_RCOD" CMPNAME="CALL_MIR4" VERSION="1" LANGU="E" DESCRIPT="call: MM-Invoice display" EXPOSURE="0" STATE="1" EDITORDER="27 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_DCS_RCOD" CMPNAME="CALL_MIR4" SCONAME="IR_SDAT" VERSION="1" LANGU="E" DESCRIPT="CSI: DCS Commission-Dealer report (salv)" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="TS_SDAT"/>
   <source>METHOD call_mir4.

    &quot;call MIR4 display Incoming Invoice
    CHECK: ir_sdat IS BOUND,
           ir_sdat-&gt;*-rbkp_belnr IS NOT INITIAL,
           ir_sdat-&gt;*-rbkp_gjahr IS NOT INITIAL.

    SET PARAMETER ID &apos;RBN&apos; FIELD ir_sdat-&gt;*-rbkp_belnr.
    SET PARAMETER ID &apos;GJR&apos; FIELD ir_sdat-&gt;*-rbkp_gjahr.

    CALL TRANSACTION &apos;MIR4&apos; WITH AUTHORITY-CHECK AND SKIP FIRST SCREEN.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOD" CMPNAME="CALL_PORD" VERSION="1" LANGU="E" DESCRIPT="call: POrd display" EXPOSURE="0" STATE="1" EDITORDER="26 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_DCS_RCOD" CMPNAME="CALL_PORD" SCONAME="IR_SDAT" VERSION="1" LANGU="E" DESCRIPT="enhance table IFLC for SALV (tty)" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="TS_SDAT"/>
   <source>METHOD call_pord.

    &quot;call POrd display
    CHECK: ir_sdat IS BOUND.

    CALL FUNCTION &apos;ME_DISPLAY_PURCHASE_DOCUMENT&apos;
      EXPORTING
        i_ebeln              = ir_sdat-&gt;*-ebeln
        i_ebelp              = ir_sdat-&gt;*-ebelp
      EXCEPTIONS
        not_found            = 1
        no_authority         = 2
        invalid_call         = 3
        preview_not_possible = 4
        OTHERS               = 5.
    IF sy-subrc &lt;&gt; 0. MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4. ENDIF.




  ENDMETHOD.</source>
  </method>
