  <method CLSNAME="ZCL_VNCSI_SUB_IFMS" CMPNAME="CALL_IFLS" VERSION="1" LANGU="E" DESCRIPT="call: maint-view IFLS" EXPOSURE="0" STATE="1" EDITORDER="21 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_SUB_IFMS" CMPNAME="CALL_IFLS" SCONAME="IR_SDAT" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="TS_SDAT"/>
   <source>METHOD call_ifls.

    &quot;call maintenance cluster for ifls+ifls1 record

    &quot;prep sel-opts
    DATA lt_sel TYPE TABLE OF vimsellist.
    lt_sel = VALUE #( BASE lt_sel ( viewfield = &apos;RDATE&apos; operator = &apos;EQ&apos; value = ir_sdat-&gt;*-rdate and_or = &apos;AND&apos; ) ).
    lt_sel = VALUE #( BASE lt_sel ( viewfield = &apos;RTIME&apos; operator = &apos;EQ&apos; value = ir_sdat-&gt;*-rtime and_or = &apos;AND&apos; ) ).
    lt_sel = VALUE #( BASE lt_sel ( viewfield = &apos;PARID&apos; operator = &apos;EQ&apos; value = ir_sdat-&gt;*-parid and_or = &apos;AND&apos; ) ).
    lt_sel = VALUE #( BASE lt_sel ( viewfield = &apos;PARTY&apos; operator = &apos;EQ&apos; value = ir_sdat-&gt;*-party and_or = &apos;AND&apos; ) ).
    lt_sel = VALUE #( BASE lt_sel ( viewfield = &apos;SBVID&apos; operator = &apos;EQ&apos; value = ir_sdat-&gt;*-sbvid and_or = &apos;   &apos; ) ).

    &quot;call cluster-view: ZVNCSI_SUB_IFLS
    CALL FUNCTION &apos;VIEWCLUSTER_MAINTENANCE_CALL&apos;
      EXPORTING
        viewcluster_name   = &apos;ZVNCSI_SUB_IFLS&apos;
        maintenance_action = &apos;S&apos;
      TABLES
        dba_sellist        = lt_sel.




  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_IFMS" CMPNAME="CALL_IFPS" VERSION="1" LANGU="E" DESCRIPT="call: processing ifps" EXPOSURE="0" STATE="1" EDITORDER="22 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD call_ifps.

    &quot;*call processor for selected records


    &quot;get selected lines
    DATA(lt_rows) = me-&gt;ar_salv-&gt;get_selections( )-&gt;get_selected_rows( ).


    &quot;prepare sel-opts, per each line
    DATA: lt_rspa TYPE TABLE OF rsparams.

    LOOP AT lt_rows ASSIGNING FIELD-SYMBOL(&lt;row&gt;).

      READ TABLE me-&gt;at_sdat INDEX &lt;row&gt; ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;). CHECK sy-subrc = 0.

      lt_rspa = VALUE #( BASE lt_rspa ( selname = &apos;SO_RDATE&apos; kind = &apos;S&apos; sign = &apos;I&apos; option = &apos;EQ&apos; low = &lt;sdat&gt;-rdate ) ).
      lt_rspa = VALUE #( BASE lt_rspa ( selname = &apos;SO_RTIME&apos; kind = &apos;S&apos; sign = &apos;I&apos; option = &apos;EQ&apos; low = &lt;sdat&gt;-rtime ) ).
      lt_rspa = VALUE #( BASE lt_rspa ( selname = &apos;SO_PARID&apos; kind = &apos;S&apos; sign = &apos;I&apos; option = &apos;EQ&apos; low = &lt;sdat&gt;-parid ) ).
      lt_rspa = VALUE #( BASE lt_rspa ( selname = &apos;SO_PARTY&apos; kind = &apos;S&apos; sign = &apos;I&apos; option = &apos;EQ&apos; low = &lt;sdat&gt;-party ) ).
      lt_rspa = VALUE #( BASE lt_rspa ( selname = &apos;SO_SBVID&apos; kind = &apos;S&apos; sign = &apos;I&apos; option = &apos;EQ&apos; low = &lt;sdat&gt;-sbvid ) ).
      lt_rspa = VALUE #( BASE lt_rspa ( selname = &apos;SO_STATU&apos; kind = &apos;S&apos; sign = &apos;I&apos; option = &apos;EQ&apos; low = &lt;sdat&gt;-statu ) ).

    ENDLOOP.

    &quot;prepare pars
    lt_rspa = VALUE #( BASE lt_rspa ( selname = &apos;PA_WSEP&apos;  kind = &apos;P&apos; low = &apos;X&apos; ) ).

    &quot;remove duplicates
    SORT lt_rspa.
    DELETE ADJACENT DUPLICATES FROM lt_rspa.

    &quot;..call report
    SUBMIT zvncsi_sub_ifps VIA SELECTION-SCREEN WITH SELECTION-TABLE lt_rspa AND RETURN.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_IFMS" CMPNAME="CONSTRUCTOR" VERSION="1" LANGU="E" DESCRIPT="constuctor" EXPOSURE="2" STATE="1" EDITORDER="1 " DISPID="0 " MTDTYPE="2" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_SUB_IFMS" CMPNAME="CONSTRUCTOR" SCONAME="IT_KEYS" VERSION="1" LANGU="E" DESCRIPT="CSI: SUB Subvention log: key (tty)" CMPTYPE="1" MTDTYPE="2" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="ZVNCSI_SUB_IFLS_KEY_T"/>
   <source>METHOD CONSTRUCTOR.

    me-&gt;at_keys = it_keys.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_IFMS" CMPNAME="DATA_SELC" VERSION="1" LANGU="E" DESCRIPT="select data per keys" EXPOSURE="0" STATE="1" EDITORDER="1 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD data_selc.

    &quot;get all IFLS as per keys

    CHECK  me-&gt;at_keys[] IS NOT INITIAL.

    SELECT * FROM zvncsi_sub_ifls INTO TABLE @me-&gt;at_ifls
      FOR ALL ENTRIES IN @me-&gt;at_keys
     WHERE rdate = @me-&gt;at_keys-rdate
       AND rtime = @me-&gt;at_keys-rtime
       AND parid = @me-&gt;at_keys-parid
       AND party = @me-&gt;at_keys-party
       AND sbvid = @me-&gt;at_keys-sbvid.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_IFMS" CMPNAME="ON_DBCL" VERSION="1" LANGU="E" DESCRIPT="handler: on double-click" EXPOSURE="0" STATE="1" EDITORDER="19 " DISPID="0 " MTDTYPE="1" MTDDECLTYP="0" REFCLSNAME="CL_SALV_EVENTS_TABLE" REFCMPNAME="DOUBLE_CLICK" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_SUB_IFMS" CMPNAME="ON_DBCL" SCONAME="ROW" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="1" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="0"/>
   <parameter CLSNAME="ZCL_VNCSI_SUB_IFMS" CMPNAME="ON_DBCL" SCONAME="COLUMN" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="1" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="0"/>
   <source>METHOD ON_DBCL.

    &quot;get double-clicked row
    READ TABLE me-&gt;at_sdat REFERENCE INTO DATA(lr_sdat) INDEX row.

    me-&gt;call_ifls( lr_sdat ). &quot;call m-views for ilfc

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_IFMS" CMPNAME="ON_UCOM" VERSION="1" LANGU="E" DESCRIPT="handler: on user-command" EXPOSURE="0" STATE="1" EDITORDER="20 " DISPID="0 " MTDTYPE="1" MTDDECLTYP="0" REFCLSNAME="CL_SALV_EVENTS" REFCMPNAME="ADDED_FUNCTION" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_SUB_IFMS" CMPNAME="ON_UCOM" SCONAME="E_SALV_FUNCTION" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="1" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="0"/>
   <source>METHOD on_ucom.

    CASE e_salv_function.

      WHEN &apos;REPROCESS&apos;. me-&gt;call_ifps( ).

      WHEN &apos;REFRESH&apos;.   me-&gt;salv_refr( ).

    ENDCASE.

    &quot;refresh, as data might be changed
    me-&gt;salv_refr( ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_IFMS" CMPNAME="REPORT" VERSION="1" LANGU="E" DESCRIPT="main report" EXPOSURE="2" STATE="1" EDITORDER="2 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD REPORT.

    me-&gt;data_selc( ).
    me-&gt;sdat_data( ).

    me-&gt;sdat_enha( ).


    me-&gt;salv_init( ).
    me-&gt;salv_disp( ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_IFMS" CMPNAME="SALV_DISP" VERSION="1" LANGU="E" DESCRIPT="salv-object: display" EXPOSURE="0" STATE="1" EDITORDER="17 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SALV_DISP.

***    &quot; optimise columns
***    me-&gt;ar_salv-&gt;get_columns( )-&gt;set_optimize( abap_true ).


    &quot; and finally display SALV table as full screen
    me-&gt;ar_salv-&gt;display( ).


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_IFMS" CMPNAME="SALV_INIT" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise" EXPOSURE="0" STATE="1" EDITORDER="8 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SALV_INIT.

    &quot; create SAVL table object
    TRY.
        cl_salv_table=&gt;factory( IMPORTING r_salv_table = me-&gt;ar_salv
                                CHANGING  t_table      = me-&gt;at_sdat ).
      CATCH cx_salv_msg.
    ENDTRY.


    me-&gt;salv_init_mlts( ).  &quot;set multi-sel
    me-&gt;salv_init_excp( ).  &quot;set exception columns
    me-&gt;salv_init_func( ).  &quot;set all grid-functions
    me-&gt;salv_init_pfst( ).  &quot;set own gui-status
    me-&gt;salv_init_ccol( ).  &quot;set colours per column
    me-&gt;salv_init_ccel( ).  &quot;set colours per cell-row
    me-&gt;salv_init_invs( ).  &quot;set column invisible
    me-&gt;salv_init_hndl( ).  &quot;set event handlers





  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_IFMS" CMPNAME="SALV_INIT_CCEL" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: colours cell(row)" EXPOSURE="0" STATE="1" EDITORDER="14 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SALV_INIT_CCEL.

    &quot;set colours per cell/row

    &quot;get field list (from columns)
    DATA(lo_cols) =  me-&gt;ar_salv-&gt;get_columns( ).
    DATA(lt_cols) = lo_cols-&gt;get( ).

    &quot;set column SCOL
    TRY. lo_cols-&gt;set_color_column( value = &apos;TSCOL&apos; ). CATCH cx_salv_data_error. ENDTRY.

    &quot;for each line..
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      &quot;..prepare dynamic colour per status
      DATA: ls_scol TYPE lvc_s_colo.
      CASE &lt;sdat&gt;-statu.
        WHEN zif_vncsi_gc=&gt;statu_e. ls_scol-col = 7. &quot;orange
        WHEN zif_vncsi_gc=&gt;statu_f. ls_scol-col = 7. &quot;orange
        WHEN zif_vncsi_gc=&gt;statu_p. ls_scol-col = 1. &quot;blue
        WHEN zif_vncsi_gc=&gt;statu_s. ls_scol-col = 3. &quot;yellow
      ENDCASE.

      &quot;..for each column
      LOOP AT lt_cols ASSIGNING FIELD-SYMBOL(&lt;cols&gt;).
        APPEND INITIAL LINE TO &lt;sdat&gt;-tscol ASSIGNING FIELD-SYMBOL(&lt;tscol&gt;).

        CASE &lt;cols&gt;-columnname.
          WHEN &apos;RDATE&apos;
            OR &apos;RTIME&apos;
            OR &apos;PARID&apos;
            OR &apos;PARTY&apos;
            OR &apos;SBVID&apos;.
            &quot;key fields: highlighted
            MOVE: &lt;cols&gt;-columnname TO &lt;tscol&gt;-fname,
                  ls_scol           TO &lt;tscol&gt;-color,
                  1                 TO &lt;tscol&gt;-color-int.

          WHEN OTHERS.
            &quot;data fields: normal
            MOVE: &lt;cols&gt;-columnname TO &lt;tscol&gt;-fname,
                  ls_scol           TO &lt;tscol&gt;-color,
                  0                 TO &lt;tscol&gt;-color-int.

        ENDCASE.

      ENDLOOP.


    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_IFMS" CMPNAME="SALV_INIT_CCOL" VERSION="1" LANGU="E" DESCRIPT="***salv-object: initialise: colours row" EXPOSURE="0" STATE="1" EDITORDER="13 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SALV_INIT_CCOL.

***    &quot;init: column colours
***
***    DATA: lr_col   TYPE REF TO cl_salv_column_list, &quot; object of column, allowing set_color
***          ls_color TYPE        lvc_s_colo.          &quot; colours structure
***
***    DATA(lr_cols) = me-&gt;ar_salv-&gt;get_columns( ).
***    DATA(lt_cols) = lr_cols-&gt;get( ).
***
***
***    LOOP AT lt_cols ASSIGNING FIELD-SYMBOL(&lt;col&gt;).
***      TRY.
***          lr_col ?= lr_cols-&gt;get_column( &lt;col&gt;-columnname ).
***        CATCH cx_salv_not_found.
***      ENDTRY.
***      &quot;setting colours
***      CASE &lt;col&gt;-columnname.
***        WHEN &apos;RDATE&apos;
***          OR &apos;RTIME&apos;
***          OR &apos;DEALR&apos;
***          OR &apos;SLMAN&apos;
***          OR &apos;COMID&apos;                          . ls_color-col = col_heading.   &quot;key  = grey/blue
***        WHEN &apos;STATU&apos;
***          OR &apos;STATU_TX&apos;
***          OR &apos;STATU_IC&apos;
***          OR &apos;EBELN&apos;
***          OR &apos;EBELP&apos;                          . ls_color-col = col_group   .  &quot;sta  = orange
***        WHEN &apos;MSGID&apos;
***          OR &apos;MSGNO&apos;
***          OR &apos;MSGTY&apos;
***          OR &apos;MSGTXT&apos;                         . ls_color-col = col_total.     &quot;msg  = yellow
***        WHEN &apos;CACTV&apos;
***          OR &apos;CSTAT&apos;
***          OR &apos;CDATE&apos;
***          OR &apos;CAMNT&apos;
***          OR &apos;CCURR&apos;
***          OR &apos;SMGRS&apos;
***          OR &apos;SMPIT&apos;
***          OR &apos;SMDED&apos;
***          OR &apos;SMADJ&apos;
***          OR &apos;SMDCN&apos;                          . ls_color-col = col_positive.  &quot;dat  = green
***      ENDCASE.
***      lr_col-&gt;set_color( ls_color ).
***    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_IFMS" CMPNAME="SALV_INIT_EXCP" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: exception columns" EXPOSURE="0" STATE="1" EDITORDER="10 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SALV_INIT_EXCP.

    &quot; set column for lights
    DATA(lr_cols) = me-&gt;ar_salv-&gt;get_columns( ).
    TRY.
        lr_cols-&gt;set_exception_column( value = &apos;STATU_IC&apos; ).
      CATCH cx_salv_data_error.
    ENDTRY.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_IFMS" CMPNAME="SALV_INIT_FUNC" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: functions" EXPOSURE="0" STATE="1" EDITORDER="11 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SALV_INIT_FUNC.

    &quot;salv all functions
    me-&gt;ar_salv-&gt;get_functions( )-&gt;set_all( abap_true ).

    &quot; layout save &amp; default (&apos;all functions&apos; above are not really all)
    DATA: lr_layout TYPE REF TO cl_salv_layout,
          ls_key    TYPE salv_s_layout_key.

    ls_key-report = me-&gt;lc_repid.

    lr_layout     = me-&gt;ar_salv-&gt;get_layout( ).
    lr_layout-&gt;set_key( ls_key ).         &quot;
    lr_layout-&gt;set_save_restriction( cl_salv_layout=&gt;restrict_none ). &quot;User can save
    lr_layout-&gt;set_default( abap_true ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_IFMS" CMPNAME="SALV_INIT_HNDL" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: event handlers" EXPOSURE="0" STATE="1" EDITORDER="16 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SALV_INIT_HNDL.

    &quot;set handler
    SET HANDLER me-&gt;on_dbcl FOR me-&gt;ar_salv-&gt;get_event( ).
    SET HANDLER me-&gt;on_ucom FOR me-&gt;ar_salv-&gt;get_event( ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_IFMS" CMPNAME="SALV_INIT_INVS" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: invisible fields" EXPOSURE="0" STATE="1" EDITORDER="15 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SALV_INIT_INVS.

    &quot;set invisible fields

    DATA: lr_col   TYPE REF TO cl_salv_column_list. &quot; object of column, allowing set_color

    DATA(lr_cols) = me-&gt;ar_salv-&gt;get_columns( ).
    DATA(lt_cols) = lr_cols-&gt;get( ).

    &quot; remove MANDT
    LOOP AT lt_cols ASSIGNING FIELD-SYMBOL(&lt;col&gt;) WHERE columnname = &apos;MANDT&apos;.
      TRY.
          lr_col ?= lr_cols-&gt;get_column( &lt;col&gt;-columnname ).
        CATCH cx_salv_not_found.
      ENDTRY.
      lr_col-&gt;set_technical( if_salv_c_bool_sap=&gt;true ).
    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_IFMS" CMPNAME="SALV_INIT_MLTS" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: mutli-selection" EXPOSURE="0" STATE="1" EDITORDER="9 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SALV_INIT_MLTS.

    &quot;set multiple-selection
    DATA(lr_sel) = me-&gt;ar_salv-&gt;get_selections( ).
    lr_sel-&gt;set_selection_mode( if_salv_c_selection_mode=&gt;row_column ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_IFMS" CMPNAME="SALV_INIT_PFST" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: gui status" EXPOSURE="0" STATE="1" EDITORDER="12 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SALV_INIT_PFST.

    &quot;init: gui status from calling program

    &quot;set own function by own screen status (seriously, I hate SALV)
    me-&gt;ar_salv-&gt;set_screen_status(
                                    pfstatus      = me-&gt;lc_pfkey
                                    report        = me-&gt;lc_repid
                                    set_functions = me-&gt;ar_salv-&gt;c_functions_all ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_IFMS" CMPNAME="SALV_REFR" VERSION="1" LANGU="E" DESCRIPT="salv-object: refresh" EXPOSURE="0" STATE="1" EDITORDER="18 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SALV_REFR.

    &quot;refresh SALV

    &quot;..re-read data
    CLEAR: me-&gt;at_sdat.

    me-&gt;data_selc( ).
    me-&gt;sdat_data( ).
    me-&gt;sdat_enha( ).

    me-&gt;salv_init_ccel( ).    &quot;re-colour cells (per row-line)


    &quot;..refresh salv
    me-&gt;ar_salv-&gt;refresh( ).



  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_IFMS" CMPNAME="SDAT_DATA" VERSION="1" LANGU="E" DESCRIPT="salv-data: from data" EXPOSURE="0" STATE="1" EDITORDER="2 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_data.

    &quot;pass IFLS to SDAT for SALV processing
    LOOP AT me-&gt;at_ifls ASSIGNING FIELD-SYMBOL(&lt;ifls&gt;).
      APPEND INITIAL LINE TO me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).
      MOVE: &lt;ifls&gt; TO &lt;sdat&gt;-_ifls.
    ENDLOOP.

    &quot; default sort
    SORT me-&gt;at_sdat BY rdate DESCENDING
                        rtime DESCENDING
                        parid DESCENDING
                        party DESCENDING
                        sbvid DESCENDING.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_IFMS" CMPNAME="SDAT_ENHA" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance" EXPOSURE="0" STATE="1" EDITORDER="3 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha.

    &quot;enhance salv-data

    me-&gt;sdat_enha_statu_tx( ).
    me-&gt;sdat_enha_statu_ic( ).
    me-&gt;sdat_enha_proct_tx( ).
    me-&gt;sdat_enha_sbsta_tx( ).


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_IFMS" CMPNAME="SDAT_ENHA_PROCT_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: process text" EXPOSURE="0" STATE="1" EDITORDER="6 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_proct_tx.

    &quot;enhance: process text
    DATA(lo_dref) = CAST cl_abap_elemdescr( cl_abap_elemdescr=&gt;describe_by_name( p_name = &apos;ZVNCSI_SUB_IFLS_PROCT&apos; ) ).
    DATA(lt_vals) = lo_dref-&gt;get_ddic_fixed_values( ).

    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      READ TABLE lt_vals ASSIGNING FIELD-SYMBOL(&lt;vals&gt;) WITH KEY low = &lt;sdat&gt;-proct. CHECK sy-subrc = 0.

      MOVE: &lt;vals&gt;-ddtext TO &lt;sdat&gt;-proct_tx.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_IFMS" CMPNAME="SDAT_ENHA_SBSTA_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: subv-stat text" EXPOSURE="0" STATE="1" EDITORDER="7 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_sbsta_tx.

    &quot;enhance: subv-stat text
    DATA(lo_dref) = CAST cl_abap_elemdescr( cl_abap_elemdescr=&gt;describe_by_name( p_name = &apos;ZVNCSI_SBSTA&apos; ) ).
    DATA(lt_vals) = lo_dref-&gt;get_ddic_fixed_values( ).

    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      READ TABLE lt_vals ASSIGNING FIELD-SYMBOL(&lt;vals&gt;) WITH KEY low = &lt;sdat&gt;-sbsta. CHECK sy-subrc = 0.

      MOVE: &lt;vals&gt;-ddtext TO &lt;sdat&gt;-sbsta_tx.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_IFMS" CMPNAME="SDAT_ENHA_STATU_IC" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: status icon" EXPOSURE="0" STATE="1" EDITORDER="5 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SDAT_ENHA_STATU_IC.

    &quot;set status icon/exception
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      CASE &lt;sdat&gt;-statu.
        WHEN zif_vncsi_gc=&gt;statu_e. &lt;sdat&gt;-statu_ic = 1. &quot;red
        WHEN zif_vncsi_gc=&gt;statu_f. &lt;sdat&gt;-statu_ic = 1. &quot;red
        WHEN zif_vncsi_gc=&gt;statu_p. &lt;sdat&gt;-statu_ic = 3. &quot;green
        WHEN zif_vncsi_gc=&gt;statu_s. &lt;sdat&gt;-statu_ic = 2. &quot;yellow
      ENDCASE.

    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_IFMS" CMPNAME="SDAT_ENHA_STATU_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: status text" EXPOSURE="0" STATE="1" EDITORDER="4 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SDAT_ENHA_STATU_TX.

    &quot;enhance: status text
    DATA(lo_dref) = CAST cl_abap_elemdescr( cl_abap_elemdescr=&gt;describe_by_name( p_name = &apos;ZVNCSI_STATU&apos; ) ).
    DATA(lt_vals) = lo_dref-&gt;get_ddic_fixed_values( ).

    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      READ TABLE lt_vals ASSIGNING FIELD-SYMBOL(&lt;vals&gt;) WITH KEY low = &lt;sdat&gt;-statu. CHECK sy-subrc = 0.

      MOVE: &lt;vals&gt;-ddtext TO &lt;sdat&gt;-statu_tx.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
