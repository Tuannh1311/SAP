  <method CLSNAME="ZCL_VNCSI_DCS_IFMC" CMPNAME="SDAT_ENHA_STATU_IC" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: status icon" EXPOSURE="0" STATE="1" EDITORDER="5 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_statu_ic.

    &quot;set status icon/exception
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      CASE &lt;sdat&gt;-statu.
        WHEN zif_vncsi_gc=&gt;statu_e. &lt;sdat&gt;-statu_ic = 1. &quot;red
        WHEN zif_vncsi_gc=&gt;statu_f. &lt;sdat&gt;-statu_ic = 1. &quot;red
        WHEN zif_vncsi_gc=&gt;statu_p. &lt;sdat&gt;-statu_ic = 3. &quot;green
        WHEN zif_vncsi_gc=&gt;statu_s. &lt;sdat&gt;-statu_ic = 2. &quot;yellow
      ENDCASE.

    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_IFMC" CMPNAME="SDAT_ENHA_STATU_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: status text" EXPOSURE="0" STATE="1" EDITORDER="4 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_statu_tx.

    &quot;enhance: status text
    DATA(lo_dref) = CAST cl_abap_elemdescr( cl_abap_elemdescr=&gt;describe_by_name( p_name = &apos;ZVNCSI_STATU&apos; ) ).
    DATA(lt_vals) = lo_dref-&gt;get_ddic_fixed_values( ).

    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      READ TABLE lt_vals ASSIGNING FIELD-SYMBOL(&lt;vals&gt;) WITH KEY low = &lt;sdat&gt;-statu. CHECK sy-subrc = 0.

      MOVE: &lt;vals&gt;-ddtext TO &lt;sdat&gt;-statu_tx.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
