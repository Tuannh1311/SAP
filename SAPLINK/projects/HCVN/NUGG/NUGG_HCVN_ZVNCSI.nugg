  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOD" CMPNAME="SALV_REFR" VERSION="1" LANGU="E" DESCRIPT="salv-object: refresh" EXPOSURE="0" STATE="1" EDITORDER="23 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_refr.

    &quot;refresh SALV

    &quot;..re-read data
    CLEAR: me-&gt;at_sdat.

    me-&gt;sdat_selc( ). &quot;data select
    me-&gt;sdat_auth( ). &quot;data authorisation check
    me-&gt;sdat_enha( ). &quot;data enhance

    me-&gt;salv_init_ccel( ). &quot;re-colour cells (per row-line)

    &quot;..refresh salv
    me-&gt;ar_salv-&gt;refresh( ).







  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOD" CMPNAME="SDAT_AUTH" VERSION="1" LANGU="E" DESCRIPT="salv-data: check authorisation" EXPOSURE="0" STATE="1" EDITORDER="2 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_auth.

    &quot;authorithy check: display POrd

    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      &quot;M_BEST_EKO= Purchasing Organization in Purchase Order
      AUTHORITY-CHECK OBJECT &apos;M_BEST_EKO&apos;  ID &apos;ACTVT&apos; FIELD &apos;03&apos; ID &apos;EKORG&apos; FIELD &lt;sdat&gt;-ekorg.
      IF sy-subrc &lt;&gt; 0. CLEAR: &lt;sdat&gt;. ENDIF.

      &quot;M_BEST_EKG= Purchasing Group in Purchase Order
      AUTHORITY-CHECK OBJECT &apos;M_BEST_EKG&apos; ID &apos;ACTVT&apos; FIELD &apos;03&apos; ID &apos;EKGRP&apos; FIELD &lt;sdat&gt;-ekgrp.
      IF sy-subrc &lt;&gt; 0. CLEAR: &lt;sdat&gt;. ENDIF.

      &quot;M_BEST_BSA= Document Type in Purchase Order
      AUTHORITY-CHECK OBJECT &apos;M_BEST_BSA&apos; ID &apos;ACTVT&apos; FIELD &apos;03&apos; ID &apos;BSART&apos; FIELD &lt;sdat&gt;-bsart.
      IF sy-subrc &lt;&gt; 0. CLEAR: &lt;sdat&gt;. ENDIF.

      &quot;M_BEST_WRK= Plant in Purchase Order
      AUTHORITY-CHECK OBJECT &apos;M_BEST_WRK&apos; ID &apos;ACTVT&apos; FIELD &apos;03&apos; ID &apos;WERKS&apos; FIELD &lt;sdat&gt;-werks.
      IF sy-subrc &lt;&gt; 0. CLEAR: &lt;sdat&gt;. ENDIF.

    ENDLOOP.
    &quot;..delete deleted
    DELETE me-&gt;at_sdat WHERE ebeln IS INITIAL.



  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOD" CMPNAME="SDAT_ENHA" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance" EXPOSURE="0" STATE="1" EDITORDER="3 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha.

    &quot;enhance salv-data
    CHECK: me-&gt;at_sdat[] IS NOT INITIAL.

    me-&gt;sdat_enha_loekz( ).     &quot;add deletion icon
    me-&gt;sdat_enha_iamnt( ).     &quot;calc invoiced amount

    me-&gt;sdat_enha_belnr( ).     &quot;enha add last incoming invoice

    me-&gt;sdat_enha_mwskz_tx( ).  &quot;enha tax-code description
    me-&gt;sdat_enha_kostl_tx( ).
    me-&gt;sdat_enha_aufnr_tx( ).
    me-&gt;sdat_enha_cpprf_tx( ).
    me-&gt;sdat_enha_blkrs_tx( ).
    me-&gt;sdat_enha_comm_type( ). &quot;*hq_dvy CBL-12796

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOD" CMPNAME="SDAT_ENHA_AUFNR_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: descr order" EXPOSURE="0" STATE="1" EDITORDER="9 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_aufnr_tx.

    &quot;enha order descriptions
    SELECT aufk~aufnr,
           aufk~ktext INTO TABLE @DATA(lt_aufk) FROM aufk
      FOR ALL ENTRIES IN @me-&gt;at_sdat
      WHERE aufnr = @me-&gt;at_sdat-aufnr.

    &quot;pass to items
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;) WHERE aufnr IS NOT INITIAL.

      READ TABLE lt_aufk ASSIGNING FIELD-SYMBOL(&lt;aufk&gt;) WITH KEY aufnr = &lt;sdat&gt;-aufnr. CHECK sy-subrc = 0.
      MOVE: &lt;aufk&gt;-ktext TO &lt;sdat&gt;-aufnr_tx.

    ENDLOOP.



  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOD" CMPNAME="SDAT_ENHA_BELNR" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: incoming invoice" EXPOSURE="0" STATE="1" EDITORDER="12 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_belnr.

    &quot;enha incoming invoice

    &quot;..get all history
    SELECT      ekbe~ebeln,
                ekbe~ebelp,
                ekbe~zekkn,
                ekbe~vgabe,
                ekbe~gjahr,
                ekbe~belnr,
                ekbe~buzei
      FROM            ekbe   AS ekbe
      INTO TABLE @DATA(lt_ekbe)  FOR ALL ENTRIES IN @me-&gt;at_sdat
     WHERE ekbe~ebeln = @me-&gt;at_sdat-ebeln
       AND ekbe~ebelp = @me-&gt;at_sdat-ebelp
       AND ( ekbe~vgabe = 2 OR              &quot;2= Invoice Receipt
             ekbe~vgabe = 3   ).            &quot;3= Subseq. Debit/Credit

    &quot;..keep only last record
    SORT lt_ekbe BY ebeln ASCENDING
                    ebelp ASCENDING
                    gjahr DESCENDING
                    belnr DESCENDING.
    DELETE ADJACENT DUPLICATES FROM lt_ekbe COMPARING ebeln
                                                      ebelp.

    &quot;..pass last invoice
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).
      LOOP AT lt_ekbe ASSIGNING FIELD-SYMBOL(&lt;ekbe&gt;) WHERE ebeln = &lt;sdat&gt;-ebeln
                                                       AND ebelp = &lt;sdat&gt;-ebelp.
        MOVE: &lt;ekbe&gt;-belnr TO &lt;sdat&gt;-rbkp_belnr,
              &lt;ekbe&gt;-gjahr TO &lt;sdat&gt;-rbkp_gjahr.
      ENDLOOP.
    ENDLOOP.

    &quot;..enha with fin-doc data
    IF me-&gt;at_sdat[] IS NOT INITIAL.
      SELECT rbkp~belnr  AS rbkp_belnr,
             rbkp~gjahr  AS rbkp_gjahr,
             rbkp~stblg  AS rbkp_stblg,
             rbkp~stjah  AS rbkp_stjah,
             rbkp~bktxt  AS rbkp_bktxt,
             acdc~rbukrs AS bkpf_bukrs,
             acdc~belnr  AS bkpf_belnr,
             acdc~gjahr  AS bkpf_gjahr,
             acdc~blart  AS bkpf_blart,
             bkpf~xblnr  AS bkpf_xblnr,
             bkpf~bktxt  AS bkpf_bktxt,
             bkpf~stblg  AS bkpf_stblg,
             bkpf~stjah  AS bkpf_stjah,
             aitm~buzei  AS bseg_buzei,
             aitm~augdt  AS bseg_augdt,
             aitm~augbl  AS bseg_augbl,
             cdoc~blart  AS bseg_augbl_blart,
             cdoc~cpudt  AS bseg_augcp
        FROM              rbkp AS rbkp
             INNER JOIN acdoca AS acdc ON acdc~rldnr  = &apos;0L&apos;        AND   &quot;+acdoca~ BKPF
                                          acdc~awtyp  = &apos;RMRP&apos;      AND   &quot;(via ACDOCA as concat/substring for BKPF-AWKEY..
                                          acdc~awref  = rbkp~belnr  AND   &quot; ..doesn&apos;t work with FAE                        )
                                          acdc~aworg  = rbkp~gjahr  AND
                                          acdc~awitem = &apos;000000&apos;
             INNER JOIN bkpf   AS bkpf ON bkpf~bukrs  = acdc~rbukrs AND
                                          bkpf~belnr  = acdc~belnr  AND
                                          bkpf~gjahr  = acdc~gjahr
             INNER JOIN acdoca AS aitm ON aitm~rldnr  = acdc~rldnr  AND   &quot;+acdoca~ BSEG vendor item
                                          aitm~rbukrs = acdc~rbukrs AND
                                          aitm~gjahr  = acdc~gjahr  AND
                                          aitm~belnr  = acdc~belnr  AND
                                          aitm~koart  = &apos;K&apos;
        LEFT OUTER JOIN bkpf   AS cdoc ON cdoc~bukrs  = aitm~rbukrs AND   &quot;+bseg: clearing document
                                          cdoc~belnr  = aitm~augbl  AND
                                          cdoc~gjahr  = aitm~auggj
        LEFT OUTER JOIN t003t  AS t03t ON t03t~spras  = @sy-langu   AND   &quot;+t003: clearing doc-type description
                                          t03t~blart  = cdoc~blart
       INTO TABLE @DATA(lt_fdoc) FOR ALL ENTRIES IN @me-&gt;at_sdat
       WHERE rbkp~belnr = @me-&gt;at_sdat-rbkp_belnr
         AND rbkp~gjahr = @me-&gt;at_sdat-rbkp_gjahr.
      &quot;..pass to report
      LOOP AT me-&gt;at_sdat ASSIGNING &lt;sdat&gt;.
        LOOP AT lt_fdoc ASSIGNING FIELD-SYMBOL(&lt;fdoc&gt;) WHERE rbkp_belnr = &lt;sdat&gt;-rbkp_belnr
                                                         AND rbkp_gjahr = &lt;sdat&gt;-rbkp_gjahr.
          MOVE: &lt;fdoc&gt;-rbkp_stblg TO &lt;sdat&gt;-rbkp_stblg ,
                &lt;fdoc&gt;-rbkp_stjah TO &lt;sdat&gt;-rbkp_stjah ,
                &lt;fdoc&gt;-rbkp_bktxt TO &lt;sdat&gt;-rbkp_bktxt ,
                &lt;fdoc&gt;-bkpf_bukrs TO &lt;sdat&gt;-bkpf_bukrs ,
                &lt;fdoc&gt;-bkpf_belnr TO &lt;sdat&gt;-bkpf_belnr ,
                &lt;fdoc&gt;-bkpf_gjahr TO &lt;sdat&gt;-bkpf_gjahr ,
                &lt;fdoc&gt;-bkpf_blart TO &lt;sdat&gt;-bkpf_blart ,
                &lt;fdoc&gt;-bkpf_xblnr TO &lt;sdat&gt;-bkpf_xblnr ,
                &lt;fdoc&gt;-bkpf_bktxt TO &lt;sdat&gt;-bkpf_bktxt ,
                &lt;fdoc&gt;-bseg_buzei TO &lt;sdat&gt;-bseg_buzei ,
                &lt;fdoc&gt;-bseg_augdt TO &lt;sdat&gt;-bseg_augdt ,
                &lt;fdoc&gt;-bseg_augbl TO &lt;sdat&gt;-bseg_augbl ,
                &lt;fdoc&gt;-bseg_augbl_blart TO &lt;sdat&gt;-bseg_augbl_blart,
                &lt;fdoc&gt;-bseg_augcp TO &lt;sdat&gt;-bseg_augcp.
        ENDLOOP.
      ENDLOOP.
    ENDIF.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOD" CMPNAME="SDAT_ENHA_BLKRS_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: blocking reason" EXPOSURE="0" STATE="1" EDITORDER="11 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_blkrs_tx.
    &quot;salv-data: enhance: blocking reason

    &quot;+CBL-12415 DCS waive-action: add reason text from customizing, if missing
    SELECT * FROM blk_reason_tx INTO TABLE @DATA(lt_brtx).
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;) WHERE blk_reason_id  IS NOT INITIAL
                                                         AND blk_reason_txt IS     INITIAL.

      READ TABLE lt_brtx ASSIGNING FIELD-SYMBOL(&lt;brtx&gt;) WITH KEY spras         = sy-langu
                                                                 blk_reason_id = &lt;sdat&gt;-blk_reason_id. CHECK sy-subrc = 0.

      MOVE: &lt;brtx&gt;-blk_reason_txt TO &lt;sdat&gt;-blk_reason_txt.

    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOD" CMPNAME="SDAT_ENHA_COMM_TYPE" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: descr tax code" EXPOSURE="0" STATE="1" EDITORDER="6 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_comm_type.
*hq_dvy CBL-12796 get commission type based on material
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).
      SELECT SINGLE comm_type FROM zvncsi_dcs_c000
                              INTO &lt;sdat&gt;-comm_type
                              WHERE party = zif_vncsi_gc=&gt;party_d
                                AND matnr = &lt;sdat&gt;-matnr.
    ENDLOOP.
  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOD" CMPNAME="SDAT_ENHA_CPPRF_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: product profile" EXPOSURE="0" STATE="1" EDITORDER="10 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_cpprf_tx.

    &quot;enha product-profile (=segment) descriptions
    SELECT fagl_segmt~segment,
           fagl_segmt~name    INTO TABLE @DATA(lt_segm) FROM fagl_segmt
      FOR ALL ENTRIES IN @me-&gt;at_sdat
      WHERE segment = @me-&gt;at_sdat-cpprf.

    &quot;pass to items
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;) WHERE cpprf IS NOT INITIAL.

      READ TABLE lt_segm ASSIGNING FIELD-SYMBOL(&lt;segm&gt;) WITH KEY segment = &lt;sdat&gt;-cpprf. CHECK sy-subrc = 0.
      MOVE: &lt;segm&gt;-name TO &lt;sdat&gt;-cpprf_tx.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOD" CMPNAME="SDAT_ENHA_IAMNT" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: calculate invoiced amount" EXPOSURE="0" STATE="1" EDITORDER="5 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_iamnt.

    &quot;calculate invoiced amount

    &quot;..calc per item: invoice
    SELECT      ekbe~ebeln,
                ekbe~ebelp,
                ekbe~zekkn,
                ekbe~vgabe,
                ekbe~gjahr,
                ekbe~belnr,
                ekbe~buzei,
                ekbe~wrbtr,
                ekbe~waers,
                ekbe~shkzg
      FROM ekbe INTO TABLE @DATA(lt_ekbe)  FOR ALL ENTRIES IN @me-&gt;at_sdat
     WHERE ekbe~ebeln = @me-&gt;at_sdat-ebeln
       AND ekbe~ebelp = @me-&gt;at_sdat-ebelp
       AND ( ekbe~vgabe = 2 OR              &quot;2= Invoice Receipt
             ekbe~vgabe = 3   ).            &quot;3= Subseq. Debit/Credit

    &quot;..sum per item
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).
      LOOP AT lt_ekbe ASSIGNING FIELD-SYMBOL(&lt;ekbe&gt;) WHERE ebeln = &lt;sdat&gt;-ebeln
                                                       AND ebelp = &lt;sdat&gt;-ebelp.

        CASE &lt;ekbe&gt;-shkzg.
          WHEN &apos;S&apos;. &lt;sdat&gt;-iamnt = &lt;sdat&gt;-iamnt + &lt;ekbe&gt;-wrbtr.
          WHEN &apos;H&apos;. &lt;sdat&gt;-iamnt = &lt;sdat&gt;-iamnt - &lt;ekbe&gt;-wrbtr.
        ENDCASE.

      ENDLOOP.
    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOD" CMPNAME="SDAT_ENHA_KOSTL_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: descr cost centre" EXPOSURE="0" STATE="1" EDITORDER="8 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_kostl_tx.

    &quot;enha cost centre descriptions
    SELECT cskt~kostl,
           cskt~ltext INTO TABLE @DATA(lt_cskt)
      FROM            tka02 AS tka02
           INNER JOIN cskt  AS cskt ON cskt~spras = @sy-langu AND
                                       cskt~kokrs = tka02~kokrs
      FOR ALL ENTRIES IN @me-&gt;at_sdat
      WHERE cskt~kostl = @me-&gt;at_sdat-kostl
        AND tka02~bukrs = @me-&gt;at_sdat-bukrs.

    &quot;pass to items
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;) WHERE kostl IS NOT INITIAL.

      READ TABLE lt_cskt ASSIGNING FIELD-SYMBOL(&lt;cskt&gt;) WITH KEY kostl = &lt;sdat&gt;-kostl. CHECK sy-subrc = 0.
      MOVE: &lt;cskt&gt;-ltext TO &lt;sdat&gt;-kostl_tx.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOD" CMPNAME="SDAT_ENHA_LOEKZ" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: deletion icon" EXPOSURE="0" STATE="1" EDITORDER="4 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SDAT_ENHA_LOEKZ.

    &quot;set icon del/lock
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).
      CASE &lt;sdat&gt;-loekz.
        WHEN &apos;L&apos;. &lt;sdat&gt;-loekz_ic = icon_delete .
        WHEN &apos;S&apos;. &lt;sdat&gt;-loekz_ic = icon_locked.
      ENDCASE.
    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOD" CMPNAME="SDAT_ENHA_MWSKZ_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: descr tax code" EXPOSURE="0" STATE="1" EDITORDER="7 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_mwskz_tx.

    &quot;enha tax-code description

    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      SELECT SINGLE t007s~text1 INTO @&lt;sdat&gt;-mwskz_tx
        FROM            t001  AS t001
             INNER JOIN t005  AS t005  ON t005~land1 = t001~land1
             INNER JOIN t007a AS t007a ON t007a~kalsm = t005~kalsm
             INNER JOIN t007s AS t007s ON t007s~kalsm = t007a~kalsm AND
                                          t007s~mwskz = t007a~mwskz
       WHERE t001~bukrs  = @&lt;sdat&gt;-bukrs
         AND t007a~mwskz = @&lt;sdat&gt;-mwskz
         AND t007s~spras = @sy-langu.

    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOD" CMPNAME="SDAT_SELC" VERSION="1" LANGU="E" DESCRIPT="salv-data: select data per keys" EXPOSURE="0" STATE="1" EDITORDER="1 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_selc.

    &quot;get Commission data as per keys
    CHECK  me-&gt;at_keys[] IS NOT INITIAL.

    SELECT ekko~ebeln,
           ekpo~ebelp,
           ekko~bsart,
           ekko~bukrs,
           ekko~ekorg,
           ekko~ekgrp,
           ekko~lifnr,
           exid~idnumber      AS dealr,
           lfa1~name1         AS lifnr_tx,
           ekpo~loekz,
           ekpo~werks,
           ekpo~bednr         AS comid,
           ekpo~creationdate  AS crdat,
           ekpo~matnr,
           ekpo~txz01,
           ekpo~netwr         AS ekpo_netwr,
           ekpo~menge,
           ekpo~meins,
           ekpo~peinh,
           ekpo~netpr,
           ekko~waers,
           eket~eindt,
           ekpo~mwskz,
           ekpo~erekz,
           ekpo~blk_reason_id,    &quot;+CBL-12415 DCS waive-action: block reason
           ekpo~blk_reason_txt,
           ekkn~zekkn,
           ekkn~sakto,
           skat~txt50         AS sakto_tx,
           ekkn~kostl,
           ekkn~aufnr,
           ekkn~netwr         AS ekkn_netwr,
           ekkn~menge         AS ekkn_menge,
           flck~cpprf,
           flck~cpamt

      INTO CORRESPONDING FIELDS OF TABLE @me-&gt;at_sdat
      FROM            ekko             AS ekko
           INNER JOIN ekpo             AS ekpo ON ekpo~ebeln  = ekko~ebeln
           INNER JOIN lfa1             AS lfa1 ON lfa1~lifnr  = ekko~lifnr
           INNER JOIN zvncsi_exid_vend AS exid ON exid~vendor = ekko~lifnr
           INNER JOIN eket             AS eket ON eket~ebeln  = ekpo~ebeln AND
                                                  eket~ebelp  = ekpo~ebelp
           INNER JOIN ekkn             AS ekkn ON ekkn~ebeln  = ekpo~ebeln AND
                                                  ekkn~ebelp  = ekpo~ebelp
      LEFT OUTER JOIN zvncsi_dcs_iflck AS flck ON flck~ebeln  = ekkn~ebeln AND    &quot;(lef outer for demoes)
                                                  flck~ebelp  = ekkn~ebelp AND
                                                  flck~zekkn  = ekkn~zekkn
           INNER JOIN t001             AS t001 ON t001~bukrs  = ekko~bukrs
           INNER JOIN skat             AS skat ON skat~spras  = @sy-langu  AND
                                                  skat~ktopl  = t001~ktopl AND
                                                  skat~saknr  = ekkn~sakto


      FOR ALL ENTRIES IN @me-&gt;at_keys
     WHERE ekko~ebeln = @me-&gt;at_keys-ebeln
       AND ekpo~ebelp = @me-&gt;at_keys-ebelp.


    &quot; default sort
    SORT me-&gt;at_sdat BY ebeln DESCENDING
                        ebelp ASCENDING
                        zekkn ASCENDING.


    &quot; recalc EKKN-NETWR (is zero when single AccAsg, :( grrr )
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;) WHERE ekkn_netwr = 0.

      &lt;sdat&gt;-ekkn_netwr = &lt;sdat&gt;-ekkn_menge * &lt;sdat&gt;-netpr / &lt;sdat&gt;-peinh .

    ENDLOOP.


    &quot; remove account assignment, if not requested
    IF me-&gt;ax_ekkn = &apos; &apos;.
      DELETE ADJACENT DUPLICATES FROM me-&gt;at_sdat COMPARING ebeln
                                                            ebelp.
    ENDIF.


  ENDMETHOD.</source>
  </method>
 </CLAS>
