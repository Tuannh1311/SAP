  <method CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="SALV_INIT_CCOL" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: colours per columns" EXPOSURE="0" STATE="1" EDITORDER="20 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_ccol.

    &quot;init: column colours

    DATA: lr_col   TYPE REF TO cl_salv_column_list, &quot; object of column, allowing set_color
          ls_color TYPE        lvc_s_colo.          &quot; colours structure

    DATA(lr_cols) = me-&gt;ar_salv-&gt;get_columns( ).
    DATA(lt_cols) = lr_cols-&gt;get( ).

    LOOP AT lt_cols ASSIGNING FIELD-SYMBOL(&lt;col&gt;).
      TRY.
          lr_col ?= lr_cols-&gt;get_column( &lt;col&gt;-columnname ).
        CATCH cx_salv_not_found.
      ENDTRY.

      DATA(ls_c101) = VALUE lvc_s_colo( col = 1 inv = 0 int = 1 ).  &quot;blue+h
      DATA(ls_c100) = VALUE lvc_s_colo( col = 1 inv = 0 int = 0 ).  &quot;blue
      DATA(ls_c300) = VALUE lvc_s_colo( col = 3 inv = 0 int = 0 ).  &quot;yellow
      DATA(ls_c700) = VALUE lvc_s_colo( col = 7 inv = 0 int = 0 ).  &quot;orange
      DATA(ls_c701) = VALUE lvc_s_colo( col = 7 inv = 0 int = 1 ).  &quot;orange+h

      &quot;setting colours
      CASE &lt;col&gt;-columnname.
        WHEN &apos;EBELN&apos;
          OR &apos;EBELP&apos;
          OR &apos;COMID&apos;
          OR &apos;ZEKKN&apos; . lr_col-&gt;set_color( ls_c101 ). &quot;keys : blue+h (comid is kinda-key)
        WHEN &apos;LIFNR_TX&apos;
          OR &apos;TXZ01&apos;
          OR &apos;SAKTO_TX&apos;
          OR &apos;KOSTL_TX&apos;
          OR &apos;AUFNR_TX&apos;
          OR &apos;CPPRF_TX&apos; . lr_col-&gt;set_color( ls_c300 ). &quot;descr: yellow
        WHEN &apos;FLCK_SMGRS&apos; OR &apos;FLCK_SMGRS_CUR&apos;
          OR &apos;FLCK_SMPIT&apos; OR &apos;FLCK_SMPIT_CUR&apos;
          OR &apos;FLCK_SMDED&apos; OR &apos;FLCK_SMDED_CUR&apos;
          OR &apos;FLCK_SMADJ&apos; OR &apos;FLCK_SMADJ_CUR&apos;
          OR &apos;FLCK_SMDCN&apos; OR &apos;FLCK_SMDCN_CUR&apos;
          OR &apos;EKPO_SMGRS&apos; OR &apos;EKPO_SMGRS_CUR&apos;
          OR &apos;EKPO_SMPIT&apos; OR &apos;EKPO_SMPIT_CUR&apos;
          OR &apos;EKPO_SMDED&apos; OR &apos;EKPO_SMDED_CUR&apos;
          OR &apos;EKPO_SMADJ&apos; OR &apos;EKPO_SMADJ_CUR&apos;
          OR &apos;EKPO_SMDCN&apos; OR &apos;EKPO_SMDCN_CUR&apos;   . lr_col-&gt;set_color( ls_c700 ). &quot;sman : orange
        WHEN  OTHERS    . lr_col-&gt;set_color( ls_c100 ). &quot;data : blue
      ENDCASE.

    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="SALV_INIT_COLS" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: columns types (exc, icon, chkbox)" EXPOSURE="0" STATE="1" EDITORDER="17 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_cols.

    &quot;init columns type: exception, icon, checkbox, etc

***    &quot; set column for lights
***    DATA(lr_cols) = me-&gt;ar_salv-&gt;get_columns( ).
***    TRY.
***        lr_cols-&gt;set_exception_column( value = &apos;STATU_IC&apos; ).
***      CATCH cx_salv_data_error.
***    ENDTRY.

***    &quot;init cols: icon
***    TRY.
***        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;LOEKZ_IC &apos;) )-&gt;set_icon( &apos;X&apos; ).
***      CATCH cx_salv_data_error.
***      CATCH cx_salv_not_found.
***    ENDTRY.

    TRY.

        &quot;init cols: F1 rollname
        DATA(lr_cols) = me-&gt;ar_salv-&gt;get_columns( ).
        DATA(lt_cols) = lr_cols-&gt;get( ).
        LOOP AT lt_cols ASSIGNING FIELD-SYMBOL(&lt;cols&gt;).
          &quot;update DDIC ref together with rollname
          &quot;(this will get normal F1 help pointing to our structure)
          DATA(ddic) = &lt;cols&gt;-r_column-&gt;get_ddic_reference( ).
          ddic-table = &apos;ZVNCSI_DCS_RCOS_SDAT&apos;.
          &lt;cols&gt;-r_column-&gt;set_ddic_reference( value = ddic ).
          &lt;cols&gt;-r_column-&gt;set_f1_rollname( value = &apos;&apos; ).
        ENDLOOP.


        &quot;init cols: checkbox
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;EREKZ &apos;) )-&gt;set_cell_type( 1 ).

        &quot;init cols: centered
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;LOEKZ_IC &apos;) )-&gt;set_alignment( if_salv_c_alignment=&gt;centered ).
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;SCSTA_IC &apos;) )-&gt;set_alignment( if_salv_c_alignment=&gt;centered ).

        &quot;init cols: currency
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;NETPR&apos;) )-&gt;set_currency_column( &apos;WAERS&apos; ).
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;EKPO_NETWR&apos;) )-&gt;set_currency_column( &apos;WAERS&apos; ).
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;EKKN_NETWR&apos;) )-&gt;set_currency_column( &apos;WAERS&apos; ).
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;IAMNT&apos;) )-&gt;set_currency_column( &apos;WAERS&apos; ).

        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;EKPO_SMGRS_CUR&apos;) )-&gt;set_currency_column( &apos;WAERS&apos; ).
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;EKPO_SMPIT_CUR&apos;) )-&gt;set_currency_column( &apos;WAERS&apos; ).
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;EKPO_SMDED_CUR&apos;) )-&gt;set_currency_column( &apos;WAERS&apos; ).
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;EKPO_SMADJ_CUR&apos;) )-&gt;set_currency_column( &apos;WAERS&apos; ).
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;EKPO_SMDCN_CUR&apos;) )-&gt;set_currency_column( &apos;WAERS&apos; ).

        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;FLCK_SMGRS_CUR&apos;) )-&gt;set_currency_column( &apos;WAERS&apos; ).
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;FLCK_SMPIT_CUR&apos;) )-&gt;set_currency_column( &apos;WAERS&apos; ).
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;FLCK_SMDED_CUR&apos;) )-&gt;set_currency_column( &apos;WAERS&apos; ).
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;FLCK_SMADJ_CUR&apos;) )-&gt;set_currency_column( &apos;WAERS&apos; ).
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;FLCK_SMDCN_CUR&apos;) )-&gt;set_currency_column( &apos;WAERS&apos; ).

        &quot;init cols: decimals
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;CPAMT&apos;) )-&gt;set_decimals( value = &apos;0&apos; ).

        &quot;hide all amt-numc (now as amt-curr)
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;EKPO_SMGRS&apos;) )-&gt;set_technical( value = &apos;X&apos; ).
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;EKPO_SMPIT&apos;) )-&gt;set_technical( value = &apos;X&apos; ).
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;EKPO_SMDED&apos;) )-&gt;set_technical( value = &apos;X&apos; ).
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;EKPO_SMADJ&apos;) )-&gt;set_technical( value = &apos;X&apos; ).
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;EKPO_SMDCN&apos;) )-&gt;set_technical( value = &apos;X&apos; ).

        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;FLCK_SMGRS&apos;) )-&gt;set_technical( value = &apos;X&apos; ).
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;FLCK_SMPIT&apos;) )-&gt;set_technical( value = &apos;X&apos; ).
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;FLCK_SMDED&apos;) )-&gt;set_technical( value = &apos;X&apos; ).
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;FLCK_SMADJ&apos;) )-&gt;set_technical( value = &apos;X&apos; ).
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;FLCK_SMDCN&apos;) )-&gt;set_technical( value = &apos;X&apos; ).

      CATCH cx_salv_data_error.
      CATCH cx_salv_not_found.
    ENDTRY.



  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="SALV_INIT_FUNC" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: functions" EXPOSURE="0" STATE="1" EDITORDER="18 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_func.

    &quot;salv all functions
    me-&gt;ar_salv-&gt;get_functions( )-&gt;set_all( abap_true ).

    &quot; layout save &amp; default (&apos;all functions&apos; above are not really all)
    DATA: lr_layout TYPE REF TO cl_salv_layout,
          ls_key    TYPE salv_s_layout_key.

    ls_key-report = me-&gt;lc_repid.

    lr_layout     = me-&gt;ar_salv-&gt;get_layout( ).
    lr_layout-&gt;set_key( ls_key ).         &quot;
    lr_layout-&gt;set_save_restriction( cl_salv_layout=&gt;restrict_none ). &quot;User can save
    lr_layout-&gt;set_default( abap_true ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="SALV_INIT_HNDL" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: event handlers" EXPOSURE="0" STATE="1" EDITORDER="23 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_hndl.

    &quot;set handler
    SET HANDLER me-&gt;on_dbcl FOR me-&gt;ar_salv-&gt;get_event( ).
    SET HANDLER me-&gt;on_ucom FOR me-&gt;ar_salv-&gt;get_event( ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="SALV_INIT_INVS" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: invisible fields" EXPOSURE="0" STATE="1" EDITORDER="22 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_invs.

    &quot;set invisible fields

    DATA: lr_col   TYPE REF TO cl_salv_column_list. &quot; object of column, allowing set_color

    DATA(lr_cols) = me-&gt;ar_salv-&gt;get_columns( ).
    DATA(lt_cols) = lr_cols-&gt;get( ).

    &quot; remove MANDT
    LOOP AT lt_cols ASSIGNING FIELD-SYMBOL(&lt;col&gt;) WHERE columnname = &apos;MANDT&apos;.
      TRY.
          lr_col ?= lr_cols-&gt;get_column( &lt;col&gt;-columnname ).
        CATCH cx_salv_not_found.
      ENDTRY.
      lr_col-&gt;set_technical( if_salv_c_bool_sap=&gt;true ).
    ENDLOOP.

    &quot; remove account assignment, if not requested
    IF me-&gt;ax_ekkn = &apos; &apos;.
      LOOP AT lt_cols ASSIGNING              &lt;col&gt;  WHERE columnname = &apos;ZEKKN&apos;
                                                       OR columnname = &apos;CPPRF&apos;
                                                       OR columnname = &apos;CPPRF_TX&apos;
                                                       OR columnname = &apos;CPAMT&apos;
                                                       OR columnname = &apos;EKKN_NETWR&apos;
                                                       OR columnname = &apos;SAKTO&apos;
                                                       OR columnname = &apos;SAKTO_TX&apos;
                                                       OR columnname = &apos;KOSTL&apos;
                                                       OR columnname = &apos;KOSTL_TX&apos;
                                                       OR columnname = &apos;AUFNR&apos;
                                                       OR columnname = &apos;AUFNR_TX&apos;
                                                       OR columnname = &apos;FLCK_SMGRS&apos;
                                                       OR columnname = &apos;FLCK_SMPIT&apos;
                                                       OR columnname = &apos;FLCK_SMDED&apos;
                                                       OR columnname = &apos;FLCK_SMADJ&apos;
                                                       OR columnname = &apos;FLCK_SMDCN&apos;.
        TRY.
            lr_col ?= lr_cols-&gt;get_column( &lt;col&gt;-columnname ).
          CATCH cx_salv_not_found.
        ENDTRY.
        lr_col-&gt;set_technical( if_salv_c_bool_sap=&gt;true ).
      ENDLOOP.
    ENDIF.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="SALV_INIT_MLTS" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: mutli-selection" EXPOSURE="0" STATE="1" EDITORDER="16 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_mlts.

    &quot;set multiple-selection
    DATA(lr_sel) = me-&gt;ar_salv-&gt;get_selections( ).
