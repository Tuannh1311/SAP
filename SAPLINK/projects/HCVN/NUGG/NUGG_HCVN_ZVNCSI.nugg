  <method CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="SDAT_ENHA_IFLE" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: eInvoice log" EXPOSURE="0" STATE="1" EDITORDER="11 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_ifle.

    &quot;get IFLE eInvoice log
    IF me-&gt;at_sdat[] IS NOT INITIAL.

      &quot;get IFLE data
      SELECT * FROM zvneinv_ifle INTO TABLE @DATA(lt_ifle) FOR ALL ENTRIES IN @me-&gt;at_sdat
       WHERE bukrs EQ @me-&gt;at_sdat-bukrs
         AND belnr EQ @me-&gt;at_sdat-belnr
         AND gjahr EQ @me-&gt;at_sdat-gjahr.

      &quot;keep only last rec per each invoice
      SORT lt_ifle BY bukrs ASCENDING
                      gjahr ASCENDING
                      belnr ASCENDING
                      rdate DESCENDING
                      rtime DESCENDING.
      DELETE ADJACENT DUPLICATES FROM lt_ifle COMPARING bukrs gjahr belnr.

      &quot;pass to report
      LOOP AT lt_ifle ASSIGNING FIELD-SYMBOL(&lt;ifle&gt;).
        READ TABLE me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;) WITH KEY bukrs = &lt;ifle&gt;-bukrs
                                                                       belnr = &lt;ifle&gt;-belnr
                                                                       gjahr = &lt;ifle&gt;-gjahr. CHECK sy-subrc = 0.
        MOVE: &lt;ifle&gt; TO &lt;sdat&gt;-_ifle.
      ENDLOOP.

    ENDIF.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="SDAT_ENHA_KUNNR_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: customer description" EXPOSURE="0" STATE="1" EDITORDER="7 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_kunnr_tx.

    &quot;enha customer description
    CHECK: me-&gt;at_sdat IS NOT INITIAL.

    SELECT kunnr,
           name1  FROM kna1 INTO TABLE @DATA(lt_kna1) FOR ALL ENTRIES IN @me-&gt;at_sdat WHERE kunnr = @me-&gt;at_sdat-kunnr.

    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      READ TABLE lt_kna1 ASSIGNING FIELD-SYMBOL(&lt;kna1&gt;) WITH KEY kunnr = &lt;sdat&gt;-kunnr. CHECK sy-subrc = 0.

      MOVE: &lt;kna1&gt;-name1 TO &lt;sdat&gt;-kunnr_tx.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="SDAT_ENHA_PROCT_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: processing type descr" EXPOSURE="0" STATE="1" EDITORDER="8 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_proct_tx.

    &quot;enha processing type description
    DATA(lo_dref) = CAST cl_abap_elemdescr( cl_abap_elemdescr=&gt;describe_by_name( p_name = &apos;ZVNCSI_SUB_IFLS_PROCT&apos; ) ).
    DATA(lt_vals) = lo_dref-&gt;get_ddic_fixed_values( ).

    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      READ TABLE lt_vals ASSIGNING FIELD-SYMBOL(&lt;vals&gt;) WITH KEY low = &lt;sdat&gt;-proct. CHECK sy-subrc = 0.

      MOVE: &lt;vals&gt;-ddtext TO &lt;sdat&gt;-proct_tx.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="SDAT_ENHA_SBSTA_IC" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: subvention status icon" EXPOSURE="0" STATE="1" EDITORDER="10 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_sbsta_ic.

    &quot;ehna status icon
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      CASE &lt;sdat&gt;-sbsta.
        WHEN zif_vncsi_gc=&gt;sbsta_xx. &lt;sdat&gt;-sbsta_ic = &apos;&apos;                          . &quot;status: __ not relevant
        WHEN zif_vncsi_gc=&gt;sbsta_0p. &lt;sdat&gt;-sbsta_ic = icon_status_open            . &quot;status: 0P parked
        WHEN zif_vncsi_gc=&gt;sbsta_1r. &lt;sdat&gt;-sbsta_ic = icon_status_booked          . &quot;status: 1R reviewed
        WHEN zif_vncsi_gc=&gt;sbsta_2a. &lt;sdat&gt;-sbsta_ic = icon_status_partly_booked   . &quot;status: 2A approved
        WHEN zif_vncsi_gc=&gt;sbsta_5p. &lt;sdat&gt;-sbsta_ic = icon_closed_folder_uptodate . &quot;status: 5P posted
      ENDCASE.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="SDAT_ENHA_SBSTA_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: subvention status descr" EXPOSURE="0" STATE="1" EDITORDER="9 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_sbsta_tx.

    &quot;enha status description
    DATA(lo_dref) = CAST cl_abap_elemdescr( cl_abap_elemdescr=&gt;describe_by_name( p_name = &apos;ZVNCSI_SBSTA&apos; ) ).
    DATA(lt_vals) = lo_dref-&gt;get_ddic_fixed_values( ).

    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      READ TABLE lt_vals ASSIGNING FIELD-SYMBOL(&lt;vals&gt;) WITH KEY low = &lt;sdat&gt;-sbsta. CHECK sy-subrc = 0.

      MOVE: &lt;vals&gt;-ddtext TO &lt;sdat&gt;-sbsta_tx.

    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="SDAT_ENHA_WRBTR" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: wrbtr amount (bseg or vbseg)" EXPOSURE="0" STATE="1" EDITORDER="6 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_wrbtr.

    &quot;customer item amount, either bseg or vbseg
    CHECK: me-&gt;at_sdat IS NOT INITIAL.

    &quot;bseg: posted documents
    SELECT bukrs, belnr, gjahr, buzei, wrbtr FROM bseg   INTO TABLE @DATA(lt_bseg) FOR ALL ENTRIES IN @me-&gt;at_sdat
     WHERE bukrs = @me-&gt;at_sdat-bukrs
       AND belnr = @me-&gt;at_sdat-belnr
       AND gjahr = @me-&gt;at_sdat-gjahr
       AND koart = &apos;D&apos;.

    &quot;vbsegd: parked
    SELECT bukrs, belnr, gjahr, buzei, wrbtr FROM vbsegd INTO TABLE @DATA(lt_vseg) FOR ALL ENTRIES IN @me-&gt;at_sdat
     WHERE bukrs = @me-&gt;at_sdat-bukrs
       AND belnr = @me-&gt;at_sdat-belnr
       AND gjahr = @me-&gt;at_sdat-gjahr.

    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      &quot;try posted
      READ TABLE lt_bseg ASSIGNING FIELD-SYMBOL(&lt;bseg&gt;) WITH KEY bukrs = &lt;sdat&gt;-bukrs
                                                                 belnr = &lt;sdat&gt;-belnr
                                                                 gjahr = &lt;sdat&gt;-gjahr.
      IF sy-subrc = 0.
        MOVE: &lt;bseg&gt;-wrbtr TO &lt;sdat&gt;-adoc_wrbtr.
      ENDIF.

      &quot;try parked
      READ TABLE lt_vseg ASSIGNING FIELD-SYMBOL(&lt;vseg&gt;) WITH KEY bukrs = &lt;sdat&gt;-bukrs
                                                                 belnr = &lt;sdat&gt;-belnr
                                                                 gjahr = &lt;sdat&gt;-gjahr.
      IF sy-subrc = 0.
        MOVE: &lt;vseg&gt;-wrbtr TO &lt;sdat&gt;-adoc_wrbtr.
      ENDIF.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="SDAT_SELC" VERSION="1" LANGU="E" DESCRIPT="salv-data: select data per keys" EXPOSURE="0" STATE="1" EDITORDER="3 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_selc.

    &quot;select data per keys
    CHECK  me-&gt;at_keys[] IS NOT INITIAL.

    SELECT ifls~rdate,
           ifls~rtime,
           ifls~parid,
           ifls~party,
           exid~customer AS kunnr,
           ifls~sbvid,
           ifls~proct,
           ifls~sbsta,
           ifls~bukrs,
           ifls~belnr,
           ifls~gjahr,
           ifls~ref_bukrs,
           ifls~ref_belnr,
           ifls~ref_gjahr,
           ifls~sxadj,
           ifls~sdadj,
           ifls~syadj,
           ifls~sdctp,
           ifls~sxcnc,
           ifls~stcnc,
           ifls~sredi,
           ifls~sdate,
           ifls~scurr,
           bkpf~blart AS adoc_blart,
           bkpf~bldat AS adoc_bldat,
           bkpf~budat AS adoc_budat,
           bkpf~cpudt AS adoc_cpudt,
           bkpf~waers AS adoc_waers,
           bkpf~xblnr AS adoc_xblnr,
           bkpf~bktxt AS adoc_bktxt,
           bkpf~stblg AS adoc_stblg,
           bkpf~stjah AS adoc_stjah,
           bsar~rebzg AS adoc_rebzg,
           bsar~rebzj AS adoc_rebzj,
           bsar~augdt AS adoc_augdt,
           bsar~augcp AS adoc_augcp,
           bsar~augbl AS adoc_augbl,
           bsar~auggj AS adoc_auggj,
           cdoc~blart AS adoc_augbl_blart
      FROM            zvncsi_sub_ifls   AS ifls
           INNER JOIN zvncsi_exid_cust  AS exid ON exid~idnumber = ifls~parid
      LEFT OUTER JOIN bkpf              AS bkpf ON bkpf~bukrs    = ifls~bukrs AND
                                                   bkpf~belnr    = ifls~belnr AND
                                                   bkpf~gjahr    = ifls~gjahr
      LEFT OUTER JOIN bseg              AS bsar ON bsar~bukrs    = bkpf~bukrs AND
                                                   bsar~belnr    = bkpf~belnr AND
                                                   bsar~gjahr    = bkpf~gjahr AND
                                                   bsar~koart    = &apos;D&apos;              &quot;D= customer item
      LEFT OUTER JOIN bkpf              AS cdoc ON cdoc~bukrs    = bsar~bukrs AND
                                                   cdoc~belnr    = bsar~augbl AND
                                                   cdoc~gjahr    = bsar~auggj
      INTO CORRESPONDING FIELDS OF TABLE @me-&gt;at_sdat
      FOR ALL ENTRIES IN @me-&gt;at_keys
     WHERE ifls~rdate = @me-&gt;at_keys-rdate
       AND ifls~rtime = @me-&gt;at_keys-rtime
       AND ifls~parid = @me-&gt;at_keys-parid
       AND ifls~party = @me-&gt;at_keys-party
       AND ifls~sbvid = @me-&gt;at_keys-sbvid.

    SORT me-&gt;at_sdat BY rdate DESCENDING
                        rtime DESCENDING.




  ENDMETHOD.</source>
  </method>
 </CLAS>
