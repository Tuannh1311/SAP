    ENDTRY.
  ELSE.
    TRY.
        CREATE OBJECT lo_proxy EXPORTING logical_port_name = iv_port.
      CATCH cx_ai_system_fault.
    ENDTRY.
  ENDIF.

  &quot; populate input / request (map fm to proxy)
  LOOP AT it_req ASSIGNING FIELD-SYMBOL(&lt;req&gt;).

    APPEND INITIAL LINE TO ls_soapin-invoice ASSIGNING FIELD-SYMBOL(&lt;soapin&gt;).

    &quot;invoice
    MOVE: &lt;req&gt;-dealer_code    TO &lt;soapin&gt;-dealer_code,
          &lt;req&gt;-sman_code      TO &lt;soapin&gt;-sman_code,
          &lt;req&gt;-inv_doc_no     TO &lt;soapin&gt;-inv_doc_no,
          &lt;req&gt;-inv_doc_yr     TO &lt;soapin&gt;-inv_doc_yr,
          &lt;req&gt;-post_date      TO &lt;soapin&gt;-post_date,
          &lt;req&gt;-reference      TO &lt;soapin&gt;-reference,
          &lt;req&gt;-serial         TO &lt;soapin&gt;-serial,
          &lt;req&gt;-status         TO &lt;soapin&gt;-status,
         &quot;&lt;req&gt;-amnt_tot       TO &lt;soapin&gt;-amnt_tot,
          &lt;req&gt;-currency       TO &lt;soapin&gt;-currency,
          &lt;req&gt;-inv_type       TO &lt;soapin&gt;-inv_type,
          &lt;req&gt;-vat_rate       TO &lt;soapin&gt;-vat_rate.
    &lt;soapin&gt;-amnt_tot = zvnmm_util=&gt;conv_curr_ex(  iv_curr = CONV #( &lt;soapin&gt;-currency )
                                                   iv_inta =         &lt;req&gt;-amnt_tot       ).


    &quot;invoice item
    LOOP AT &lt;req&gt;-st_inv_req_itm ASSIGNING FIELD-SYMBOL(&lt;ritm&gt;).
      APPEND INITIAL LINE TO &lt;soapin&gt;-invoice_items-invoice_item ASSIGNING FIELD-SYMBOL(&lt;sitm&gt;).
      MOVE: &lt;ritm&gt;-comm_id     TO &lt;sitm&gt;-comm_id.
      &quot;&lt;ritm&gt;-amnt_itm    TO &lt;sitm&gt;-amnt_itm.
