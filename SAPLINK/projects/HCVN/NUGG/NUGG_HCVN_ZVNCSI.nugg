  <source>*&amp;---------------------------------------------------------------------*
*&amp; Report ZVNCSI_DCS_RCOD
*&amp;---------------------------------------------------------------------*
*&amp;
*&amp;---------------------------------------------------------------------*
REPORT zvncsi_dcs_rcod.
*&amp; VN-CSI INT_MM_01 (HQ_TTa) DCS Dealer Commission System integration
*&amp;   context:
*&amp;            DCS systems manages Dealer/SalesMan Commissions
*&amp;   feature:
*&amp;            report Dealers Commissions, Purchase Orders

TABLES: ekko,  ekpo, eket, but0id, rbkp, bkpf, bseg,
        zvncsi_dcs_c000 . &quot;hq_dvy CBL-12796

**********************************************************************
*&amp; selection screen
SELECTION-SCREEN: BEGIN OF BLOCK s01 WITH FRAME TITLE TEXT-s01. &quot;block: Dealer selection
  SELECT-OPTIONS: so_idtyp FOR  but0id-type MODIF ID dis.
  SELECT-OPTIONS: so_idnum FOR  but0id-idnumber,
                  so_lifnr FOR  ekko-lifnr.
SELECTION-SCREEN: END   OF BLOCK s01.
SELECTION-SCREEN: BEGIN OF BLOCK s02 WITH FRAME TITLE TEXT-s02. &quot;block: Commission selection
  PARAMETERS:     pa_bsart TYPE ekko-bsart MODIF ID dis.
  SELECT-OPTIONS: so_bednr FOR  ekpo-bednr,
                  so_comtp FOR  zvncsi_dcs_c000-comm_type, &quot;hq_dvy CBL-12796
                  so_ebeln FOR  ekko-ebeln,
                  so_crdat FOR  ekpo-creationdate,
                  so_eindt FOR  eket-eindt,
                  so_txz01 FOR  ekpo-txz01,
                  so_matkl FOR  ekpo-matkl,
                  so_blkrs FOR  ekpo-blk_reason_id MATCHCODE OBJECT zvncsi_blkrs_h.
SELECTION-SCREEN: END   OF BLOCK s02.
SELECTION-SCREEN: BEGIN OF BLOCK s04 WITH FRAME TITLE TEXT-s04. &quot;block: MM-inv selection  &quot;+SAP-3609 RCOD add sel-fields
  SELECT-OPTIONS: so_mblnr FOR rbkp-belnr,
                  so_mgjar FOR rbkp-gjahr.
SELECTION-SCREEN: END   OF BLOCK s04.
SELECTION-SCREEN: BEGIN OF BLOCK s05 WITH FRAME TITLE TEXT-s05. &quot;block: FI-doc selection  &quot;+SAP-3609 RCOD add sel-fields
  SELECT-OPTIONS: so_fblnr FOR bkpf-belnr,
                  so_fgjar FOR bkpf-gjahr,
                  so_fxbln FOR bkpf-xblnr,
                  so_fagdt FOR bseg-augdt,
                  so_fagbl FOR bseg-augbl.
SELECTION-SCREEN: END   OF BLOCK s05.
SELECTION-SCREEN: BEGIN OF BLOCK s03 WITH FRAME TITLE TEXT-s03. &quot;block: selection level
  PARAMETERS:     px_sekkn TYPE xfeld.
SELECTION-SCREEN: END   OF BLOCK s03.

AT SELECTION-SCREEN OUTPUT. &quot;&amp; sel-screen: close pre-defined fields
  LOOP AT SCREEN.
    IF     screen-group1 EQ &apos;DIS&apos; AND
       NOT screen-name   CS &apos;_APP_%-VALU_PUSH&apos;.
      screen-input = 0.
    ENDIF.
    MODIFY SCREEN.
  ENDLOOP.

**********************************************************************
*&amp; pre-defined fields for sel-screen
INITIALIZATION.
  SELECT * FROM zvncsi_dcs_c003 INTO TABLE @DATA(lt_c003) WHERE party = @zif_vncsi_gc=&gt;party_d.
  LOOP AT lt_c003 ASSIGNING FIELD-SYMBOL(&lt;c003&gt;).
    MOVE: &lt;c003&gt;-idtyp  TO so_idtyp-low,
          &apos;I&apos;           TO so_idtyp-sign,
          &apos;EQ&apos;          TO so_idtyp-option.
    APPEND so_idtyp.
  ENDLOOP.

  SELECT SINGLE * FROM zvncsi_dcs_c000 INTO @DATA(ls_c000) WHERE party = @zif_vncsi_gc=&gt;party_d.
  MOVE: ls_c000-bsart TO pa_bsart.

********************************************************************** &quot;
*&amp; data selection
START-OF-SELECTION.
  DATA: lt_keys TYPE zcl_vncsi_dcs_rcod=&gt;tt_keys.

*begin of hq_dvy CBL-12796 - commission type based on material
  DATA lt_materials TYPE RANGE OF matnr.
  IF NOT so_comtp IS INITIAL.
    SELECT matnr FROM zvncsi_dcs_c000
                 INTO ls_c000-matnr
                 WHERE party = zif_vncsi_gc=&gt;party_d
                         AND comm_type IN so_comtp.
      IF NOT ls_c000-matnr IS INITIAL.
        APPEND INITIAL LINE TO  lt_materials REFERENCE INTO DATA(lr_material).
        MOVE: &apos;IEQ&apos; TO lr_material-&gt;*+0(3),
              ls_c000-matnr TO lr_material-&gt;low.
      ENDIF.
    ENDSELECT.
  ENDIF.
*end of hq_dvy CBL-12796 - commission type based on material

  SELECT ekpo~ebeln, ekpo~ebelp INTO TABLE @lt_keys
    FROM            ekko             AS ekko
         INNER JOIN zvncsi_exid_vend AS exid ON exid~vendor = ekko~lifnr
         INNER JOIN ekpo             AS ekpo ON ekpo~ebeln  = ekko~ebeln
         INNER JOIN eket             AS eket ON eket~ebeln  = ekpo~ebeln AND
                                                eket~ebelp  = ekpo~ebelp
    LEFT OUTER JOIN ekbe             AS ekbe ON ekbe~ebeln  = ekpo~ebeln AND
                                                ekbe~ebelp  = ekpo~ebelp AND
                                              ( ekbe~vgabe  = 2 OR
                                                ekbe~vgabe  = 3         )
    LEFT OUTER JOIN rbkp             AS rbkp ON rbkp~belnr  = ekbe~belnr  AND   &quot;+SAP-3609 RCOD add sel-fields
                                                rbkp~gjahr  = ekbe~gjahr
    LEFT OUTER JOIN acdoca           AS acdo ON acdo~rldnr  = &apos;0L&apos;        AND   &quot;+SAP-3609 RCOD add sel-fields
                                                acdo~awtyp  = &apos;RMRP&apos;      AND
                                                acdo~awref  = ekbe~belnr  AND
                                                acdo~aworg  = ekbe~gjahr  AND
                                                acdo~awitem = &apos;000000&apos;
    LEFT OUTER JOIN bkpf             AS bkpf ON bkpf~bukrs  = acdo~rbukrs AND   &quot;+SAP-3609 RCOD add sel-fields
                                                bkpf~belnr  = acdo~belnr  AND
                                                bkpf~gjahr  = acdo~gjahr
    LEFT OUTER JOIN bseg             AS bseg ON bseg~bukrs  = bkpf~bukrs  AND   &quot;+SAP-3609 RCOD add sel-fields
                                                bseg~belnr  = bkpf~belnr  AND
                                                bseg~gjahr  = bkpf~gjahr  AND
                                                bseg~buzei  = &apos;001&apos;
   WHERE exid~type          IN @so_idtyp
     AND exid~idnumber      IN @so_idnum
     AND ekko~lifnr         IN @so_lifnr
     AND ekko~bsart         EQ @pa_bsart
     AND ekko~ebeln         IN @so_ebeln
     AND ekpo~bednr         IN @so_bednr
     AND ekpo~creationdate  IN @so_crdat
     AND eket~eindt         IN @so_eindt
     AND ekpo~txz01         IN @so_txz01
     AND ekpo~matkl         IN @so_matkl
     AND ekpo~blk_reason_id IN @so_blkrs
     AND ekpo~matnr         IN @lt_materials &quot;hq_dvy CBL-12796
     &quot;+SAP-3609 RCOD add sel-fields
     AND rbkp~belnr         IN @so_mblnr
     AND rbkp~gjahr         IN @so_mgjar
     AND bkpf~belnr         IN @so_fblnr
     AND bkpf~gjahr         IN @so_fgjar
     AND bkpf~xblnr         IN @so_fxbln
     AND bseg~augdt         IN @so_fagdt
     AND bseg~augbl         IN @so_fagbl
    .

**********************************************************************
*&amp; report
  DATA(lr_rcod) = NEW zcl_vncsi_dcs_rcod( it_keys = lt_keys
                                          ix_ekkn = px_sekkn ).
  lr_rcod-&gt;report( ).</source>
 </PROG>
