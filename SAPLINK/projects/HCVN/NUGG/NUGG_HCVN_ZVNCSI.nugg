  <method CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="BAPI_INVO_REVER" VERSION="1" LANGU="E" DESCRIPT="invo: reverse invoice SalesMan self-billing" EXPOSURE="0" STATE="1" EDITORDER="45 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="1" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="BAPI_INVO_REVER" SCONAME="IV_BELNR" VERSION="1" LANGU="E" DESCRIPT="Document Number of an Invoice Document" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="RBKP-BELNR"/>
   <parameter CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="BAPI_INVO_REVER" SCONAME="IV_GJAHR" VERSION="1" LANGU="E" DESCRIPT="Fiscal Year" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="RBKP-GJAHR"/>
   <parameter CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="BAPI_INVO_REVER" SCONAME="EV_BELNR" VERSION="1" LANGU="E" DESCRIPT="Document Number of an Invoice Document" CMPTYPE="1" MTDTYPE="0" EDITORDER="3 " DISPID="0 " PARDECLTYP="1" PARPASSTYP="1" TYPTYPE="1" TYPE="RBKP-BELNR"/>
   <parameter CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="BAPI_INVO_REVER" SCONAME="EV_GJAHR" VERSION="1" LANGU="E" DESCRIPT="Fiscal Year" CMPTYPE="1" MTDTYPE="0" EDITORDER="4 " DISPID="0 " PARDECLTYP="1" PARPASSTYP="1" TYPTYPE="1" TYPE="RBKP-GJAHR"/>
   <parameter CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="BAPI_INVO_REVER" SCONAME="EX_REVOK" VERSION="1" LANGU="E" DESCRIPT="status: reversed successfuly" CMPTYPE="1" MTDTYPE="0" EDITORDER="5 " DISPID="0 " PARDECLTYP="1" PARPASSTYP="1" TYPTYPE="1" TYPE="XFELD"/>
   <source>METHOD bapi_invo_rever.

    &quot; get original posting date
    SELECT SINGLE budat FROM rbkp INTO @DATA(lv_budat) WHERE belnr = @iv_belnr
                                                         AND gjahr = @iv_gjahr.


    &quot;§2. post invoice
    DATA: lt_ret TYPE TABLE OF bapiret2.
    CALL FUNCTION &apos;BAPI_INCOMINGINVOICE_CANCEL&apos;
      EXPORTING
        invoicedocnumber          = iv_belnr
        fiscalyear                = iv_gjahr
        reasonreversal            = &apos;01&apos;      &quot;01= Reversal in current period
        postingdate               = lv_budat
      IMPORTING
        invoicedocnumber_reversal = ev_belnr
        fiscalyear_reversal       = ev_gjahr
      TABLES
        return                    = lt_ret.


    &quot;..errors
    LOOP AT lt_ret ASSIGNING FIELD-SYMBOL(&lt;ret&gt;) WHERE type = &apos;E&apos;.
      CALL FUNCTION &apos;BAPI_TRANSACTION_ROLLBACK&apos;.
      MESSAGE ID &lt;ret&gt;-id TYPE &apos;I&apos; NUMBER &lt;ret&gt;-number WITH &lt;ret&gt;-message_v1
                                                            &lt;ret&gt;-message_v2
                                                            &lt;ret&gt;-message_v3
                                                            &lt;ret&gt;-message_v4 DISPLAY LIKE &apos;E&apos;. &quot;(msgty= I to make it popup &amp; wait)
    ENDLOOP.
    &quot;..commit when no errors
    IF sy-subrc NE 0.
      CALL FUNCTION &apos;BAPI_TRANSACTION_COMMIT&apos; EXPORTING wait = &apos;X&apos;.
      MOVE: &apos;X&apos; TO ex_revok.
    ENDIF.









  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="BAPI_INVO_SLMAN" VERSION="1" LANGU="E" DESCRIPT="invo: post invoice SalesMan self-billing" EXPOSURE="0" STATE="1" EDITORDER="44 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="1" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="BAPI_INVO_SLMAN" SCONAME="IV_EBELN" VERSION="1" LANGU="E" DESCRIPT="Purchasing Document Number" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="EKPO-EBELN"/>
   <parameter CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="BAPI_INVO_SLMAN" SCONAME="IV_EBELP" VERSION="1" LANGU="E" DESCRIPT="Item Number of Purchasing Document" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="EKPO-EBELP"/>
   <parameter CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="BAPI_INVO_SLMAN" SCONAME="EV_BELNR" VERSION="1" LANGU="E" DESCRIPT="Document Number of an Invoice Document" CMPTYPE="1" MTDTYPE="0" EDITORDER="3 " DISPID="0 " PARDECLTYP="1" PARPASSTYP="1" TYPTYPE="1" TYPE="RBKP-BELNR"/>
   <parameter CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="BAPI_INVO_SLMAN" SCONAME="EV_GJAHR" VERSION="1" LANGU="E" DESCRIPT="Fiscal Year" CMPTYPE="1" MTDTYPE="0" EDITORDER="4 " DISPID="0 " PARDECLTYP="1" PARPASSTYP="1" TYPTYPE="1" TYPE="RBKP-GJAHR"/>
   <source>METHOD bapi_invo_slman.

    CLEAR: ev_belnr,
           ev_gjahr.

    &quot;*post self-billing miro invoice

    &quot;§0.0 get customizing self-billing salesman
    SELECT SINGLE * FROM zvncsi_dcs_c001 INTO @DATA(ls_c001) WHERE party = @zif_vncsi_gc=&gt;party_s.

    &quot;§0.1 get POrd data
    SELECT SINGLE * FROM ekko INTO @DATA(ls_ekko) WHERE ebeln = @iv_ebeln.
    SELECT SINGLE * FROM ekpo INTO @DATA(ls_ekpo) WHERE ebeln = @iv_ebeln
                                                    AND ebelp = @iv_ebelp.

    &quot;§1 prepare invo data
    DATA: ls_head TYPE          bapi_incinv_create_header,
          lt_item TYPE TABLE OF bapi_incinv_create_item,
          lt_acca TYPE TABLE OF bapi_incinv_create_account,
          lt_glac TYPE TABLE OF bapi_incinv_create_gl_account,
          lt_tax  TYPE TABLE OF bapi_incinv_create_tax.

    &quot;§1.1 prep header (exc total amount, after tax-calc)
    MOVE: &apos;X&apos;             TO ls_head-invoice_ind,
          ls_c001-blart   TO ls_head-doc_type,
          sy-datum        TO ls_head-doc_date,
          sy-datum        TO ls_head-pstng_date,
          ls_ekpo-bednr   TO ls_head-ref_doc_no,
          ls_ekko-bukrs   TO ls_head-comp_code,
          ls_ekko-waers   TO ls_head-currency,
          &apos;X&apos;             TO ls_head-calc_tax_ind,
          ls_c001-zterm   TO ls_head-pmnttrms,
          ls_c001-zlsch   TO ls_head-pymt_meth,
          sy-datum        TO ls_head-bline_date,
          ls_ekpo-txz01   TO ls_head-item_text.

    &quot;§1.2 prep item
    APPEND INITIAL LINE TO lt_item ASSIGNING FIELD-SYMBOL(&lt;item&gt;).
    MOVE: 000001          TO &lt;item&gt;-invoice_doc_item,
          ls_ekpo-ebeln   TO &lt;item&gt;-po_number,
          ls_ekpo-ebelp   TO &lt;item&gt;-po_item,
          ls_ekpo-mwskz   TO &lt;item&gt;-tax_code,
          ls_ekpo-netwr   TO &lt;item&gt;-item_amount,
          ls_ekpo-menge   TO &lt;item&gt;-quantity,
          ls_ekpo-meins   TO &lt;item&gt;-po_unit.

    &quot;++cross-check already invoiced..
    &quot;  ..prevent duplicate invoicing, if wrong POrd status
    SELECT * FROM ekbe INTO TABLE @DATA(lt_ekbe) WHERE ebeln = @&lt;item&gt;-po_number
                                                   AND ebelp = @&lt;item&gt;-po_item
                                                   AND vgabe = &apos;2&apos;            . &quot;2= Invoice Receipt
    LOOP AT lt_ekbe ASSIGNING FIELD-SYMBOL(&lt;ekbe&gt;).
      CASE &lt;ekbe&gt;-shkzg.
        WHEN &apos;S&apos;. SUBTRACT &lt;ekbe&gt;-menge FROM &lt;item&gt;-quantity. &quot;S= Debit (invoice ) reduce qtty
        WHEN &apos;H&apos;. ADD      &lt;ekbe&gt;-menge   TO &lt;item&gt;-quantity. &quot;H= Credit(reversal) add    qtty
      ENDCASE.
    ENDLOOP.

    IF &lt;item&gt;-quantity &lt;&gt; ls_ekpo-menge.
      MESSAGE i027(zvncsi) WITH &lt;item&gt;-po_number &lt;item&gt;-po_item DISPLAY LIKE &apos;E&apos;. &quot;msg{POrd &amp;-&amp; with open-quantity &lt;&gt; order-qtty, skipping record.}
      RETURN.
    ENDIF.



    MULTIPLY &lt;item&gt;-item_amount BY 100. &quot;(those bapi fields don&apos;t convert vnds properly? am I missing something?)

    &quot;unit-&gt;iso_unit
    SELECT SINGLE isocode FROM t006 INTO @&lt;item&gt;-po_unit_iso WHERE msehi = @&lt;item&gt;-po_unit.

    &quot;§1.3 prep acc-item
    &quot;..get account assigment of pord
    SELECT * FROM ekkn INTO TABLE @DATA(lt_ekkn) WHERE ebeln = @iv_ebeln
                                                   AND ebelp = @iv_ebelp.
    SELECT * FROM ekpo INTO TABLE @DATA(lt_ekpo) WHERE ebeln = @iv_ebeln
                                                   AND ebelp = @iv_ebelp.
    &quot;..fill
    LOOP AT lt_ekkn ASSIGNING FIELD-SYMBOL(&lt;ekkn&gt;).
      READ TABLE lt_ekpo ASSIGNING FIELD-SYMBOL(&lt;ekpo&gt;) WITH KEY ebeln = &lt;ekkn&gt;-ebeln
                                                                 ebelp = &lt;ekkn&gt;-ebelp. ASSERT sy-subrc = 0.
      APPEND INITIAL LINE TO lt_acca ASSIGNING FIELD-SYMBOL(&lt;acca&gt;).
      MOVE: &lt;item&gt;-invoice_doc_item TO &lt;acca&gt;-invoice_doc_item,
            &lt;ekkn&gt;-zekkn            TO &lt;acca&gt;-serial_no,
            &lt;ekkn&gt;-menge            TO &lt;acca&gt;-item_amount, &quot;assume: 1-to-1
            &lt;ekpo&gt;-mwskz            TO &lt;acca&gt;-tax_code,
            &lt;ekkn&gt;-menge            TO &lt;acca&gt;-quantity,
            &lt;ekpo&gt;-meins            TO &lt;acca&gt;-po_unit,
            &lt;ekkn&gt;-sakto            TO &lt;acca&gt;-gl_account,
            &lt;ekkn&gt;-kostl            TO &lt;acca&gt;-costcenter,
            &lt;ekkn&gt;-kokrs            TO &lt;acca&gt;-co_area,
            &lt;ekkn&gt;-prctr            TO &lt;acca&gt;-profit_ctr,
            &lt;ekkn&gt;-aufnr            TO &lt;acca&gt;-orderid.


      &quot;quick-&amp;-dirty: handle VN3= 1000VNDs
      IF &lt;ekpo&gt;-meins = &apos;VN3&apos;.
        MULTIPLY &lt;acca&gt;-item_amount BY 1000.
      ELSE.
        MULTIPLY &lt;acca&gt;-item_amount BY 100.
      ENDIF.

      &quot;unit-&gt;iso_unit
      SELECT SINGLE isocode FROM t006 INTO @&lt;acca&gt;-po_unit_iso WHERE msehi = @&lt;acca&gt;-po_unit.

    ENDLOOP.


    &quot;§1.4 prep tax
    APPEND INITIAL LINE TO lt_tax ASSIGNING FIELD-SYMBOL(&lt;tax&gt;).
    MOVE: &lt;item&gt;-tax_code    TO &lt;tax&gt;-tax_code,
          &lt;item&gt;-item_amount TO &lt;tax&gt;-tax_base_amount.

    &quot;..calc tax
    DATA: lt_mwdat TYPE TABLE OF rtax1u15.
    CALL FUNCTION &apos;CALCULATE_TAX_FROM_NET_AMOUNT&apos;
      EXPORTING
        i_bukrs           = ls_head-comp_code
        i_mwskz           = &lt;tax&gt;-tax_code
        i_waers           = ls_head-currency
        i_wrbtr           = &lt;tax&gt;-tax_base_amount
      IMPORTING
        &quot;E_FWNAV           = E_FWNAV          &quot;E_FWNAV= Non-deductible tax calculated (total)
        &quot;E_FWNVV           = E_FWNVV          &quot;E_FWNVV= Non-deductible tax calculated which is allocated
        e_fwste           = &lt;tax&gt;-tax_amount  &quot;E_FWSTE= Tax calculated (total)
        &quot;E_FWAST           = E_FWAST          &quot;E_FWAST= Calculated deductible tax
      TABLES
        t_mwdat           = lt_mwdat
      EXCEPTIONS
        bukrs_not_found   = 1
        country_not_found = 2
        mwskz_not_defined = 3
        mwskz_not_valid   = 4
        ktosl_not_found   = 5
        kalsm_not_found   = 6
        parameter_error   = 7
        knumh_not_found   = 8
        kschl_not_found   = 9
        unknown_error     = 10
        account_not_found = 11
        txjcd_not_valid   = 12
        OTHERS            = 13.
    IF sy-subrc &lt;&gt; 0.
      MESSAGE ID sy-msgid TYPE &apos;I&apos; NUMBER sy-msgno WITH sy-msgv1
                                                        sy-msgv2
                                                        sy-msgv3
                                                        sy-msgv4 DISPLAY LIKE &apos;E&apos;. &quot;(msgty= I to make it popup &amp; wait)
      RETURN.
    ENDIF.

    &quot;..header: total = base + tax
    ls_head-gross_amount = &lt;tax&gt;-tax_base_amount + &lt;tax&gt;-tax_amount.
    &quot;NOTE: the tax tab is NOT passed to bapi


    &quot;§1.5 prepare GL-posting: PIT Personal-Income-Tax for Salesman
    IF ls_c001-pitac IS NOT INITIAL.

      SELECT * FROM zvncsi_dcs_iflck INTO TABLE @DATA(lt_iflck) WHERE ebeln = @iv_ebeln
                                                                  AND ebelp = @iv_ebelp.
      LOOP AT lt_acca ASSIGNING &lt;acca&gt;. &quot;(per each acc-item of invoice.. the inv-item, them pord-acc-asgn)
        READ TABLE lt_item  ASSIGNING &lt;item&gt; WITH KEY invoice_doc_item = &lt;acca&gt;-invoice_doc_item. CHECK sy-subrc = 0.
        READ TABLE lt_iflck ASSIGNING FIELD-SYMBOL(&lt;iflck&gt;) WITH KEY ebeln = &lt;item&gt;-po_number
                                                                     ebelp = &lt;item&gt;-po_item
                                                                     zekkn = &lt;acca&gt;-serial_no   .  CHECK sy-subrc = 0.

        &quot;item: debit, like pord-item/acc-item, with PIT amount
        APPEND INITIAL LINE TO lt_glac ASSIGNING FIELD-SYMBOL(&lt;glac&gt;).
        MOVE: &apos;00001&apos;               TO &lt;glac&gt;-invoice_doc_item,
              &lt;acca&gt;-gl_account     TO &lt;glac&gt;-gl_account,
              &lt;iflck&gt;-smpit         TO &lt;glac&gt;-item_amount,
              &apos;S&apos;                   TO &lt;glac&gt;-db_cr_ind,
              ls_ekko-bukrs         TO &lt;glac&gt;-comp_code,
              &lt;acca&gt;-costcenter     TO &lt;glac&gt;-costcenter,
              &lt;acca&gt;-orderid        TO &lt;glac&gt;-orderid.
        CASE ls_c001-pitcx.
          WHEN &apos;X&apos;. &lt;glac&gt;-item_text = |{ ls_c001-pittx }  { ls_ekpo-bednr }|.
          WHEN &apos; &apos;. &lt;glac&gt;-item_text = |{ ls_c001-pittx }| .
        ENDCASE.

        &quot;item: credit, for PIT account
        APPEND INITIAL LINE TO lt_glac ASSIGNING &lt;glac&gt;.
        MOVE: &apos;00001&apos;               TO &lt;glac&gt;-invoice_doc_item,
              ls_c001-pitac         TO &lt;glac&gt;-gl_account,
              &lt;iflck&gt;-smpit         TO &lt;glac&gt;-item_amount,
              &apos;H&apos;                   TO &lt;glac&gt;-db_cr_ind,
              ls_ekko-bukrs         TO &lt;glac&gt;-comp_code.
        CASE ls_c001-pitcx.
          WHEN &apos;X&apos;. &lt;glac&gt;-item_text = |{ ls_c001-pittx }  { ls_ekpo-bednr }|.
          WHEN &apos; &apos;. &lt;glac&gt;-item_text = |{ ls_c001-pittx }| .
        ENDCASE.

      ENDLOOP.
    ENDIF.

    &quot;§2. post invoice
    DATA: lt_ret TYPE TABLE OF bapiret2.
    CALL FUNCTION &apos;BAPI_INCOMINGINVOICE_CREATE1&apos;
      EXPORTING
        headerdata       = ls_head
        invoicestatus    = &apos;5&apos;      &quot;5= Posted
        &quot;invoicestatus    = &apos;A&apos;      &quot;A= Parked
      IMPORTING
        invoicedocnumber = ev_belnr
        fiscalyear       = ev_gjahr
      TABLES
        itemdata         = lt_item
        accountingdata   = lt_acca
        glaccountdata    = lt_glac
        &quot;taxdata          = lt_tax
        return           = lt_ret.

    &quot;..errors
    LOOP AT lt_ret ASSIGNING FIELD-SYMBOL(&lt;ret&gt;) WHERE type = &apos;E&apos;.
      CALL FUNCTION &apos;BAPI_TRANSACTION_ROLLBACK&apos;.
      MESSAGE ID &lt;ret&gt;-id TYPE &apos;I&apos; NUMBER &lt;ret&gt;-number WITH &lt;ret&gt;-message_v1
                                                            &lt;ret&gt;-message_v2
                                                            &lt;ret&gt;-message_v3
                                                            &lt;ret&gt;-message_v4 DISPLAY LIKE &apos;E&apos;. &quot;(msgty= I to make it popup &amp; wait)
    ENDLOOP.
    &quot;..commit when no errors
    IF sy-subrc NE 0.
      CALL FUNCTION &apos;BAPI_TRANSACTION_COMMIT&apos; EXPORTING wait = &apos;X&apos;.
    ENDIF.









  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="BAPI_PORD_SCSTA" VERSION="1" LANGU="E" DESCRIPT="pord: set status SCSta via bapi" EXPOSURE="0" STATE="1" EDITORDER="43 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="1" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="BAPI_PORD_SCSTA" SCONAME="IV_SCSTA" VERSION="1" LANGU="E" DESCRIPT="CSI: SalesMan Commission Status" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="EKPO-ZVNCSI_SCSTA"/>
   <parameter CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="BAPI_PORD_SCSTA" SCONAME="IV_EBELN" VERSION="1" LANGU="E" DESCRIPT="Purchasing Document Number" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="EKPO-EBELN"/>
   <parameter CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="BAPI_PORD_SCSTA" SCONAME="IV_EBELP" VERSION="1" LANGU="E" DESCRIPT="Item Number of Purchasing Document" CMPTYPE="1" MTDTYPE="0" EDITORDER="3 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="EKPO-EBELP"/>
   <source>METHOD bapi_pord_scsta.

    &quot;call POrd BAPI: change SCSTA SalesMan Commission Status

    &quot;..prepare bapi data
    DATA: lt_ret TYPE TABLE OF bapiret2.
    DATA: ls_bmit TYPE bapi_te_mepoitem,
          ls_xmit TYPE bapi_te_mepoitemx.
    DATA: lt_exti TYPE bapiparex_tp.

    MOVE: iv_ebelp TO ls_bmit-po_item,
          iv_scsta TO ls_bmit-zvncsi_scsta.
    MOVE: iv_ebelp TO ls_xmit-po_item,
          &apos;X&apos;      TO ls_xmit-zvncsi_scsta.

    DATA: lv_valuepart1 TYPE bapiparex-valuepart1.
    CONCATENATE ls_bmit-po_item
                ls_bmit-_csi    INTO lv_valuepart1. &quot;(to exclude curr-flds)

    lt_exti = VALUE #( BASE lt_exti ( structure = &apos;BAPI_TE_MEPOITEM&apos;
                                      valuepart1 = lv_valuepart1     ) ).
    lt_exti = VALUE #( BASE lt_exti ( structure = &apos;BAPI_TE_MEPOITEMX&apos;
                                      valuepart1 = ls_xmit           ) ).

    &quot;..call bapi
    CALL FUNCTION &apos;BAPI_PO_CHANGE&apos;
      EXPORTING
        purchaseorder = iv_ebeln
        no_authority  = &apos;X&apos;       &quot;+SAP-3991 CSI: DCS Commission-SalesMan status not matching
      TABLES
        return        = lt_ret
        extensionin   = lt_exti.

    &quot;..errors
    LOOP AT lt_ret ASSIGNING FIELD-SYMBOL(&lt;ret&gt;) WHERE type = &apos;E&apos;.
      CALL FUNCTION &apos;BAPI_TRANSACTION_ROLLBACK&apos;.
      MESSAGE ID &lt;ret&gt;-id TYPE &apos;I&apos; NUMBER &lt;ret&gt;-number WITH &lt;ret&gt;-message_v1
                                                            &lt;ret&gt;-message_v2
                                                            &lt;ret&gt;-message_v3
                                                            &lt;ret&gt;-message_v4 DISPLAY LIKE &apos;E&apos;. &quot;(note: msgty= I to make it popup &amp; wait)
    ENDLOOP.
    &quot;..commit when no errors
    IF sy-subrc NE 0.
      CALL FUNCTION &apos;BAPI_TRANSACTION_COMMIT&apos; EXPORTING wait = &apos;X&apos;.
    ENDIF.



  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="CALL_CDDO_DIS" VERSION="1" LANGU="E" DESCRIPT="call: report change documents" EXPOSURE="0" STATE="1" EDITORDER="41 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="CALL_CDDO_DIS" SCONAME="IR_SDAT" VERSION="1" LANGU="E" DESCRIPT="CSI: DCS Commission-SalesMan report (salv)" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="TS_SDAT"/>
   <source>METHOD call_cddo_dis.

    &quot;++SAP-3574  SAP DCS Salesman commision Change status log

    CHECK: ir_sdat IS BOUND.
    DATA(lv_objid) = CONV cdhdr-objectid( ir_sdat-&gt;*-ebeln ).
    DATA(lv_tbkey) = CONV cdpos-tabkey( sy-mandt &amp;&amp; ir_sdat-&gt;*-ebeln &amp;&amp; ir_sdat-&gt;*-ebelp ).


    SUBMIT rsscd100 WITH objekt   = &apos;EINKBELEG&apos;
                    WITH objektid = lv_objid
                    WITH tabname  = &apos;EKPO&apos;
                    WITH tabkey   = lv_tbkey AND RETURN.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="CALL_MIR4" VERSION="1" LANGU="E" DESCRIPT="call: MM-Invoice display" EXPOSURE="0" STATE="1" EDITORDER="40 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="CALL_MIR4" SCONAME="IR_SDAT" VERSION="1" LANGU="E" DESCRIPT="CSI: DCS Commission-SalesMan report (salv)" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="TS_SDAT"/>
   <source>METHOD call_mir4.

    &quot;call MIR4 display Incoming Invoice
    CHECK: ir_sdat IS BOUND,
           ir_sdat-&gt;*-belnr IS NOT INITIAL,
           ir_sdat-&gt;*-gjahr IS NOT INITIAL.

    SET PARAMETER ID &apos;RBN&apos; FIELD ir_sdat-&gt;*-belnr.
    SET PARAMETER ID &apos;GJR&apos; FIELD ir_sdat-&gt;*-gjahr.

    CALL TRANSACTION &apos;MIR4&apos; WITH AUTHORITY-CHECK AND SKIP FIRST SCREEN.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="CALL_PORD" VERSION="1" LANGU="E" DESCRIPT="call: POrd display" EXPOSURE="0" STATE="1" EDITORDER="39 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="CALL_PORD" SCONAME="IR_SDAT" VERSION="1" LANGU="E" DESCRIPT="enhance table IFLC for SALV (tty)" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="TS_SDAT"/>
   <source>METHOD call_pord.

    &quot;call POrd display
    CHECK: ir_sdat IS BOUND.

    CALL FUNCTION &apos;ME_DISPLAY_PURCHASE_DOCUMENT&apos;
      EXPORTING
        i_ebeln              = ir_sdat-&gt;*-ebeln
        i_ebelp              = ir_sdat-&gt;*-ebelp
      EXCEPTIONS
        not_found            = 1
        no_authority         = 2
        invalid_call         = 3
        preview_not_possible = 4
        OTHERS               = 5.
    IF sy-subrc &lt;&gt; 0. MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4. ENDIF.




  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="CONSTRUCTOR" VERSION="1" LANGU="E" DESCRIPT="constuctor" EXPOSURE="2" STATE="1" EDITORDER="1 " DISPID="0 " MTDTYPE="2" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="CONSTRUCTOR" SCONAME="IT_KEYS" VERSION="1" LANGU="E" DESCRIPT="rcod keys (ebeln+ebelp)" CMPTYPE="1" MTDTYPE="2" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="TT_KEYS"/>
   <parameter CLSNAME="ZCL_VNCSI_DCS_RCOS" CMPNAME="CONSTRUCTOR" SCONAME="IX_EKKN" VERSION="1" LANGU="E" DESCRIPT="show Account Assignment lines" CMPTYPE="1" MTDTYPE="2" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="XFELD"/>
