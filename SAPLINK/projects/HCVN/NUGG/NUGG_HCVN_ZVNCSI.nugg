  <method CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="SALV_INIT_PFST" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: gui status" EXPOSURE="0" STATE="1" EDITORDER="16 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SALV_INIT_PFST.

    &quot;init: gui status from calling program

    &quot;set own function by own screen status (seriously, I hate SALV)
    me-&gt;ar_salv-&gt;set_screen_status(
                                    pfstatus      = me-&gt;lc_pfkey
                                    report        = me-&gt;lc_repid
                                    set_functions = me-&gt;ar_salv-&gt;c_functions_all ).


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="SALV_LINE_GET" VERSION="1" LANGU="E" DESCRIPT="salv-object: get single selected line" EXPOSURE="0" STATE="1" EDITORDER="22 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="SALV_LINE_GET" SCONAME="ER_SDAT" VERSION="1" LANGU="E" DESCRIPT="CSI: Subvention report (salv)" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="3" TYPE="TS_SDAT"/>
   <source>METHOD salv_line_get.

    &quot;get selected lines
    DATA(lt_rows) = me-&gt;ar_salv-&gt;get_selections( )-&gt;get_selected_rows( ).
    &quot;get selected cell
    DATA(lt_cell) = me-&gt;ar_salv-&gt;get_selections( )-&gt;get_selected_cells( ).

    &quot;check single
    IF NOT ( lines( lt_rows ) = 1 OR
             lines( lt_cell ) = 1   ).
      MESSAGE w024(zvncsi). &quot;msg{Select exactly one line for processing.}
      RETURN.
    ENDIF.

    &quot;read line..
    &quot;..per line
    IF lines( lt_rows ) = 1.
      READ TABLE lt_rows ASSIGNING FIELD-SYMBOL(&lt;row&gt;) INDEX 1.
      READ TABLE me-&gt;at_sdat REFERENCE INTO er_sdat INDEX &lt;row&gt;.
    ENDIF.
    &quot;..per cell
    IF lines( lt_cell ) = 1.
      READ TABLE lt_cell ASSIGNING FIELD-SYMBOL(&lt;cell&gt;) INDEX 1.
      READ TABLE me-&gt;at_sdat REFERENCE INTO er_sdat INDEX &lt;cell&gt;-row.
    ENDIF.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNCSI_SUB_RSUB" CMPNAME="SALV_REFR" VERSION="1" LANGU="E" DESCRIPT="salv-object: refresh" EXPOSURE="0" STATE="1" EDITORDER="25 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_refr.

    &quot;refresh SALV

    &quot;..re-read data
    CLEAR: me-&gt;at_sdat.

    me-&gt;sdat_selc( ). &quot;data select
    me-&gt;sdat_auth( ). &quot;data authorisation check
    me-&gt;sdat_enha( ). &quot;data enhance

    me-&gt;salv_init_ccel( ). &quot;re-colour cells (per row-line)

    &quot;..refresh salv
    DATA(ls_stbl) = VALUE lvc_s_stbl( row = &apos;X&apos; col = &apos;X&apos; ).
    me-&gt;ar_salv-&gt;refresh( s_stable = ls_stbl ).


  ENDMETHOD.</source>
  </method>
