 <PROG NAME="ZBOOKNG_TEST" VARCL="X" DBAPL="S" SUBC="1" RMAND="700" FIXPT="X" LDBNAME="D$S" UCCHECK="X">
  <textPool>
   <language SPRAS="E">
    <textElement ID="R" ENTRY="Test for BOOKNG processing" LENGTH="26 "/>
    <textElement ID="S" KEY="P_BATCH" ENTRY="D       ." LENGTH="9 "/>
    <textElement ID="S" KEY="P_MESS" ENTRY="D       ." LENGTH="9 "/>
    <textElement ID="S" KEY="R_CHECK" ENTRY="        Check" LENGTH="13 "/>
    <textElement ID="S" KEY="R_POST" ENTRY="        Post" LENGTH="12 "/>
   </language>
  </textPool>
  <source>*&amp;---------------------------------------------------------------------*
*&amp; Report zbookng_test
*&amp;---------------------------------------------------------------------*
*&amp;
*&amp;---------------------------------------------------------------------*
REPORT zbookng_test.

PARAMETERS: p_batch TYPE zglif_bookng_ws-batch_id OBLIGATORY,
            p_mess  TYPE zglif_bookng_ws-message_id OBLIGATORY.

SELECTION-SCREEN SKIP.

PARAMETERS: r_check RADIOBUTTON GROUP rd1 DEFAULT &apos;X&apos;,
            r_post  RADIOBUTTON GROUP rd1.

START-OF-SELECTION.

  DATA: ls_doc_header        TYPE bapiache09,
        lt_curramount        TYPE bapiaccr09_tab,
        lt_accountgl         TYPE bapiacgl09_tab,
        lt_extension2        TYPE bapiparex2_tab,
        lt_return            TYPE bapiret2_tab,
        ls_customercpd       TYPE bapiacpa09,
        lt_accountpayable    TYPE bapiacap09_tab,
        lt_accounttax        TYPE bapiactx09_tab,
        lt_accountreceivable TYPE bapiacar09_tab.


  SELECT SINGLE *
  FROM zglif_proccust
  INTO @DATA(ls_proccust)
  WHERE interface_name = @zcl_bookng_tools=&gt;mc_interface_name.

  DATA(lo_processor) = NEW zglif_processor_book_bapi( ls_proccust ).


  lo_processor-&gt;get_prepared_tax_data(
    EXPORTING
      iv_batch             = p_batch
      iv_mess_id           = p_mess
    IMPORTING
      es_doc_header = ls_doc_header
      es_customercpd = ls_customercpd
      et_curramount = lt_curramount
      et_accountgl  = lt_accountgl
      et_accountreceivable = lt_accountreceivable
      et_accountpayable = lt_accountpayable
      et_accounttax = lt_accounttax
      et_extension2 = lt_extension2
      et_return     = lt_return
  ).

  IF r_check = abap_true.

    CALL FUNCTION &apos;BAPI_ACC_DOCUMENT_CHECK&apos;
      EXPORTING
        documentheader    = ls_doc_header
        customercpd       = ls_customercpd
*       contractheader    =
      TABLES
        accountgl         = lt_accountgl
        accountreceivable = lt_accountreceivable
        accountpayable    = lt_accountpayable
        accounttax        = lt_accounttax
        currencyamount    = lt_curramount
*       criteria          =
*       valuefield        =
*       extension1        =
        return            = lt_return
*       paymentcard       =
*       contractitem      =
        extension2        = lt_extension2
*       realestate        =
*       accountwt         =
      .

  ELSEIF r_post = abap_true.

    CALL FUNCTION &apos;BAPI_ACC_DOCUMENT_POST&apos;
      EXPORTING
        documentheader    = ls_doc_header
        customercpd       = ls_customercpd
*       contractheader    =
*        IMPORTING
*       obj_type          =
*       obj_key           =
*       obj_sys           =
      TABLES
        accountgl         = lt_accountgl
        accountreceivable = lt_accountreceivable
        accountpayable    = lt_accountpayable
        accounttax        = lt_accounttax
        currencyamount    = lt_curramount
*       criteria          =
*       valuefield        =
*       extension1        =
        return            = lt_return
*       paymentcard       =
*       contractitem      =
        extension2        = lt_extension2
*       realestate        =
*       accountwt         =
      .
    COMMIT WORK AND WAIT.

  ENDIF.

  LOOP AT lt_return INTO DATA(ls_return).
    WRITE / ls_return-message.
  ENDLOOP.</source>
 </PROG>
 <PROG NAME="ZBOOKNG_BATCH_MESSAGE_DELETE" VARCL="X" DBAPL="S" SUBC="1" RMAND="700" FIXPT="X" LDBNAME="D$S" UCCHECK="X">
  <textPool>
   <language SPRAS="E">
    <textElement ID="R" ENTRY="Batch and Message cleanup" LENGTH="25 "/>
    <textElement ID="S" KEY="P_BATCH" ENTRY="D       ." LENGTH="16 "/>
    <textElement ID="S" KEY="P_DEL_B" ENTRY="        Delete Batch from table" LENGTH="31 "/>
    <textElement ID="S" KEY="P_INTER" ENTRY="D       ." LENGTH="22 "/>
    <textElement ID="S" KEY="S_MESS" ENTRY="D       ." LENGTH="18 "/>
   </language>
  </textPool>
  <source>*&amp;---------------------------------------------------------------------*
*&amp; Report zbookng_batch_message_delete
*&amp;---------------------------------------------------------------------*
*&amp;
*&amp;---------------------------------------------------------------------*
REPORT zbookng_batch_message_delete.

TABLES zglif_bookng_ws.

PARAMETERS: p_batch TYPE zglif_bookng_ws-batch_id OBLIGATORY MEMORY ID zglif_batch.
SELECT-OPTIONS: s_mess FOR zglif_bookng_ws-message_id OBLIGATORY.
PARAMETERS: p_inter TYPE zglif_batch_stat-interface_name OBLIGATORY DEFAULT zcl_bookng_tools=&gt;mc_interface_name.
SELECTION-SCREEN SKIP.
PARAMETERS: p_del_b AS CHECKBOX. &quot; delete batch itself


START-OF-SELECTION.

  WRITE: / |Deleting batch no: { p_batch }|.

  CASE p_inter.
    WHEN zcl_bookng_tools=&gt;mc_interface_name.

      WRITE: / |ZGLIF_BOOKNG_WS table:|.

      DELETE FROM zglif_bookng_ws WHERE batch_id = p_batch AND message_id IN s_mess.

      IF sy-subrc = 0.
        WRITE: / |Deleted { sy-dbcnt } rows from ZGLIF_BOOKNG_WS table|.
      ELSE.
        WRITE: / |Problem with deletion of no data found|.
      ENDIF.

  ENDCASE.

  IF p_del_b = abap_true.

    WRITE: / |ZGLIF_BATCH_STAT table:|.

    DELETE FROM zglif_batch_stat WHERE interface_name = p_inter AND batch_id = p_batch.

    IF sy-subrc = 0.
      WRITE: / |Deleted { sy-dbcnt } rows from ZGLIF_BATCH_STAT table|.
    ELSE.
      WRITE: / |Problem with deletion of no data found|.
    ENDIF.

  ENDIF.


  WRITE: / |ZGLIF_MESS_STAT table:|.

  DELETE FROM zglif_mess_stat WHERE interface_name = p_inter AND batch_id = p_batch AND message_id IN s_mess.

  IF sy-subrc = 0.
    WRITE: / |Deleted { sy-dbcnt } rows from ZGLIF_MESS_STAT table|.
  ELSE.
    WRITE: / |Problem with deletion of no data found|.
  ENDIF.

  COMMIT WORK AND WAIT.</source>
 </PROG>
 <PROG NAME="ZBOOKNG_TAX_CSV" VARCL="X" DBAPL="S" DBNA="D$" SUBC="1" RMAND="700" FIXPT="X" LDBNAME="D$S" UCCHECK="X">
  <textPool>
   <language SPRAS="E">
    <textElement ID="R" ENTRY="CSV Upload with Tax" LENGTH="19 "/>
    <textElement ID="S" KEY="P_CONV" ENTRY="        Convert file" LENGTH="23 "/>
    <textElement ID="S" KEY="P_FILE" ENTRY="D       ." LENGTH="17 "/>
    <textElement ID="S" KEY="P_RUN" ENTRY="        Real Processing - 2nd run" LENGTH="33 "/>
    <textElement ID="S" KEY="P_TEST" ENTRY="        Test Processing - 1st run" LENGTH="33 "/>
    <textElement ID="S" KEY="P_UPLO" ENTRY="        Upload file" LENGTH="19 "/>
    <textElement ID="S" KEY="R_PARK" ENTRY="        Fully saved document" LENGTH="28 "/>
    <textElement ID="S" KEY="R_POST" ENTRY="        Post document" LENGTH="21 "/>
    <textElement ID="S" KEY="R_SIMPLE" ENTRY="        Simple file template" LENGTH="28 "/>
    <textElement ID="S" KEY="R_TAX" ENTRY="        Tax file template" LENGTH="25 "/>
   </language>
  </textPool>
  <source>*&amp;---------------------------------------------------------------------*
*&amp; Report zbookng_test_csv
*&amp;---------------------------------------------------------------------*
*&amp;
*&amp;---------------------------------------------------------------------*
REPORT zbookng_tax_csv.

TABLES sscrfields.


SELECTION-SCREEN: FUNCTION KEY 1.   &quot; for button GLIF Monitor
*SELECTION-SCREEN: FUNCTION KEY 2.   &quot; LOG

PARAMETERS: r_simple RADIOBUTTON GROUP rd2.
PARAMETERS: r_tax RADIOBUTTON GROUP rd2 DEFAULT &apos;X&apos;.

SELECTION-SCREEN SKIP.

PARAMETERS: p_file TYPE rlgrap-filename.

SELECTION-SCREEN SKIP.

PARAMETERS: p_uplo AS CHECKBOX DEFAULT abap_true.
PARAMETERS: p_conv AS CHECKBOX DEFAULT abap_true.
PARAMETERS: p_test AS CHECKBOX DEFAULT abap_true.
PARAMETERS: p_run AS CHECKBOX DEFAULT abap_true.

SELECTION-SCREEN SKIP.

PARAMETERS: r_park RADIOBUTTON GROUP rd1 DEFAULT &apos;X&apos;,
            r_post RADIOBUTTON GROUP rd1 .


INITIALIZATION.

  sscrfields-functxt_01     = VALUE smp_dyntxt( icon_id = icon_systems quickinfo = &apos;Glif Monitor&apos; icon_text = &apos;GLIF Monitor&apos; ).
*  sscrfields-functxt_02     = VALUE smp_dyntxt( icon_id = icon_time_control quickinfo = &apos;Log&apos; icon_text = &apos;Log&apos; ).


AT SELECTION-SCREEN.

  CASE sscrfields-ucomm.

    WHEN &apos;FC01&apos;.
      TRY.
          CASE abap_true.
            WHEN r_simple.
              EXPORT zglif_interface = zcl_bookng_tools=&gt;mc_csv_interface TO MEMORY ID &apos;ZGLIF_INTERFACE&apos;.
            WHEN r_tax.
              EXPORT zglif_interface = zcl_bookng_tools=&gt;mc_csv_tax_interface TO MEMORY ID &apos;ZGLIF_INTERFACE&apos;.
          ENDCASE.

          CALL TRANSACTION &apos;ZGLIF_MONITOR&apos; WITH AUTHORITY-CHECK.
        CATCH cx_sy_authorization_error.
          MESSAGE &apos;You do not have authorization for Tcode ZGLIF_MONITOR&apos; TYPE &apos;S&apos; DISPLAY LIKE &apos;E&apos;.
      ENDTRY.
*    WHEN &apos;FC02&apos;.
*
*      DATA: ls_display_prog TYPE bal_s_prof,
*            lv_date_from    TYPE baldate.
*
*      lv_date_from = sy-datum.
*      lv_date_from(4) = sy-datum(4) - 1.
*
*      CALL FUNCTION &apos;BAL_DSP_PROFILE_DETLEVEL_GET&apos;
*        IMPORTING
*          e_s_display_profile = ls_display_prog.
*
*      CALL FUNCTION &apos;APPL_LOG_DISPLAY&apos;
*        EXPORTING
*          object                    = &apos;ZGLIF&apos;
*          subobject                 = &apos;*&apos;
*          external_number           = &apos;*&apos;
*          suppress_selection_dialog = abap_true
*          i_s_display_profile       = ls_display_prog
*          time_from                 = &apos;000000&apos;
*          time_to                   = &apos;235959&apos;
*          date_from                 = lv_date_from
*          date_to                   = sy-datum
*        EXCEPTIONS
*          no_authority              = 1
*          OTHERS                    = 2.

    WHEN OTHERS.
  ENDCASE.

  IF sy-ucomm = &apos;ONLI&apos;.
    IF p_uplo = abap_true AND p_file IS INITIAL.
      MESSAGE &apos;Fill required fields&apos; TYPE &apos;E&apos;.
    ENDIF.
  ENDIF.




AT SELECTION-SCREEN ON VALUE-REQUEST FOR p_file.

  DATA: lv_localfile TYPE localfile .

  CALL FUNCTION &apos;F4_FILENAME&apos;
    EXPORTING
      program_name  = sy-cprog
      dynpro_number = sy-dynnr
      field_name    = &apos;P_FILE&apos;
    IMPORTING
      file_name     = lv_localfile.

  p_file = lv_localfile.




START-OF-SELECTION.

  DATA: lt_file           TYPE TABLE OF string,
        lt_filename       TYPE TABLE OF string,
        lv_dataset        TYPE string,
        gv_interface_name TYPE zglif_interface_name.

  CASE abap_true.
    WHEN r_simple.
      gv_interface_name = zcl_bookng_tools=&gt;mc_csv_interface.
    WHEN r_tax.
      gv_interface_name = zcl_bookng_tools=&gt;mc_csv_tax_interface.
  ENDCASE.


  IF p_file IS NOT INITIAL AND p_uplo = abap_true.

    WRITE / |Upload started|.

    &quot; CSV Upload, hence ASC
    cl_gui_frontend_services=&gt;gui_upload(
      EXPORTING
        filename                = CONV #( p_file )
        filetype                = &apos;ASC&apos;
*    has_field_separator     = space
*    header_length           = 0
*    read_by_line            = &apos;X&apos;
*    dat_mode                = space
*    codepage                =
*    ignore_cerr             = abap_true
*    replacement             = &apos;#&apos;
*    virus_scan_profile      =
*  IMPORTING
*    filelength              =
*    header                  =
      CHANGING
        data_tab                = lt_file
*    isscanperformed         = space
      EXCEPTIONS
        file_open_error         = 1
        file_read_error         = 2
        no_batch                = 3
        gui_refuse_filetransfer = 4
        invalid_type            = 5
        no_authority            = 6
        unknown_error           = 7
        bad_data_format         = 8
        header_not_allowed      = 9
        separator_not_allowed   = 10
        header_too_long         = 11
        unknown_dp_error        = 12
        access_denied           = 13
        dp_out_of_memory        = 14
        disk_full               = 15
        dp_timeout              = 16
        not_supported_by_gui    = 17
        error_no_gui            = 18
        OTHERS                  = 19
    ).
    IF sy-subrc &lt;&gt; 0.
      MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno
        WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4.
      RETURN.
    ENDIF.

    WRITE / |File { p_file } uploaded successfully|.

    &quot; Prepared just for windows, last in table is filename
    SPLIT p_file AT &apos;\&apos; INTO TABLE lt_filename.
    DATA(lv_filename) = lt_filename[ lines( lt_filename ) ].

**********************************************************************
    &quot; Check for existing batchID
    TRY.
        SELECT SINGLE COUNT( * )
        FROM zglif_batch_stat
        WHERE interface_name = gv_interface_name
        AND batch_id = lv_filename+4(10)
        AND batch_status &lt;&gt; zglif_dispatcher=&gt;gc_00_downloaded
        AND batch_status &lt;&gt; zglif_dispatcher=&gt;gc_50_corrected.

        IF sy-subrc = 0.
          WRITE / |This file was already processed. Use different batch|.
          EXIT.
        ENDIF.

      CATCH cx_root.
        WRITE / |Invalid file. Ending process...|.
        EXIT.
    ENDTRY.


**********************************************************************

    SELECT SINGLE sap_folder_source
    FROM zglif_convcust
    INTO @DATA(lv_folder_source)
    WHERE interface_name = @gv_interface_name.
    IF sy-subrc &lt;&gt; 0.
      WRITE / |Missing convertor customization for interface { gv_interface_name }|.
      MESSAGE |Missing convertor customization for interface { gv_interface_name }| TYPE &apos;S&apos; DISPLAY LIKE &apos;E&apos;.
      LEAVE LIST-PROCESSING.
    ENDIF.



    &quot; Convert logical path to physical path with filename to become dataset name
    CALL FUNCTION &apos;FILE_GET_NAME_USING_PATH&apos;
      EXPORTING
        logical_path               = lv_folder_source
        file_name                  = lv_filename
      IMPORTING
        file_name_with_path        = lv_dataset
      EXCEPTIONS
        path_not_found             = 1
        missing_parameter          = 2
        operating_system_not_found = 3
        file_system_not_found      = 4
        OTHERS                     = 5.
    IF sy-subrc &lt;&gt; 0.
      MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno
        WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4.
      RETURN.
    ENDIF.

    WRITE / |Transfering file { lv_filename } TO { lv_dataset }|.

    OPEN DATASET lv_dataset FOR OUTPUT IN TEXT MODE ENCODING UTF-8 IGNORING CONVERSION ERRORS.

    IF sy-subrc &lt;&gt; 0.
      MESSAGE |File creation error| TYPE &apos;E&apos;.
      WRITE / |File creation error|.
      RETURN.
    ENDIF.

    LOOP AT lt_file REFERENCE INTO DATA(ls_file).
      TRANSFER ls_file-&gt;* TO lv_dataset.
    ENDLOOP.

    CLOSE DATASET lv_dataset.

    IF sy-subrc = 0.
      WRITE / |File transfered|.
    ENDIF.

    zglif_logger=&gt;get_timestamp( IMPORTING  pev_date = DATA(lv_date)
                                            pev_time = DATA(lv_time) ).

    &quot; Since CSV can contain more than just one message, we create dummy message which will be deleted during conversion
    &quot; Message ID will remain empty here
    DATA(ls_mess_stat) = VALUE zglif_mess_stat(
                        interface_name = gv_interface_name
                        filename = lv_filename
                        batch_id = lv_filename+4(10)
*                    message_id = lv_filename+14(10)
                        message_status = zglif_dispatcher=&gt;gc_00_downloaded
                        message_changed_on = lv_date
                        message_changed_at = lv_time
                        message_changed_by = sy-uname
                        ).

    MODIFY zglif_mess_stat FROM ls_mess_stat.

    DATA(ls_batch_stat) = VALUE zglif_batch_stat(
                         interface_name = gv_interface_name
                         batch_id = lv_filename+4(10)
                         batch_status = zglif_dispatcher=&gt;gc_00_downloaded
                         batch_changed_on = lv_date
                         batch_changed_at = lv_time
                         batch_changed_by = sy-uname
                         ).

    MODIFY zglif_batch_stat FROM ls_batch_stat.

    COMMIT WORK AND WAIT.

    WRITE / |Tables ZGLIF_MESS_STAT and ZGLIF_BATCH_STAT updated|.

    &quot; Should we create batch id?


  ENDIF.

  IF p_conv = abap_true OR p_test = abap_true OR p_run = abap_true.

    DATA(lo_dispatcher) = zglif_dispatcher=&gt;get_dispatcher( piv_interface_name = gv_interface_name ).

  ENDIF.

  IF p_conv = abap_true.
    lo_dispatcher-&gt;convert( ).
  ENDIF.

  IF p_test = abap_true.
    lo_dispatcher-&gt;process( ).
  ENDIF.


  IF p_run = abap_true.
    lo_dispatcher-&gt;process( ).
  ENDIF.

  IF p_run = abap_true OR p_test = abap_true OR p_conv = abap_true.
    lo_dispatcher-&gt;process_errors( ).
  ENDIF.</source>
 </PROG>
 <PROG NAME="ZBOOKNG_CLEAR_BATCH_DATA" VARCL="X" DBAPL="S" SUBC="1" RMAND="700" FIXPT="X" LDBNAME="D$S" UCCHECK="X">
  <textPool>
   <language SPRAS="E">
    <textElement ID="R" ENTRY="Table delete for BatchID" LENGTH="24 "/>
    <textElement ID="S" KEY="P_INTER" ENTRY="D       ." LENGTH="22 "/>
    <textElement ID="S" KEY="P_LEGER" ENTRY="D       ." LENGTH="9 "/>
    <textElement ID="S" KEY="S_BATCH" ENTRY="D       ." LENGTH="9 "/>
   </language>
  </textPool>
  <source>*&amp;---------------------------------------------------------------------*
*&amp; Report zbookng_batch_delete
*&amp;---------------------------------------------------------------------*
*&amp;
*&amp;---------------------------------------------------------------------*
REPORT zbookng_clear_batch_data.

TABLES zglif_batch_stat.

SELECT-OPTIONS: s_batch FOR zglif_batch_stat-batch_id OBLIGATORY MEMORY ID zglif_batch.

PARAMETERS: p_leger TYPE zglif_bookng_ws-ledger_group OBLIGATORY DEFAULT &apos;IFRS&apos;.

PARAMETERS: p_inter TYPE zglif_batch_stat-interface_name OBLIGATORY DEFAULT zcl_bookng_tools=&gt;mc_interface_name.


START-OF-SELECTION.

  WRITE / |Clearing batchNo: |.
  LOOP AT s_batch.
    WRITE |{ s_batch-low }, |.

  ENDLOOP.

  SELECT DISTINCT batch_id, message_id
  FROM zglif_bookng_ws
  WHERE batch_id IN @s_batch
  AND ledger_group = @p_leger
  INTO TABLE @DATA(lt_data).

  LOOP AT lt_data REFERENCE INTO DATA(ls_data).
    CASE p_inter.
      WHEN zcl_bookng_tools=&gt;mc_interface_name.
        DELETE FROM zglif_bookng_ws WHERE batch_id = ls_data-&gt;batch_id AND message_id = ls_data-&gt;message_id.
      WHEN zcl_bookng_tools=&gt;mc_csv_interface.
        DELETE FROM zbookng_csv_data WHERE batch_id = ls_data-&gt;batch_id AND message_id = ls_data-&gt;message_id.
      WHEN zcl_bookng_tools=&gt;mc_csv_tax_interface.
        DELETE FROM zbookng_csv_tax WHERE batch_id = ls_data-&gt;batch_id AND message_id = ls_data-&gt;message_id.
    ENDCASE.


    DELETE FROM zglif_mess_stat WHERE interface_name = p_inter AND batch_id = ls_data-&gt;batch_id AND message_id = ls_data-&gt;message_id.
  ENDLOOP.

  LOOP AT lt_data REFERENCE INTO ls_data GROUP BY ( batch_id = ls_data-&gt;batch_id ) INTO DATA(lv_batch).

    SELECT COUNT( * )
    FROM zglif_mess_stat
    WHERE interface_name = p_inter
    AND batch_id = lv_batch-batch_id.
    IF sy-subrc &lt;&gt; 0.
      DELETE FROM zglif_batch_stat WHERE interface_name = p_inter AND batch_id = lv_batch-batch_id.
    ENDIF.

  ENDLOOP.


  COMMIT WORK AND WAIT.</source>
 </PROG>
 <PROG NAME="ZBOOKNG_CHANGE_STATUS" VARCL="X" DBAPL="S" SUBC="1" RMAND="700" FIXPT="X" LDBNAME="D$S" UCCHECK="X">
  <textPool>
   <language SPRAS="E">
    <textElement ID="R" ENTRY="Mass Change status of Batch for test purposes" LENGTH="45 "/>
   </language>
  </textPool>
  <source>*&amp;---------------------------------------------------------------------*
*&amp; Report zbookng_change_status
*&amp;---------------------------------------------------------------------*
*&amp;
*&amp;---------------------------------------------------------------------*
REPORT zbookng_change_status.

TABLES zglif_batch_stat.

SELECT-OPTIONS s_batch FOR zglif_batch_stat-batch_id.

PARAMETERS: &quot;p_batch TYPE zglif_bookng_ws-batch_id OBLIGATORY MEMORY ID zglif_batch,
  p_inter TYPE zglif_batch_stat-interface_name OBLIGATORY DEFAULT zcl_bookng_tools=&gt;mc_interface_name,
  p_stat  TYPE zglif_batch_stat-batch_status OBLIGATORY DEFAULT &apos;00&apos;.


START-OF-SELECTION.

  WRITE / |Updating ZGLIF_BATCH_STAT table|.

  UPDATE zglif_batch_stat SET batch_status = p_stat WHERE batch_id IN s_batch AND interface_name = p_inter.

  WRITE / |Updated { sy-dbcnt } rows in ZGLIF_BATCH_STAT table|.

  WRITE / |Updating ZGLIF_MESS_STAT table|.

  UPDATE zglif_mess_stat SET message_status = p_stat WHERE batch_id IN s_batch AND interface_name = p_inter.

  WRITE / |Updated { sy-dbcnt } rows in ZGLIF_MESS_STAT table|.

  WRITE / |Updating ZGLIF_BOOKNG_WS table|.

  UPDATE zglif_bookng_ws SET message_status = p_stat WHERE batch_id IN s_batch.

  WRITE / |Updated { sy-dbcnt } rows in ZGLIF_BOOKNG_WS table|.

  COMMIT WORK.</source>
 </PROG>
 <PROG NAME="ZBOOKNG_BATCH_DELETE" VARCL="X" DBAPL="S" SUBC="1" RMAND="700" FIXPT="X" LDBNAME="D$S" UCCHECK="X">
  <textPool>
   <language SPRAS="E">
    <textElement ID="R" ENTRY="Table delete for BatchID" LENGTH="24 "/>
    <textElement ID="S" KEY="P_BATCH" ENTRY="D       ." LENGTH="9 "/>
    <textElement ID="S" KEY="P_INTER" ENTRY="D       ." LENGTH="9 "/>
   </language>
  </textPool>
  <source>*&amp;---------------------------------------------------------------------*
*&amp; Report zbookng_batch_delete
*&amp;---------------------------------------------------------------------*
*&amp;
*&amp;---------------------------------------------------------------------*
REPORT zbookng_batch_delete.

TABLES zglif_batch_stat.

SELECT-OPTIONS: s_batch FOR zglif_batch_stat-batch_id OBLIGATORY MEMORY ID zglif_batch.

PARAMETERS: p_inter TYPE zglif_batch_stat-interface_name OBLIGATORY DEFAULT zcl_bookng_tools=&gt;mc_interface_name.


START-OF-SELECTION.

  WRITE: / |Deleting batch no:|.
  LOOP AT s_batch.
    WRITE | { s_batch-low },|.
  ENDLOOP.

  CASE p_inter.
    WHEN zcl_bookng_tools=&gt;mc_interface_name.

      WRITE: / |ZGLIF_BOOKNG_WS table:|.

      DELETE FROM zglif_bookng_ws WHERE batch_id IN s_batch.

      IF sy-subrc = 0.
        WRITE: / |Deleted { sy-dbcnt } rows from ZGLIF_BOOKNG_WS table|.
      ELSE.
        WRITE: / |Problem with deletion of no data found|.
      ENDIF.

    WHEN zcl_bookng_tools=&gt;mc_csv_interface.

      WRITE: / |ZBOOKNG_CSV_DATA table:|.

      DELETE FROM zbookng_csv_data WHERE batch_id IN s_batch.

      IF sy-subrc = 0.
        WRITE: / |Deleted { sy-dbcnt } rows from ZBOOKNG_CSV_DATA table|.
      ELSE.
        WRITE: / |Problem with deletion of no data found|.
      ENDIF.


    WHEN zcl_bookng_tools=&gt;mc_csv_tax_interface.

      WRITE: / |ZBOOKNG_CSV_TAX table:|.

      DELETE FROM zbookng_csv_tax WHERE batch_id IN s_batch.

      IF sy-subrc = 0.
        WRITE: / |Deleted { sy-dbcnt } rows from ZBOOKNG_CSV_TAX table|.
      ELSE.
        WRITE: / |Problem with deletion of no data found|.
      ENDIF.


  ENDCASE.

  WRITE: / |ZGLIF_BATCH_STAT table:|.

  DELETE FROM zglif_batch_stat WHERE interface_name = p_inter AND batch_id IN s_batch.

  IF sy-subrc = 0.
    WRITE: / |Deleted { sy-dbcnt } rows from ZGLIF_BATCH_STAT table|.
  ELSE.
    WRITE: / |Problem with deletion of no data found|.
  ENDIF.


  WRITE: / |ZGLIF_MESS_STAT table:|.

  DELETE FROM zglif_mess_stat WHERE interface_name = p_inter AND batch_id IN s_batch.

  IF sy-subrc = 0.
    WRITE: / |Deleted { sy-dbcnt } rows from ZGLIF_MESS_STAT table|.
  ELSE.
    WRITE: / |Problem with deletion of no data found|.
  ENDIF.

  COMMIT WORK AND WAIT.</source>
 </PROG>
 <PROG NAME="ZBOOKNG_AUTO_PROCESSING" VARCL="X" DBAPL="S" SUBC="1" RMAND="700" FIXPT="X" LDBNAME="D$S" UCCHECK="X">
  <textPool>
   <language SPRAS="E">
    <textElement ID="R" ENTRY="Automatic processing of BOOKNG CSV files" LENGTH="40 "/>
    <textElement ID="S" KEY="P_INTERF" ENTRY="D       ." LENGTH="14 "/>
    <textElement ID="S" KEY="R_PARK" ENTRY="        Fully saved document" LENGTH="28 "/>
    <textElement ID="S" KEY="R_POST" ENTRY="        Post document" LENGTH="21 "/>
   </language>
  </textPool>
  <source>*&amp;---------------------------------------------------------------------*
*&amp; Report zbookng_auto_processing
*&amp;---------------------------------------------------------------------*
*&amp;
*&amp;---------------------------------------------------------------------*
REPORT zbookng_auto_processing.

TABLES sscrfields.

CONSTANTS: gc_interface TYPE zglif_interface_name VALUE zcl_bookng_tools=&gt;mc_csv_auto.

SELECTION-SCREEN: FUNCTION KEY 1.   &quot; for button GLIF Monitor

PARAMETERS: p_interf TYPE zbookng_csv_auto_folder OBLIGATORY DEFAULT &apos;BOOKNG&apos;.

SELECTION-SCREEN SKIP.

PARAMETERS: r_park RADIOBUTTON GROUP rd1 DEFAULT &apos;X&apos;,
            r_post RADIOBUTTON GROUP rd1.



INITIALIZATION.

  sscrfields-functxt_01     = VALUE smp_dyntxt( icon_id = icon_systems quickinfo = &apos;Glif Monitor&apos; icon_text = &apos;GLIF Monitor&apos; ).
*  sscrfields-functxt_02     = VALUE smp_dyntxt( icon_id = icon_time_control quickinfo = &apos;Log&apos; icon_text = &apos;Log&apos; ).


AT SELECTION-SCREEN.

  CASE sscrfields-ucomm.

    WHEN &apos;FC01&apos;.
      TRY.
          EXPORT zglif_interface = gc_interface TO MEMORY ID &apos;ZGLIF_INTERFACE&apos;.
          CALL TRANSACTION &apos;ZGLIF_MONITOR&apos; WITH AUTHORITY-CHECK.
        CATCH cx_sy_authorization_error.
          MESSAGE &apos;You do not have authorization for Tcode ZGLIF_MONITOR&apos; TYPE &apos;S&apos; DISPLAY LIKE &apos;E&apos;.
      ENDTRY.
    WHEN OTHERS.
  ENDCASE.

START-OF-SELECTION.

  DATA(lo_dispatcher) = zglif_dispatcher=&gt;get_dispatcher( piv_interface_name = gc_interface ).

  lo_dispatcher-&gt;feed( ).

  lo_dispatcher-&gt;convert( ).

  lo_dispatcher-&gt;process( ).

  lo_dispatcher-&gt;process( ).

  lo_dispatcher-&gt;process_errors( ).</source>
 </PROG>
 <PROG NAME="ZBOOKNG_PROCESSING" VARCL="X" SUBC="1" RSTAT="K" RMAND="100" RLOAD="E" FIXPT="X" UCCHECK="X">
  <textPool>
   <language SPRAS="E">
    <textElement ID="I" KEY="001" ENTRY="Type of processing" LENGTH="28 "/>
    <textElement ID="I" KEY="002" ENTRY="Use this if you have changed something in ZBOOKNG_CUST" LENGTH="108 "/>
    <textElement ID="I" KEY="003" ENTRY="Use this if Error was FI related (closed period," LENGTH="96 "/>
    <textElement ID="I" KEY="004" ENTRY="Profit center customization, etc), NO CHANGE in ZBOOKNG_CUST" LENGTH="120 "/>
    <textElement ID="R" ENTRY="Processing of data from BOOKNG" LENGTH="30 "/>
    <textElement ID="S" KEY="P_BATCH" ENTRY="D       ." LENGTH="9 "/>
    <textElement ID="S" KEY="R_ALL" ENTRY="        Reprocess All -&gt; new iDocs" LENGTH="34 "/>
    <textElement ID="S" KEY="R_NO_CON" ENTRY="        Reprocess part -&gt; No new iDocs" LENGTH="38 "/>
   </language>
  </textPool>
  <source>*&amp;---------------------------------------------------------------------*
*&amp; Report ZBOOKNG_PROCESSING
*&amp;---------------------------------------------------------------------*
*&amp;
*&amp;---------------------------------------------------------------------*
REPORT zbookng_processing.


TABLES sscrfields.

SELECTION-SCREEN: FUNCTION KEY 1.   &quot; for button BOOK Customization
SELECTION-SCREEN: FUNCTION KEY 2.   &quot; for button GLIF Monitor
SELECTION-SCREEN: FUNCTION KEY 3.   &quot; for button WF Responsibilities
SELECTION-SCREEN: FUNCTION KEY 4.   &quot; LOG

PARAMETERS: p_batch TYPE zglif_batch_id MEMORY ID zglif_batch OBLIGATORY.

SELECTION-SCREEN SKIP.

SELECTION-SCREEN BEGIN OF BLOCK bk1 WITH FRAME TITLE TEXT-001.


PARAMETERS: r_all RADIOBUTTON GROUP rd1 DEFAULT &apos;X&apos;.
SELECTION-SCREEN COMMENT /5(79) TEXT-002 FOR FIELD r_all.

PARAMETERS: r_no_con RADIOBUTTON GROUP rd1.
SELECTION-SCREEN COMMENT /5(79) TEXT-003 FOR FIELD r_no_con.
SELECTION-SCREEN COMMENT /5(79) TEXT-004 FOR FIELD r_no_con.


SELECTION-SCREEN END OF BLOCK bk1.

INITIALIZATION.

  sscrfields-functxt_01     = VALUE smp_dyntxt( icon_id = icon_table_settings quickinfo = &apos;Customize the mapping tables of BOOKNG&apos; icon_text = &apos;BOOKNG Cust&apos; ).
  sscrfields-functxt_02     = VALUE smp_dyntxt( icon_id = icon_systems quickinfo = &apos;Glif Monitor - change status for batch&apos; icon_text = &apos;GLIF Monitor&apos; ).
  sscrfields-functxt_03     = VALUE smp_dyntxt( icon_id = icon_agent quickinfo = &apos;Workflow Responsibilities&apos; icon_text = &apos;WF Responsibilities&apos; ).
  sscrfields-functxt_04     = VALUE smp_dyntxt( icon_id = icon_time_control quickinfo = &apos;Log&apos; icon_text = &apos;Log&apos; ).


AT SELECTION-SCREEN.

  CASE sscrfields-ucomm.
    WHEN &apos;FC01&apos;.
      CALL TRANSACTION &apos;ZBOOKNG_CUST&apos; WITH AUTHORITY-CHECK.
    WHEN &apos;FC02&apos;.
      EXPORT zglif_interface = zcl_bookng_tools=&gt;mc_interface_name TO MEMORY ID &apos;ZGLIF_INTERFACE&apos;.
      CALL TRANSACTION &apos;ZGLIF_MONITOR&apos; WITH AUTHORITY-CHECK AND SKIP FIRST SCREEN.
    WHEN &apos;FC03&apos;.    &quot; Workflow Responsibilities customization
      DATA lv_param_id TYPE objektid VALUE &apos;91000001&apos;.
      DATA(ls_options) = VALUE ctu_params( dismode = &apos;E&apos; ).

      DATA(lt_bdc) = VALUE bdcdata_tab(
        ( program = &apos;SAPLRHUZ&apos; dynpro = &apos;0410&apos; dynbegin = &apos;X&apos; fnam = &apos;P1000-OBJID&apos; fval = lv_param_id )
        ( program = &apos;SAPLRHUZ&apos; dynpro = &apos;0410&apos; dynbegin = &apos;&apos; fnam = &apos;BDC_OKCODE&apos; fval = &apos;/00&apos; )
        ).

      CALL TRANSACTION &apos;OOCU_RESP&apos; WITH AUTHORITY-CHECK USING lt_bdc OPTIONS FROM ls_options.
    WHEN &apos;FC04&apos;.

      DATA: ls_display_prog TYPE bal_s_prof,
            lv_date_from    TYPE baldate.

      lv_date_from = sy-datum.
      lv_date_from(4) = sy-datum(4) - 1.

      CALL FUNCTION &apos;BAL_DSP_PROFILE_DETLEVEL_GET&apos;
        IMPORTING
          e_s_display_profile = ls_display_prog.

      CALL FUNCTION &apos;APPL_LOG_DISPLAY&apos;
        EXPORTING
          object                    = &apos;Z*&apos;
          subobject                 = &apos;*&apos;
          external_number           = CONV balnrext( p_batch )
          suppress_selection_dialog = abap_true
          i_s_display_profile       = ls_display_prog
          time_from                 = &apos;000000&apos;
          time_to                   = &apos;235959&apos;
          date_from                 = lv_date_from
          date_to                   = sy-datum
        EXCEPTIONS
          no_authority              = 1
          OTHERS                    = 2.

    WHEN OTHERS.
  ENDCASE.


**********************************************************************
CLASS lcl_book_processing DEFINITION.

  PUBLIC SECTION.
    METHODS: constructor IMPORTING iv_batch_id TYPE zglif_batch_id RAISING zcx_generic_exception.
    METHODS: convert RAISING zcx_generic_exception.
    METHODS: test_processing RAISING zcx_generic_exception.
    METHODS: real_processing RAISING zcx_generic_exception.
    METHODS: error_handling.
    METHODS: send_email IMPORTING iv_status TYPE text10 RAISING zcx_generic_exception,
      get_log_handle RETURNING VALUE(rv_log_handle) TYPE balloghndl,
      log_end.
*      log_add_text IMPORTING iv_text TYPE string iv_type TYPE symsgty iv_detlevel TYPE ballevel.
  PROTECTED SECTION.
  PRIVATE SECTION.


    DATA: mv_batch_id   TYPE zglif_batch_id,
          mo_dispatcher TYPE REF TO zglif_dispatcher,
          mt_email      TYPE soli_tab,

          mv_log_handle TYPE balloghndl,
          mv_time_start TYPE syst-uzeit.

    METHODS: check_batch_status IMPORTING iv_status TYPE zglif_batch_status OPTIONAL RAISING zcx_generic_exception,
      collect_text RETURNING VALUE(rt_email) TYPE soli_tab,

      log_create.


ENDCLASS.

CLASS lcl_book_processing IMPLEMENTATION.

  METHOD constructor.

    GET TIME.
    mv_time_start = sy-uzeit.

    mv_batch_id = iv_batch_id.

    log_create( ).

    check_batch_status( ).
    mo_dispatcher = zglif_dispatcher=&gt;get_dispatcher( piv_interface_name = zcl_bookng_tools=&gt;mc_interface_name ).

  ENDMETHOD.

  METHOD log_create.
    DATA: lv_subobject TYPE balobj_d.
    lv_subobject = |Z{ zcl_bookng_tools=&gt;mc_interface_name }|.

    SELECT SINGLE log_days_keep, log_active
    INTO @DATA(ls_log)
    FROM zglif_dispcust
    WHERE interface_name = @zcl_bookng_tools=&gt;mc_interface_name.

    IF sy-subrc = 0.
      IF ls_log-log_days_keep IS INITIAL.
        ls_log-log_days_keep = 60.
      ENDIF.

      IF ls_log-log_active = abap_true.
        mv_log_handle = zglif_logger=&gt;log_create(
          EXPORTING
            iv_object          = lv_subobject
            iv_subobject       = CONV #( zcl_bookng_tools=&gt;mc_interface_name )
            iv_extnumber       = CONV #( mv_batch_id )
            iv_expiration_days = ls_log-log_days_keep
        ).
      ENDIF.

    ENDIF.

  ENDMETHOD.


  METHOD check_batch_status.

    SELECT SINGLE batch_status
    FROM zglif_batch_stat
    INTO @DATA(lv_status)
    WHERE batch_id = @mv_batch_id
    AND interface_name = @zcl_bookng_tools=&gt;mc_interface_name.
    IF sy-subrc &lt;&gt; 0.
      zcx_generic_exception=&gt;raise( |Batch does not exist| ).
    ELSEIF iv_status IS NOT INITIAL AND iv_status IS SUPPLIED AND lv_status &lt;&gt; iv_status.

      DATA: ls_dd07v            TYPE dd07v,
            lv_status_is        TYPE string,
            lv_status_requested TYPE string.

      CALL FUNCTION &apos;DD_DOMVALUE_TEXT_GET&apos;
        EXPORTING
          domname  = &apos;ZGLIF_BATCH_STATUS_DOM&apos;                  &quot; Domain Name
          value    = CONV dd07l-domvalue_l( lv_status )        &quot; Fixed value (DOMVALUE_L)
          langu    = &apos;E&apos;            &quot; Language of text; if SPACE; SY_LANGU,
        IMPORTING
          dd07v_wa = ls_dd07v.
      lv_status_is = ls_dd07v-ddtext.
      CALL FUNCTION &apos;DD_DOMVALUE_TEXT_GET&apos;
        EXPORTING
          domname  = &apos;ZGLIF_BATCH_STATUS_DOM&apos;                  &quot; Domain Name
          value    = CONV dd07l-domvalue_l( iv_status )        &quot; Fixed value (DOMVALUE_L)
          langu    = &apos;E&apos;            &quot; Language of text; if SPACE; SY_LANGU,
        IMPORTING
          dd07v_wa = ls_dd07v.
      lv_status_requested = ls_dd07v-ddtext.
      zcx_generic_exception=&gt;raise( |Batch not in status { iv_status } - { lv_status_requested }, current status is { lv_status } - { lv_status_is }| ).

    ELSE.
      zglif_logger=&gt;log_add_text( iv_text = |Batch checked Okay. Current status: { lv_status }| iv_detlevel = &apos;2&apos; iv_log_handle = mv_log_handle ).
    ENDIF.

  ENDMETHOD.

  METHOD convert.
    WRITE / &apos;---------------------------------&apos;.
    WRITE / &apos;START CONVERSION OF DATA TO IDOC&apos;.
    WRITE / &apos;---------------------------------&apos;.

    GET TIME.
    WRITE: / |Time stamp: { sy-datum DATE = ENVIRONMENT } - { sy-uzeit TIME = ENVIRONMENT }|.

    zglif_logger=&gt;log_add_text( iv_text = &apos;Start of conversion&apos;  iv_detlevel = &apos;1&apos; iv_log_handle = mv_log_handle  ).

    mo_dispatcher-&gt;convert( ).

    COMMIT WORK AND WAIT.

    check_batch_status( iv_status = zglif_dispatcher=&gt;gc_10_converted ).

  ENDMETHOD.

  METHOD test_processing.

    WRITE / &apos;---------------------------------&apos;.
    WRITE / &apos;START TEST PROCESSING&apos;.
    WRITE / &apos;---------------------------------&apos;.

    GET TIME.
    WRITE: / |Time stamp: { sy-datum DATE = ENVIRONMENT } - { sy-uzeit TIME = ENVIRONMENT }|.

    zglif_logger=&gt;log_add_text( iv_text = &apos;Start test processing&apos; iv_detlevel = &apos;1&apos; iv_log_handle = mv_log_handle ).

    mo_dispatcher-&gt;process( ).

    COMMIT WORK AND WAIT.

    check_batch_status( iv_status = zglif_dispatcher=&gt;gc_20_test_processing_ok ).

  ENDMETHOD.

  METHOD real_processing.

    WRITE / &apos;---------------------------------&apos;.
    WRITE / &apos;START REAL PROCESSING&apos;.
    WRITE / &apos;---------------------------------&apos;.

    GET TIME.
    WRITE: / |Time stamp: { sy-datum DATE = ENVIRONMENT } - { sy-uzeit TIME = ENVIRONMENT }|.

    zglif_logger=&gt;log_add_text( iv_text = &apos;Start real processing&apos; iv_detlevel = &apos;1&apos; iv_log_handle = mv_log_handle ).

    mo_dispatcher-&gt;process( ).

    COMMIT WORK AND WAIT.

    check_batch_status( iv_status = zglif_dispatcher=&gt;gc_30_processing_ok ).

  ENDMETHOD.

  METHOD error_handling.
    WRITE / &apos;---------------------------------&apos;.
    WRITE / &apos;RUNNING ERROR HANDLER&apos;.
    WRITE / &apos;---------------------------------&apos;.

    GET TIME.
    WRITE: / |Time stamp: { sy-datum DATE = ENVIRONMENT } - { sy-uzeit TIME = ENVIRONMENT }|.

    zglif_logger=&gt;log_add_text( iv_text = &apos;Start Error handler&apos; iv_detlevel = &apos;1&apos; iv_log_handle = mv_log_handle ).


    mo_dispatcher-&gt;process_errors( ).

    COMMIT WORK AND WAIT.

  ENDMETHOD.


  METHOD send_email.

    zglif_logger=&gt;log_add_text( iv_text = &apos;Sending emails&apos; iv_detlevel = &apos;1&apos; iv_log_handle = mv_log_handle ).

    TRY.

        SELECT SINGLE mtext
        FROM t000
        INTO @DATA(lv_system_text)
        WHERE mandt = @sy-mandt.

        DATA(lo_bcs) = cl_bcs=&gt;create_persistent( ).
        DATA(lo_document) = cl_document_bcs=&gt;create_document(
        i_type    = &apos;RAW&apos;
        i_text    = collect_text( )
        i_subject = |{ sy-sysid } - { iv_status } - Batch ID: { mv_batch_id } - { lv_system_text }| ).


        lo_bcs-&gt;set_document( lo_document ).

        lo_bcs-&gt;set_sender( i_sender = cl_cam_address_bcs=&gt;create_internet_address( |pozdrav_ze_{ sy-sysid }_sapu@embedit.cz| ) ).

        CASE iv_status.
          WHEN &apos;OK&apos;.
            SELECT email
                FROM zbookng_email
                INTO TABLE @DATA(lt_email)
                WHERE positive = @abap_true.
            IF lt_email IS INITIAL.
              RETURN.
            ENDIF.
          WHEN &apos;ERROR&apos;.
            SELECT email
                FROM zbookng_email
                INTO TABLE @lt_email
                WHERE negative = @abap_true.
            IF lt_email IS INITIAL.
              RETURN.
            ENDIF.
        ENDCASE.


        LOOP AT lt_email INTO DATA(ls_email).
          DATA(lo_recipient) = cl_cam_address_bcs=&gt;create_internet_address( ls_email-email ).
          lo_bcs-&gt;add_recipient( lo_recipient ).
          zglif_logger=&gt;log_add_text( iv_text = |Sending email to: { ls_email-email }| iv_detlevel = &apos;2&apos; iv_log_handle = mv_log_handle ).
        ENDLOOP.

        lo_bcs-&gt;set_send_immediately( abap_true  ).

        lo_bcs-&gt;send_without_dialog(  ).

        COMMIT WORK.

        zglif_logger=&gt;log_add_text( iv_text = &apos;All emails were sent&apos; iv_detlevel = &apos;2&apos; iv_log_handle = mv_log_handle ).

        zglif_logger=&gt;log_save( iv_log_handle = mv_log_handle ).

      CATCH cx_bcs INTO DATA(lo_exception).
*        zcx_generic_exception=&gt;raise( lo_exception-&gt;get_text( ) ).
    ENDTRY.
  ENDMETHOD.


  METHOD collect_text.

    DATA: lv_lines TYPE i,
          lv_value TYPE LINE OF soli_tab,
          lv_page  TYPE i,
          lv_pages TYPE i.


    IF sy-spono IS NOT INITIAL.

      DATA: lt_buffer TYPE TABLE OF text255.

      CALL FUNCTION &apos;RSPO_RETURN_ABAP_SPOOLJOB&apos;
        EXPORTING
          rqident              = CONV rspoid( sy-spono )
*         first_line           = 1
*         last_line            =
*         pages                =
        TABLES
          buffer               = lt_buffer
        EXCEPTIONS
          no_such_job          = 1
          not_abap_list        = 2
          job_contains_no_data = 3
          selection_empty      = 4
          no_permission        = 5
          can_not_access       = 6
          read_error           = 7
          OTHERS               = 8.

      APPEND LINES OF lt_buffer TO rt_email.

    ENDIF.

    DESCRIBE LIST : NUMBER OF PAGES lv_pages INDEX 0.

    DO lv_pages TIMES.
      lv_page = sy-index.

      WHILE 1 = 1.

        READ LINE sy-index OF PAGE lv_page LINE VALUE INTO lv_value.
        IF sy-subrc = 0.
          APPEND lv_value TO rt_email.
        ELSE.
          EXIT.
        ENDIF.

      ENDWHILE.

    ENDDO.

  ENDMETHOD.


  METHOD get_log_handle.
    rv_log_handle = mv_log_handle.
  ENDMETHOD.


  METHOD log_end.

    DATA: lv_time_end   TYPE sy-uzeit.

    GET TIME.
    lv_time_end = sy-uzeit.
    lv_time_end = lv_time_end - mv_time_start.

    zglif_logger=&gt;log_add_text( iv_text = |Running time: { lv_time_end TIME = ENVIRONMENT }| iv_type = &apos;S&apos; iv_detlevel = &apos;1&apos; iv_log_handle = mv_log_handle ).

  ENDMETHOD.

ENDCLASS.

**********************************************************************
START-OF-SELECTION.

  TRY.
      DATA(lo_book_processing) = NEW lcl_book_processing( p_batch ).

      IF r_all = abap_true.
        lo_book_processing-&gt;convert( ).
      ENDIF.
      lo_book_processing-&gt;test_processing( ).
      lo_book_processing-&gt;real_processing( ).
      lo_book_processing-&gt;error_handling( ).

      lo_book_processing-&gt;log_end( ).

      lo_book_processing-&gt;send_email( &apos;OK&apos; ).

    CATCH zcx_generic_exception INTO DATA(lo_exception).
      WRITE / &apos;---------------------------------&apos;.
      IF lo_book_processing IS BOUND.
        zglif_logger=&gt;log_add_text( iv_text = &apos;ERROR HAPPENED&apos; iv_type = &apos;E&apos; iv_detlevel = &apos;1&apos; iv_log_handle = lo_book_processing-&gt;get_log_handle( ) ).
        zglif_logger=&gt;log_add_text( iv_text = lo_exception-&gt;get_message( ) iv_type = &apos;E&apos; iv_detlevel = &apos;2&apos; iv_log_handle = lo_book_processing-&gt;get_log_handle( )  ).
      ENDIF.
      WRITE / lo_exception-&gt;get_message( ).
      WRITE / &apos;---------------------------------&apos;.

      IF lo_book_processing IS BOUND.
        lo_book_processing-&gt;error_handling( ).
        lo_book_processing-&gt;send_email( &apos;ERROR&apos; ).
      ENDIF.

  ENDTRY.</source>
 </PROG>
