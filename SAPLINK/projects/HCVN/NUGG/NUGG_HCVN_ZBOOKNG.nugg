   <functionmodule NAME="ZBOOKNG_WS_DOCUMENT" REMOTE="R" STEXT="BookNG: demo for ws-call">
    <importing PARAMETER="IS_DOCUMENT" TYP="ZBOOKNG_WS_MESSAGE"/>
    <exporting PARAMETER="EV_RESPONSE" TYP="ZBOOKNG_WS_RESPONSE"/>
    <documentation PARAMETER="IS_DOCUMENT" KIND="P" STEXT="BookNG: Document" INDEX=" 001"/>
    <documentation PARAMETER="EV_RESPONSE" KIND="P" STEXT="BookNG WS Response" INDEX=" 002"/>
    <fm_source_new>&quot; Because this is a webservice called from outside without our knowledge when and how and how many times. We cannot just call glif_activate
  &quot; with data exported to MEMORY ID because webservice can be called many times at same time and data could be corrupted then.
  &quot; Insted we will just straight put them into database
  &quot; Glif activate will just check the consistency of data, that both WS were called and so we know that it is end of BATCH
  &quot; (Batch contains multiple messages - Message is one call of this WS which contains many ITEMS which has to add up to 0 on DEBIT and CREDIT)

  CONSTANTS: gc_error  TYPE text10 VALUE &apos;Error&apos;,
             gc_ok     TYPE text10 VALUE &apos;OK&apos;,
             gc_debit  TYPE zbookng_debit_credit VALUE &apos;DEBIT&apos;,
             gc_credit TYPE zbookng_debit_credit VALUE &apos;CREDIT&apos;.



  zglif_logger=&gt;get_timestamp( IMPORTING  pev_date = DATA(lv_date)
                                          pev_time = DATA(lv_time) ).
  DATA: lt_bookng_data    TYPE TABLE OF zglif_bookng_ws,
        ls_bookng_data    TYPE zglif_bookng_ws,
        lv_total_amout    TYPE wrbtr,
        lv_message_status TYPE zglif_message_status.
*
*  DATA(lt_bookng_data) = VALUE zglif_bookng_ws_tt(
*  FOR ls_item IN is_document-items
*      (
*          batch_id        = is_document-head-batch_id
*          message_id      = is_document-head-message_id
*          reference1      = ls_item-reference1
*          message_status  = zglif_dispatcher=&gt;gc_00_downloaded
*          company_code    = is_document-head-company_code
*          document_date   = is_document-head-document_date
*          posting_date    = is_document-head-posting_date
*          ledger_group    = is_document-head-ledger_group
*          debit_credit    = ls_item-debit_credit
*          gl_account      = ls_item-gl_account
*          amount          = ls_item-amount
*          negative_posting = ls_item-negative_posting
*          value_date      = ls_item-value_date
*          profit_center   = ls_item-profit_center
*          business_area   = ls_item-business_area
*          segment         = ls_item-segment
*          item_text       = ls_item-item_text
*          item_model      = ls_item-item_model
*          changed_on      = lv_date
*          changed_at      = lv_time
*          changed_by      = sy-uname
*      )
*  ).

  &quot; Unless something happens everything is fine
  ev_response-status = gc_ok.
  ev_response-text = &apos;Data successfully saved&apos;.
  lv_message_status = zglif_dispatcher=&gt;gc_00_downloaded.


  LOOP AT is_document-items INTO DATA(ls_item).
    APPEND INITIAL LINE TO lt_bookng_data ASSIGNING FIELD-SYMBOL(&lt;ls_bookng_data&gt;).
    &lt;ls_bookng_data&gt;-batch_id           = |{ is_document-head-batch_id ALPHA = IN }|.
    &lt;ls_bookng_data&gt;-message_id         = |{ is_document-head-message_id ALPHA = IN }|.
    &lt;ls_bookng_data&gt;-item               = ls_item-line_item.
    &lt;ls_bookng_data&gt;-message_status     = zglif_dispatcher=&gt;gc_00_downloaded.
    &lt;ls_bookng_data&gt;-company_code       = is_document-head-company_code.
    &lt;ls_bookng_data&gt;-document_date      = is_document-head-document_date.
    &lt;ls_bookng_data&gt;-posting_date       = is_document-head-posting_date.
    &lt;ls_bookng_data&gt;-ledger_group       = is_document-head-ledger_group.
    &lt;ls_bookng_data&gt;-header_text        = is_document-head-header_text.
    &lt;ls_bookng_data&gt;-debit_credit       = to_upper( ls_item-debit_credit ).
    &lt;ls_bookng_data&gt;-gl_account         = ls_item-gl_account.
    &lt;ls_bookng_data&gt;-amount             = ls_item-amount.
*    &lt;ls_bookng_data&gt;-negative_posting   = ls_item-negative_posting.
    &lt;ls_bookng_data&gt;-value_date         = ls_item-value_date.
    &lt;ls_bookng_data&gt;-profit_center      = ls_item-profit_center.
    &lt;ls_bookng_data&gt;-business_area      = ls_item-business_area.
    &lt;ls_bookng_data&gt;-segment            = ls_item-segment.
    &lt;ls_bookng_data&gt;-item_text          = ls_item-item_text.
    &lt;ls_bookng_data&gt;-reference1         = ls_item-reference1.
    &lt;ls_bookng_data&gt;-reference2         = ls_item-reference2.
    &lt;ls_bookng_data&gt;-reference3         = ls_item-reference3.
    &lt;ls_bookng_data&gt;-tax_code           = ls_item-tax_code.
    &lt;ls_bookng_data&gt;-changed_on         = lv_date.
    &lt;ls_bookng_data&gt;-changed_at         = lv_time.
    &lt;ls_bookng_data&gt;-changed_by         = sy-uname.

    CASE to_upper( ls_item-debit_credit ).
      WHEN gc_debit.
        ADD ls_item-amount TO lv_total_amout.
      WHEN gc_credit.
        SUBTRACT ls_item-amount FROM lv_total_amout.
      WHEN OTHERS.
    ENDCASE.

  ENDLOOP.
  IF sy-subrc &lt;&gt; 0.
    ev_response-status = gc_error.
    ev_response-text = &apos;No Items found in data&apos;.
    RETURN.
  ENDIF.

  IF lv_total_amout &lt;&gt; 0.
    ev_response-status = gc_error.
    ev_response-text = &apos;Message does not have ZERO balance of Debit/Credit&apos;.
    CLEAR: ls_bookng_data.
    ls_bookng_data-message_status = lv_message_status = zglif_dispatcher=&gt;gc_02_downloaded_data_error.
    MODIFY lt_bookng_data FROM ls_bookng_data TRANSPORTING message_status WHERE message_status IS NOT INITIAL.
  ENDIF.

**********************************************************************
  &quot; TESTING NEW PRODUCT - check testing ledger

  DATA: ltr_test_ledger TYPE RANGE OF zbookng_ledger_group.

  SELECT &apos;I&apos; AS sign, &apos;EQ&apos; AS option, test_ledger AS low
  FROM zbookng_test_led
  INTO CORRESPONDING FIELDS OF TABLE @ltr_test_ledger.

  IF ltr_test_ledger IS NOT INITIAL AND is_document-head-ledger_group IN ltr_test_ledger.

    &quot; if testing ledger, than do not save at all into zglif_bookng_ws or zglif_mess_stat

    MODIFY zbookng_staging FROM TABLE lt_bookng_data.
    ev_response-text = &apos;Data saved to staging table&apos;.
    ev_response-info = &apos;Testing ledger recognized&apos;.

    &quot; End further processing...
    RETURN.

  ENDIF.


**********************************************************************
  TRY .
      INSERT zglif_bookng_ws FROM TABLE lt_bookng_data ACCEPTING DUPLICATE KEYS.
      IF sy-subrc = 0.
        DATA(ls_glif_mess_stat) = VALUE zglif_mess_stat(
                  interface_name      = zcl_bookng_tools=&gt;mc_interface_name
                  &quot; filename            = &quot; no filename for webservice
                  batch_id            = |{ is_document-head-batch_id ALPHA = IN }|
                  message_id          = |{ is_document-head-message_id ALPHA = IN }|
                  message_status      = lv_message_status    &quot; #TODO: logger
                  message_changed_on  = lv_date
                  message_changed_at  = lv_time
                  message_changed_by  = sy-uname
        ).

        MODIFY zglif_mess_stat FROM ls_glif_mess_stat.

      ELSE.
        IF sy-dbcnt &gt; 0.
          UPDATE zglif_bookng_ws SET message_status = @zglif_dispatcher=&gt;gc_02_downloaded_data_error
                WHERE batch_id = @( |{ is_document-head-batch_id ALPHA = IN }| )
                AND message_id = @( |{ is_document-head-message_id ALPHA = IN }| ).
          ls_glif_mess_stat = VALUE zglif_mess_stat(
                  interface_name      = zcl_bookng_tools=&gt;mc_interface_name
                  &quot; filename            = &quot; no filename for webservice
                  batch_id            = |{ is_document-head-batch_id ALPHA = IN }|
                  message_id          = |{ is_document-head-message_id ALPHA = IN }|
                  message_status      = zglif_dispatcher=&gt;gc_02_downloaded_data_error    &quot; #TODO: logger
                  message_changed_on  = lv_date
                  message_changed_at  = lv_time
                  message_changed_by  = sy-uname ).
          MODIFY zglif_mess_stat FROM ls_glif_mess_stat.
          ev_response-text = |This Batch ID { is_document-head-batch_id } with Message ID { is_document-head-message_id } was either already received or contains duplicate items. Whole Message is discarted|.
        ELSE.
          ev_response-text = |This Batch ID { is_document-head-batch_id } with Message ID { is_document-head-message_id } was already received|.
        ENDIF.
        ev_response-status = gc_error.

      ENDIF.

    CATCH cx_root.



  ENDTRY.

  COMMIT WORK AND WAIT.</fm_source_new>
    <functionModuleDocumentation/>
   </functionmodule>
   <functionmodule NAME="ZBOOKNG_WS_DOCUMENT2" REMOTE="R" STEXT="BookNG: demo for ws-call">
    <importing PARAMETER="IS_DOCUMENT_REQ" TYP="ZBOOKNG_DOCUMENT"/>
    <exporting PARAMETER="EV_RESPONSE" TYP="TEXT255"/>
    <documentation PARAMETER="IS_DOCUMENT_REQ" KIND="P" STEXT="BookNG: Document" INDEX=" 001"/>
    <documentation PARAMETER="EV_RESPONSE" KIND="P" STEXT="Response" INDEX=" 002"/>
    <fm_source_new>&quot; Save the webservice data so we can access it from GLIF (Global Interface Framework)
  &quot; Please note that the ASSING with syntax lv_fieldname = &apos;(ZBOOKNG_WS_DOCUMENT)GS_WS_DATA&apos;. will not work as we use SUBMIT report
  DATA(ls_exp_bookng_ws_data) = VALUE zbookng_document( ).
  ls_exp_bookng_ws_data = is_document_req.

  EXPORT ls_exp_bookng_ws_data TO MEMORY ID &apos;LS_BOOKNG_WS_DATA&apos;.

  &quot; Start defined GLIF: Feeder for the interface - in our case the interface is &apos;HCCN_BOOKNG&apos;
  &quot; and the defined (see TC ZGLIF_CUSTOMIZATION) Feeder is ZGLIF_FEEDER_WEBSERVICE
  DATA(lv_spoolname) = VALUE sypri_prtxt( ).
  lv_spoolname = &apos;BookNG_WS&apos;.

  &quot; Submit...and return - we want to continue in this program in rder to send the response
  SUBMIT zglif_activate
  WITH pa_inter = &apos;HCCN_BOOKNG&apos;     &quot; Interface name
  WITH pa_feed  = abap_true         &quot; Radio button value in RB group - run only Feeder
  WITH pa_all   = abap_false        &quot; Radio button value in RB group - disable Run all steps
  TO SAP-SPOOL
    LIST NAME lv_spoolname
    IMMEDIATELY space
    WITHOUT SPOOL DYNPRO
    KEEP IN SPOOL &apos;X&apos;
  AND RETURN.


  MOVE: &apos;Hello, pac &amp; pusu&apos; TO ev_response.</fm_source_new>
    <functionModuleDocumentation/>
   </functionmodule>
   <functionmodule NAME="ZBOOKNG_WS_BATCH_END" REMOTE="R" STEXT="BookNG WS for End of Batch">
    <importing PARAMETER="IS_DOCUMENT" OPTIONAL="X" TYP="ZBOOKNG_WS_MESSAGE_END"/>
    <exporting PARAMETER="EV_RESPONSE" TYP="ZBOOKNG_WS_RESPONSE"/>
    <documentation PARAMETER="IS_DOCUMENT" KIND="P" STEXT="BookNG: Document END" INDEX=" 001"/>
    <documentation PARAMETER="EV_RESPONSE" KIND="P" STEXT="BookNG WS Response" INDEX=" 002"/>
    <fm_source_new>CONSTANTS: gc_error TYPE text10 VALUE &apos;Error&apos;,
             gc_ok    TYPE text10 VALUE &apos;OK&apos;.

  DATA: lv_message_status TYPE zglif_message_status,
        ls_batch_stat     TYPE zglif_batch_stat,
        ltr_batch_id      TYPE RANGE OF zglif_batch_stat-batch_id.

  ev_response-status = gc_ok.
  ev_response-text = &apos;Data received, system check successful, start processing&apos;.
  lv_message_status = zglif_dispatcher=&gt;gc_00_downloaded.


  IF is_document-batch_id IS INITIAL.
    ev_response-status = gc_error.
    ev_response-text = &apos;Batch ID is missing&apos;.
    RETURN.
  ENDIF.


  SELECT SINGLE batch_status
    FROM zglif_batch_stat
    WHERE interface_name = @zcl_bookng_tools=&gt;mc_interface_name
    AND batch_id = @( |{ is_document-batch_id ALPHA = IN }| )
    AND batch_status &gt;= &apos;10&apos;
    INTO @DATA(lv_batch_stat).
  IF sy-subrc = 0.
    DATA: ls_dd07v TYPE dd07v.
    CALL FUNCTION &apos;DD_DOMVALUE_TEXT_GET&apos;
      EXPORTING
        domname  = &apos;ZGLIF_BATCH_STATUS_DOM&apos;                  &quot; Domain Name
        value    = CONV dd07l-domvalue_l( lv_batch_stat )                 &quot; Fixed value (DOMVALUE_L)
        langu    = &apos;E&apos;            &quot; Language of text; if SPACE; SY_LANGU,
*       bypass_buffer = space
      IMPORTING
        dd07v_wa = ls_dd07v
*       rc       =
      .
    ev_response-status = gc_error.
    ev_response-text = &apos;Data already received and processed&apos;.
    ev_response-info = |Current status of batch id { is_document-batch_id } is: { ls_dd07v-ddtext }|.
    RETURN.
  ENDIF.


  SELECT COUNT( DISTINCT message_id ) AS count
    FROM zglif_bookng_ws
    WHERE batch_id = @( |{ is_document-batch_id ALPHA = IN }| )
    AND message_status = @( |{ zglif_dispatcher=&gt;gc_00_downloaded ALPHA = IN }| )
    INTO @DATA(lv_count_book).

  SELECT COUNT(*)
    FROM zglif_mess_stat
    WHERE interface_name = @zcl_bookng_tools=&gt;mc_interface_name
    AND batch_id = @( |{ is_document-batch_id ALPHA = IN }| )
    AND message_status = @( |{ zglif_dispatcher=&gt;gc_00_downloaded ALPHA = IN }| )
    INTO @DATA(lv_count_mess_stat).

**********************************************************************
  &quot; TESTING NEW PRODUCT
  &quot; When testing new product, the total count will not match the zglif_mess_stat and zglif_bookng_ws tables.
  &quot; So we subtract count from staging table

  SELECT COUNT( DISTINCT message_id ) AS count
    FROM zbookng_staging
    WHERE batch_id = @( |{ is_document-batch_id ALPHA = IN }| )
    AND message_status = @( |{ zglif_dispatcher=&gt;gc_00_downloaded ALPHA = IN }| )
    INTO @DATA(lv_count_staging).

  DATA(lv_claim_count) = is_document-number_of_messages.

  SUBTRACT lv_count_staging FROM lv_claim_count.


**********************************************************************


  IF lv_count_book &lt;&gt; lv_count_mess_stat OR lv_count_book &lt;&gt; lv_claim_count OR lv_count_mess_stat &lt;&gt; lv_claim_count.
    ev_response-status = gc_error.
    ev_response-text = |Data inconsistency. Received messages: { lv_count_book }. Claimed messages: { is_document-number_of_messages }. Start over...|.
    lv_message_status = zglif_dispatcher=&gt;gc_02_downloaded_data_error.
  ENDIF.

  &quot; when claimed count without testing count is zero, only testing data were send, do not proceed with processing data
  IF lv_claim_count = 0 OR lv_count_mess_stat = 0.
    ev_response-text = |Only testing data found. Do not start the processing|.
    RETURN.
  ENDIF.


  ls_batch_stat = VALUE #(
                  interface_name      = zcl_bookng_tools=&gt;mc_interface_name
                  batch_id            = |{ is_document-batch_id ALPHA = IN }|
                  batch_status        = lv_message_status
                  batch_changed_on    = sy-datum
                  batch_changed_at    = sy-uzeit
                  batch_changed_by    = sy-uname
   ).

  MODIFY zglif_batch_stat FROM ls_batch_stat.


  ev_response-info = |Batch ID { is_document-batch_id } successfully stored in SAP with { lv_count_book } messages|.

  COMMIT WORK AND WAIT.

  IF ev_response-status = gc_error.
    RETURN.
  ENDIF.

  &quot; Is processing enabled on system
  IF zcl_bookng_tools=&gt;mc_processing_enabled = abap_true.

    DATA: lv_job_name  TYPE tbtcjob-jobname,
          lv_job_count TYPE tbtcjob-jobcount.

    lv_job_name = |BOOKNG_{ is_document-batch_id ALPHA = IN }_{ sy-datum }_{ sy-uzeit }|.
    CONDENSE lv_job_name NO-GAPS.

    CALL FUNCTION &apos;JOB_OPEN&apos;
      EXPORTING
        jobname          = lv_job_name                 &quot; Job Name
      IMPORTING
        jobcount         = lv_job_count                 &quot; ID Number of Background Job
      EXCEPTIONS
        cant_create_job  = 1                &quot; Job cannot be created, see system log
        invalid_job_data = 2                &quot; Job Contains Invalid Job Data, See SYSLOG
        jobname_missing  = 3                &quot; Job Name Not Specified
        OTHERS           = 4.
    IF sy-subrc &lt;&gt; 0.
      MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno
        WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4.
    ENDIF.

    SUBMIT zbookng_processing WITH p_batch = is_document-batch_id VIA JOB lv_job_name NUMBER lv_job_count TO SAP-SPOOL LIST NAME &apos;PROCESS_BATCH&apos; AND RETURN WITHOUT SPOOL DYNPRO.

*  SUBMIT zbookng_processing WITH p_batch = is_document-batch_id WITH r_all = abap_true TO SAP-SPOOL LIST NAME &apos;PROCESS_BATCH&apos;
*        VIA JOB lv_job_name
*        NUMBER lv_job_count
*        IMMEDIATELY space
*        WITHOUT SPOOL DYNPRO
*        KEEP IN SPOOL &apos;X&apos;
*        AND RETURN.

    CALL FUNCTION &apos;JOB_CLOSE&apos;
      EXPORTING
        jobcount             = lv_job_count                   &quot; Job number
        jobname              = lv_job_name                   &quot; Job Name
        strtimmed            = abap_true              &quot; Immediate Execution of Background Job
      EXCEPTIONS
        cant_start_immediate = 1                  &quot; Cannot Start Immediately
        invalid_startdate    = 2                  &quot; Start Condition is Invalid
        jobname_missing      = 3                  &quot; Job Name Missing (Wildcards Allowed)
        job_close_failed     = 4                  &quot; Error During JOB_CLOSE, See SYSLOG
        job_nosteps          = 5                  &quot; Job Specified Does Not Contain Any Steps
        job_notex            = 6                  &quot; Specified Job Does Not Exist
        lock_failed          = 7                  &quot; Lock Attempt Failed
        invalid_target       = 8                  &quot; Target Server or Group is Invalid
        invalid_time_zone    = 9                  &quot; Time Zone Invalid
        OTHERS               = 10.
    IF sy-subrc &lt;&gt; 0.
      MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno
        WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4.
    ENDIF.

    COMMIT WORK AND WAIT.

  ELSE.
    RETURN.
  ENDIF.</fm_source_new>
    <functionModuleDocumentation/>
   </functionmodule>
  </functionmodules>
