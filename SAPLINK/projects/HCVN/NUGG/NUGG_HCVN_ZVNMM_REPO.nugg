  <method CLSNAME="ZCL_VNMM_DPMT_RINV" CMPNAME="SALV_REFR" VERSION="1" LANGU="E" DESCRIPT="salv-object: refresh" EXPOSURE="0" STATE="1" EDITORDER="18 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SALV_REFR.

    &quot;refresh SALV

    &quot;..re-read data
    CLEAR: me-&gt;at_sdat.

    me-&gt;sdat_selc( ). &quot;data select
    me-&gt;sdat_auth( ). &quot;data authorisation check
    me-&gt;sdat_enha( ). &quot;data enhance

    me-&gt;salv_init_ccel( ). &quot;re-colour cells (per row-line)

    &quot;..refresh salv
    me-&gt;ar_salv-&gt;refresh( ).







  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_DPMT_RINV" CMPNAME="SDAT_AUTH" VERSION="1" LANGU="E" DESCRIPT="salv-data: check authorisation" EXPOSURE="0" STATE="1" EDITORDER="2 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_auth.

    &quot;authorithy check
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      &quot;M_RECH_BUK= Invoices: Company Code
      AUTHORITY-CHECK OBJECT &apos;M_RECH_BUK&apos;
       ID &apos;BUKRS&apos; FIELD &lt;sdat&gt;-_dpmt-bukrs
       ID &apos;ACTVT&apos; FIELD &apos;03&apos;.
      IF sy-subrc &lt;&gt; 0. CLEAR &lt;sdat&gt;. ENDIF.

      &quot;F_BKPF_KOA= Accounting Document: Authorization for Account Types
      AUTHORITY-CHECK OBJECT &apos;F_BKPF_KOA&apos;
       ID &apos;KOART&apos; FIELD &lt;sdat&gt;-_dpmt-koart
       ID &apos;ACTVT&apos; FIELD &apos;03&apos;.
      IF sy-subrc &lt;&gt; 0. CLEAR &lt;sdat&gt;. ENDIF.

      IF &lt;sdat&gt;-_invo-belnr IS NOT INITIAL.
        &quot;M_RECH_BUK= Invoices: Company Code
        AUTHORITY-CHECK OBJECT &apos;M_RECH_BUK&apos;
         ID &apos;BUKRS&apos; FIELD &lt;sdat&gt;-_invo-bkpf_bukrs
         ID &apos;ACTVT&apos; FIELD &apos;03&apos;.
        IF sy-subrc &lt;&gt; 0. CLEAR &lt;sdat&gt;. ENDIF.

        &quot;M_RECH_WRK= Materials Management - Invoice Verification
        AUTHORITY-CHECK OBJECT &apos;M_RECH_WRK&apos;
         ID &apos;WERKS&apos; FIELD &lt;sdat&gt;-_invo-werks
         ID &apos;ACTVT&apos; FIELD &apos;03&apos;.
        IF sy-subrc &lt;&gt; 0. CLEAR &lt;sdat&gt;. ENDIF.
      ENDIF.

    ENDLOOP.
    DELETE me-&gt;at_sdat WHERE _dpmt-belnr IS INITIAL.



  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_DPMT_RINV" CMPNAME="SDAT_ENHA" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance" EXPOSURE="0" STATE="1" EDITORDER="3 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha.

    &quot;enhance salv-data
    me-&gt;sdat_enha_wfsta_tx( ).
    me-&gt;sdat_enha_lifnr_tx( ).
    me-&gt;sdat_enha_zterm_tx( ).
    me-&gt;sdat_enha_erfna_tx( ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_DPMT_RINV" CMPNAME="SDAT_ENHA_ERFNA_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: creators user name" EXPOSURE="0" STATE="1" EDITORDER="7 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_erfna_tx.

    &quot;enha Creator&apos;s user name

    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      DATA: ls_addr TYPE          bapiaddr3,
            lt_ret  TYPE TABLE OF bapiret2.

      &quot;_DPMT-USNAME= Name of the Processor Who Entered the Object
      CALL FUNCTION &apos;BAPI_USER_GET_DETAIL&apos;
        EXPORTING
          username = &lt;sdat&gt;-_dpmt-usnam
        IMPORTING
          address  = ls_addr
        TABLES
          return   = lt_ret.
      IF sy-subrc = 0. MOVE: ls_addr-fullname TO &lt;sdat&gt;-_dpmt-usnam_tx. ENDIF.

      &quot;_DPMT-PPNAM= Name of User Who Parked This Document
      CALL FUNCTION &apos;BAPI_USER_GET_DETAIL&apos;
        EXPORTING
          username = &lt;sdat&gt;-_dpmt-ppnam
        IMPORTING
          address  = ls_addr
        TABLES
          return   = lt_ret.
      IF sy-subrc = 0. MOVE: ls_addr-fullname TO &lt;sdat&gt;-_dpmt-ppnam_tx. ENDIF.

      &quot;_INVO-ERFNAM= Name of the Processor Who Entered the Object
      CALL FUNCTION &apos;BAPI_USER_GET_DETAIL&apos;
        EXPORTING
          username = &lt;sdat&gt;-_invo-erfnam
        IMPORTING
          address  = ls_addr
        TABLES
          return   = lt_ret.
      IF sy-subrc = 0. MOVE: ls_addr-fullname TO &lt;sdat&gt;-_invo-erfnam_tx. ENDIF.

    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_DPMT_RINV" CMPNAME="SDAT_ENHA_LIFNR_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: vendor name" EXPOSURE="0" STATE="1" EDITORDER="5 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_lifnr_tx.

    &quot;enha Vendor&apos;s descriptions
    IF me-&gt;at_sdat IS NOT INITIAL.
      SELECT lfa1~lifnr,
             lfa1~adrnr,
             adrc~addrnumber,
             adrc~name1,
             adrc~name2 INTO TABLE @DATA(lt_lfnm)
        FROM            lfa1 AS lfa1
             INNER JOIN adrc ON adrc~addrnumber = lfa1~adrnr
        FOR ALL ENTRIES IN @me-&gt;at_sdat
        WHERE lfa1~lifnr = @me-&gt;at_sdat-_dpmt-lifnr.
    ENDIF.

    &quot;pass to items
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      READ TABLE lt_lfnm ASSIGNING FIELD-SYMBOL(&lt;lfnm&gt;) WITH KEY lifnr = &lt;sdat&gt;-_dpmt-lifnr. CHECK sy-subrc = 0.
      MOVE: &lt;lfnm&gt;-name1 TO &lt;sdat&gt;-_dpmt-lifnr_tx1,
            &lt;lfnm&gt;-name2 TO &lt;sdat&gt;-_dpmt-lifnr_tx2.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_DPMT_RINV" CMPNAME="SDAT_ENHA_WFSTA_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: wf-status description" EXPOSURE="0" STATE="1" EDITORDER="4 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_wfsta_tx.

    &quot;enha WFSTA status description
    DATA(lo_dref) = CAST cl_abap_elemdescr( cl_abap_elemdescr=&gt;describe_by_name( p_name = &apos;ZVNMM_MIRO_WFSTA_D&apos; ) ).
    DATA(lt_vals) = lo_dref-&gt;get_ddic_fixed_values( ).

    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).
      READ TABLE lt_vals ASSIGNING FIELD-SYMBOL(&lt;vals&gt;) WITH KEY low = &lt;sdat&gt;-_invo-wfsta.
      IF sy-subrc = 0. MOVE: &lt;vals&gt;-ddtext TO &lt;sdat&gt;-_invo-wfsta_tx.ENDIF.
    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_DPMT_RINV" CMPNAME="SDAT_ENHA_ZTERM_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: payment term description" EXPOSURE="0" STATE="1" EDITORDER="6 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_zterm_tx.

    &quot;enha Payment Terms decription
    IF me-&gt;at_sdat IS NOT INITIAL.
      SELECT *  INTO TABLE @DATA(lt_t052u)
        FROM t052u AS t052u
        FOR ALL ENTRIES IN @me-&gt;at_sdat
        WHERE spras = @sy-langu
          AND zterm = @me-&gt;at_sdat-_invo-zterm.
    ENDIF.

    &quot;pass to items
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      READ TABLE lt_t052u ASSIGNING FIELD-SYMBOL(&lt;t052u&gt;) WITH KEY zterm = &lt;sdat&gt;-_invo-zterm. CHECK sy-subrc = 0.
      MOVE: &lt;t052u&gt;-text1 TO &lt;sdat&gt;-_invo-zterm_tx.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_DPMT_RINV" CMPNAME="SDAT_SELC" VERSION="1" LANGU="E" DESCRIPT="salv-data: select data per keys" EXPOSURE="0" STATE="1" EDITORDER="1 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_selc.

    &quot;get data
    CHECK me-&gt;at_keys IS NOT INITIAL.

    &quot;§1.1 data of Down Payment
    SELECT bkpf~bukrs,
           bkpf~belnr,
           bkpf~gjahr,
           bkpf~xblnr,
           bkpf~bktxt,
           bkpf~budat,
           bkpf~bldat,
           bseg~buzei,
           bseg~koart,
           bseg~lifnr,
           bseg~wrbtr,
           bkpf~waers,
           bseg~zuonr,
           bseg~sgtxt,
           bseg~augdt,
           bseg~augcp,
           bseg~augbl,
           bkpf~xreversed,
           bkpf~stblg,
           bkpf~stjah,
           bkpf~usnam,
           bkpf~ppnam     INTO TABLE @DATA(lt_dpmt)
      FROM            bkpf AS bkpf
           INNER JOIN bseg AS bseg ON bseg~bukrs = bkpf~bukrs AND
                                      bseg~belnr = bkpf~belnr AND
                                      bseg~gjahr = bkpf~gjahr
      FOR ALL ENTRIES IN @me-&gt;at_keys
     WHERE bkpf~bukrs EQ @me-&gt;at_keys-bukrs_dp
       AND bkpf~belnr EQ @me-&gt;at_keys-belnr_dp
       AND bkpf~gjahr EQ @me-&gt;at_keys-gjahr_dp
       AND bseg~buzei EQ @me-&gt;at_keys-buzei_dp.


    &quot;§1.2a data of Invoice-mminv
    SELECT rbkp~belnr,
           rbkp~gjahr,
           wfst~wfsta,
           rbkp~bldat,
           rbkp~budat,
           rbkp~cpudt,
           rbkp~lifnr,
           rbkp~xblnr,
           rbkp~rmwwr,
           rbkp~waers,
           rbkp~wmwst1,
           rbkp~zterm,
           rbkp~bktxt,
           rbkp~rbstat,
           rbkp~erfnam,
           rbkp~bukrs,
           rseg~ebeln,
           rseg~werks   INTO TABLE @DATA(lt_invo)
      FROM                 rbkp             AS rbkp
           INNER JOIN      rseg             AS rseg ON rseg~belnr = rbkp~belnr AND
                                                       rseg~gjahr = rbkp~gjahr AND
                                                       rseg~buzei = &apos;001&apos;
           LEFT OUTER JOIN zvnmm_miro_wfsta AS wfst ON wfst~belnr = rbkp~belnr AND
                                                       wfst~gjahr = rbkp~gjahr
      FOR ALL ENTRIES  IN @me-&gt;at_keys
      WHERE rbkp~belnr EQ @me-&gt;at_keys-belnr_iv
        AND rbkp~gjahr EQ @me-&gt;at_keys-gjahr_iv.

    &quot;§1.2b data of Invoice-fiinv
    &quot;prepare fi ref-key
    LOOP AT me-&gt;at_keys ASSIGNING FIELD-SYMBOL(&lt;key&gt;).
      CONCATENATE &lt;key&gt;-belnr_iv
                  &lt;key&gt;-gjahr_iv INTO &lt;key&gt;-awkey_fi.
    ENDLOOP.
    &quot;select fi-doc for mm-invo
    SELECT bkpf~awkey AS bkpf_awkey,
           bkpf~bukrs AS bkpf_bukrs,
           bkpf~belnr AS bkpf_belnr,
           bkpf~gjahr AS bkpf_gjahr,
           bseg~buzei AS bseg_k_buzei,
           bseg~augdt AS bseg_k_augdt,
           bseg~augcp AS bseg_k_augcp,
           bseg~augbl AS bseg_k_augbl   INTO TABLE @DATA(lt_inv2)
      FROM            bkpf AS bkpf
           INNER JOIN bseg AS bseg ON bseg~bukrs = bkpf~bukrs AND
                                      bseg~belnr = bkpf~belnr AND
                                      bseg~gjahr = bkpf~gjahr
      FOR ALL ENTRIES IN @me-&gt;at_keys
      WHERE bkpf~awkey EQ @me-&gt;at_keys-awkey_fi
        AND bseg~buzei EQ &apos;001&apos;.
    &quot;(hard-code buzei is weird, but better than KOART..
    &quot;..with VN weird VAT codes there is sometimes second vendor item)


    &quot;§2 map all together

    &quot;§2.0 map: keys
    LOOP AT me-&gt;at_keys ASSIGNING &lt;key&gt;.
      APPEND INITIAL LINE TO me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).
      MOVE: &lt;key&gt;-bukrs_dp TO &lt;sdat&gt;-_dpmt-bukrs,
            &lt;key&gt;-belnr_dp TO &lt;sdat&gt;-_dpmt-belnr,
            &lt;key&gt;-gjahr_dp TO &lt;sdat&gt;-_dpmt-gjahr,
            &lt;key&gt;-buzei_dp TO &lt;sdat&gt;-_dpmt-buzei,
            &lt;key&gt;-belnr_iv TO &lt;sdat&gt;-_invo-belnr,
            &lt;key&gt;-gjahr_iv TO &lt;sdat&gt;-_invo-gjahr.
      CONCATENATE &lt;key&gt;-belnr_iv
                  &lt;key&gt;-gjahr_iv INTO &lt;sdat&gt;-_invo-bkpf_awkey.
    ENDLOOP.


    &quot;§2.1 map: downpayment
    LOOP AT me-&gt;at_sdat ASSIGNING &lt;sdat&gt;.
      READ TABLE lt_dpmt ASSIGNING FIELD-SYMBOL(&lt;dpmt&gt;) WITH KEY bukrs = &lt;sdat&gt;-_dpmt-bukrs
                                                                 gjahr = &lt;sdat&gt;-_dpmt-gjahr
                                                                 belnr = &lt;sdat&gt;-_dpmt-belnr. CHECK sy-subrc = 0.
      MOVE-CORRESPONDING: &lt;dpmt&gt; TO &lt;sdat&gt;-_dpmt.
    ENDLOOP.

    &quot;§2.2a map: invoice-mminv
    LOOP AT me-&gt;at_sdat ASSIGNING &lt;sdat&gt;.
      READ TABLE lt_invo ASSIGNING FIELD-SYMBOL(&lt;invo&gt;) WITH KEY gjahr = &lt;sdat&gt;-_invo-gjahr
                                                                 belnr = &lt;sdat&gt;-_invo-belnr. CHECK sy-subrc = 0.
      MOVE-CORRESPONDING: &lt;invo&gt; TO &lt;sdat&gt;-_invo.
    ENDLOOP.

    &quot;§2.2b map: invoice-fiinvo
    LOOP AT me-&gt;at_sdat ASSIGNING &lt;sdat&gt;.
      READ TABLE lt_inv2 ASSIGNING FIELD-SYMBOL(&lt;inv2&gt;) WITH KEY bkpf_awkey = &lt;sdat&gt;-_invo-bkpf_awkey. CHECK sy-subrc = 0.
      MOVE-CORRESPONDING: &lt;inv2&gt; TO &lt;sdat&gt;-_invo.
    ENDLOOP.


    &quot;default sort
    SORT me-&gt;at_sdat BY _dpmt-bukrs DESCENDING
                        _dpmt-gjahr DESCENDING
                        _dpmt-belnr DESCENDING.



  ENDMETHOD.</source>
