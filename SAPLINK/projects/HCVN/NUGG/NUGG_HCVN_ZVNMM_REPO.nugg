  <method CLSNAME="ZCL_VNMM_VEND_REXV" CMPNAME="CALL_BUPA" VERSION="1" LANGU="E" DESCRIPT="call: BuPa display" EXPOSURE="0" STATE="1" EDITORDER="22 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNMM_VEND_REXV" CMPNAME="CALL_BUPA" SCONAME="IR_SDAT" VERSION="1" LANGU="E" DESCRIPT="enhance table IFLC for SALV (tty)" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="TS_SDAT"/>
   <source>METHOD call_bupa.

    &quot;call BuPa display
    CHECK: ir_sdat IS BOUND.

    SET PARAMETER ID &apos;BPA&apos; FIELD ir_sdat-&gt;*-lifnr.

    CALL TRANSACTION &apos;BP&apos;.






  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_VEND_REXV" CMPNAME="CALL_ME2L" VERSION="1" LANGU="E" DESCRIPT="call: ME2L repor" EXPOSURE="0" STATE="1" EDITORDER="23 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNMM_VEND_REXV" CMPNAME="CALL_ME2L" SCONAME="IR_SDAT" VERSION="1" LANGU="E" DESCRIPT="CSI: DCS Commission-Dealer report (salv)" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="TS_SDAT"/>
   <source>METHOD call_me2l.

    &quot;call ME2N report
    CHECK: ir_sdat IS BOUND.

    SUBMIT rm06el00
      WITH el_lifnr EQ ir_sdat-&gt;lifnr
      WITH listu    EQ &apos;ALV&apos;
      WITH s_eindt BETWEEN ir_sdat-&gt;datfr AND ir_sdat-&gt;datto
    AND RETURN.


  ENDMETHOD.</source>
  </method>
