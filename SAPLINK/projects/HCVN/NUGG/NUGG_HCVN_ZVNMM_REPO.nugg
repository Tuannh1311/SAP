  <method CLSNAME="ZCL_VNMM_VEND_REXV" CMPNAME="SALV_REFR" VERSION="1" LANGU="E" DESCRIPT="salv-object: refresh" EXPOSURE="0" STATE="1" EDITORDER="19 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SALV_REFR.

    &quot;refresh SALV

    &quot;..re-read data
    CLEAR: me-&gt;at_sdat.

    me-&gt;sdat_selc( ). &quot;data select
    me-&gt;sdat_auth( ). &quot;data authorisation check
    me-&gt;sdat_enha( ). &quot;data enhance

    me-&gt;salv_init_ccel( ). &quot;re-colour cells (per row-line)

    &quot;..refresh salv
    me-&gt;ar_salv-&gt;refresh( ).







  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_VEND_REXV" CMPNAME="SDAT_AUTH" VERSION="1" LANGU="E" DESCRIPT="***salv-data: check authorisation" EXPOSURE="0" STATE="1" EDITORDER="3 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_auth.

