  <method CLSNAME="ZCL_VNMM_DPMT_RINV" CMPNAME="CALL_CDLOG" VERSION="1" LANGU="E" DESCRIPT="call: WFSTA change log" EXPOSURE="0" STATE="1" EDITORDER="25 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD call_cdlog.

    DATA(ls_cell) = me-&gt;ar_salv-&gt;get_selections( )-&gt;get_current_cell( ).

    CHECK ls_cell-row IS NOT INITIAL.

    READ TABLE me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;) INDEX ls_cell-row. CHECK sy-subrc = 0.

    &quot;call WFSTA change log
    CHECK &lt;sdat&gt; IS ASSIGNED.

    DATA: lv_objid TYPE cdhdr-objectid.
    CONCATENATE &lt;sdat&gt;-_invo-belnr
                &lt;sdat&gt;-_invo-gjahr INTO lv_objid RESPECTING BLANKS.


    SUBMIT rsscd100 WITH objekt   = &apos;ZVNMMMIRO_WFSTA&apos;
                    WITH objektid = lv_objid      AND RETURN.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_DPMT_RINV" CMPNAME="CALL_FB03_DP" VERSION="1" LANGU="E" DESCRIPT="call: FB03 FIdoc display: Down Payment" EXPOSURE="0" STATE="1" EDITORDER="22 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNMM_DPMT_RINV" CMPNAME="CALL_FB03_DP" SCONAME="IR_SDAT" VERSION="1" LANGU="E" DESCRIPT="enhance table IFLC for SALV (tty)" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="TS_SDAT"/>
   <source>METHOD CALL_FB03_DP.

    &quot;call: FB03 FIdoc display
    CHECK: ir_sdat IS BOUND.

    CHECK: ir_sdat-&gt;*-_dpmt-belnr IS NOT INITIAL,
           ir_sdat-&gt;*-_dpmt-bukrs IS NOT INITIAL,
           ir_sdat-&gt;*-_dpmt-gjahr IS NOT INITIAL.

    SET PARAMETER ID &apos;BLN&apos; FIELD ir_sdat-&gt;*-_dpmt-belnr.
    SET PARAMETER ID &apos;BUK&apos; FIELD ir_sdat-&gt;*-_dpmt-bukrs.
    SET PARAMETER ID &apos;GJR&apos; FIELD ir_sdat-&gt;*-_dpmt-gjahr.
    CALL TRANSACTION &apos;FB03&apos; WITH AUTHORITY-CHECK AND SKIP FIRST SCREEN.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_DPMT_RINV" CMPNAME="CALL_FB03_IV" VERSION="1" LANGU="E" DESCRIPT="call: FB03 FIdoc display: Invoice" EXPOSURE="0" STATE="1" EDITORDER="23 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNMM_DPMT_RINV" CMPNAME="CALL_FB03_IV" SCONAME="IR_SDAT" VERSION="1" LANGU="E" DESCRIPT="enhance table IFLC for SALV (tty)" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="TS_SDAT"/>
   <source>METHOD call_fb03_iv.

    &quot;call: FB03 FIdoc display
    CHECK: ir_sdat IS BOUND.

    CHECK: ir_sdat-&gt;*-_invo-bkpf_bukrs IS NOT INITIAL,
           ir_sdat-&gt;*-_invo-bkpf_belnr IS NOT INITIAL,
           ir_sdat-&gt;*-_invo-bkpf_gjahr IS NOT INITIAL.

    SET PARAMETER ID &apos;BLN&apos; FIELD ir_sdat-&gt;*-_invo-bkpf_belnr.
    SET PARAMETER ID &apos;BUK&apos; FIELD ir_sdat-&gt;*-_invo-bkpf_bukrs.
    SET PARAMETER ID &apos;GJR&apos; FIELD ir_sdat-&gt;*-_invo-bkpf_gjahr.
    CALL TRANSACTION &apos;FB03&apos; WITH AUTHORITY-CHECK AND SKIP FIRST SCREEN.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_DPMT_RINV" CMPNAME="CALL_MIR4" VERSION="1" LANGU="E" DESCRIPT="call: MIR4 Invoice display" EXPOSURE="0" STATE="1" EDITORDER="21 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNMM_DPMT_RINV" CMPNAME="CALL_MIR4" SCONAME="IR_SDAT" VERSION="1" LANGU="E" DESCRIPT="enhance table IFLC for SALV (tty)" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="TS_SDAT"/>
   <source>METHOD CALL_MIR4.

    &quot;call MIR4 invoice display
    CHECK: ir_sdat IS BOUND.

    SET PARAMETER ID &apos;RBN&apos; FIELD ir_sdat-&gt;*-_invo-belnr.
    SET PARAMETER ID &apos;GJR&apos; FIELD ir_sdat-&gt;*-_invo-gjahr.
    CALL TRANSACTION &apos;MIR4&apos; WITH AUTHORITY-CHECK AND SKIP FIRST SCREEN.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_DPMT_RINV" CMPNAME="CALL_WFOVERVIEW" VERSION="1" LANGU="E" DESCRIPT="call: GOS workflow overview" EXPOSURE="0" STATE="1" EDITORDER="24 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD call_wfoverview.

    DATA(ls_cell) = me-&gt;ar_salv-&gt;get_selections( )-&gt;get_current_cell( ).

    CHECK ls_cell-row IS NOT INITIAL.

    READ TABLE me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;) INDEX ls_cell-row. CHECK sy-subrc = 0.

    &quot;call GOS workflow-overview
    DATA: ls_borid TYPE borident.
    CONCATENATE &lt;sdat&gt;-_invo-belnr
                &lt;sdat&gt;-_invo-gjahr INTO ls_borid-objkey.
    MOVE: &apos;BUS2081&apos; TO ls_borid-objtype.
    CALL FUNCTION &apos;GOS_EXECUTE_SERVICE&apos;
      EXPORTING
        ip_service       = &apos;WF_OVERVIEW&apos;
        is_object        = ls_borid
        ip_no_commit     = &apos;&apos;
        ip_popup         = &apos;X&apos;
      EXCEPTIONS
        execution_failed = 1
        OTHERS           = 2.
    IF sy-subrc &lt;&gt; 0.
    ENDIF.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_DPMT_RINV" CMPNAME="CONSTRUCTOR" VERSION="1" LANGU="E" DESCRIPT="constuctor" EXPOSURE="2" STATE="1" EDITORDER="1 " DISPID="0 " MTDTYPE="2" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNMM_DPMT_RINV" CMPNAME="CONSTRUCTOR" SCONAME="IT_KEYS" VERSION="1" LANGU="E" DESCRIPT="rinv keys (~bkpf+rbkp)" CMPTYPE="1" MTDTYPE="2" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="TT_KEYS"/>
   <source>METHOD constructor.
*&amp; &quot;CBL-14020(HQ_TTa) MM Report Down Payment &amp; Invoice
*&amp;   context:
*&amp;            DownPayments are posted with reference to POrd
*&amp;            Invoices     are posted with reference to POrd
*&amp;   feature:
*&amp;            report matching DownPayments and Invoice per POrd

    me-&gt;at_keys = it_keys.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_DPMT_RINV" CMPNAME="ON_DBCL" VERSION="1" LANGU="E" DESCRIPT="handler: on double-click" EXPOSURE="0" STATE="1" EDITORDER="19 " DISPID="0 " MTDTYPE="1" MTDDECLTYP="0" REFCLSNAME="CL_SALV_EVENTS_TABLE" REFCMPNAME="DOUBLE_CLICK" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNMM_DPMT_RINV" CMPNAME="ON_DBCL" SCONAME="ROW" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="1" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="0"/>
   <parameter CLSNAME="ZCL_VNMM_DPMT_RINV" CMPNAME="ON_DBCL" SCONAME="COLUMN" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="1" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="0"/>
   <source>METHOD on_dbcl.

    &quot;get double-clicked row
    READ TABLE me-&gt;at_sdat REFERENCE INTO DATA(lr_sdat) INDEX row.

    &quot;by column
    IF column CS &apos;_DP&apos;.
      me-&gt;call_fb03_dp( lr_sdat ).    &quot;-&gt;FIdoc: Down Payment
    ELSEIF column CS &apos;_IV&apos;.
      IF  column CS &apos;BKPF_&apos; OR
          column CS &apos;BKPF_&apos;   .
        me-&gt;call_fb03_iv( lr_sdat ).  &quot;-&gt;FIdoc: Invoice
      ELSE.
        me-&gt;call_mir4( lr_sdat ).     &quot;-&gt;MIR4 : Invoice
      ENDIF.
    ENDIF.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_DPMT_RINV" CMPNAME="ON_UCOM" VERSION="1" LANGU="E" DESCRIPT="handler: on user-command" EXPOSURE="0" STATE="1" EDITORDER="20 " DISPID="0 " MTDTYPE="1" MTDDECLTYP="0" REFCLSNAME="CL_SALV_EVENTS" REFCMPNAME="ADDED_FUNCTION" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNMM_DPMT_RINV" CMPNAME="ON_UCOM" SCONAME="E_SALV_FUNCTION" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="1" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="0"/>
   <source>METHOD ON_UCOM.

    &quot;default: get selected rows
    me-&gt;at_rsel = me-&gt;ar_salv-&gt;get_selections( )-&gt;get_selected_rows( ).


    CASE e_salv_function.

      WHEN &apos;REFRESH&apos;.     me-&gt;salv_refr( ).

      WHEN &apos;WF_OVERVIEW&apos;. me-&gt;call_wfoverview( ).

      WHEN &apos;WFSTA_CD&apos;.    me-&gt;call_cdlog( ).

    ENDCASE.

  ENDMETHOD.</source>
  </method>
