  <method CLSNAME="ZCL_VNMM_MIRO_RINV" CMPNAME="_CONV_USERNAME" VERSION="1" LANGU="E" DESCRIPT="conv: user to name" EXPOSURE="0" STATE="1" EDITORDER="26 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNMM_MIRO_RINV" CMPNAME="_CONV_USERNAME" SCONAME="IV_USNAM" VERSION="1" LANGU="E" DESCRIPT="User Name" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="UNAME"/>
   <parameter CLSNAME="ZCL_VNMM_MIRO_RINV" CMPNAME="_CONV_USERNAME" SCONAME="RESULT" VERSION="1" LANGU="E" DESCRIPT="Field name" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="AD_NAMTEXT"/>
   <source>METHOD _conv_username.
    &quot;conv: user to name


    DATA: ls_addr TYPE bapiaddr3,
          lt_bret TYPE TABLE OF bapiret2.

    CALL FUNCTION &apos;BAPI_USER_GET_DETAIL&apos;
      EXPORTING
        username      = iv_usnam
        cache_results = &apos;X&apos;
      IMPORTING
        address       = ls_addr
      TABLES
        return        = lt_bret.

    IF ls_addr IS NOT INITIAL.
      MOVE: ls_addr-fullname TO result.
    ENDIF.

  ENDMETHOD.</source>
