  <method CLSNAME="ZCL_VNMM_DPMT_RINV" CMPNAME="SALV_INIT" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise" EXPOSURE="0" STATE="1" EDITORDER="8 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SALV_INIT.

    &quot; create SAVL table object
    TRY.
        cl_salv_table=&gt;factory( IMPORTING r_salv_table = me-&gt;ar_salv
                                CHANGING  t_table      = me-&gt;at_sdat ).
      CATCH cx_salv_msg.
    ENDTRY.


    me-&gt;salv_init_mlts( ).  &quot;set multi-sel
    me-&gt;salv_init_cols( ).  &quot;set columns
    me-&gt;salv_init_func( ).  &quot;set all grid-functions
    me-&gt;salv_init_pfst( ).  &quot;set own gui-status
    me-&gt;salv_init_ccol( ).  &quot;set colours per column
    me-&gt;salv_init_ccel( ).  &quot;set colours per cell(row)
    me-&gt;salv_init_invs( ).  &quot;set column invisible
    me-&gt;salv_init_hndl( ).  &quot;set event handlers





  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_DPMT_RINV" CMPNAME="SALV_INIT_CCEL" VERSION="1" LANGU="E" DESCRIPT="***salv-object: initialise: colours per cell (row)" EXPOSURE="0" STATE="1" EDITORDER="14 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SALV_INIT_CCEL.

***    &quot;set colours per cell/row
***
***    &quot;get field list (from columns)
***    DATA(lo_cols) =  me-&gt;ar_salv-&gt;get_columns( ).
***    DATA(lt_cols) = lo_cols-&gt;get( ).
***
***    &quot;set column SCOL
***    TRY. lo_cols-&gt;set_color_column( value = &apos;TSCOL&apos; ). CATCH cx_salv_data_error. ENDTRY.
***
***    &quot;for each line..
***    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).
***
***      &quot;..prepare dynamic colour per XXXX
***      DATA: ls_scol TYPE lvc_s_colo.
***      CASE &lt;sdat&gt;-XXXX.
***        WHEN gc_statu_e. ls_scol-col = 7. &quot;orange
***      ENDCASE.
***
***      &quot;..for each column
***      LOOP AT lt_cols ASSIGNING FIELD-SYMBOL(&lt;cols&gt;).
***        APPEND INITIAL LINE TO &lt;sdat&gt;-tscol ASSIGNING FIELD-SYMBOL(&lt;tscol&gt;).
***
***        CASE &lt;cols&gt;-columnname.
***          WHEN &apos;XXXXX&apos;.
***            &quot;key fields: highlighted
***            MOVE: &lt;cols&gt;-columnname TO &lt;tscol&gt;-fname,
***                  ls_scol           TO &lt;tscol&gt;-color,
***                  1                 TO &lt;tscol&gt;-color-int.
***
***          WHEN OTHERS.
***            &quot;data fields: normal
***            MOVE: &lt;cols&gt;-columnname TO &lt;tscol&gt;-fname,
***                  ls_scol           TO &lt;tscol&gt;-color,
***                  0                 TO &lt;tscol&gt;-color-int.
***
***        ENDCASE.
***
***      ENDLOOP.
***
***
***    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_DPMT_RINV" CMPNAME="SALV_INIT_CCOL" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: colours per columns" EXPOSURE="0" STATE="1" EDITORDER="13 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_ccol.

    &quot;init: column colours

    DATA: lr_col   TYPE REF TO cl_salv_column_list, &quot; object of column, allowing set_color
          ls_color TYPE        lvc_s_colo.          &quot; colours structure

    DATA(lr_cols) = me-&gt;ar_salv-&gt;get_columns( ).
    DATA(lt_cols) = lr_cols-&gt;get( ).

    LOOP AT lt_cols ASSIGNING FIELD-SYMBOL(&lt;col&gt;).
      TRY.
          lr_col ?= lr_cols-&gt;get_column( &lt;col&gt;-columnname ).
        CATCH cx_salv_not_found.
      ENDTRY.

      DATA(ls_c101) = VALUE lvc_s_colo( col = 1 inv = 0 int = 1 ).  &quot;blue+h
      DATA(ls_c100) = VALUE lvc_s_colo( col = 1 inv = 0 int = 0 ).  &quot;blue
      DATA(ls_c300) = VALUE lvc_s_colo( col = 3 inv = 0 int = 0 ).  &quot;yellow
      DATA(ls_c700) = VALUE lvc_s_colo( col = 7 inv = 0 int = 0 ).  &quot;orange
      DATA(ls_c500) = VALUE lvc_s_colo( col = 5 inv = 0 int = 0 ).  &quot;green
      DATA(ls_c501) = VALUE lvc_s_colo( col = 5 inv = 0 int = 1 ).  &quot;green+h

      &quot;setting colours
      CASE &lt;col&gt;-columnname.
        WHEN &apos;BUKRS_DP&apos;
          OR &apos;BELNR_DP&apos;
          OR &apos;GJAHR_DP&apos;
          OR &apos;BUZEI_DP&apos;      . lr_col-&gt;set_color( ls_c101 ). &quot;keys dpmt: blue+h
        WHEN &apos;BELNR_IV&apos;
          OR &apos;GJAHR_IV&apos;      . lr_col-&gt;set_color( ls_c501 ). &quot;keys invo: green+h
        WHEN &apos;LIFNR_TX1_DP&apos;
          OR &apos;LIFNR_TX2_DP&apos;
          OR &apos;USNAM_TX_DP&apos;
          OR &apos;PPNAM_TX_DP&apos;   . lr_col-&gt;set_color( ls_c300 ). &quot;desc dpmt: yellow
        WHEN &apos;ZTERM_TX_IV&apos;
          OR &apos;ZTERM_TX_IV&apos;
          OR &apos;ERFNAM_TX_IV&apos;  . lr_col-&gt;set_color( ls_c300 ). &quot;desc invo: yellow
        WHEN &apos;WFSTA_IV&apos;
          OR &apos;WFSTA_TX_IV&apos;   . lr_col-&gt;set_color( ls_c700 ). &quot;stat invo: orange
        WHEN  OTHERS     .
          IF     &lt;col&gt;-columnname CS &apos;_DP&apos;.
            .                  lr_col-&gt;set_color( ls_c100 ). &quot;dpmt : blue
          ELSEIF &lt;col&gt;-columnname CS &apos;_IV&apos;.
            .                  lr_col-&gt;set_color( ls_c500 ). &quot;data : green
          ENDIF.
      ENDCASE.

    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_DPMT_RINV" CMPNAME="SALV_INIT_COLS" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: columns types (exc, icon, chkbox)" EXPOSURE="0" STATE="1" EDITORDER="10 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_cols.

    &quot;init columns type: exception, icon, checkbox, etc

    &quot;init cols: F1 rollname
    DATA(lr_cols) = me-&gt;ar_salv-&gt;get_columns( ).
    DATA(lt_cols) = lr_cols-&gt;get( ).
    LOOP AT lt_cols ASSIGNING FIELD-SYMBOL(&lt;cols&gt;).
      &quot;update DDIC ref together with rollname
      &quot;(this will get normal F1 help pointing to our structure)
      DATA(ddic) = &lt;cols&gt;-r_column-&gt;get_ddic_reference( ).
      ddic-table = &apos;ZVNMM_DPMT_RINV_SDAT&apos;.
      &lt;cols&gt;-r_column-&gt;set_ddic_reference( value = ddic ).
      &lt;cols&gt;-r_column-&gt;set_f1_rollname( value = &apos;&apos; ).
    ENDLOOP.

    TRY.
        &quot;init cols: checkbox

***        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;XFELD&apos;) )-&gt;set_cell_type( if_salv_c_cell_type=&gt;checkbox ).

        &quot;init cols: centered
***        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;STEXV&apos;) )-&gt;set_alignment( if_salv_c_alignment=&gt;centered ).


        &quot;init cols: currency
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;WRBTR_DP&apos;)  )-&gt;set_currency_column( &apos;WAERS&apos; ).
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;RMWWR_IV&apos;)  )-&gt;set_currency_column( &apos;WAERS&apos; ).
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;WMWST1_IV&apos;) )-&gt;set_currency_column( &apos;WAERS&apos; ).

        &quot;init cols: decimals
**        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;CPAMT&apos;) )-&gt;set_decimals( value = &apos;0&apos; ).


      CATCH cx_salv_data_error.
      CATCH cx_salv_not_found.
    ENDTRY.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_DPMT_RINV" CMPNAME="SALV_INIT_FUNC" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: functions" EXPOSURE="0" STATE="1" EDITORDER="11 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SALV_INIT_FUNC.

    &quot;salv all functions
    me-&gt;ar_salv-&gt;get_functions( )-&gt;set_all( abap_true ).

    &quot; layout save &amp; default (&apos;all functions&apos; above are not really all)
    DATA: lr_layout TYPE REF TO cl_salv_layout,
          ls_key    TYPE salv_s_layout_key.

    ls_key-report = me-&gt;lc_repid.
    lr_layout     = me-&gt;ar_salv-&gt;get_layout( ).

    lr_layout-&gt;set_key( ls_key ).         &quot;
    lr_layout-&gt;set_save_restriction( cl_salv_layout=&gt;restrict_none ). &quot;User can save
    lr_layout-&gt;set_default( abap_true ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_DPMT_RINV" CMPNAME="SALV_INIT_HNDL" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: event handlers" EXPOSURE="0" STATE="1" EDITORDER="16 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SALV_INIT_HNDL.

    &quot;set handler
    SET HANDLER me-&gt;on_dbcl FOR me-&gt;ar_salv-&gt;get_event( ).
    SET HANDLER me-&gt;on_ucom FOR me-&gt;ar_salv-&gt;get_event( ).

  ENDMETHOD.</source>
  </method>
