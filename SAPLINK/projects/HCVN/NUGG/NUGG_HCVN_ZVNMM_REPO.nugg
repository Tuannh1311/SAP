  <method CLSNAME="ZCL_VNMM_MIRO_RINV" CMPNAME="SALV_INIT_CCOL" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: colours per columns" EXPOSURE="0" STATE="1" EDITORDER="14 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_ccol.

    &quot;init: column colours

    DATA: lr_col   TYPE REF TO cl_salv_column_list, &quot; object of column, allowing set_color
          ls_color TYPE        lvc_s_colo.          &quot; colours structure

    DATA(lr_cols) = me-&gt;ar_salv-&gt;get_columns( ).
    DATA(lt_cols) = lr_cols-&gt;get( ).

    LOOP AT lt_cols ASSIGNING FIELD-SYMBOL(&lt;col&gt;).
      TRY.
          lr_col ?= lr_cols-&gt;get_column( &lt;col&gt;-columnname ).
        CATCH cx_salv_not_found.
      ENDTRY.

      DATA(ls_c101) = VALUE lvc_s_colo( col = 1 inv = 0 int = 1 ).  &quot;blue+h
      DATA(ls_c100) = VALUE lvc_s_colo( col = 1 inv = 0 int = 0 ).  &quot;blue
      DATA(ls_c300) = VALUE lvc_s_colo( col = 3 inv = 0 int = 0 ).  &quot;yellow
      DATA(ls_c700) = VALUE lvc_s_colo( col = 7 inv = 0 int = 0 ).  &quot;orange
      DATA(ls_c500) = VALUE lvc_s_colo( col = 5 inv = 0 int = 0 ).  &quot;green

      &quot;setting colours
      CASE &lt;col&gt;-columnname.
        WHEN &apos;BELNR&apos;
          OR &apos;GJAHR&apos;     . lr_col-&gt;set_color( ls_c101 ). &quot;keys : blue+h (comid is kinda-key)
        WHEN &apos;LIFNR_TX&apos;
          OR &apos;ZTERM_TX&apos;
          OR &apos;ERFNAM_TX&apos; . lr_col-&gt;set_color( ls_c300 ). &quot;descr: yellow
        WHEN &apos;WFSTA&apos;
          OR &apos;WFSTA_TX&apos;  . lr_col-&gt;set_color( ls_c700 ). &quot;statu: orange
        WHEN  OTHERS     .
          IF &lt;col&gt;-columnname(5) = &apos;BKPF_&apos; OR
             &lt;col&gt;-columnname(5) = &apos;BSEG_&apos;   .
            lr_col-&gt;set_color( ls_c500 ). &quot;fico : green
          ELSEIF &lt;col&gt;-columnname(3) = &apos;SH_&apos;.
            lr_col-&gt;set_color( ls_c700 ). &quot;status history: orange
          ELSE.
            lr_col-&gt;set_color( ls_c100 ). &quot;data : blue
          ENDIF.
      ENDCASE.

    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MIRO_RINV" CMPNAME="SALV_INIT_COLS" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: columns types (exc, icon, chkbox)" EXPOSURE="0" STATE="1" EDITORDER="11 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_cols.

    &quot;init columns type: exception, icon, checkbox, etc

        &quot;init cols: F1 rollname
        DATA(lr_cols) = me-&gt;ar_salv-&gt;get_columns( ).
        DATA(lt_cols) = lr_cols-&gt;get( ).
        LOOP AT lt_cols ASSIGNING FIELD-SYMBOL(&lt;cols&gt;).
          &quot;update DDIC ref together with rollname
          &quot;(this will get normal F1 help pointing to our structure)
          DATA(ddic) = &lt;cols&gt;-r_column-&gt;get_ddic_reference( ).
          ddic-table = &apos;ZVNMM_MIRO_RINV_SDAT&apos;.
          &lt;cols&gt;-r_column-&gt;set_ddic_reference( value = ddic ).
          &lt;cols&gt;-r_column-&gt;set_f1_rollname( value = &apos;&apos; ).
        ENDLOOP.

    TRY.
        &quot;init cols: checkbox

***        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;XFELD&apos;) )-&gt;set_cell_type( if_salv_c_cell_type=&gt;checkbox ).

        &quot;init cols: centered
***        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;STEXV&apos;) )-&gt;set_alignment( if_salv_c_alignment=&gt;centered ).


        &quot;init cols: currency
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;RMWWR&apos;) )-&gt;set_currency_column( &apos;WAERS&apos; ).
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;WMWST1&apos;) )-&gt;set_currency_column( &apos;WAERS&apos; ).

        &quot;init cols: decimals
**        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;CPAMT&apos;) )-&gt;set_decimals( value = &apos;0&apos; ).


      CATCH cx_salv_data_error.
      CATCH cx_salv_not_found.
    ENDTRY.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MIRO_RINV" CMPNAME="SALV_INIT_FUNC" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: functions" EXPOSURE="0" STATE="1" EDITORDER="12 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SALV_INIT_FUNC.

    &quot;salv all functions
    me-&gt;ar_salv-&gt;get_functions( )-&gt;set_all( abap_true ).

    &quot; layout save &amp; default (&apos;all functions&apos; above are not really all)
    DATA: lr_layout TYPE REF TO cl_salv_layout,
          ls_key    TYPE salv_s_layout_key.

    ls_key-report = me-&gt;lc_repid.
    lr_layout     = me-&gt;ar_salv-&gt;get_layout( ).

    lr_layout-&gt;set_key( ls_key ).         &quot;
    lr_layout-&gt;set_save_restriction( cl_salv_layout=&gt;restrict_none ). &quot;User can save
    lr_layout-&gt;set_default( abap_true ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MIRO_RINV" CMPNAME="SALV_INIT_HNDL" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: event handlers" EXPOSURE="0" STATE="1" EDITORDER="17 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SALV_INIT_HNDL.

    &quot;set handler
    SET HANDLER me-&gt;on_dbcl FOR me-&gt;ar_salv-&gt;get_event( ).
    SET HANDLER me-&gt;on_ucom FOR me-&gt;ar_salv-&gt;get_event( ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MIRO_RINV" CMPNAME="SALV_INIT_INVS" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: invisible fields" EXPOSURE="0" STATE="1" EDITORDER="16 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_invs.

    &quot;set invisible fields

    DATA: lr_col   TYPE REF TO cl_salv_column_list. &quot; object of column, allowing set_color

    DATA(lr_cols) = me-&gt;ar_salv-&gt;get_columns( ).
    DATA(lt_cols) = lr_cols-&gt;get( ).

    &quot; remove MANDT
    LOOP AT lt_cols ASSIGNING FIELD-SYMBOL(&lt;col&gt;) WHERE columnname = &apos;MANDT&apos;.
      TRY.
          lr_col ?= lr_cols-&gt;get_column( &lt;col&gt;-columnname ).
        CATCH cx_salv_not_found.
      ENDTRY.
      lr_col-&gt;set_technical( if_salv_c_bool_sap=&gt;true ).
    ENDLOOP.


    &quot; remove: Status History when not requested
    IF me-&gt;ax_hist = &apos; &apos;.
      LOOP AT lt_cols ASSIGNING            &lt;col&gt;  WHERE columnname CS &apos;SH_&apos;.
        TRY.
            lr_col ?= lr_cols-&gt;get_column( &lt;col&gt;-columnname ).
          CATCH cx_salv_not_found.
        ENDTRY.
        lr_col-&gt;set_technical( if_salv_c_bool_sap=&gt;true ).
      ENDLOOP.
    ENDIF.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MIRO_RINV" CMPNAME="SALV_INIT_MLTS" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: mutli-selection" EXPOSURE="0" STATE="1" EDITORDER="10 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SALV_INIT_MLTS.

    &quot;set multiple-selection
    DATA(lr_sel) = me-&gt;ar_salv-&gt;get_selections( ).
    lr_sel-&gt;set_selection_mode( if_salv_c_selection_mode=&gt;row_column ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MIRO_RINV" CMPNAME="SALV_INIT_PFST" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: gui status" EXPOSURE="0" STATE="1" EDITORDER="13 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SALV_INIT_PFST.

    &quot;init: gui status from calling program

    &quot;set own function by own screen status (seriously, I hate SALV)
    me-&gt;ar_salv-&gt;set_screen_status(
                                    pfstatus      = me-&gt;lc_pfkey
                                    report        = me-&gt;lc_repid
                                    set_functions = me-&gt;ar_salv-&gt;c_functions_all ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MIRO_RINV" CMPNAME="SALV_REFR" VERSION="1" LANGU="E" DESCRIPT="salv-object: refresh" EXPOSURE="0" STATE="1" EDITORDER="19 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SALV_REFR.

    &quot;refresh SALV

    &quot;..re-read data
    CLEAR: me-&gt;at_sdat.

    me-&gt;sdat_selc( ). &quot;data select
    me-&gt;sdat_auth( ). &quot;data authorisation check
    me-&gt;sdat_enha( ). &quot;data enhance

    me-&gt;salv_init_ccel( ). &quot;re-colour cells (per row-line)

    &quot;..refresh salv
    me-&gt;ar_salv-&gt;refresh( ).







  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MIRO_RINV" CMPNAME="SDAT_AUTH" VERSION="1" LANGU="E" DESCRIPT="salv-data: check authorisation" EXPOSURE="0" STATE="1" EDITORDER="3 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_auth.

    &quot;authorithy check
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      AUTHORITY-CHECK OBJECT &apos;M_RECH_BUK&apos;
       ID &apos;BUKRS&apos; FIELD &lt;sdat&gt;-bukrs
       ID &apos;ACTVT&apos; FIELD &apos;03&apos;.
      IF sy-subrc &lt;&gt; 0. CLEAR &lt;sdat&gt;.  ENDIF.

    ENDLOOP.

    DELETE me-&gt;at_sdat WHERE belnr IS INITIAL.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MIRO_RINV" CMPNAME="SDAT_ENHA" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance" EXPOSURE="0" STATE="1" EDITORDER="4 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha.

    &quot;enhance salv-data

    me-&gt;sdat_enha_wfsta_tx( ).
    me-&gt;sdat_enha_lifnr_tx( ).
    me-&gt;sdat_enha_zterm_tx( ).
    me-&gt;sdat_enha_erfna_tx( ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MIRO_RINV" CMPNAME="SDAT_ENHA_ERFNA_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: creators user name" EXPOSURE="0" STATE="1" EDITORDER="8 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_erfna_tx.

    &quot;enha Creator&apos;s user name

    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      DATA: ls_addr TYPE          bapiaddr3,
            lt_ret  TYPE TABLE OF bapiret2.
      CALL FUNCTION &apos;BAPI_USER_GET_DETAIL&apos;
        EXPORTING
          username = &lt;sdat&gt;-erfnam
        IMPORTING
          address  = ls_addr
        TABLES
          return   = lt_ret.
      IF sy-subrc = 0. MOVE: ls_addr-fullname TO &lt;sdat&gt;-erfnam_tx. ENDIF.

    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MIRO_RINV" CMPNAME="SDAT_ENHA_LIFNR_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: vendor name" EXPOSURE="0" STATE="1" EDITORDER="6 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SDAT_ENHA_LIFNR_TX.

    &quot;enha Vendor&apos;s descriptions
    IF me-&gt;at_sdat IS NOT INITIAL.
      SELECT lfa1~lifnr,
             lfa1~name1  INTO TABLE @DATA(lt_lfa1)
        FROM lfa1 AS lfa1
        FOR ALL ENTRIES IN @me-&gt;at_sdat
        WHERE lifnr = @me-&gt;at_sdat-lifnr.
    ENDIF.

    &quot;pass to items
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      READ TABLE lt_lfa1 ASSIGNING FIELD-SYMBOL(&lt;lfa1&gt;) WITH KEY lifnr = &lt;sdat&gt;-lifnr. CHECK sy-subrc = 0.
      MOVE: &lt;lfa1&gt;-name1 TO &lt;sdat&gt;-lifnr_tx.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MIRO_RINV" CMPNAME="SDAT_ENHA_WFSTA_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: wf-status description" EXPOSURE="0" STATE="1" EDITORDER="5 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_wfsta_tx.

    &quot;enha WFSTA status description
    DATA(lo_dref) = CAST cl_abap_elemdescr( cl_abap_elemdescr=&gt;describe_by_name( p_name = &apos;ZVNMM_MIRO_WFSTA_D&apos; ) ).
    DATA(lt_vals) = lo_dref-&gt;get_ddic_fixed_values( ).

    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).
      READ TABLE lt_vals ASSIGNING FIELD-SYMBOL(&lt;vals&gt;) WITH KEY low = &lt;sdat&gt;-wfsta.
      IF sy-subrc = 0. MOVE: &lt;vals&gt;-ddtext TO &lt;sdat&gt;-wfsta_tx.ENDIF.
    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MIRO_RINV" CMPNAME="SDAT_ENHA_ZTERM_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: payment term description" EXPOSURE="0" STATE="1" EDITORDER="7 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_zterm_tx.

    &quot;enha Payment Terms decription
    IF me-&gt;at_sdat IS NOT INITIAL.
      SELECT *  INTO TABLE @DATA(lt_t052u)
        FROM t052u AS t052u
        FOR ALL ENTRIES IN @me-&gt;at_sdat
        WHERE spras = @sy-langu
          AND zterm = @me-&gt;at_sdat-zterm.
    ENDIF.

    &quot;pass to items
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      READ TABLE lt_t052u ASSIGNING FIELD-SYMBOL(&lt;t052u&gt;) WITH KEY zterm = &lt;sdat&gt;-zterm. CHECK sy-subrc = 0.
      MOVE: &lt;t052u&gt;-text1 TO &lt;sdat&gt;-zterm_tx.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MIRO_RINV" CMPNAME="SDAT_SELC" VERSION="1" LANGU="E" DESCRIPT="salv-data: select data per keys" EXPOSURE="0" STATE="1" EDITORDER="1 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_selc.

    &quot;get data
    CHECK me-&gt;at_keys IS NOT INITIAL.
    SELECT rbkp~belnr,
           rbkp~gjahr,
           wfst~wfsta,
           rbkp~bldat,
           rbkp~budat,
           rbkp~cpudt,
           rbkp~lifnr,
           rbkp~xblnr,
           rbkp~rmwwr,
           rbkp~waers,
           rbkp~wmwst1,
           rbkp~zterm,
           rbkp~bktxt,
           rbkp~rbstat,
           rbkp~erfnam,
           rbkp~bukrs    INTO CORRESPONDING FIELDS OF TABLE @me-&gt;at_sdat
      FROM                 rbkp             AS rbkp
           LEFT OUTER JOIN zvnmm_miro_wfsta AS wfst ON wfst~belnr = rbkp~belnr AND
                                                       wfst~gjahr = rbkp~gjahr
      FOR ALL ENTRIES  IN @me-&gt;at_keys
      WHERE rbkp~belnr EQ @me-&gt;at_keys-belnr
        AND rbkp~gjahr EQ @me-&gt;at_keys-gjahr.

    &quot;default sort (I never know whether this is good idea)
    SORT me-&gt;at_sdat BY belnr DESCENDING
                        gjahr DESCENDING.

    &quot;prepare fi ref-keys
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).
      CONCATENATE &lt;sdat&gt;-belnr
                  &lt;sdat&gt;-gjahr INTO &lt;sdat&gt;-bkpf_awkey.
    ENDLOOP.

    &quot;get fi doc+vend-item
    IF me-&gt;at_sdat[] IS NOT INITIAL.
      SELECT bkpf~bukrs,
             bkpf~belnr,
             bkpf~gjahr,
             bkpf~awkey,
             bseg~buzei,
             bseg~augdt,
             bseg~augcp,
             bseg~augbl
        FROM            bkpf AS bkpf
             INNER JOIN bseg AS bseg ON bseg~bukrs = bkpf~bukrs AND
                                        bseg~belnr = bkpf~belnr AND
                                        bseg~gjahr = bkpf~gjahr
        INTO TABLE @DATA(lt_bsel)
        FOR ALL ENTRIES IN @me-&gt;at_sdat
        WHERE bkpf~awkey EQ @me-&gt;at_sdat-bkpf_awkey
          AND bseg~buzei EQ &apos;001&apos;.
      &quot;hard-code buzei looks weird, but better than KOART..
      &quot;..with VN weird VAT codes there is sometimes second vendor item

      &quot;map to sdat
      LOOP AT lt_bsel ASSIGNING FIELD-SYMBOL(&lt;bsel&gt;).
        READ TABLE me-&gt;at_sdat ASSIGNING &lt;sdat&gt; WITH KEY bkpf_awkey = &lt;bsel&gt;-awkey. CHECK sy-subrc = 0.
        MOVE: &lt;bsel&gt;-bukrs TO &lt;sdat&gt;-bkpf_bukrs,
              &lt;bsel&gt;-belnr TO &lt;sdat&gt;-bkpf_belnr,
              &lt;bsel&gt;-gjahr TO &lt;sdat&gt;-bkpf_gjahr,
              &lt;bsel&gt;-buzei TO &lt;sdat&gt;-bseg_k_buzei,
              &lt;bsel&gt;-augdt TO &lt;sdat&gt;-bseg_k_augdt,
              &lt;bsel&gt;-augcp TO &lt;sdat&gt;-bseg_k_augcp,
              &lt;bsel&gt;-augbl TO &lt;sdat&gt;-bseg_k_augbl.
      ENDLOOP.


    ENDIF.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_MIRO_RINV" CMPNAME="SDAT_SELC_SHIST" VERSION="1" LANGU="E" DESCRIPT="salv-data: select status history" EXPOSURE="0" STATE="1" EDITORDER="2 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_selc_shist.
    &quot;salv-data: select status history
    &quot;+CBL-14541(HQ_TTa) MM Invoice status with historical status

    CHECK me-&gt;ax_hist = &apos;X&apos;.

    &quot;§0.0 for change documents: prepare key
    CHECK me-&gt;at_sdat[] IS NOT INITIAL.
    DATA: lt_cdhd TYPE TABLE OF cdhdr.
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).
      APPEND INITIAL LINE TO lt_cdhd ASSIGNING FIELD-SYMBOL(&lt;cdhd&gt;).
      CONCATENATE &lt;sdat&gt;-belnr
                  &lt;sdat&gt;-gjahr INTO &lt;cdhd&gt;-objectid.
    ENDLOOP.

    &quot;§0   get data

    &quot;§0.1 get change documents: for status workflow
    CHECK lt_cdhd[] IS NOT INITIAL.
    SELECT * FROM zvnmm_vcdall INTO TABLE @DATA(lt_vcda) FOR ALL ENTRIES IN @lt_cdhd
     WHERE objectclas     = &apos;ZVNMMMIRO_WFSTA&apos;
       AND cdpos_objectid = @lt_cdhd-objectid
       AND tabname        = &apos;ZVNMM_MIRO_WFSTA&apos;
       AND ( ( fname = &apos;WFSTA&apos; AND change_ind   = &apos;U&apos; ) OR
             ( fname = &apos;KEY&apos;   AND change_ind   = &apos;I&apos; )   ).

    &quot;§0.2 get change document: for status invoice
    CHECK lt_cdhd[] IS NOT INITIAL.
    SELECT * FROM zvnmm_vcdall INTO TABLE @DATA(lt_vcd2) FOR ALL ENTRIES IN @lt_cdhd
     WHERE objectclas     = &apos;INCOMINGINVOICE&apos;
       AND cdpos_objectid = @lt_cdhd-objectid
       AND tabname        = &apos;RBKP&apos;
       AND ( ( fname = &apos;RBSTAT&apos; AND change_ind   = &apos;U&apos; ) OR
             ( fname = &apos;KEY&apos;    AND change_ind   = &apos;I&apos; )   ).



    &quot;§1. pass from cdata into invoice
    SORT lt_vcda BY udate DESCENDING
                    utime DESCENDING.
    LOOP AT me-&gt;at_sdat ASSIGNING &lt;sdat&gt;.
      DATA(lv_objid) = &lt;sdat&gt;-belnr &amp;&amp; &lt;sdat&gt;-gjahr.


      &quot;§0.3 get workflows (has to be inside invoice cycle, due SAP_WAPI* working with single object)
      DATA: ls_porb  TYPE sibflporb,
            lt_wlist TYPE STANDARD TABLE OF swr_wihdr,
            lt_mline TYPE STANDARD TABLE OF swr_messag,
            lt_mstr  TYPE STANDARD TABLE OF swr_mstruc.
      CLEAR: ls_porb,
             lt_wlist[], lt_mline[], lt_mstr[].

      ls_porb-instid = &lt;sdat&gt;-belnr &amp;&amp; &lt;sdat&gt;-gjahr.
      ls_porb-typeid = &apos;BUS2081&apos;. &quot;= Incoming Invoice
      ls_porb-catid  = &apos;BO&apos;.      &quot;= BOR object

      CALL FUNCTION &apos;SAP_WAPI_WORKITEMS_TO_OBJECT&apos;
        EXPORTING
          object_por               = ls_porb
          objtype                  = CONV swotobjid-objtype( ls_porb-typeid )
          objkey                   = ls_porb-instid
          top_level_items          = &apos; &apos;
          selection_status_variant = 0002   &quot;= Finished Instances (Completed, Canceled)
          text                     = &apos;X&apos;
          output_only_top_level    = &apos; &apos;
          language                 = sy-langu
          determine_task_filter    = &apos; &apos;
          removed_objects          = &apos; &apos;
        TABLES
          worklist                 = lt_wlist
          message_lines            = lt_mline
          message_struct           = lt_mstr.

      &quot;newest top
      SORT lt_wlist BY wi_cd DESCENDING
                       wi_ct DESCENDING.



      &quot;..per each code:

      &quot;0P1P parked
      &quot;(from status, via loop, to catch either init entry, or change)
      LOOP AT lt_vcda ASSIGNING FIELD-SYMBOL(&lt;0p1p&gt;) WHERE         objectid = lv_objid
                                                       AND (    ( value_new = &apos;0P1P&apos;                 )     &quot;change to 0P1P
                                                             OR ( value_new = &apos;&apos; AND change_ind = &apos;I&apos;) ).  &quot;init save into 0P1P
        MOVE: &lt;0p1p&gt;-username TO &lt;sdat&gt;-sh_0p1p_user,
              &lt;0p1p&gt;-udate    TO &lt;sdat&gt;-sh_0p1p_date,
              &lt;0p1p&gt;-utime    TO &lt;sdat&gt;-sh_0p1p_time.
        &lt;sdat&gt;-sh_0p1p_dati = |{ &lt;sdat&gt;-sh_0p1p_date DATE = USER } { &lt;sdat&gt;-sh_0p1p_time TIME = USER }|.
        &lt;sdat&gt;-sh_0p1p_name = me-&gt;_conv_username( &lt;sdat&gt;-sh_0p1p_user ).
        EXIT.
      ENDLOOP.


      &quot;0P2C	parked &amp; completed
      &quot;(from status, via loop, to catch either init entry, or change)
      &quot;note: 0P2C is actually never set, so reading B= &apos;Parked and completed&apos;
      IF NOT ( &lt;sdat&gt;-rbstat = &apos;A&apos; OR   &quot;A= Parked
               &lt;sdat&gt;-rbstat = &apos;C&apos; OR   &quot;C= Parked and held
               &lt;sdat&gt;-rbstat = &apos;D&apos;  ).  &quot;D= Entered and held
        LOOP AT lt_vcd2 ASSIGNING FIELD-SYMBOL(&lt;0p2c&gt;) WHERE         objectid = lv_objid
                                                         AND (    ( value_new = &apos;B&apos;                    )     &quot;B= Parked and completed
                                                               OR ( value_new = &apos;&apos; AND change_ind = &apos;I&apos;) ).  &quot;init save into 0P2C
          MOVE: &lt;0p2c&gt;-username TO &lt;sdat&gt;-sh_0p2c_user,
                &lt;0p2c&gt;-udate    TO &lt;sdat&gt;-sh_0p2c_date,
                &lt;0p2c&gt;-utime    TO &lt;sdat&gt;-sh_0p2c_time.
          &lt;sdat&gt;-sh_0p2c_dati = |{ &lt;sdat&gt;-sh_0p2c_date DATE = USER } { &lt;sdat&gt;-sh_0p2c_time TIME = USER }|.
          &lt;sdat&gt;-sh_0p2c_name = me-&gt;_conv_username( &lt;sdat&gt;-sh_0p2c_user ).
          EXIT.
        ENDLOOP.
      ENDIF.


      &quot;1D1A	difference in-approval
      &quot;(1st from status, set by user parked-by..)
***      READ TABLE lt_vcda ASSIGNING FIELD-SYMBOL(&lt;1d1a&gt;) WITH KEY objectid = lv_objid
***                                                                 value_new = &apos;1D1A&apos;.
***      IF sy-subrc = 0                  AND
***         &lt;1d1a&gt;-username NE &apos;SAP_WFRT&apos;    .
***        MOVE: &lt;1d1a&gt;-username TO &lt;sdat&gt;-sh_1d1a_user,
***              &lt;1d1a&gt;-udate    TO &lt;sdat&gt;-sh_1d1a_date,
***              &lt;1d1a&gt;-utime    TO &lt;sdat&gt;-sh_1d1a_time.
***        &lt;sdat&gt;-sh_1d1a_dati = |{ &lt;sdat&gt;-sh_1d1a_date DATE = USER } { &lt;sdat&gt;-sh_1d1a_time TIME = USER }|.
***        &lt;sdat&gt;-sh_1d1r_name = me-&gt;_conv_username( &lt;sdat&gt;-sh_1d1r_user ).
***      ELSE.
      &quot;(..2nd from workflow)
      READ TABLE lt_wlist ASSIGNING FIELD-SYMBOL(&lt;ts33&gt;) WITH KEY wi_rh_task = &apos;TS97000033&apos;. &quot;TS97000033= Invo Difference rejection info
      IF sy-subrc = 0.
        DATA: ls_detail TYPE swr_widtl,
              lv_result TYPE swr_result.
        CLEAR: lv_result, ls_detail.
        CALL FUNCTION &apos;SAP_WAPI_GET_WORKITEM_DETAIL&apos;
          EXPORTING
            workitem_id     = &lt;ts33&gt;-wi_id
          IMPORTING
            workitem_detail = ls_detail
            workitem_result = lv_result
          TABLES
            message_lines   = lt_mline
            message_struct  = lt_mstr.
        IF ls_detail IS NOT INITIAL AND
           lv_result EQ &apos;approved&apos;.
          MOVE: ls_detail-wi_aagent TO &lt;sdat&gt;-sh_1d1a_user,
                ls_detail-wi_aed    TO &lt;sdat&gt;-sh_1d1a_date,
                ls_detail-wi_aet    TO &lt;sdat&gt;-sh_1d1a_time.
          &lt;sdat&gt;-sh_1d1a_dati = |{ &lt;sdat&gt;-sh_1d1a_date DATE = USER } { &lt;sdat&gt;-sh_1d1a_time TIME = USER }|.
          &lt;sdat&gt;-sh_1d1a_name = me-&gt;_conv_username( &lt;sdat&gt;-sh_1d1a_user ).
        ENDIF.
