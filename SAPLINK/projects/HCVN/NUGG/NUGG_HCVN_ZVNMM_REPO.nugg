    &quot;..given that summarisation must run over all kind of docs

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_VEND_REXV" CMPNAME="SDAT_CALC" VERSION="1" LANGU="E" DESCRIPT="salv-data: calculate data" EXPOSURE="0" STATE="1" EDITORDER="2 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_calc.

    &quot;calculate: total volume
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      &lt;sdat&gt;-tamnt = zvnmm_util=&gt;calc_vend_tvol( iv_lifnr = &lt;sdat&gt;-lifnr
                                                 iv_waers = &lt;sdat&gt;-curr
                                                 iv_datfr = &lt;sdat&gt;-datfr
                                                 iv_datto = &lt;sdat&gt;-datto
                                                 iv_excld = &apos;&apos;            ).


    ENDLOOP.

    &quot;calculate: %
    LOOP AT me-&gt;at_sdat ASSIGNING &lt;sdat&gt; WHERE amnt  IS NOT INITIAL
                                           AND tamnt IS NOT INITIAL.
      TRY.
          &lt;sdat&gt;-cperc = &lt;sdat&gt;-tamnt / &lt;sdat&gt;-amnt * 100.
        CATCH CX_SY_ARITHMETIC_OVERFLOW.
          &lt;sdat&gt;-cperc = &apos;999.99&apos;.
      ENDTRY.
    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_VEND_REXV" CMPNAME="SDAT_ENHA" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance" EXPOSURE="0" STATE="1" EDITORDER="4 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha.

    &quot;enhance salv-data

    me-&gt;sdat_enha_lifnr_tx( ).
    me-&gt;sdat_enha_datty_tx( ).
    me-&gt;sdat_enha_addty_tx( ).
    me-&gt;sdat_enha_ctfld_tx( ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_VEND_REXV" CMPNAME="SDAT_ENHA_ADDTY_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: info-cat descr" EXPOSURE="0" STATE="1" EDITORDER="7 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_addty_tx.

    &quot;enha Information Category descriptions
    IF me-&gt;at_sdat IS NOT INITIAL.
      SELECT * FROM tp19t INTO TABLE @DATA(lt_tp19t)
        FOR ALL ENTRIES IN @me-&gt;at_sdat
        WHERE spras     = @sy-langu
          AND addtype   = @me-&gt;at_sdat-addty
          AND data_type = @me-&gt;at_sdat-datty.
    ENDIF.

    &quot;pass to items
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      READ TABLE lt_tp19t ASSIGNING FIELD-SYMBOL(&lt;tp19t&gt;) WITH KEY addtype   = &lt;sdat&gt;-addty
                                                                   data_type = &lt;sdat&gt;-datty. CHECK sy-subrc = 0.
      MOVE: &lt;tp19t&gt;-text TO &lt;sdat&gt;-datty_tx.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_VEND_REXV" CMPNAME="SDAT_ENHA_CTFLD_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: type field" EXPOSURE="0" STATE="1" EDITORDER="8 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_ctfld_tx.

    &quot;enha CTFiled descriptions
    IF me-&gt;at_sdat IS NOT INITIAL.
      SELECT * FROM tp22t INTO TABLE @DATA(lt_tp22t)
        FOR ALL ENTRIES IN @me-&gt;at_sdat
        WHERE spras     = @sy-langu
          AND addtype   = @me-&gt;at_sdat-addty
          AND data_type = @me-&gt;at_sdat-datty
          AND ct_field  =  @me-&gt;at_sdat-ctfld.
    ENDIF.

    &quot;pass to items
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      READ TABLE lt_tp22t ASSIGNING FIELD-SYMBOL(&lt;tp22t&gt;) WITH KEY addtype   = &lt;sdat&gt;-addty
                                                                 data_type = &lt;sdat&gt;-datty
                                                                 ct_field  = &lt;sdat&gt;-ctfld. CHECK sy-subrc = 0.
      MOVE: &lt;tp22t&gt;-text TO &lt;sdat&gt;-ctfld_tx.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_VEND_REXV" CMPNAME="SDAT_ENHA_DATTY_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: info-type descr" EXPOSURE="0" STATE="1" EDITORDER="6 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_datty_tx.

    &quot;enha Information Type descriptions
    IF me-&gt;at_sdat IS NOT INITIAL.
      SELECT * FROM tpz20t INTO TABLE @DATA(lt_tpz20t)
        FOR ALL ENTRIES IN @me-&gt;at_sdat
        WHERE spras     = @sy-langu
          AND addtype   = @me-&gt;at_sdat-addty.
    ENDIF.

    &quot;pass to items
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      READ TABLE lt_tpz20t ASSIGNING FIELD-SYMBOL(&lt;tpz20t&gt;) WITH KEY addtype   = &lt;sdat&gt;-addty. CHECK sy-subrc = 0.
      MOVE: &lt;tpz20t&gt;-text TO &lt;sdat&gt;-addty_tx.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_VEND_REXV" CMPNAME="SDAT_ENHA_LIFNR_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: vendor name" EXPOSURE="0" STATE="1" EDITORDER="5 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_lifnr_tx.

    &quot;enha Vendor&apos;s descriptions
    IF me-&gt;at_sdat IS NOT INITIAL.
      SELECT lfa1~lifnr,
             lfa1~name1  INTO TABLE @DATA(lt_lfa1)
        FROM lfa1 AS lfa1
        FOR ALL ENTRIES IN @me-&gt;at_sdat
        WHERE lifnr = @me-&gt;at_sdat-lifnr.
    ENDIF.

    &quot;pass to items
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      READ TABLE lt_lfa1 ASSIGNING FIELD-SYMBOL(&lt;lfa1&gt;) WITH KEY lifnr = &lt;sdat&gt;-lifnr. CHECK sy-subrc = 0.
      MOVE: &lt;lfa1&gt;-name1 TO &lt;sdat&gt;-lifnr_tx.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_VEND_REXV" CMPNAME="SDAT_SELC" VERSION="1" LANGU="E" DESCRIPT="salv-data: select data per keys" EXPOSURE="0" STATE="1" EDITORDER="1 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_selc.

    &quot;get data
    CHECK me-&gt;at_keys IS NOT INITIAL.
    SELECT lfa1~lifnr     AS lifnr,
           bp31~addtype   AS addty,
           bp31~data_type AS datty,
           bp31~ct_field  AS ctfld,
           bp31~xfeld     AS xfeld,
           bp31~amnt      AS amnt,
           bp31~curr      AS curr,
           bp31~datefr    AS datfr,
           bp31~dateto    AS datto     INTO CORRESPONDING FIELDS OF TABLE @me-&gt;at_sdat
      FROM            lfa1            AS lfa1
           INNER JOIN zvnmm_bupa_vend AS bupa ON bupa~vendor  = lfa1~lifnr
           INNER JOIN bp3100          AS bp31 ON bp31~partner = bupa~partner
      FOR ALL ENTRIES IN @me-&gt;at_keys
      WHERE lfa1~lifnr     EQ @me-&gt;at_keys-lifnr
        AND bp31~addtype   EQ @me-&gt;at_keys-addtype
        AND bp31~data_type EQ @me-&gt;at_keys-data_type
        AND bp31~ct_field  EQ @me-&gt;at_keys-ct_field
        AND bp31~datefr    EQ @me-&gt;at_keys-datefr
        AND bp31~dateto    EQ @me-&gt;at_keys-dateto.
    .

  ENDMETHOD.</source>
  </method>
