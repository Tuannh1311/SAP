  </method>
  <method CLSNAME="ZCL_VNMM_PREQ_RQUP" CMPNAME="SDAT_ENHA_STATU_IC" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: status icon" EXPOSURE="0" STATE="1" EDITORDER="5 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_statu_ic.

    &quot;enha Status icon
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      CASE &lt;sdat&gt;-xupl_statu.
        WHEN &apos; &apos;.  &quot;_= initial
          MOVE: icon_led_inactive   TO &lt;sdat&gt;-xupl_statu_ic.

        WHEN &apos;U&apos;. &quot;U= updated
          MOVE: icon_checked        TO &lt;sdat&gt;-xupl_statu_ic.

        WHEN &apos;E&apos;. &quot;E= error
          MOVE: icon_led_red        TO &lt;sdat&gt;-xupl_statu_ic.

      ENDCASE.

      &quot;icon messages
      IF &lt;sdat&gt;-ret_t[] IS INITIAL.
        CLEAR: &lt;sdat&gt;-retic.
      ELSEIF &lt;sdat&gt;-ret_t[] IS NOT INITIAL.
        MOVE: icon_history TO &lt;sdat&gt;-retic.
      ENDIF.


    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_PREQ_RQUP" CMPNAME="SDAT_ENHA_STATU_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: status text" EXPOSURE="0" STATE="1" EDITORDER="4 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SDAT_ENHA_STATU_TX.

    &quot;enha Status text
    DATA(lo_dref) = CAST cl_abap_elemdescr( cl_abap_elemdescr=&gt;describe_by_name( p_name = &apos;ZVNMM_PREQ_RQUP_STATU&apos; ) ).
    DATA(lt_vals) = lo_dref-&gt;get_ddic_fixed_values( ).

    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      READ TABLE lt_vals ASSIGNING FIELD-SYMBOL(&lt;vals&gt;) WITH KEY low = &lt;sdat&gt;-xupl_statu. CHECK sy-subrc = 0.

      MOVE: &lt;vals&gt;-ddtext TO &lt;sdat&gt;-xupl_statu_tx.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_PREQ_RQUP" CMPNAME="SDAT_SELC" VERSION="1" LANGU="E" DESCRIPT="salv-data: select data per keys" EXPOSURE="0" STATE="1" EDITORDER="1 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_selc.
    &quot;salv-data: select data per keys

    CHECK me-&gt;at_keys IS NOT INITIAL.
    CLEAR: me-&gt;at_sdat[].

    &quot;get data..
    &quot;..preq-item
    SELECT * FROM eban    FOR ALL ENTRIES IN @me-&gt;at_keys WHERE banfn = @me-&gt;at_keys-banfn
                                                            AND bnfpo = @me-&gt;at_keys-bnfpo INTO TABLE @DATA(lt_eban).
    &quot;..pord-preq
    SELECT * FROM       ekpo AS ekpo
             INNER JOIN eket AS eket ON eket~ebeln = ekpo~ebeln AND
                                        eket~ebelp = ekpo~ebelp
                          FOR ALL ENTRIES IN @me-&gt;at_keys WHERE eket~banfn = @me-&gt;at_keys-banfn
                                                            AND eket~bnfpo = @me-&gt;at_keys-bnfpo
                                                            AND ekpo~loekz &lt;&gt; &apos;L&apos;         INTO TABLE @DATA(lt_ekxx).
    SORT lt_ekxx BY eket-banfn eket-bnfpo eket-eindt DESCENDING.
    DELETE ADJACENT DUPLICATES FROM lt_ekxx COMPARING eket-banfn eket-bnfpo.

    &quot;..preq-texts (header)
    DATA: lt_tdname TYPE TABLE OF stxh.
    LOOP AT me-&gt;at_keys ASSIGNING FIELD-SYMBOL(&lt;key&gt;).
      APPEND INITIAL LINE TO lt_tdname ASSIGNING FIELD-SYMBOL(&lt;tdname&gt;).
      CONCATENATE: &lt;key&gt;-banfn &lt;key&gt;-bnfpo INTO &lt;tdname&gt;-tdname.
    ENDLOOP.
    SELECT * FROM stxh    FOR ALL ENTRIES IN @lt_tdname   WHERE tdobject = &apos;EBAN&apos;
                                                            AND tdname   = @lt_tdname-tdname
                                                            AND tdid     = &apos;B01&apos;           INTO TABLE @DATA(lt_sthx).



    &quot;map together..
    &quot;init line (only when no previous data, otherwise keep keys)
    LOOP AT me-&gt;at_keys ASSIGNING &lt;key&gt;.
      APPEND INITIAL LINE TO me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).
      MOVE: &lt;key&gt;-banfn TO &lt;sdat&gt;-banfn,
            &lt;key&gt;-bnfpo TO &lt;sdat&gt;-bnfpo.
    ENDLOOP.


    &quot;..preq-items
    LOOP AT lt_eban ASSIGNING FIELD-SYMBOL(&lt;eban&gt;).

      READ TABLE me-&gt;at_sdat ASSIGNING &lt;sdat&gt; WITH KEY banfn = &lt;eban&gt;-banfn
                                                       bnfpo = &lt;eban&gt;-bnfpo. CHECK sy-subrc = 0.
      MOVE-CORRESPONDING: &lt;eban&gt; TO &lt;sdat&gt;.
    ENDLOOP.

    &quot;..preq-text
    LOOP AT lt_sthx ASSIGNING FIELD-SYMBOL(&lt;stxh&gt;).
      DATA: lt_lines TYPE TABLE OF tline.
      CLEAR: lt_lines.
      CALL FUNCTION &apos;READ_TEXT&apos;
        EXPORTING
          id       = &lt;stxh&gt;-tdid
          language = &lt;stxh&gt;-tdspras
          name     = &lt;stxh&gt;-tdname
          object   = &lt;stxh&gt;-tdobject
        TABLES
          lines    = lt_lines.
      &quot;itf to stream
      DATA: lt_stream TYPE TABLE OF zvnmm_preq_rupl_text_b01.
      CALL FUNCTION &apos;CONVERT_ITF_TO_STREAM_TEXT&apos;
        TABLES
          itf_text    = lt_lines
          text_stream = lt_stream.
      READ TABLE lt_stream ASSIGNING FIELD-SYMBOL(&lt;stream&gt;) INDEX 1.
      IF sy-subrc = 0.
        READ TABLE me-&gt;at_sdat ASSIGNING &lt;sdat&gt; WITH KEY banfn = &lt;stxh&gt;-tdname(10)
                                                         bnfpo = &lt;stxh&gt;-tdname+10(5).
        IF sy-subrc = 0.
          MOVE: &lt;stream&gt; TO &lt;sdat&gt;-text_b01.
        ENDIF.
      ENDIF.

    ENDLOOP.

    &quot;..pord
    LOOP AT lt_ekxx ASSIGNING FIELD-SYMBOL(&lt;ekxx&gt;).
      READ TABLE me-&gt;at_sdat ASSIGNING &lt;sdat&gt; WITH KEY banfn = &lt;ekxx&gt;-eket-banfn
                                                       bnfpo = &lt;ekxx&gt;-eket-bnfpo. CHECK sy-subrc = 0.
      MOVE: &lt;ekxx&gt;-eket-ebeln TO &lt;sdat&gt;-ebeln,
            &lt;ekxx&gt;-eket-ebelp TO &lt;sdat&gt;-ebelp,
            &lt;ekxx&gt;-eket-eindt TO &lt;sdat&gt;-eindt.
    ENDLOOP.


    &quot;default status
    LOOP AT me-&gt;at_sdat ASSIGNING &lt;sdat&gt;.
      MOVE: &apos; &apos; TO &lt;sdat&gt;-xupl_statu.
    ENDLOOP.


    &quot;default sort (I never know whether this is good idea)
    SORT me-&gt;at_sdat BY banfn DESCENDING
                        bnfpo ASCENDING.

  ENDMETHOD.</source>
  </method>
