  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_PREQ_RUPL" CMPNAME="SDAT_AUTH" VERSION="1" LANGU="E" DESCRIPT="salv-data: check authorisation" EXPOSURE="0" STATE="1" EDITORDER="2 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_auth.

    &quot;authorithy check
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      AUTHORITY-CHECK OBJECT &apos;M_BANF_BSA&apos; ID &apos;ACTVT&apos; FIELD &apos;03&apos;
                                          ID &apos;BSART&apos; FIELD &lt;sdat&gt;-bsart.
      IF sy-subrc &lt;&gt; 0. CLEAR: &lt;sdat&gt;. ENDIF.

      AUTHORITY-CHECK OBJECT &apos;M_BANF_EKO&apos; ID &apos;ACTVT&apos; FIELD &apos;03&apos;
                                          ID &apos;EKORG&apos; FIELD &lt;sdat&gt;-ekorg.
      IF sy-subrc &lt;&gt; 0. CLEAR: &lt;sdat&gt;. ENDIF.

      AUTHORITY-CHECK OBJECT &apos;M_BANF_EKG&apos; ID &apos;ACTVT&apos; FIELD &apos;03&apos;
                                          ID &apos;EKGRP&apos; FIELD &lt;sdat&gt;-ekgrp.
      IF sy-subrc &lt;&gt; 0. CLEAR: &lt;sdat&gt;. ENDIF.

      AUTHORITY-CHECK OBJECT &apos;M_BANF_WRK&apos; ID &apos;ACTVT&apos; FIELD &apos;03&apos;
                                          ID &apos;WERKS&apos; FIELD &lt;sdat&gt;-werks.
      IF sy-subrc &lt;&gt; 0. CLEAR: &lt;sdat&gt;. ENDIF.

    ENDLOOP.
    DELETE me-&gt;at_sdat WHERE xupl_statu IS INITIAL.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_PREQ_RUPL" CMPNAME="SDAT_ENHA" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance" EXPOSURE="0" STATE="1" EDITORDER="3 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha.

    &quot;enhance salv-data

    me-&gt;sdat_enha_statu_tx( ).
    me-&gt;sdat_enha_statu_ic( ).
    me-&gt;sdat_enha_field_tx( ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_PREQ_RUPL" CMPNAME="SDAT_ENHA_FIELD_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: various descriptions" EXPOSURE="0" STATE="1" EDITORDER="6 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_field_tx.

    CHECK: me-&gt;at_sdat IS NOT INITIAL.

    &quot;matnr_tx: material description
    SELECT * FROM makt INTO TABLE @DATA(lt_makt) FOR ALL ENTRIES IN @me-&gt;at_sdat WHERE matnr = @me-&gt;at_sdat-matnr
                                                                                   AND spras = @sy-langu.
    &quot;ekgrp_tx: purch-grp description
    SELECT * FROM t024 INTO TABLE @DATA(lt_t024) FOR ALL ENTRIES IN @me-&gt;at_sdat WHERE ekgrp = @me-&gt;at_sdat-ekgrp.

    &quot;afnam_tx: requestor name
    &quot;(not here, as FAE needs same data types (pernr-vs-afnam) )

    &quot;kostl_tx: cost centre description
    SELECT * FROM cskt INTO TABLE @DATA(lt_cskt) FOR ALL ENTRIES IN @me-&gt;at_sdat WHERE spras =  @sy-langu
                                                                                   AND kostl =  @me-&gt;at_sdat-kostl
                                                                                   AND datbi &gt;= @sy-datum.

    &quot;aufnr_tx: order description
    SELECT * FROM aufk INTO TABLE @DATA(lt_aufk) FOR ALL ENTRIES IN @me-&gt;at_sdat WHERE aufnr = @me-&gt;at_sdat-aufnr.

    &quot;lifnr_tx+flief_tx: vendor name
    SELECT * FROM lfa1 INTO TABLE @DATA(lt_lfa1) FOR ALL ENTRIES IN @me-&gt;at_sdat WHERE lifnr = @me-&gt;at_sdat-lifnr
                                                                                    OR lifnr = @me-&gt;at_sdat-flief.

    &quot;map together
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      &quot;matnr_tx: material description
      READ TABLE lt_makt ASSIGNING FIELD-SYMBOL(&lt;makt&gt;) WITH KEY matnr = &lt;sdat&gt;-matnr.
      IF sy-subrc = 0. MOVE: &lt;makt&gt;-maktx TO &lt;sdat&gt;-matnr_tx. ENDIF.

      &quot;ekgrp_tx: purch-grp description
      READ TABLE lt_t024 ASSIGNING FIELD-SYMBOL(&lt;t024&gt;) WITH KEY ekgrp = &lt;sdat&gt;-ekgrp.
      IF sy-subrc = 0. MOVE: &lt;t024&gt;-eknam TO &lt;sdat&gt;-ekgrp_tx. ENDIF.

      &quot;afnam_tx: requestor name
      &quot;(single select, as FAE needs same data types (pernr-vs-afnam) )
      SELECT SINGLE ename FROM pa0001 INTO @&lt;sdat&gt;-afnam_tx WHERE pernr = @&lt;sdat&gt;-afnam
                                                              AND begda &lt;= @sy-datum
                                                              AND endda &gt;= @sy-datum.

      &quot;kostl_tx: cost centre description
      READ TABLE lt_cskt ASSIGNING FIELD-SYMBOL(&lt;cskt&gt;) WITH KEY kostl = &lt;sdat&gt;-kostl.
      IF sy-subrc = 0. MOVE: &lt;cskt&gt;-ltext TO &lt;sdat&gt;-kostl_tx. ENDIF.

      &quot;aufnr_tx: order description
      READ TABLE lt_aufk ASSIGNING FIELD-SYMBOL(&lt;aufk&gt;) WITH KEY aufnr = &lt;sdat&gt;-aufnr.
      IF sy-subrc = 0. MOVE: &lt;aufk&gt;-ktext TO &lt;sdat&gt;-aufnr_tx. ENDIF.

      &quot;lifnr_tx+flief_tx: vendor name
      READ TABLE lt_lfa1 ASSIGNING FIELD-SYMBOL(&lt;lfa1&gt;) WITH KEY lifnr = &lt;sdat&gt;-lifnr.
      IF sy-subrc = 0. MOVE: &lt;lfa1&gt;-name1 TO &lt;sdat&gt;-lifnr_tx. ENDIF.
      READ TABLE lt_lfa1 ASSIGNING              &lt;lfa1&gt;  WITH KEY lifnr = &lt;sdat&gt;-flief.
      IF sy-subrc = 0. MOVE: &lt;lfa1&gt;-name1 TO &lt;sdat&gt;-flief_tx. ENDIF.


    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_PREQ_RUPL" CMPNAME="SDAT_ENHA_STATU_IC" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: status icon" EXPOSURE="0" STATE="1" EDITORDER="5 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_statu_ic.

    &quot;enha Status icon
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      CASE &lt;sdat&gt;-xupl_statu.
        WHEN &apos;T&apos;.  &quot;T= template (existing item)
          MOVE: icon_led_inactive TO &lt;sdat&gt;-xupl_statu_ic.

        WHEN &apos;L&apos;. &quot;L= loaded from file
          MOVE: icon_led_yellow   TO &lt;sdat&gt;-xupl_statu_ic.

        WHEN &apos;S&apos;. &quot;S= tested successfully
          MOVE: icon_checked      TO &lt;sdat&gt;-xupl_statu_ic.

        WHEN &apos;P&apos;. &quot;P= published to PReq
          MOVE: icon_led_green    TO &lt;sdat&gt;-xupl_statu_ic.

        WHEN &apos;E&apos;.
          MOVE: icon_led_red      TO &lt;sdat&gt;-xupl_statu_ic.

      ENDCASE.

      &quot;icon messages
      IF &lt;sdat&gt;-ret_t[] IS INITIAL.
        CLEAR: &lt;sdat&gt;-retic.
      ELSEIF &lt;sdat&gt;-ret_t[] IS NOT INITIAL.
        MOVE: icon_history TO &lt;sdat&gt;-retic.
      ENDIF.


    ENDLOOP.


