    <include_source>*----------------------------------------------------------------------*
***INCLUDE LZVNMM_PREQ_RQUPI01.
*----------------------------------------------------------------------*
*&amp;---------------------------------------------------------------------*
*&amp;      Module  USER_COMMAND_9001  INPUT
*&amp;---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
MODULE user_command_9001 INPUT.

  &quot;alpha-like
  IF eban-afnam IS NOT INITIAL AND
     ok_code NE &apos;CANCEL&apos;.
    DATA: lv_pernr TYPE pa0001-pernr,
          lv_ename TYPE pa0001-ename.
    SELECT SINGLE pernr, ename FROM pa0001 INTO ( @lv_pernr, @lv_ename ) WHERE pernr EQ @eban-afnam.
    IF sy-subrc = 0.
      MOVE: lv_pernr TO eban-afnam,
            lv_ename TO eban-zzeban_afnam_tx.
    ELSE.
      MESSAGE s008(zvnmm_preq) WITH eban-afnam DISPLAY LIKE &apos;E&apos;.
      CLEAR ok_code.
    ENDIF.
  ENDIF.


  &quot;
  CASE ok_code.
    WHEN &apos;SAVE&apos;.                      LEAVE TO SCREEN 0.
    WHEN &apos;CANCEL&apos;. CLEAR: eban-afnam. LEAVE TO SCREEN 0.
  ENDCASE.

ENDMODULE.</include_source>
   </include>
