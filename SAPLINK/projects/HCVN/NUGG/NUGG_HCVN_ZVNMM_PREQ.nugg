  <method CLSNAME="ZCL_VNMM_PREQ_RUPL" CMPNAME="SDAT_ENHA_STATU_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: status text" EXPOSURE="0" STATE="1" EDITORDER="4 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SDAT_ENHA_STATU_TX.

    &quot;enha Status text
    DATA(lo_dref) = CAST cl_abap_elemdescr( cl_abap_elemdescr=&gt;describe_by_name( p_name = &apos;ZVNMM_PREQ_RUPL_STATU&apos; ) ).
    DATA(lt_vals) = lo_dref-&gt;get_ddic_fixed_values( ).

    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      READ TABLE lt_vals ASSIGNING FIELD-SYMBOL(&lt;vals&gt;) WITH KEY low = &lt;sdat&gt;-xupl_statu. CHECK sy-subrc = 0.

      MOVE: &lt;vals&gt;-ddtext TO &lt;sdat&gt;-xupl_statu_tx.

    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_PREQ_RUPL" CMPNAME="SDAT_SELC" VERSION="1" LANGU="E" DESCRIPT="salv-data: select data per keys (or item, from file)" EXPOSURE="0" STATE="1" EDITORDER="1 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_selc.

    &quot;keys: either init from sel-screen, or upd from file
    IF me-&gt;ax_file = &apos;X&apos;.
      CLEAR: me-&gt;at_keys.
      LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;) WHERE banfn IS NOT INITIAL
                                                           AND bnfpo IS NOT INITIAL.
        APPEND INITIAL LINE TO me-&gt;at_keys ASSIGNING FIELD-SYMBOL(&lt;key&gt;).
        MOVE: &lt;sdat&gt;-banfn TO &lt;key&gt;-banfn,
              &lt;sdat&gt;-bnfpo TO &lt;key&gt;-bnfpo.
      ENDLOOP.

      &quot;actually, no selections after upload
      &quot;(we are working with uploaded data now)
      RETURN.
    ELSE.
      CLEAR: me-&gt;at_sdat.
    ENDIF.



    &quot;get data
    IF me-&gt;at_keys IS NOT INITIAL.
      &quot;select..
      &quot;..preq-item
      SELECT * FROM eban    FOR ALL ENTRIES IN @me-&gt;at_keys WHERE banfn = @me-&gt;at_keys-banfn
                                                              AND bnfpo = @me-&gt;at_keys-bnfpo INTO TABLE @DATA(lt_eban).
      &quot;..preq-acc_assignment
      SELECT * FROM ebkn    FOR ALL ENTRIES IN @me-&gt;at_keys WHERE banfn = @me-&gt;at_keys-banfn
                                                              AND bnfpo = @me-&gt;at_keys-bnfpo
                                                              AND zebkn = &apos;01&apos;               INTO TABLE @DATA(lt_ebkn).

      &quot;..preq-texts (header)
      DATA: lt_tdname TYPE TABLE OF stxh.
      LOOP AT me-&gt;at_keys ASSIGNING &lt;key&gt;.
        APPEND INITIAL LINE TO lt_tdname ASSIGNING FIELD-SYMBOL(&lt;tdname&gt;).
        CONCATENATE: &lt;key&gt;-banfn &lt;key&gt;-bnfpo INTO &lt;tdname&gt;-tdname.
      ENDLOOP.
      SELECT * FROM stxh    FOR ALL ENTRIES IN @lt_tdname   WHERE tdobject = &apos;EBAN&apos;
                                                              AND tdname   = @lt_tdname-tdname
                                                              AND tdid     = &apos;B01&apos;           INTO TABLE @DATA(lt_sthx).
    ENDIF.

    &quot;map together..
    &quot;..preq-items
    LOOP AT lt_eban ASSIGNING FIELD-SYMBOL(&lt;eban&gt;).
      IF me-&gt;ax_file = &apos; &apos;.
        &quot;init sel from sel-screen: new items
        APPEND INITIAL LINE TO me-&gt;at_sdat ASSIGNING &lt;sdat&gt;.
        MOVE-CORRESPONDING: &lt;eban&gt; TO &lt;sdat&gt;.
      ENDIF.
      IF me-&gt;ax_file = &apos;X&apos;.
        &quot;form upload: only update items
        READ TABLE me-&gt;at_sdat ASSIGNING &lt;sdat&gt; WITH KEY banfn = &lt;eban&gt;-banfn
                                                         bnfpo = &lt;eban&gt;-bnfpo. CHECK sy-subrc = 0.
        MOVE-CORRESPONDING: &lt;eban&gt; TO &lt;sdat&gt;.
      ENDIF.
    ENDLOOP.

    &quot;..preq-acc_assignment
    LOOP AT lt_ebkn ASSIGNING FIELD-SYMBOL(&lt;ebkn&gt;).
      READ TABLE me-&gt;at_sdat ASSIGNING &lt;sdat&gt; WITH KEY banfn = &lt;ebkn&gt;-banfn
                                                       bnfpo = &lt;ebkn&gt;-bnfpo. CHECK sy-subrc = 0.
      MOVE-CORRESPONDING: &lt;ebkn&gt; TO &lt;sdat&gt;.
    ENDLOOP.

    &quot;..preq-text
    LOOP AT lt_sthx ASSIGNING FIELD-SYMBOL(&lt;stxh&gt;).
      DATA: lt_lines TYPE TABLE OF tline.
      CLEAR: lt_lines.
      CALL FUNCTION &apos;READ_TEXT&apos;
        EXPORTING
          id       = &lt;stxh&gt;-tdid
          language = &lt;stxh&gt;-tdspras
          name     = &lt;stxh&gt;-tdname
          object   = &lt;stxh&gt;-tdobject
        TABLES
          lines    = lt_lines.
      &quot;itf to stream
      DATA: lt_stream TYPE TABLE OF zvnmm_preq_rupl_text_b01.
      CALL FUNCTION &apos;CONVERT_ITF_TO_STREAM_TEXT&apos;
        TABLES
          itf_text    = lt_lines
          text_stream = lt_stream.
      READ TABLE lt_stream ASSIGNING FIELD-SYMBOL(&lt;stream&gt;) INDEX 1.
      IF sy-subrc = 0.
        READ TABLE me-&gt;at_sdat ASSIGNING &lt;sdat&gt; WITH KEY banfn = &lt;stxh&gt;-tdname(10)
                                                         bnfpo = &lt;stxh&gt;-tdname+10(5).
        IF sy-subrc = 0.
          MOVE: &lt;stream&gt; TO &lt;sdat&gt;-text_b01.
        ENDIF.
      ENDIF.

    ENDLOOP.

    &quot;default status (items from preq, template)
    IF me-&gt;ax_file IS INITIAL.
      LOOP AT me-&gt;at_sdat ASSIGNING &lt;sdat&gt;.
        MOVE: &apos;T&apos; TO &lt;sdat&gt;-xupl_statu.
      ENDLOOP.
    ENDIF.


    &quot;default sort (I never know whether this is good idea)
    SORT me-&gt;at_sdat BY banfn DESCENDING
                        bnfpo DESCENDING.

  ENDMETHOD.</source>
  </method>
 </CLAS>
