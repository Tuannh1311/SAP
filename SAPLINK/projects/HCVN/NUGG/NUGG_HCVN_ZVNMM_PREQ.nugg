   <functionmodule NAME="ZVNMM_PREQ_ENH_ME_UPD_REQ_CHDO" STEXT="MM-PReq: enhance ME_UPDATE_REQUISITION_CHNGDOC">
    <tables PARAMETER="XEBAN" DBSTRUCT="UEBAN"/>
    <tables PARAMETER="YEBAN" DBSTRUCT="UEBAN"/>
    <tables PARAMETER="XEBKN" DBSTRUCT="UEBKN"/>
    <tables PARAMETER="YEBKN" DBSTRUCT="UEBKN"/>
    <documentation PARAMETER="XEBAN" KIND="P" STEXT="Change Document Structure; Generated by RSSCD000" INDEX=" 001"/>
    <documentation PARAMETER="YEBAN" KIND="P" STEXT="Change Document Structure; Generated by RSSCD000" INDEX=" 002"/>
    <documentation PARAMETER="XEBKN" KIND="P" STEXT="Change Document Structure; Generated by RSSCD000" INDEX=" 003"/>
    <documentation PARAMETER="YEBKN" KIND="P" STEXT="Change Document Structure; Generated by RSSCD000" INDEX=" 004"/>
    <fm_source_new>&quot;DEV_MM_99(HQ_TTa) close PReq (after OAgr)
  &quot;
  &quot;update EBAKZ from EBAN (might be changed from ME_UPDATE_REQUISITION_PO, see its impl-enh)
  LOOP AT xeban ASSIGNING FIELD-SYMBOL(&lt;xeban&gt;).
    SELECT SINGLE ebakz INTO &lt;xeban&gt;-ebakz FROM eban WHERE banfn = &lt;xeban&gt;-banfn AND bnfpo = &lt;xeban&gt;-bnfpo.
  ENDLOOP.</fm_source_new>
    <functionModuleDocumentation/>
   </functionmodule>
   <functionmodule NAME="ZVNMM_PREQ_ENH_ME_UPD_REQ_PO" STEXT="MM-PReq: enhance ME_UPDATE_REQUISITION_PO">
    <importing PARAMETER="BSTYP" DBFIELD="EKKO-BSTYP"/>
    <importing PARAMETER="EBELN" DBFIELD="EKKO-EBELN"/>
    <tables PARAMETER="XBATU" DBSTRUCT="FEBAN"/>
    <documentation PARAMETER="BSTYP" KIND="P" STEXT="Purchasing Document Category" INDEX=" 001"/>
    <documentation PARAMETER="EBELN" KIND="P" STEXT="Purchasing Document Number" INDEX=" 002"/>
    <documentation PARAMETER="XBATU" KIND="P" STEXT="Transfer Structure: Requisition to Update Program" INDEX=" 003"/>
    <fm_source_new>&quot;DEV_MM_99(HQ_TTa) close PReq (after OAgr)
  &quot;context: PReqs are processed into OAgrs
  &quot;         ..after OAgr they should be closed (like RVs, but they cannot be RV, due SRM)
  &quot;feature:
  &quot;         set EBAN-EBAKZ &apos;Closed&apos;, to prevent further processing
  &quot;   note:
  &quot;         called either from ME31K..
  &quot;         ..or via BLAORD processing from SRM (see fm IDOC_INPUT_BLAORD, enh at end, actually update after creation)
  &quot;   note:
  &quot;         change-doc update handled in enha of

  IF bstyp = &apos;K&apos;. &quot;are we coming from OAgr?

    &quot;compare OAgr with PReq (the OAgr is not yet filled into PReq)
    DATA: zzlt_ekpo TYPE TABLE OF ekpo.
    SELECT * FROM ekpo INTO TABLE zzlt_ekpo FOR ALL ENTRIES IN xbatu WHERE ebeln = ebeln
                                                                       AND banfn = xbatu-banfn
                                                                       AND bnfpo = xbatu-bnfpo.
    LOOP AT zzlt_ekpo ASSIGNING FIELD-SYMBOL(&lt;zzekpo&gt;).
      LOOP AT xbatu ASSIGNING FIELD-SYMBOL(&lt;zzxbatu&gt;) WHERE banfn = &lt;zzekpo&gt;-banfn
                                                        AND bnfpo = &lt;zzekpo&gt;-bnfpo.
        IF &lt;zzekpo&gt;-loekz NE &apos;L&apos;.
          &lt;zzxbatu&gt;-ebakz = &apos;X&apos;.
        ELSEIF &lt;zzekpo&gt;-loekz EQ &apos;L&apos;.
          &lt;zzxbatu&gt;-ebakz = &apos; &apos;.
        ENDIF.
      ENDLOOP.
    ENDLOOP.

  ENDIF.

  &quot;event: update commitment
  IF &lt;zzxbatu&gt; IS ASSIGNED.
    CALL FUNCTION &apos;SAP_WAPI_CREATE_EVENT&apos;
      EXPORTING
        object_type = &apos;BUS2105&apos;
        object_key  = CONV swr_struct-object_key( &lt;zzxbatu&gt;-banfn )
        event       = &apos;ZCOCmtmUpd&apos;
        commit_work = &apos;&apos;.
  ENDIF.</fm_source_new>
    <functionModuleDocumentation/>
   </functionmodule>
  </functionmodules>
