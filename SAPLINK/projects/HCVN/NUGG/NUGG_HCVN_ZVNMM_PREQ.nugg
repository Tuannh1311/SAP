MODULE status_9001 OUTPUT.
  SET PF-STATUS &apos;STATUS_9001&apos;.
  SET TITLEBAR &apos;TITLE_9001&apos;.
  CLEAR: ok_code.

  &quot;name
  SELECT SINGLE ename FROM pa0001 INTO @pa0001-ename WHERE pernr = @eban-afnam
                                                       AND subty = &apos;&apos;
                                                       AND objps = &apos;&apos;
                                                       AND sprps = &apos;&apos;
                                                       AND begda &lt;= @sy-datum
                                                       AND endda &gt;= @sy-datum
                                                       AND seqnr =  000.
  IF sy-subrc NE 0. CLEAR: pa0001-ename. ENDIF.

ENDMODULE.</include_source>
