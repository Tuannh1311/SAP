  <method CLSNAME="ZCL_VNMM_PREQ_RUPL" CMPNAME="ON_DBCL" VERSION="1" LANGU="E" DESCRIPT="handler: on double-click" EXPOSURE="0" STATE="1" EDITORDER="18 " DISPID="0 " MTDTYPE="1" MTDDECLTYP="0" REFCLSNAME="CL_SALV_EVENTS_TABLE" REFCMPNAME="DOUBLE_CLICK" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNMM_PREQ_RUPL" CMPNAME="ON_DBCL" SCONAME="ROW" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="1" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="0"/>
   <parameter CLSNAME="ZCL_VNMM_PREQ_RUPL" CMPNAME="ON_DBCL" SCONAME="COLUMN" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="1" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="0"/>
   <source>METHOD on_dbcl.

    &quot;get double-clicked row
    READ TABLE me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;) INDEX row.

    &quot;by column
    CASE column.

      WHEN &apos;BANFN&apos; OR &apos;BNFPO&apos;.
        &quot;PReq: display (if created)
        IF &lt;sdat&gt;-xupl_statu = &apos;P&apos; OR  &quot;P= &apos;created&apos;
           &lt;sdat&gt;-xupl_statu = &apos;T&apos;   . &quot;T= &apos;existing item (template)&apos;
          CALL FUNCTION &apos;MMPUR_REQUISITION_DISPLAY&apos;
            EXPORTING
              im_banfn = &lt;sdat&gt;-banfn
              im_bnfpo = &lt;sdat&gt;-bnfpo.

        ENDIF.

      WHEN &apos;RETIC&apos;.
        &quot;messages: popup with msg-list
        CALL FUNCTION &apos;C14ALD_BAPIRET2_SHOW&apos;
          TABLES
            i_bapiret2_tab = &lt;sdat&gt;-ret_t.




    ENDCASE.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_PREQ_RUPL" CMPNAME="ON_UCOM" VERSION="1" LANGU="E" DESCRIPT="handler: on user-command" EXPOSURE="0" STATE="1" EDITORDER="19 " DISPID="0 " MTDTYPE="1" MTDDECLTYP="0" REFCLSNAME="CL_SALV_EVENTS" REFCMPNAME="ADDED_FUNCTION" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNMM_PREQ_RUPL" CMPNAME="ON_UCOM" SCONAME="E_SALV_FUNCTION" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="1" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="0"/>
   <source>METHOD on_ucom.

    &quot;default: get selected rows
    me-&gt;at_rsel = me-&gt;ar_salv-&gt;get_selections( )-&gt;get_selected_rows( ).
    &quot;default: set selections
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;). CLEAR: &lt;sdat&gt;-xsel. ENDLOOP.
    LOOP AT me-&gt;at_rsel ASSIGNING FIELD-SYMBOL(&lt;rsel&gt;).
      READ TABLE me-&gt;at_sdat ASSIGNING &lt;sdat&gt; INDEX &lt;rsel&gt;. CHECK sy-subrc = 0.
      MOVE: &apos;X&apos; TO &lt;sdat&gt;-xsel.
    ENDLOOP.


    CASE e_salv_function.

      WHEN &apos;REFRESH&apos;. me-&gt;salv_refr( ).

      WHEN &apos;DOWNLOAD&apos;. me-&gt;on_ucom_download( )            .
      WHEN &apos;UPLOAD&apos;  . me-&gt;on_ucom_upload( )              .   me-&gt;salv_refr( ).
      WHEN &apos;TEST&apos;    . me-&gt;on_ucom_bapi( ix_test = &apos;X&apos;)   .   me-&gt;salv_refr( ).
      WHEN &apos;SAVE&apos;    . me-&gt;on_ucom_bapi( ix_test = &apos; &apos;)   .   me-&gt;salv_refr( ).


    ENDCASE.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_PREQ_RUPL" CMPNAME="ON_UCOM_BAPI" VERSION="1" LANGU="E" DESCRIPT="call: bapi BAPI_PR_CREATE" EXPOSURE="0" STATE="1" EDITORDER="24 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNMM_PREQ_RUPL" CMPNAME="ON_UCOM_BAPI" SCONAME="IX_TEST" VERSION="1" LANGU="E" DESCRIPT="text run" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="XFELD" PAROPTIONL="X"/>
   <source>METHOD on_ucom_bapi.

    &quot;check any selected
    IF me-&gt;at_rsel[] IS INITIAL.
      MESSAGE w007(zvnmm_preq). &quot;msg{Select lines for processing.}
      RETURN.
    ENDIF.

    &quot;prepare: unique documents
    &quot;(sdat-xsel already set from ON_UCOM)
    DATA: lt_uban TYPE TABLE OF eban-banfn.
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;) WHERE xsel = &apos;X&apos;
                                                         AND NOT ( xupl_statu = &apos;T&apos; OR    &quot;T= &apos;existing item (template)&apos;
                                                                   xupl_statu = &apos;P&apos;    ). &quot;P= &apos;published to PReq&apos;
      APPEND INITIAL LINE TO lt_uban ASSIGNING FIELD-SYMBOL(&lt;uban&gt;).
      MOVE: &lt;sdat&gt;-banfn TO &lt;uban&gt;.
    ENDLOOP.
    SORT lt_uban.
    DELETE ADJACENT DUPLICATES FROM lt_uban.

    &quot;per each unique document: map to bapi
    LOOP AT lt_uban ASSIGNING &lt;uban&gt;.

      DATA: ls_dhdr TYPE bapimereqheader,
            ls_xhdr TYPE bapimereqheaderx,
            lt_ditm TYPE TABLE OF bapimereqitemimp,
            lt_xitm TYPE TABLE OF bapimereqitemx,
            lt_dacc TYPE TABLE OF bapimereqaccount,
            lt_xacc TYPE TABLE OF bapimereqaccountx,
            lt_itxt TYPE TABLE OF bapimereqitemtext,
            lt_ret  TYPE TABLE OF bapiret2.
      CLEAR: ls_dhdr, ls_xhdr,
             lt_ditm[], lt_xitm[],
             lt_dacc[], lt_xacc[],
             lt_itxt[],
             lt_ret[].

      &quot;map its items
      LOOP AT me-&gt;at_sdat ASSIGNING &lt;sdat&gt; WHERE banfn = &lt;uban&gt; &quot;per unique document
                                             AND xsel  = &apos;X&apos;.   &quot;per selected

        MOVE: &lt;sdat&gt;-bsart TO ls_dhdr-pr_type,
              &apos;X&apos;          TO ls_xhdr-pr_type.

        &quot;..item
        APPEND INITIAL LINE TO: lt_ditm ASSIGNING FIELD-SYMBOL(&lt;ditm&gt;),
                                lt_xitm ASSIGNING FIELD-SYMBOL(&lt;xitm&gt;).
        MOVE: &lt;sdat&gt;-bnfpo TO &lt;ditm&gt;-preq_item  ,   &lt;ditm&gt;-preq_item  TO &lt;xitm&gt;-preq_item,
                                                    &apos;X&apos;  TO &lt;xitm&gt;-preq_itemx,
              &lt;sdat&gt;-matnr TO &lt;ditm&gt;-material   ,   &apos;X&apos;  TO &lt;xitm&gt;-material,
              &lt;sdat&gt;-idnlf TO &lt;ditm&gt;-vend_mat   ,   &apos;X&apos;  TO &lt;xitm&gt;-vend_mat,
              &lt;sdat&gt;-menge TO &lt;ditm&gt;-quantity   ,   &apos;X&apos;  TO &lt;xitm&gt;-quantity,
              &lt;sdat&gt;-meins TO &lt;ditm&gt;-unit       ,   &apos;X&apos;  TO &lt;xitm&gt;-unit,
              &lt;sdat&gt;-preis TO &lt;ditm&gt;-preq_price ,   &apos;X&apos;  TO &lt;xitm&gt;-preq_price,
              &lt;sdat&gt;-waers TO &lt;ditm&gt;-currency   ,   &apos;X&apos;  TO &lt;xitm&gt;-currency,
              &lt;sdat&gt;-peinh TO &lt;ditm&gt;-price_unit ,   &apos;X&apos;  TO &lt;xitm&gt;-price_unit,
              &lt;sdat&gt;-lfdat TO &lt;ditm&gt;-deliv_date ,   &apos;X&apos;  TO &lt;xitm&gt;-deliv_date,
              &lt;sdat&gt;-ekorg TO &lt;ditm&gt;-purch_org  ,   &apos;X&apos;  TO &lt;xitm&gt;-purch_org,
              &lt;sdat&gt;-ekgrp TO &lt;ditm&gt;-pur_group  ,   &apos;X&apos;  TO &lt;xitm&gt;-pur_group,
              &lt;sdat&gt;-werks TO &lt;ditm&gt;-plant      ,   &apos;X&apos;  TO &lt;xitm&gt;-plant,
              &lt;sdat&gt;-lgort TO &lt;ditm&gt;-store_loc  ,   &apos;X&apos;  TO &lt;xitm&gt;-store_loc,
              &lt;sdat&gt;-afnam TO &lt;ditm&gt;-preq_name  ,   &apos;X&apos;  TO &lt;xitm&gt;-preq_name,
              &lt;sdat&gt;-bednr TO &lt;ditm&gt;-trackingno ,   &apos;X&apos;  TO &lt;xitm&gt;-trackingno,
              &lt;sdat&gt;-knttp TO &lt;ditm&gt;-acctasscat ,   &apos;X&apos;  TO &lt;xitm&gt;-acctasscat,
              &lt;sdat&gt;-konnr TO &lt;ditm&gt;-agreement  ,   &apos;X&apos;  TO &lt;xitm&gt;-agreement,
              &lt;sdat&gt;-ktpnr TO &lt;ditm&gt;-agmt_item  ,   &apos;X&apos;  TO &lt;xitm&gt;-agmt_item,
              &lt;sdat&gt;-lifnr TO &lt;ditm&gt;-des_vendor ,   &apos;X&apos;  TO &lt;xitm&gt;-des_vendor,
              &lt;sdat&gt;-flief TO &lt;ditm&gt;-fixed_vend ,   &apos;X&apos;  TO &lt;xitm&gt;-fixed_vend.

        &quot;currency amoutn: SAP-internal to BAPI-like
        DATA: lv_amt_ex TYPE  bapicurr-bapicurr.
        CALL FUNCTION &apos;BAPI_CURRENCY_CONV_TO_EXTERNAL&apos;
          EXPORTING
            currency        = &lt;ditm&gt;-currency
            amount_internal = &lt;ditm&gt;-preq_price
          IMPORTING
            amount_external = lv_amt_ex.
        MOVE: lv_amt_ex TO &lt;ditm&gt;-preq_price.

        &quot;..account assignment
        APPEND INITIAL LINE TO: lt_dacc ASSIGNING FIELD-SYMBOL(&lt;dacc&gt;),
                                lt_xacc ASSIGNING FIELD-SYMBOL(&lt;xacc&gt;).
        MOVE: &lt;sdat&gt;-bnfpo TO &lt;dacc&gt;-preq_item  ,  &lt;sdat&gt;-bnfpo TO &lt;xacc&gt;-preq_item,
                                                   &apos;X&apos;   TO &lt;xacc&gt;-preq_itemx,
              &apos;01&apos;         TO &lt;dacc&gt;-serial_no  ,  &apos;01&apos;  TO &lt;xacc&gt;-serial_no,
                                                   &apos;X&apos;   TO &lt;xacc&gt;-serial_nox,
              &lt;sdat&gt;-menge TO &lt;dacc&gt;-quantity   ,  &apos;X&apos;   TO &lt;xacc&gt;-quantity,
              &lt;sdat&gt;-kostl TO &lt;dacc&gt;-costcenter ,
              &lt;sdat&gt;-aufnr TO &lt;dacc&gt;-orderid    .
        IF &lt;dacc&gt;-costcenter IS NOT INITIAL. MOVE &apos;X&apos; TO &lt;xacc&gt;-costcenter. ENDIF.
        IF &lt;dacc&gt;-orderid    IS NOT INITIAL. MOVE &apos;X&apos; TO &lt;xacc&gt;-orderid   . ENDIF.


        &quot;..long text
        IF &lt;sdat&gt;-text_b01 IS NOT INITIAL.
          DATA: lt_lines  TYPE TABLE OF tline,
                lt_stream TYPE TABLE OF zvnmm_preq_rupl_text_b01.
          CLEAR: lt_lines[],
                 lt_stream[].
          APPEND INITIAL LINE TO lt_stream ASSIGNING FIELD-SYMBOL(&lt;stream&gt;).
          MOVE: &lt;sdat&gt;-text_b01 TO &lt;stream&gt;.
          CALL FUNCTION &apos;CONVERT_STREAM_TO_ITF_TEXT&apos;
          &quot;EXPORTING
          &quot;  STREAM_LINES       = STREAM_LINES
          &quot;  LANGUAGE           = SY-LANGU
          &quot;  LF                 = &apos; &apos;
          &quot;  IV_FW              = 72
          &quot;  IV_LW              = 132
            TABLES
              text_stream = lt_stream
              itf_text    = lt_lines.

          LOOP AT lt_lines ASSIGNING FIELD-SYMBOL(&lt;line&gt;).
            APPEND INITIAL LINE TO lt_itxt ASSIGNING FIELD-SYMBOL(&lt;itxt&gt;).
            MOVE: &apos;&apos;    TO &lt;itxt&gt;-preq_no,
                  &lt;sdat&gt;-bnfpo    TO &lt;itxt&gt;-preq_item,
                  &apos;B01&apos;           TO &lt;itxt&gt;-text_id,
                  &lt;line&gt;-tdformat TO &lt;itxt&gt;-text_form,
                  &lt;line&gt;-tdline   TO &lt;itxt&gt;-text_line.
          ENDLOOP.

        ENDIF.

        &quot;clear messages
        CLEAR: &lt;sdat&gt;-ret_t[].

      ENDLOOP.

      &quot;call bapi
      DATA: lv_banfn TYPE eban-banfn.
      CLEAR lv_banfn.
      CALL FUNCTION &apos;BAPI_PR_CREATE&apos;
        EXPORTING
          prheader   = ls_dhdr
          prheaderx  = ls_xhdr
          testrun    = ix_test
        IMPORTING
          number     = lv_banfn
          &quot;PRHEADEREXP                  = PRHEADEREXP
        TABLES
          return     = lt_ret
          pritem     = lt_ditm
          pritemx    = lt_xitm
          praccount  = lt_dacc
          praccountx = lt_xacc
          pritemtext = lt_itxt.

      &quot;pass return back to processed items
      LOOP AT me-&gt;at_sdat ASSIGNING &lt;sdat&gt; WHERE banfn = &lt;uban&gt; &quot;per unique document
                                               AND xsel  = &apos;X&apos;.   &quot;per selected
        MOVE:  lt_ret TO &lt;sdat&gt;-ret_t.
      ENDLOOP.

      &quot;set status/message/preq-num from bapiret
      LOOP AT me-&gt;at_sdat ASSIGNING &lt;sdat&gt; WHERE banfn = &lt;uban&gt; &quot;per unique document
                                               AND xsel  = &apos;X&apos;. &quot;per selected
        &quot;any error message?
        LOOP AT &lt;sdat&gt;-ret_t ASSIGNING FIELD-SYMBOL(&lt;rett&gt;) WHERE type = &apos;E&apos;
                                                               OR type = &apos;A&apos;.
          &quot;..set error
          MOVE: &apos;E&apos; TO &lt;sdat&gt;-xupl_statu. &quot;E= &apos;error&apos;
        ENDLOOP.
        IF sy-subrc NE 0.
          &quot;no error message:
          CASE ix_test.
            WHEN &apos;X&apos;. MOVE: &apos;S&apos; TO &lt;sdat&gt;-xupl_statu.                          &quot;S= &apos;tested successfully&apos;
            WHEN &apos; &apos;. MOVE: &apos;P&apos; TO &lt;sdat&gt;-xupl_statu. &lt;sdat&gt;-banfn = lv_banfn. &quot;P= &apos;created PReq&apos;
          ENDCASE.
        ENDIF.
      ENDLOOP.


      &quot;commit
      IF ix_test NE &apos;X&apos;.
        CALL FUNCTION &apos;BAPI_TRANSACTION_COMMIT&apos; EXPORTING wait = &apos;X&apos;.
      ENDIF.


    ENDLOOP.





    &quot;refresh salv
    me-&gt;salv_refr( ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_PREQ_RUPL" CMPNAME="ON_UCOM_DOWNLOAD" VERSION="1" LANGU="E" DESCRIPT="call: download" EXPOSURE="0" STATE="1" EDITORDER="20 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD on_ucom_download.
    CLEAR: me-&gt;at_cdat[].

    &quot;excel (template) download..
    &quot;..done via para c-like table, to handle type diffs etc

    &quot;Â§ prepare header..
    DATA: lt_dfies TYPE STANDARD TABLE OF dfies.
    CALL FUNCTION &apos;DDIF_FIELDINFO_GET&apos;
      EXPORTING
        tabname   = &apos;ZVNMM_PREQ_RUPL_CDAT&apos;
      TABLES
        dfies_tab = lt_dfies.
    &quot;..-line1: technical names
    APPEND INITIAL LINE TO me-&gt;at_cdat ASSIGNING FIELD-SYMBOL(&lt;chd1&gt;).
    LOOP AT lt_dfies ASSIGNING FIELD-SYMBOL(&lt;dfie&gt;).
      ASSIGN COMPONENT &lt;dfie&gt;-fieldname OF STRUCTURE &lt;chd1&gt; TO FIELD-SYMBOL(&lt;fhd1&gt;).
      MOVE: &lt;dfie&gt;-fieldname TO &lt;fhd1&gt;.
    ENDLOOP.
    &quot;..-line2: descriptions
    APPEND INITIAL LINE TO me-&gt;at_cdat ASSIGNING FIELD-SYMBOL(&lt;chd2&gt;).
    LOOP AT lt_dfies ASSIGNING &lt;dfie&gt;.
      ASSIGN COMPONENT &lt;dfie&gt;-fieldname OF STRUCTURE &lt;chd2&gt; TO FIELD-SYMBOL(&lt;fhd2&gt;).
      MOVE: &lt;dfie&gt;-fieldtext TO &lt;fhd2&gt;.
    ENDLOOP.

    &quot;Â§ populate items (if selected)
    LOOP AT me-&gt;at_rsel ASSIGNING FIELD-SYMBOL(&lt;rsel&gt;).
      READ TABLE me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;) INDEX &lt;rsel&gt;. CHECK sy-subrc = 0.
      APPEND INITIAL LINE TO me-&gt;at_cdat ASSIGNING FIELD-SYMBOL(&lt;cdat&gt;).

      &quot;move what we can, then handle specials
      MOVE-CORRESPONDING: &lt;sdat&gt; TO &lt;cdat&gt;.

      &quot;date
      WRITE: &lt;sdat&gt;-lfdat TO &lt;cdat&gt;-lfdat.

      &quot;qtty &amp; uom
      WRITE: &lt;sdat&gt;-menge UNIT &lt;sdat&gt;-meins TO &lt;cdat&gt;-menge,
                               &lt;sdat&gt;-meins TO &lt;cdat&gt;-meins.
      CONDENSE: &lt;cdat&gt;-menge.

      &quot;item price
      WRITE: &lt;sdat&gt;-preis CURRENCY &lt;sdat&gt;-waers TO &lt;cdat&gt;-preis.
      CONDENSE: &lt;cdat&gt;-preis.

    ENDLOOP.


    &quot;Â§ ask for file
    DATA: lv_file TYPE string,
          lv_path TYPE string,
          lv_full TYPE string.
    CALL METHOD cl_gui_frontend_services=&gt;file_save_dialog
      EXPORTING
        window_title              = &apos;PReq items download&apos;     &quot; Window Title
        default_extension         = &apos;&apos; &quot; Default Extension
        default_file_name         = &apos;PReqItemUpload.xlsx&apos; &quot; Default File Name
        &quot;with_encoding             = with_encoding
        &quot;file_filter               = file_filter       &quot; File Type Filter Table
        &quot;initial_directory         = initial_directory &quot; Initial Directory
        &quot;prompt_on_overwrite       = &apos;X&apos;
      CHANGING
        filename                  = lv_file          &quot; File Name to Save
        path                      = lv_path              &quot; Path to File
        fullpath                  = lv_full          &quot; Path + File Name
        &quot;user_action               = user_action       &quot; User Action (C Class Const ACTION_OK, ACTION_OVERWRITE etc)
        &quot;file_encoding             = file_encoding
      EXCEPTIONS
        cntl_error                = 1                 &quot; Control error
        error_no_gui              = 2                 &quot; No GUI available
        not_supported_by_gui      = 3                 &quot; GUI does not support this
        invalid_default_file_name = 4                 &quot; Invalid default file name
        OTHERS                    = 5.
    IF sy-subrc &lt;&gt; 0. MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4. ENDIF.

    CHECK lv_full IS NOT INITIAL.

    &quot;Â§ file download
    CALL FUNCTION &apos;SAP_CONVERT_TO_XLS_FORMAT&apos;
      EXPORTING
        i_filename        = CONV rlgrap-filename( lv_full )
        i_appl_keep       = &apos;&apos;
      TABLES
        i_tab_sap_data    = me-&gt;at_cdat
      EXCEPTIONS
        conversion_failed = 1
        OTHERS            = 2.
    IF sy-subrc &lt;&gt; 0. MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4. ENDIF.
    IF sy-subrc =  0. MESSAGE s001(zvnmm_ctlg) WITH lv_full. ENDIF.







  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_PREQ_RUPL" CMPNAME="ON_UCOM_UPLOAD" VERSION="1" LANGU="E" DESCRIPT="call: upload" EXPOSURE="0" STATE="1" EDITORDER="21 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD on_ucom_upload.

    &quot;upload from file
    CLEAR: me-&gt;at_cdat.

    &quot;Â§ ask for file
    DATA: lt_ftab TYPE filetable,
          lv_retc TYPE i.
    CALL METHOD cl_gui_frontend_services=&gt;file_open_dialog
      EXPORTING
        window_title            = &apos;PReq items upload&apos;     &quot; Title Of File Open Dialog
        default_extension       = &apos;XLSX&apos; &quot; Default Extension
        &quot;default_filename        = default_filename  &quot; Default File Name
        &quot;file_filter             = file_filter       &quot; File Extension Filter String
        &quot;with_encoding           = with_encoding     &quot; File Encoding
        &quot;initial_directory       = initial_directory &quot; Initial Directory
        &quot;multiselection          = multiselection    &quot; Multiple selections poss.
      CHANGING
        file_table              = lt_ftab           &quot; Table Holding Selected Files
        rc                      = lv_retc           &quot; Return Code, Number of Files or -1 If Error Occurred
        &quot;user_action             = user_action       &quot; User Action (See Class Constants ACTION_OK, ACTION_CANCEL)
        &quot;file_encoding           = file_encoding
      EXCEPTIONS
        file_open_dialog_failed = 1                 &quot; &quot;Open File&quot; dialog failed
        cntl_error              = 2                 &quot; Control error
        error_no_gui            = 3                 &quot; No GUI available
        not_supported_by_gui    = 4                 &quot; GUI does not support this
        OTHERS                  = 5.
    IF sy-subrc &lt;&gt; 0. MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4. ENDIF.

    CHECK lt_ftab IS NOT INITIAL.
    READ TABLE lt_ftab ASSIGNING FIELD-SYMBOL(&lt;ftab&gt;) INDEX 1. CHECK sy-subrc = 0.

    &quot;Â§ load file to table
    DATA: lt_dummy TYPE truxs_t_text_data.
    CALL FUNCTION &apos;TEXT_CONVERT_XLS_TO_SAP&apos;
      EXPORTING
        i_tab_raw_data       = lt_dummy
        i_filename           = CONV rlgrap-filename( &lt;ftab&gt;-filename )
      TABLES
       i_tab_converted_data = me-&gt;at_cdat
      EXCEPTIONS
        conversion_failed    = 1
        OTHERS               = 2.
    IF sy-subrc &lt;&gt; 0. MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4. RETURN. ENDIF.

    &quot;Â§ pass file to salv

    DELETE me-&gt;at_cdat INDEX 2. &quot;header-descriptions
    DELETE me-&gt;at_cdat INDEX 1. &quot;header-technames

    me-&gt;on_ucom_upload_sdat( ).
    me-&gt;on_ucom_upload_enha( ).




  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_PREQ_RUPL" CMPNAME="ON_UCOM_UPLOAD_ENHA" VERSION="1" LANGU="E" DESCRIPT="call: upload: enhance SDat" EXPOSURE="0" STATE="1" EDITORDER="23 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD on_ucom_upload_enha.

    &quot;call: upload: enhance SDat

    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      &quot;status: default after upload
      MOVE: &apos;L&apos; TO &lt;sdat&gt;-xupl_statu. &quot;L= loaded from file

      &quot;bsart: fixed NB
      MOVE: &apos;NB&apos;  TO &lt;sdat&gt;-bsart.

      &quot;werks: default vn01
      IF &lt;sdat&gt;-werks IS INITIAL.
        MOVE: &apos;VN01&apos; TO &lt;sdat&gt;-werks.
      ENDIF.

    ENDLOOP.

    &quot;renumber docs: first empty
    DATA: lv_index TYPE i.
    LOOP AT me-&gt;at_sdat ASSIGNING &lt;sdat&gt; WHERE banfn IS INITIAL.
      ADD 1 TO lv_index.
      MOVE: lv_index TO &lt;sdat&gt;-banfn.
    ENDLOOP.

    &quot;renumber docs: all with $
    LOOP AT me-&gt;at_sdat ASSIGNING &lt;sdat&gt;.
      IF &lt;sdat&gt;-banfn+9(1) = &apos;&apos;.
        CONCATENATE &apos;$&apos; &lt;sdat&gt;-banfn INTO &lt;sdat&gt;-banfn.
      ELSE.
        MOVE: &apos;$&apos; TO &lt;sdat&gt;-banfn(1).
      ENDIF.
    ENDLOOP.

    &quot;re-sort by doc-num (ensure that manually numbered in excel goes together)
    SORT me-&gt;at_sdat BY banfn ASCENDING.

    &quot;re-number items: within doc
    DATA: lv_banfn_pre TYPE eban-banfn,
          lv_bnfpo_new TYPE eban-bnfpo.
    LOOP AT me-&gt;at_sdat ASSIGNING &lt;sdat&gt;.
      IF     &lt;sdat&gt;-banfn NE lv_banfn_pre. &quot;new doc: start from 00010
        lv_bnfpo_new = &apos;00010&apos;.
      ELSEIF &lt;sdat&gt;-banfn EQ lv_banfn_pre. &quot;same doc: increment by 10
        ADD 10 TO lv_bnfpo_new.
      ENDIF.
      &lt;sdat&gt;-bnfpo = lv_bnfpo_new.
      lv_banfn_pre = &lt;sdat&gt;-banfn.
    ENDLOOP.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_PREQ_RUPL" CMPNAME="ON_UCOM_UPLOAD_SDAT" VERSION="1" LANGU="E" DESCRIPT="call: upload: pass CDat to SDat" EXPOSURE="0" STATE="1" EDITORDER="22 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD on_ucom_upload_sdat.

    &quot;pass c-like CDAT to salv SDAT

    CHECK: me-&gt;at_cdat IS NOT INITIAL.
    CLEAR: me-&gt;at_sdat.
    MOVE: &apos;X&apos; TO me-&gt;ax_file.

    LOOP AT me-&gt;at_cdat ASSIGNING FIELD-SYMBOL(&lt;cdat&gt;).

      APPEND INITIAL LINE TO me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      &quot;move item by item &amp; handle specials (no move-crsponding since c-like to others)
      MOVE: &lt;cdat&gt;-banfn    TO &lt;sdat&gt;-banfn,
            &lt;cdat&gt;-matnr    TO &lt;sdat&gt;-matnr,
            &lt;cdat&gt;-idnlf    TO &lt;sdat&gt;-idnlf.

      &quot;menge/meins: qtty &amp; uom
      CALL FUNCTION &apos;RSDS_CONVERT_NUMBER&apos;
        EXPORTING
          input  = &lt;cdat&gt;-menge
        IMPORTING
          output = &lt;sdat&gt;-menge.
      CALL FUNCTION &apos;CONVERSION_EXIT_CUNIT_INPUT&apos;
        EXPORTING
          input  = &lt;cdat&gt;-meins
        IMPORTING
          output = &lt;sdat&gt;-meins.

      &quot;price
      CALL FUNCTION &apos;RSDS_CONVERT_NUMBER&apos;
        EXPORTING
          input  = &lt;cdat&gt;-preis
        IMPORTING
          output = &lt;cdat&gt;-preis.
      MOVE: &lt;cdat&gt;-waers TO &lt;sdat&gt;-waers.
      CALL FUNCTION &apos;BAPI_CURRENCY_CONV_TO_INTERNAL&apos;
        EXPORTING
          currency             = &lt;sdat&gt;-waers
          amount_external      = CONV bapicurr-bapicurr( &lt;cdat&gt;-preis )
          max_number_of_digits = 13
        IMPORTING
          amount_internal      = &lt;sdat&gt;-preis.

      &quot;peinh: price unit
      MOVE: &lt;cdat&gt;-peinh TO &lt;sdat&gt;-peinh.

      &quot;lfdat: delivery date
      CALL FUNCTION &apos;CONVERSION_EXIT_SDATE_INPUT&apos;
        EXPORTING
          input  = &lt;cdat&gt;-lfdat
        IMPORTING
          output = &lt;sdat&gt;-lfdat.

      &quot;...
      MOVE: &lt;cdat&gt;-ekorg TO &lt;sdat&gt;-ekorg,
            &lt;cdat&gt;-ekgrp TO &lt;sdat&gt;-ekgrp,
            &lt;cdat&gt;-werks TO &lt;sdat&gt;-werks,
            &lt;cdat&gt;-lgort TO &lt;sdat&gt;-lgort.

      &quot;afnam: requisitioner (as pernr)
      DATA: lv_pernr TYPE pa0001-pernr.
      me-&gt;conversion_exit_alpha_input( EXPORTING input  = &lt;cdat&gt;-afnam
                                       IMPORTING output = lv_pernr     ).
      &lt;sdat&gt;-afnam = lv_pernr.

      &quot;...
      MOVE: &lt;cdat&gt;-bednr TO &lt;sdat&gt;-bednr,
            &lt;cdat&gt;-knttp TO &lt;sdat&gt;-knttp.

      &quot;kostl: cost centre
      CALL FUNCTION &apos;CONVERSION_EXIT_ALPHA_INPUT&apos;
        EXPORTING
          input  = &lt;cdat&gt;-kostl
        IMPORTING
          output = &lt;sdat&gt;-kostl.

      &quot;kostl: cost centre
      me-&gt;conversion_exit_alpha_input( EXPORTING input  = &lt;cdat&gt;-kostl
                                       IMPORTING output = &lt;sdat&gt;-kostl ).

      &quot;aufnr: order
      me-&gt;conversion_exit_alpha_input( EXPORTING input  = &lt;cdat&gt;-aufnr
                                       IMPORTING output = &lt;sdat&gt;-aufnr ).

      &quot;konnr/ktpnr: outline agreement/item
      me-&gt;conversion_exit_alpha_input( EXPORTING input  = &lt;cdat&gt;-konnr
                                       IMPORTING output = &lt;sdat&gt;-konnr ).
      me-&gt;conversion_exit_alpha_input( EXPORTING input  = &lt;cdat&gt;-ktpnr
                                       IMPORTING output = &lt;sdat&gt;-ktpnr ).

      &quot;lifnr/flief: desired/fixed vendor
      me-&gt;conversion_exit_alpha_input( EXPORTING input  = &lt;cdat&gt;-lifnr
                                       IMPORTING output = &lt;sdat&gt;-lifnr ).
      me-&gt;conversion_exit_alpha_input( EXPORTING input  = &lt;cdat&gt;-flief
                                       IMPORTING output = &lt;sdat&gt;-flief ).

      &quot;text_b01
      MOVE: &lt;cdat&gt;-text_b01 TO &lt;sdat&gt;-text_b01.



    ENDLOOP.

  ENDMETHOD.</source>
  </method>
