  <method CLSNAME="ZCL_VNMM_PREQ_RQUP" CMPNAME="SDAT_ENHA_FIELD_TX" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: various descriptions" EXPOSURE="0" STATE="1" EDITORDER="6 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD sdat_enha_field_tx.

    CHECK: me-&gt;at_sdat IS NOT INITIAL.

    &quot;matnr_tx: material description
    SELECT * FROM makt INTO TABLE @DATA(lt_makt) FOR ALL ENTRIES IN @me-&gt;at_sdat WHERE matnr = @me-&gt;at_sdat-matnr
                                                                                   AND spras = @sy-langu.
    &quot;ekgrp_tx: purch-grp description
    SELECT * FROM t024 INTO TABLE @DATA(lt_t024) FOR ALL ENTRIES IN @me-&gt;at_sdat WHERE ekgrp = @me-&gt;at_sdat-ekgrp.

    &quot;person&apos;s name + current CCtr (afnam, sc_requestor)
    SELECT &quot;eban~banfn, eban~bnfpo,
           pa01~pernr, pa01~ename, pa01~kostl    INTO TABLE @DATA(lt_ebpa)
      FROM            eban   AS eban
           INNER JOIN pa0001 AS pa01 ON ( pa01~pernr = eban~afnam        OR
                                          pa01~pernr = eban~sc_requestor   )
      FOR ALL ENTRIES IN @me-&gt;at_sdat WHERE eban~banfn = @me-&gt;at_sdat-banfn
                                        AND eban~bnfpo = @me-&gt;at_sdat-bnfpo
                                        AND pa01~subty =  &apos;&apos;
                                        AND pa01~objps =  &apos;&apos;
                                        AND pa01~sprps =  &apos;&apos;
                                        AND pa01~begda &lt;= @sy-datum
                                        AND pa01~endda &gt;= @sy-datum
                                        AND pa01~seqnr =  000.

    &quot;future(next) CCtr (per afnam only)
    SELECT &quot;eban~banfn, eban~bnfpo,
           pa01~pernr, pa01~begda, pa01~kostl    INTO TABLE @DATA(lt_ebp2)
      FROM            eban   AS eban
           INNER JOIN pa0001 AS pa01 ON ( pa01~pernr = eban~afnam          )
      FOR ALL ENTRIES IN @me-&gt;at_sdat WHERE eban~banfn = @me-&gt;at_sdat-banfn
                                        AND eban~bnfpo = @me-&gt;at_sdat-bnfpo
                                        AND pa01~subty =  &apos;&apos;
                                        AND pa01~objps =  &apos;&apos;
                                        AND pa01~sprps =  &apos;&apos;
                                        AND pa01~begda IN @me-&gt;ra_begda
                                        AND pa01~seqnr =  000.
    SORT lt_ebp2 BY pernr begda ASCENDING.
    DELETE ADJACENT DUPLICATES FROM lt_ebp2 COMPARING pernr begda.


    &quot;map together
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;).

      &quot;matnr_tx: material description
      READ TABLE lt_makt ASSIGNING FIELD-SYMBOL(&lt;makt&gt;) WITH KEY matnr = &lt;sdat&gt;-matnr.
      IF sy-subrc = 0. MOVE: &lt;makt&gt;-maktx TO &lt;sdat&gt;-matnr_tx. ENDIF.

      &quot;afnam_tx: requestor name
      &quot;kostl: cost centre (current)
      READ TABLE lt_ebpa ASSIGNING FIELD-SYMBOL(&lt;ebpa&gt;) WITH KEY pernr = &lt;sdat&gt;-afnam.
      IF sy-subrc = 0.
        MOVE: &lt;ebpa&gt;-ename TO &lt;sdat&gt;-afnam_tx,
              &lt;ebpa&gt;-kostl TO &lt;sdat&gt;-afnam1_kostl.
      ENDIF.

      &quot;kostl: future
      READ TABLE lt_ebp2 ASSIGNING FIELD-SYMBOL(&lt;ebp2&gt;) WITH KEY pernr = &lt;sdat&gt;-afnam.
      IF sy-subrc = 0.
        MOVE: &lt;ebp2&gt;-begda TO &lt;sdat&gt;-afnam2_begda,
              &lt;ebp2&gt;-kostl TO &lt;sdat&gt;-afnam2_kostl.
      ENDIF.

      &quot;sc_requestor_tx: fiori requestor name
      READ TABLE lt_ebpa ASSIGNING              &lt;ebpa&gt; WITH KEY pernr = &lt;sdat&gt;-sc_requestor.
      IF sy-subrc = 0. MOVE: &lt;ebpa&gt;-ename TO &lt;sdat&gt;-sc_requestor_tx. ENDIF.




    ENDLOOP.

    &quot;ernam_tx: sap creator&apos;s name (loop in loop, as the bapi is slow as hell)
    LOOP AT me-&gt;at_sdat ASSIGNING &lt;sdat&gt; WHERE ernam_tx IS INITIAL.
      DATA: ls_addr TYPE bapiaddr3,
            lt_retu TYPE TABLE OF bapiret2.
      CLEAR: ls_addr, lt_retu[].
      CALL FUNCTION &apos;BAPI_USER_GET_DETAIL&apos;
        EXPORTING
          username      = &lt;sdat&gt;-ernam
          cache_results = &apos;X&apos;
        IMPORTING
          address       = ls_addr
        TABLES
          return        = lt_retu.
      IF ls_addr-fullname IS NOT INITIAL.
        LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat_ernam&gt;) WHERE ernam    EQ &lt;sdat&gt;-ernam
                                                                   AND ernam_tx IS INITIAL     .
          MOVE: ls_addr-fullname TO &lt;sdat_ernam&gt;-ernam_tx.
        ENDLOOP.
      ENDIF.
    ENDLOOP.

    &quot;sc_requestor_tx: fiori requestor&apos;s name (loop in loop, as the bapi is slow as hell)
    LOOP AT me-&gt;at_sdat ASSIGNING &lt;sdat&gt; WHERE sc_requestor_tx IS INITIAL.
      CLEAR: ls_addr, lt_retu[].
      CALL FUNCTION &apos;BAPI_USER_GET_DETAIL&apos;
        EXPORTING
          username      = CONV bapibname-bapibname( &lt;sdat&gt;-sc_requestor )
          cache_results = &apos;X&apos;
        IMPORTING
          address       = ls_addr
        TABLES
          return        = lt_retu.
      IF ls_addr-fullname IS NOT INITIAL.
        LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat_screq&gt;) WHERE sc_requestor    EQ &lt;sdat&gt;-sc_requestor
                                                                   AND sc_requestor_tx IS INITIAL            .
          MOVE: ls_addr-fullname TO &lt;sdat_screq&gt;-sc_requestor_tx.
        ENDLOOP.
      ENDIF.
    ENDLOOP.

    &quot;


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_PREQ_RQUP" CMPNAME="SDAT_ENHA_ORGAS" VERSION="1" LANGU="E" DESCRIPT="salv-data: enhance: organizational assignment" EXPOSURE="0" STATE="1" EDITORDER="7 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>method SDAT_ENHA_ORGAS.
