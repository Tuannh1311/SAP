   <source>METHOD on_dbcl.

    &quot;get double-clicked row
    READ TABLE me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;) INDEX row. CHECK sy-subrc = 0.

    &quot;by column
    CASE column.

      WHEN &apos;EBELN&apos; OR &apos;EBELP&apos; OR &apos;EINDT&apos;.

        CALL FUNCTION &apos;ME_DISPLAY_PURCHASE_DOCUMENT&apos;
          EXPORTING
            i_ebeln              = &lt;sdat&gt;-ebeln
            i_ebelp              = &lt;sdat&gt;-ebelp
          EXCEPTIONS
            not_found            = 1
            no_authority         = 2
            invalid_call         = 3
            preview_not_possible = 4
            OTHERS               = 5.
        IF sy-subrc &lt;&gt; 0. MESSAGE ID sy-msgid TYPE &apos;W&apos; NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4 DISPLAY LIKE sy-msgty. ENDIF.

      WHEN &apos;AFNAM1_KOSTL&apos;
        OR &apos;AFNAM2_BEGDA&apos;
        OR &apos;AFNAM2_KOSTL&apos;.

        SET PARAMETER ID &apos;ITP&apos; FIELD &apos;0001&apos;.
        SET PARAMETER ID &apos;PER&apos; FIELD &lt;sdat&gt;-afnam.
        SET PARAMETER ID &apos;FCD&apos; FIELD &apos;LIST&apos;.

        CALL TRANSACTION &apos;PA20&apos; WITH AUTHORITY-CHECK AND SKIP FIRST SCREEN.

      WHEN &apos;RETIC&apos;.
        &quot;messages: popup with msg-list
        CALL FUNCTION &apos;C14ALD_BAPIRET2_SHOW&apos;
          TABLES
            i_bapiret2_tab = &lt;sdat&gt;-ret_t.

        &quot;WHEN &apos;BANFN&apos; OR &apos;BNFPO&apos;.
      WHEN OTHERS.
        &quot;PReq: display
        CALL FUNCTION &apos;MMPUR_REQUISITION_DISPLAY&apos;
          EXPORTING
            im_banfn = &lt;sdat&gt;-banfn
            im_bnfpo = &lt;sdat&gt;-bnfpo.


    ENDCASE.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_PREQ_RQUP" CMPNAME="ON_UCOM" VERSION="1" LANGU="E" DESCRIPT="handler: on user-command" EXPOSURE="0" STATE="1" EDITORDER="20 " DISPID="0 " MTDTYPE="1" MTDDECLTYP="0" REFCLSNAME="CL_SALV_EVENTS" REFCMPNAME="ADDED_FUNCTION" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNMM_PREQ_RQUP" CMPNAME="ON_UCOM" SCONAME="E_SALV_FUNCTION" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="1" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="0"/>
   <source>METHOD ON_UCOM.

    &quot;default: get selected rows
    me-&gt;at_rsel = me-&gt;ar_salv-&gt;get_selections( )-&gt;get_selected_rows( ).
    &quot;default: set selections
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;). CLEAR: &lt;sdat&gt;-xsel. ENDLOOP.
    LOOP AT me-&gt;at_rsel ASSIGNING FIELD-SYMBOL(&lt;rsel&gt;).
      READ TABLE me-&gt;at_sdat ASSIGNING &lt;sdat&gt; INDEX &lt;rsel&gt;. CHECK sy-subrc = 0.
      MOVE: &apos;X&apos; TO &lt;sdat&gt;-xsel.
    ENDLOOP.


    CASE e_salv_function.

      WHEN &apos;REFRESH&apos;. me-&gt;salv_refr( ).

      WHEN &apos;UPDATE&apos; . me-&gt;on_ucom_update( ix_test = &apos; &apos;).

    ENDCASE.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_PREQ_RQUP" CMPNAME="ON_UCOM_UPDATE" VERSION="1" LANGU="E" DESCRIPT="***TBD UPDATE" EXPOSURE="0" STATE="1" EDITORDER="21 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_VNMM_PREQ_RQUP" CMPNAME="ON_UCOM_UPDATE" SCONAME="IX_TEST" VERSION="1" LANGU="E" DESCRIPT="text run" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="XFELD" PAROPTIONL="X"/>
   <source>METHOD on_ucom_update.

    &quot;check any selected
    IF me-&gt;at_rsel[] IS INITIAL.
      MESSAGE w007(zvnmm_preq). &quot;msg{Select lines for processing.}
      RETURN.
    ENDIF.

    &quot;popup: new requestor
    DATA: lv_rqnew    TYPE eban-afnam,
          lv_rqnew_tx TYPE eban-zzeban_afnam_tx.
    CALL FUNCTION &apos;ZVNMM_PREQ_RQUP_POPUP&apos;
      IMPORTING
        ev_afnam    = lv_rqnew
        ev_afnam_tx = lv_rqnew_tx.
    CHECK lv_rqnew IS NOT INITIAL.

    &quot;update Requestor: directly in table, then change document
    LOOP AT me-&gt;at_sdat ASSIGNING FIELD-SYMBOL(&lt;sdat&gt;) WHERE xsel = &apos;X&apos;.

      &quot;lock preq
      CALL FUNCTION &apos;ENQUEUE_EMEBANE&apos;
        EXPORTING
          banfn          = &lt;sdat&gt;-banfn
          bnfpo          = &lt;sdat&gt;-bnfpo
        EXCEPTIONS
          foreign_lock   = 1
          system_failure = 2
          OTHERS         = 3.
      IF sy-subrc &lt;&gt; 0.
        MESSAGE ID sy-msgid TYPE &apos;I&apos; NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4 DISPLAY LIKE sy-msgty.
        move: &apos;E&apos; to &lt;sdat&gt;-xupl_statu. &quot;E= error
        CONTINUE. &quot;(next loop)
      ENDIF.

      &quot;prepare for change document
      DATA: icdtxt_banf TYPE STANDARD TABLE OF cdtxt,
            xeban       TYPE STANDARD TABLE OF ueban, &quot;(x-new)
            yeban       TYPE STANDARD TABLE OF ueban. &quot;(y-old)
      CLEAR: xeban[], yeban[].
      SELECT * FROM eban INTO TABLE @yeban WHERE banfn = @&lt;sdat&gt;-banfn
                                             AND bnfpo = @&lt;sdat&gt;-bnfpo.

      &quot;update requestor
      UPDATE eban SET afnam           = @lv_rqnew,
                      zzeban_afnam_tx = @lv_rqnew_tx  WHERE banfn = @&lt;sdat&gt;-banfn
                                                        AND bnfpo = @&lt;sdat&gt;-bnfpo.
      &quot;SC only if from Fiori
      IF &lt;sdat&gt;-sc_requestor IS NOT INITIAL.
        UPDATE eban SET sc_requestor = @lv_rqnew  WHERE banfn = @&lt;sdat&gt;-banfn
                                                    AND bnfpo = @&lt;sdat&gt;-bnfpo.
      ENDIF.
      SELECT * FROM eban INTO TABLE @xeban WHERE banfn = @&lt;sdat&gt;-banfn
                                             AND bnfpo = @&lt;sdat&gt;-bnfpo.
      LOOP AT xeban ASSIGNING FIELD-SYMBOL(&lt;xeban&gt;).
        MOVE: &apos;U&apos; TO &lt;xeban&gt;-kz.
      ENDLOOP.

      &quot;create change document
      CALL FUNCTION &apos;BANF_WRITE_DOCUMENT&apos;
        EXPORTING
          objectid    = CONV cdhdr-objectid( &lt;sdat&gt;-banfn )
          tcode       = sy-tcode
          utime       = sy-uzeit
          udate       = sy-datum
          username    = sy-uname
          upd_eban    = &apos;U&apos;
        TABLES
          icdtxt_banf = icdtxt_banf
          xeban       = xeban
          yeban       = yeban.

      &quot;commit
      CALL FUNCTION &apos;BAPI_TRANSACTION_COMMIT&apos; EXPORTING wait = &apos;X&apos;.

      &quot;unlock
      CALL FUNCTION &apos;DEQUEUE_EMEBANE&apos;
        EXPORTING
          banfn = &lt;sdat&gt;-banfn
          bnfpo = &lt;sdat&gt;-bnfpo.

      &quot;update status
      MOVE: &apos;U&apos; TO &lt;sdat&gt;-xupl_statu. &quot;U= updated

    ENDLOOP.




    &quot;refresh salv (partial)
    me-&gt;sdat_enha( ). &quot;data enhance
    me-&gt;ar_salv-&gt;refresh( ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_PREQ_RQUP" CMPNAME="REPORT" VERSION="1" LANGU="E" DESCRIPT="main report" EXPOSURE="2" STATE="1" EDITORDER="2 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD REPORT.

