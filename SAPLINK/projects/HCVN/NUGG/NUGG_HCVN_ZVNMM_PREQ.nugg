  <method CLSNAME="ZCL_VNMM_PREQ_RQUP" CMPNAME="SALV_INIT_CCOL" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: colours per columns" EXPOSURE="0" STATE="1" EDITORDER="13 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_ccol.

    &quot;init: column colours

    DATA: lr_col   TYPE REF TO cl_salv_column_list, &quot; object of column, allowing set_color
          ls_color TYPE        lvc_s_colo.          &quot; colours structure

    DATA(lr_cols) = me-&gt;ar_salv-&gt;get_columns( ).
    DATA(lt_cols) = lr_cols-&gt;get( ).

    LOOP AT lt_cols ASSIGNING FIELD-SYMBOL(&lt;col&gt;).
      TRY.
          lr_col ?= lr_cols-&gt;get_column( &lt;col&gt;-columnname ).
        CATCH cx_salv_not_found.
      ENDTRY.

      DATA(ls_c101) = VALUE lvc_s_colo( col = 1 inv = 0 int = 1 ).  &quot;blue+h
      DATA(ls_c100) = VALUE lvc_s_colo( col = 1 inv = 0 int = 0 ).  &quot;blue
      DATA(ls_c300) = VALUE lvc_s_colo( col = 3 inv = 0 int = 0 ).  &quot;yellow
      DATA(ls_c700) = VALUE lvc_s_colo( col = 7 inv = 0 int = 0 ).  &quot;orange
      DATA(ls_c500) = VALUE lvc_s_colo( col = 5 inv = 0 int = 0 ).  &quot;green

      &quot;setting colours
      CASE &lt;col&gt;-columnname.
        WHEN &apos;BANFN&apos;
          OR &apos;BNFPO&apos;            . lr_col-&gt;set_color( ls_c101 ). &quot;keys : blue+h
        WHEN &apos;MATNR_TX&apos;
          OR &apos;EKGRP_TX&apos;
          OR &apos;AFNAM_TX&apos;
          OR &apos;ERNAM_TX&apos;
          OR &apos;SC_AUTHOR_TX&apos;
          OR &apos;SC_REQUESTOR_TX&apos;
          OR &apos;KOSTL_TX&apos;
          OR &apos;AUFNR_TX&apos;         . lr_col-&gt;set_color( ls_c300 ). &quot;descr: yellow
        WHEN &apos;AFNAM1_KOSTL&apos;
          OR &apos;AFNAM2_BEGDA&apos;
          OR &apos;AFNAM2_KOSTL&apos;     . lr_col-&gt;set_color( ls_c500 ). &quot;orga: green
        WHEN  OTHERS            . lr_col-&gt;set_color( ls_c100 ). &quot;data : blue
      ENDCASE.

    ENDLOOP.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_PREQ_RQUP" CMPNAME="SALV_INIT_COLS" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: columns types (exc, icon, chkbox)" EXPOSURE="0" STATE="1" EDITORDER="10 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SALV_INIT_COLS.

    &quot;init columns type: exception, icon, checkbox, etc

    TRY.

        &quot;init cols: F1 rollname
        DATA(lr_cols) = me-&gt;ar_salv-&gt;get_columns( ).
        DATA(lt_cols) = lr_cols-&gt;get( ).
        LOOP AT lt_cols ASSIGNING FIELD-SYMBOL(&lt;cols&gt;).
          &quot;update DDIC ref together with rollname
          &quot;(this will get normal F1 help pointing to our structure)
          DATA(ddic) = &lt;cols&gt;-r_column-&gt;get_ddic_reference( ).
          ddic-table = &apos;ZVNMM_PREQ_RQUP_SDAT&apos;.
          &lt;cols&gt;-r_column-&gt;set_ddic_reference( value = ddic ).
          &lt;cols&gt;-r_column-&gt;set_f1_rollname( value = &apos;&apos; ).
        ENDLOOP.


        &quot;init cols: checkbox
***        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;XUPL_STATU&apos;) )-&gt;set_cell_type( if_salv_c_cell_type=&gt;checkbox ).

        &quot;init cols: centered
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;XUPL_STATU_IC&apos;) )-&gt;set_alignment( if_salv_c_alignment=&gt;centered ).
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;RETIC&apos;) )-&gt;set_alignment( if_salv_c_alignment=&gt;centered ).

        &quot;init cols: buttons
        &quot;shoot me, button is dumping.. why??
        &quot;CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;RET_B&apos;) )-&gt;set_cell_type( if_salv_c_cell_type=&gt;button ).

        &quot;init cols: no conv-output
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;BANFN&apos;) )-&gt;set_edit_mask( value = &apos;__________&apos; ). &quot;c10
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;BNFPO&apos;) )-&gt;set_edit_mask( value = &apos;_____&apos; ).      &quot;c5

        &quot;uoms
*        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;MEINS&apos;) )-&gt;set_edit_mask( value = &apos;==CUNIT&apos; ) .
*        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;MENGE&apos;) )-&gt;set_quantity_column( value = &apos;MEINS&apos; ).


        &quot;init cols: currency
*        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;PREIS&apos;) )-&gt;set_currency_column( &apos;WAERS&apos; ).

        &quot;init cols: decimals
**        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;CPAMT&apos;) )-&gt;set_decimals( value = &apos;0&apos; ).

        &quot;init cols: technical
        CAST cl_salv_column_table( me-&gt;ar_salv-&gt;get_columns( )-&gt;get_column( &apos;XSEL&apos;) )-&gt;set_technical( value = &apos;X&apos;).


      CATCH cx_salv_data_error.
      CATCH cx_salv_not_found.
    ENDTRY.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_PREQ_RQUP" CMPNAME="SALV_INIT_FUNC" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: functions" EXPOSURE="0" STATE="1" EDITORDER="11 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SALV_INIT_FUNC.

    &quot;salv all functions
    me-&gt;ar_salv-&gt;get_functions( )-&gt;set_all( abap_true ).

    &quot; layout save &amp; default (&apos;all functions&apos; above are not really all)
    DATA: lr_layout TYPE REF TO cl_salv_layout,
          ls_key    TYPE        salv_s_layout_key.

    ls_key-report = me-&gt;lc_repid.
    lr_layout     = me-&gt;ar_salv-&gt;get_layout( ).

    lr_layout-&gt;set_key( ls_key ).
    lr_layout-&gt;set_save_restriction( cl_salv_layout=&gt;restrict_none ). &quot;User can save
    lr_layout-&gt;set_default( abap_true ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_PREQ_RQUP" CMPNAME="SALV_INIT_HNDL" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: event handlers" EXPOSURE="0" STATE="1" EDITORDER="16 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SALV_INIT_HNDL.

    &quot;set handler
    SET HANDLER me-&gt;on_dbcl FOR me-&gt;ar_salv-&gt;get_event( ).
    SET HANDLER me-&gt;on_ucom FOR me-&gt;ar_salv-&gt;get_event( ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_PREQ_RQUP" CMPNAME="SALV_INIT_INVS" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: invisible fields" EXPOSURE="0" STATE="1" EDITORDER="15 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD salv_init_invs.

    &quot;set invisible fields

    DATA: lr_col   TYPE REF TO cl_salv_column_list. &quot; object of column, allowing set_color

    DATA(lr_cols) = me-&gt;ar_salv-&gt;get_columns( ).
    DATA(lt_cols) = lr_cols-&gt;get( ).

    &quot; remove MANDT
    LOOP AT lt_cols ASSIGNING FIELD-SYMBOL(&lt;col&gt;) WHERE columnname = &apos;MANDT&apos;.
      TRY.
          lr_col ?= lr_cols-&gt;get_column( &lt;col&gt;-columnname ).
        CATCH cx_salv_not_found.
      ENDTRY.
      lr_col-&gt;set_technical( if_salv_c_bool_sap=&gt;true ).
    ENDLOOP.

    &quot;remove RET_T
    LOOP AT lt_cols ASSIGNING  &lt;col&gt; WHERE columnname = &apos;RETIC&apos;.
      TRY.
          lr_col ?= lr_cols-&gt;get_column( &lt;col&gt;-columnname ).
        CATCH cx_salv_not_found.
      ENDTRY.
      lr_col-&gt;set_technical( if_salv_c_bool_sap=&gt;true ).
    ENDLOOP.



  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_PREQ_RQUP" CMPNAME="SALV_INIT_MLTS" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: mutli-selection" EXPOSURE="0" STATE="1" EDITORDER="9 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SALV_INIT_MLTS.

    &quot;set multiple-selection
    DATA(lr_sel) = me-&gt;ar_salv-&gt;get_selections( ).
    lr_sel-&gt;set_selection_mode( if_salv_c_selection_mode=&gt;row_column ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_PREQ_RQUP" CMPNAME="SALV_INIT_PFST" VERSION="1" LANGU="E" DESCRIPT="salv-object: initialise: gui status" EXPOSURE="0" STATE="1" EDITORDER="12 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SALV_INIT_PFST.

    &quot;init: gui status from calling program

    &quot;set own function by own screen status (seriously, I hate SALV)
    me-&gt;ar_salv-&gt;set_screen_status(
                                    pfstatus      = me-&gt;lc_pfkey
                                    report        = me-&gt;lc_repid
                                    set_functions = me-&gt;ar_salv-&gt;c_functions_all ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZCL_VNMM_PREQ_RQUP" CMPNAME="SALV_REFR" VERSION="1" LANGU="E" DESCRIPT="salv-object: refresh" EXPOSURE="0" STATE="1" EDITORDER="18 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD SALV_REFR.

