                                                                      AND ebelp = @gv_ebelp.

  SELECT SINGLE msehl FROM t006a INTO @gs_eval-msehl WHERE spras = @sy-langu
                                                       AND msehi = @gs_eval-meins.

  MOVE: gs_eval TO zvnmm_pord_eval_scr.

