*&amp;---------------------------------------------------------------------*
*&amp; Form pord_save
*&amp;---------------------------------------------------------------------*
*&amp; save pord &amp; text
*&amp;---------------------------------------------------------------------*
FORM pord_save.

  &quot;§1st pord
  DATA: ls_teitem TYPE bapi_te_mepoitem,
        ls_txitem TYPE bapi_te_mepoitemx.

  MOVE: gs_eval-ebelp         TO ls_teitem-po_item,
        gs_eval-zzekpo_qqual  TO ls_teitem-zzekpo_qqual,
        gs_eval-zzekpo_qsupp  TO ls_teitem-zzekpo_qsupp.
  MOVE: gs_eval-ebelp         TO ls_txitem-po_item,
        &apos;X&apos;                   TO ls_txitem-zzekpo_qqual,
        &apos;X&apos;                   TO ls_txitem-zzekpo_qsupp.

  DATA: ls_teclike TYPE zvnmm_bapi_te_mepoitem_clike.
  MOVE: ls_teitem-po_item      TO ls_teclike-po_item,
        ls_teitem-_csi         TO ls_teclike-_csi,
        ls_teitem-zvnmm_ekpodb TO ls_teclike-zvnmm_ekpodb.

  DATA: lt_parex TYPE TABLE OF bapiparex.
  APPEND INITIAL LINE TO lt_parex ASSIGNING FIELD-SYMBOL(&lt;parex&gt;).
  MOVE: &apos;BAPI_TE_MEPOITEM&apos;  TO &lt;parex&gt;-structure,
        ls_teclike          TO &lt;parex&gt;-valuepart1.
  APPEND INITIAL LINE TO lt_parex ASSIGNING              &lt;parex&gt;.
  MOVE: &apos;BAPI_TE_MEPOITEMX&apos; TO &lt;parex&gt;-structure,
        ls_txitem           TO &lt;parex&gt;-valuepart1.

  DATA: lt_ret TYPE TABLE OF bapiret2.

  CALL FUNCTION &apos;BAPI_PO_CHANGE&apos;
    EXPORTING
      purchaseorder = gs_eval-ebeln
      no_authority  = &apos;X&apos;
    TABLES
      return        = lt_ret
*     POITEM        = POITEM
*     POITEMX       = POITEMX
      extensionin   = lt_parex
*     EXTENSIONOUT  = EXTENSIONOUT
    .

  &quot;check errors
  LOOP AT lt_ret ASSIGNING FIELD-SYMBOL(&lt;ret&gt;) WHERE type = &apos;E&apos;.
    MESSAGE ID &lt;ret&gt;-id TYPE &lt;ret&gt;-type NUMBER &lt;ret&gt;-number WITH &lt;ret&gt;-message_v1 &lt;ret&gt;-message_v2 &lt;ret&gt;-message_v3 &lt;ret&gt;-message_v4.
    RETURN.
  ENDLOOP.


  &quot;§2nd text
  DATA: ls_thead TYPE thead .
  MOVE: &apos;EKPO&apos;        TO ls_thead-tdobject,
        &apos;F91&apos;         TO ls_thead-tdid,
        gs_eval-spras TO ls_thead-tdspras.
  CONCATENATE gs_eval-ebeln
              gs_eval-ebelp INTO ls_thead-tdname.

  CALL FUNCTION &apos;SAVE_TEXT&apos;
    EXPORTING
*     CLIENT   = SY-MANDT
      header   = ls_thead
*     INSERT   = &apos; &apos;
*     SAVEMODE_DIRECT         = &apos; &apos;
*     OWNER_SPECIFIED         = &apos; &apos;
*     LOCAL_CAT               = &apos; &apos;
*     KEEP_LAST_CHANGED       = &apos; &apos;
* IMPORTING
*     FUNCTION = FUNCTION
*     NEWHEADER               = NEWHEADER
    TABLES
      lines    = gt_tx91
    EXCEPTIONS
      id       = 1
      language = 2
      name     = 3
      object   = 4
      OTHERS   = 5.
  IF sy-subrc &lt;&gt; 0. MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4. ENDIF.

  CALL FUNCTION &apos;COMMIT_TEXT&apos;
*   EXPORTING
*     OBJECT                  = &apos;*&apos;
*     NAME                    = &apos;*&apos;
*     ID                      = &apos;*&apos;
*     LANGUAGE                = &apos;*&apos;
*     SAVEMODE_DIRECT         = &apos; &apos;
*     KEEP                    = &apos; &apos;
*     LOCAL_CAT               = &apos; &apos;
*     KEEP_LAST_CHANGED       = &apos; &apos;
*   IMPORTING
*     COMMIT_COUNT            = COMMIT_COUNT
*   TABLES
*     T_OBJECT                = T_OBJECT
*     T_NAME                  = T_NAME
*     T_ID                    = T_ID
*     T_LANGUAGE              = T_LANGUAGE
    .

  &quot;commit
  CALL FUNCTION &apos;BAPI_TRANSACTION_COMMIT&apos; EXPORTING wait = &apos;X&apos;.


ENDFORM.</include_source>
