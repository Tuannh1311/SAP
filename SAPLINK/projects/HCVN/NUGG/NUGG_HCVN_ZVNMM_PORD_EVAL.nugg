    &quot;none, just no text
  ENDIF.
  &quot;into stream
  CALL FUNCTION &apos;CONVERT_ITF_TO_STREAM_TEXT&apos;
    TABLES
      itf_text    = gt_tx01
      text_stream = gt_st01.

  &quot;pord text: F91 delivery evaluation note
  CALL FUNCTION &apos;READ_TEXT&apos;
    EXPORTING
      id                      = &apos;F91&apos;
      language                = gs_eval-spras
      name                    = lv_tname
      object                  = &apos;EKPO&apos;
    TABLES
      lines                   = gt_tx91
    EXCEPTIONS
      id                      = 1
      language                = 2
      name                    = 3
      not_found               = 4
      object                  = 5
      reference_check         = 6
      wrong_access_to_archive = 7
      OTHERS                  = 8.
  IF sy-subrc &lt;&gt; 0.
    &quot;none, just no text
  ENDIF.
  &quot;into stream
  CALL FUNCTION &apos;CONVERT_ITF_TO_STREAM_TEXT&apos;
    TABLES
      itf_text    = gt_tx91
      text_stream = gt_st91.




