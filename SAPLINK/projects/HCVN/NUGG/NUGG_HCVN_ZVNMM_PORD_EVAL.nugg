INCLUDE lzvnmm_pord_evalf01.</source>
  </mainprogram>
  <includeprograms>
   <include NAME="LZVNMM_PORD_EVALF01" VARCL="X" SUBC="I" APPL="S" RMAND="700" RLOAD="E" UCCHECK="X">
    <include_source>*----------------------------------------------------------------------*
***INCLUDE LZVNMM_PORD_EVALF01.
*----------------------------------------------------------------------*
*&amp;---------------------------------------------------------------------*
*&amp; Form pord_read
*&amp;---------------------------------------------------------------------*
*&amp; read pord
*&amp;---------------------------------------------------------------------*
FORM pord_read .

  CHECK: gv_ebeln IS NOT INITIAL,
         gv_ebelp IS NOT INITIAL.

  &quot;pord data
  SELECT SINGLE * FROM: ekko  INTO CORRESPONDING FIELDS OF @gs_eval WHERE ebeln = @gv_ebeln,
                        lfa1  INTO CORRESPONDING FIELDS OF @gs_eval WHERE lifnr = @gs_eval-lifnr,
                        ekpo  INTO CORRESPONDING FIELDS OF @gs_eval WHERE ebeln = @gv_ebeln
                                                                      AND ebelp = @gv_ebelp,
                        eket  INTO CORRESPONDING FIELDS OF @gs_eval WHERE ebeln = @gv_ebeln
                                                                      AND ebelp = @gv_ebelp.

  SELECT SINGLE msehl FROM t006a INTO @gs_eval-msehl WHERE spras = @sy-langu
                                                       AND msehi = @gs_eval-meins.

  MOVE: gs_eval TO zvnmm_pord_eval_scr.

  &quot;err-msg when none?

  &quot;pord text: F01 item text
  DATA: lv_tname TYPE thead-tdname.
  CONCATENATE gs_eval-ebeln gs_eval-ebelp INTO lv_tname.
  CALL FUNCTION &apos;READ_TEXT&apos;
    EXPORTING
      id                      = &apos;F01&apos;
      language                = gs_eval-spras
      name                    = lv_tname
      object                  = &apos;EKPO&apos;
    TABLES
      lines                   = gt_tx01
    EXCEPTIONS
      id                      = 1
      language                = 2
      name                    = 3
      not_found               = 4
      object                  = 5
      reference_check         = 6
      wrong_access_to_archive = 7
      OTHERS                  = 8.
  IF sy-subrc &lt;&gt; 0.
    &quot;none, just no text
  ENDIF.
  &quot;into stream
  CALL FUNCTION &apos;CONVERT_ITF_TO_STREAM_TEXT&apos;
    TABLES
      itf_text    = gt_tx01
      text_stream = gt_st01.

  &quot;pord text: F91 delivery evaluation note
