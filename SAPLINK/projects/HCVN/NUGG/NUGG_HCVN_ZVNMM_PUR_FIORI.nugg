	&quot;ui/s2p/mm/requisition/maintain/s1/misc/EntityConstants&quot;,
	&quot;sap/m/MessageToast&quot;
], function (B, J, M, f, E, a) {
	&quot;use strict&quot;;
	var p = new Object();
	p.closed = true;
	return sap.ui.controller(&quot;ui.s2p.mm.requisition.maintain.s1.MM_REQ_MAINTS1Extension.controller.SearchCustom&quot;, {
		//    onInit: function () {
		//    },

		onAfterRendering: function () {
			var i18nModel = new sap.ui.model.resource.ResourceModel({
				bundleName: &quot;ui.s2p.mm.requisition.maintain.s1.MM_REQ_MAINTS1Extension.i18nExt.i18n&quot;
			});
			
			var _catText =  this.getView().byId(&quot;catalogText&quot;);
			
			_catText.setText(i18nModel.getProperty(&quot;externalCatalogs&quot;));

			var _hBox = _catText.getParent();

			_hBox.addItem(new sap.m.Button({
				id: this.getView().createId(&quot;interCatalogs&quot;),
				press: function (oEvent) {
					this.destroyContent(&quot;NoSearchresult&quot;);
					//this.getView().byId(&quot;catalogText&quot;).setEnabled(false);
					this.getView().byId(&quot;filterBtn&quot;).setType(&quot;Default&quot;).setVisible(false);
					this.extShowFrag();
				}.bind(this),
				text: i18nModel.getProperty(&quot;catalogFragmentTitle&quot;),
				type: sap.m.ButtonType.Transparent,
				enabled: true
			}));

		},
		
		// METHOD IS FROM ORIGINAL APP AND REDEFINED
		onSuccessSearchBinding: function (d) {
			this.getView().byId(&quot;middleContent&quot;).setBusy(false);
			this.getView().byId(&quot;catalogText&quot;).setEnabled(true);
			this.getView().setBusy(false);
			if (d.results.length) {
				for (var i = 0; i &lt; d.results.length; i++) {
					var F = sap.ui.core.format.NumberFormat.getFloatInstance();
					var v = d.results[i].OpnCtlgMinOrderQuantity;
					var V = F.format(v, &quot;&quot;);
					if (V != &quot;&quot;) d.results[i].OpnCtlgMinOrderQuantity = V;
					else d.results[i].OpnCtlgMinOrderQuantity = v;
				}
				this.destroyContent(&quot;middleContent&quot;);
				this.createFragment(&quot;ui.s2p.mm.requisition.maintain.s1.MM_REQ_MAINTS1Extension.view.SearchResultsCustom&quot;, &quot;middleContent&quot;);
				var j = new sap.ui.model.json.JSONModel(d);
				this.getView().byId(&quot;middleContent&quot;).setModel(j);
				this.getView().byId(&quot;middleContent&quot;).getAggregation(&quot;content&quot;)[0].bindElement(&quot;/results&quot;);
				this.destroyContent(&quot;NoSearchresult&quot;);
				this.getView().byId(&quot;filterBtn&quot;).setVisible(true);
				var t = d.results.length;
			} else {
				this.destroyContent(&quot;middleContent&quot;);
				this.destroyContent(&quot;NoSearchresult&quot;);
				this.createFragment(&quot;ui.s2p.mm.requisition.maintain.s1.view.NoserachResult&quot;, &quot;NoSearchresult&quot;);
				this.createFragment(&quot;ui.s2p.mm.requisition.maintain.s1.view.CatalogList&quot;, &quot;middleContent&quot;);
				this.bindCatalog(&quot;middleContent&quot;);
				this.getView().byId(&quot;filterBtn&quot;).setVisible(false);
				this.oSplitContainer.setShowSecondaryContent(false);
			}
		},

		extShowInterCatalogs: function () {
			this.destroyContent(&quot;NoSearchresult&quot;);
			this.getView().byId(&quot;catalogText&quot;).setEnabled(false);
			this.getView().byId(&quot;filterBtn&quot;).setType(&quot;Default&quot;).setVisible(false);
			this.extShowFrag();
		},

		extShowFrag: function () {
			if (!this._catalogDialog) {
				this._catalogDialog = sap.ui
					.xmlfragment(
						&quot;ui.s2p.mm.requisition.maintain.s1.MM_REQ_MAINTS1Extension.view.InternalCatalogs&quot;,
						this);

				this._catalogDialog.setModel(new sap.ui.model.json.JSONModel());

				// setup message bundle on view
				var i18nModel = new sap.ui.model.resource.ResourceModel({
					bundleName: &quot;ui.s2p.mm.requisition.maintain.s1.MM_REQ_MAINTS1Extension.i18nExt.i18n&quot;
				});
				this._catalogDialog.setModel(i18nModel, &quot;i18n&quot;);

				this._catalogDialog._sOwnerId = this.getView()._sOwnerId;

				var serviceCatURL = &quot;/sap/opu/odata/sap/MM_PUR_CATALOGITEMS_MNG_SRV/&quot;;
				var oDataModel = new sap.ui.model.odata.ODataModel(
					serviceCatURL);
				oDataModel.setSizeLimit(200);
				this._catalogDialog.setModel(oDataModel, &quot;ODataCatalog&quot;);
				
				var serviceUsrURL = &quot;/sap/opu/odata/sap/USERDEFAULTS/&quot;;
				var oDataUsrModel = new sap.ui.model.odata.ODataModel(
					serviceUsrURL);
				oDataUsrModel.setSizeLimit(200);
				this._catalogDialog.setModel(oDataUsrModel, &quot;ODataUserDefaults&quot;);

				this.getView().addDependent(
					this._catalogDialog);

				this._catalogDialog.getModel().setData({
					Type: &quot; &quot;,
					catVisible: true,
					catItemVisible: false,
					itemDetailVisible: false,
					catalogId: &quot;&quot;,
					Quantity: &quot;1&quot;

				});

			} else {

				this._catalogDialog.getModel().setData({
					Type: &quot; &quot;,
					catVisible: true,
					catItemVisible: false,
					itemDetailVisible: false,
					catalogId: &quot;&quot;,
					Quantity: &quot;1&quot;

				});

			}

			this.extOnSearchCatalogs();
			
			this._catalogDialog.open();
			
		},

		extOnCatalogPress: function (oEvent) {
			// The source is the list item that got pressed
			var dialogModel = this._catalogDialog.getModel();
			dialogModel.setProperty(&quot;/catVisible&quot;, false);
			dialogModel.setProperty(&quot;/catItemVisible&quot;, true);
			this.extInitItemBinding(oEvent.getSource().getBindingContext(&quot;ODataUserDefaults&quot;).getProperty(
				&quot;PurReqnSSPCatalog&quot;));
		},

		extOnCatalogItemPress: function (oEvent) {
			// The source is the list item that got pressed
			var dialogModel = this._catalogDialog.getModel();
			dialogModel.setProperty(&quot;/catItemVisible&quot;, false);
			dialogModel.setProperty(&quot;/itemDetailVisible&quot;, true);
			dialogModel.setProperty(&quot;/itmTitle&quot;, oEvent.getSource().getBindingContext(&quot;ODataCatalog&quot;).getProperty(&quot;CatalogItemDescription&quot;));
			dialogModel.setProperty(&quot;/itmImage&quot;, oEvent.getSource().getBindingContext(&quot;ODataCatalog&quot;).getProperty(&quot;OpnCtlgMainImageURL&quot;));
			dialogModel.setProperty(&quot;/itmMaterial&quot;, oEvent.getSource().getBindingContext(&quot;ODataCatalog&quot;).getProperty(&quot;Material&quot;));
			dialogModel.setProperty(&quot;/itmSupplier&quot;, oEvent.getSource().getBindingContext(&quot;ODataCatalog&quot;).getProperty(&quot;OpnCtlgSupplierID&quot;));
			this.extInitItemDetailBinding(oEvent.getSource().getBindingContext(&quot;ODataCatalog&quot;).getProperty(
				&quot;OpnCtlgItemID&quot;), oEvent.getSource().getBindingContext(&quot;ODataCatalog&quot;).getProperty(
				&quot;DraftUUID&quot;), oEvent.getSource().getBindingContext(&quot;ODataCatalog&quot;).getProperty(
				&quot;IsActiveEntity&quot;));
		},

		extInitItemBinding: function (catalogId) {

			var dialogModel = this._catalogDialog.getModel();
			dialogModel.setProperty(&quot;/catalogId&quot;, catalogId);

			var oTable = sap.ui.getCore().byId(&quot;itemTable&quot;);

			var oItems = sap.ui.getCore().byId(&quot;itemTemplateId&quot;);

			var catalogFilter = [new sap.ui.model.Filter(&quot;OpnCtlgWebServiceID&quot;, sap.ui.model.FilterOperator.EQ, catalogId),
				new sap.ui.model.Filter(&quot;DueDateCriticality&quot;, sap.ui.model.FilterOperator.EQ, &quot;01&quot;)
			]; // Status active 01 , inactive 02

			oTable.bindItems({
				path: &quot;ODataCatalog&gt;/C_CatalogItemMngTP&quot;,
				template: oItems,
				filters: catalogFilter,
				parameters: {
					$select: &apos;*&apos;,
					expand: &apos;to_UnitOfMeasure&apos;
				},
				templateShareable: true
			});

		},

		extInitItemDetailBinding: function (opnCtlgItemID, draftUUID, isActiveEntity) {

			var sObjectPath = this._catalogDialog.getModel(&quot;ODataCatalog&quot;).createKey(&quot;C_CatalogItemMngTP&quot;, {
				OpnCtlgItemID: opnCtlgItemID,
				DraftUUID: draftUUID,
				IsActiveEntity: isActiveEntity
			});

			sap.ui.getCore().byId(&quot;MyForm&quot;).bindElement({
				path: &quot;/&quot; + sObjectPath,
				model: &quot;ODataCatalog&quot;,
				// use OData parameters here if needed
				parameters: {
					$select: &apos;*&apos;,
					expand: &apos;to_UnitOfMeasure&apos;
				},
				// react on binding events here
				events: {
					change: function (oEv) {},
					dataRequested: function (oEv) {},
					dataReceived: function (oEv) {

					}
				}
			});

		},

		extOnBackCatalogs: function (event) {
			var dialogModel = this._catalogDialog.getModel();

			if (dialogModel.getProperty(&quot;/catItemVisible&quot;)) {
				dialogModel.setProperty(&quot;/catVisible&quot;, true);
				dialogModel.setProperty(&quot;/catItemVisible&quot;, false);
			} else {
				dialogModel.setProperty(&quot;/catItemVisible&quot;, true);
				dialogModel.setProperty(&quot;/itemDetailVisible&quot;, false);
			}
		},

		extOnCloseDialog: function (event) {

			this._catalogDialog.close();
		},

		extOnSearchCatalogs: function (oEvent) {

			var aTableSearchState = [];
			if (oEvent) {
			var sQuery = oEvent.getParameter(&quot;query&quot;);
			}

			if (sQuery &amp;&amp; sQuery.length &gt; 0) {
				aTableSearchState = [new sap.ui.model.Filter(&quot;OpnCtlgWebServiceName&quot;, sap.ui.model.FilterOperator.Contains, sQuery)];
			}
			var userId = sap.ushell.Container.getService(&quot;UserInfo&quot;).getId();
			aTableSearchState.push(new sap.ui.model.Filter(&quot;Employee&quot;, sap.ui.model.FilterOperator.EQ, userId));
			var oTable = sap.ui.getCore().byId(&quot;tableCatalog&quot;);
			oTable.getBinding(&quot;items&quot;).filter(aTableSearchState, &quot;Application&quot;);

		},

		extOnSearchCatalogItems: function (oEvent) {

			var dialogModel = this._catalogDialog.getModel();
			var aTableSearchState = [];
			var sQuery = oEvent.getParameter(&quot;query&quot;);

			if (sQuery &amp;&amp; sQuery.length &gt; 0) {
				aTableSearchState = [new sap.ui.model.Filter(&quot;CatalogItemDescription&quot;, sap.ui.model.FilterOperator.Contains, sQuery)];
			}
			aTableSearchState.push(new sap.ui.model.Filter(&quot;OpnCtlgWebServiceID&quot;, sap.ui.model.FilterOperator.EQ, dialogModel.getProperty(
				&quot;/catalogId&quot;)));
			aTableSearchState.push(new sap.ui.model.Filter(&quot;DueDateCriticality&quot;, sap.ui.model.FilterOperator.EQ, &quot;01&quot;));

			var oTable = sap.ui.getCore().byId(&quot;itemTable&quot;);
			oTable.getBinding(&quot;items&quot;).filter(aTableSearchState, &quot;Application&quot;);

		},

		extAddToCartFromDetail: function () {
			var itemKey = sap.ui.getCore().byId(&quot;MyForm&quot;).getBindingContext(&quot;ODataCatalog&quot;).getProperty(&quot;OpnCtlgItemID&quot;);

			var data = {
				RequestedQuantity: &quot;1&quot;,
				ParentDraftUUID: this.getHeaderDraftKey(),
				PurReqnSSPCrossCatalogItem: Number(itemKey)
			};
			this.getView().setBusy(true);
			var parameters = this.getServiceCallParameter(this.successhandleAddtoCartPress, this.serviceFail);
			this.dataManager.addToCart(parameters, data, this.getHeaderDraftKey());
			this.getView().setBusy(true);

			this.extOnCloseDialog();
		},

		extAddToCartFromItemList: function (oEvent) {
			var dialogModel = this._catalogDialog.getModel();
			var itemKey = oEvent.getSource().getBindingContext(&quot;ODataCatalog&quot;).getProperty(&quot;OpnCtlgItemID&quot;);
			var quantity = dialogModel.getProperty(&quot;/Quantity&quot;);

			if (quantity &gt; 0 &amp;&amp; quantity.match(/^[0-9]{1,10}(\.\d{1,3})?$/i)) {
				var data = {
					RequestedQuantity: quantity,
					ParentDraftUUID: this.getHeaderDraftKey(),
					PurReqnSSPCrossCatalogItem: Number(itemKey)
				};
				this.getView().setBusy(true);
				var parameters = this.getServiceCallParameter(this.successhandleAddtoCartPress, this.serviceFail);
				this.dataManager.addToCart(parameters, data, this.getHeaderDraftKey());
				this.getView().setBusy(true);

				this.extOnCloseDialog();
			} else {
				sap.m.MessageToast.show(this.getResourceBundle().getText(&quot;MESSAGE_ERROR_ADDTOCART&quot;));
				dialogModel.setProperty(&quot;/Quantity&quot;, &quot;1&quot;);
			}
		}

	});
});</layout>
