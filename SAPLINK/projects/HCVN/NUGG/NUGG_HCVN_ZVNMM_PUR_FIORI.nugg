  <page APPLNAME="ZVN_PUR_CATALOG" PAGEKEY="UI5204CA8FA66E8E0F0CC4C7F41BDE07D9F9AD09376" PAGENAME="UI5204CA8FA66E8E0F0CC4C7F41BDE07D9F9AD09376" PAGETYPE="X" BROWSER_CACHE="0 " SERVER_CACHE="0 " LAYOUTLANGU="E" VERSION="A" DEVCLASS="$TMP" LANGU="E">
   <layout>/*global QUnit*/

sap.ui.define([
	&quot;sap/ui/test/opaQunit&quot;,
	&quot;./pages/Worklist&quot;,
	&quot;./pages/Browser&quot;,
	&quot;./pages/Object&quot;,
	&quot;./pages/App&quot;
], function (opaTest) {
	&quot;use strict&quot;;

	QUnit.module(&quot;Object&quot;);

	opaTest(&quot;Should remember the first item&quot;, function (Given, When, Then) {
		// Arrangements
		Given.iStartMyFLPApp({intent : &quot;Catalogs-display&quot;});

		//Actions
		When.onTheWorklistPage.iRememberTheItemAtPosition(1);

