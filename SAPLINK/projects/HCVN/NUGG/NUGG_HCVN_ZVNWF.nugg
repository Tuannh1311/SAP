  <page APPLNAME="ZSWN_MESSAGE1" PAGEKEY="HTML_INDIV.HTM" PAGENAME="html_indiv.htm" PAGETYPE="V" CONTROLLERURL="CL_BSP_CONTROLLER" MIMETYPE="text/html" BROWSER_CACHE="0 " SERVER_CACHE="0 " LAYOUTLANGU="E" VERSION="A" DEVCLASS="$TMP" LANGU="E" DESCRIPT="One Work Item per Message (Text/HTML)">
   <layout>&lt;%@page language=&quot;abap&quot; %&gt;
&lt;%@extension name=&quot;bsp&quot; prefix=&quot;bsp&quot; %&gt;
&lt;%@extension name=&quot;htmlb&quot; prefix=&quot;htmlb&quot; %&gt;
&lt;%
  DATA wa_details TYPE swnsnotifdetails.
  DATA wa_link    TYPE swnslink.
  DATA l_notif_link type xfeld.
  READ TABLE notif_details INDEX 1 INTO wa_details.
%&gt;
&lt;htmlb:content id=&quot;EmailBodyIndiv&quot; checkForCookiesAndJavaScript=&quot;FALSE&quot; design=&quot;DESIGN2003+DESIGN2008&quot;&gt;
  &lt;htmlb:document&gt;
    &lt;htmlb:documentHead&gt;
      &lt;base href=&quot;&lt;%= BASE_HREF %&gt;&quot;&gt;
      &lt;htmlb:headInclude/&gt;
    &lt;/htmlb:documentHead&gt;
    &lt;htmlb:documentBody&gt;
      &lt;htmlb:textView wrapping=&quot;TRUE&quot; &gt;
        &lt;%= TEXT_PROLOG_INDIV %&gt;
        &lt;p&gt;
      &lt;/htmlb:textView&gt;
      &lt;htmlb:group design=&quot;SECONDARYBOXCOLOR&quot; &gt;
        &lt;htmlb:groupHeader&gt;
          &lt;%= wa_details-subject %&gt;
        &lt;/htmlb:groupHeader&gt;
        &lt;htmlb:groupBody&gt;
          &lt;%= wa_details-body %&gt;
          &lt;p&gt;
          &lt;%
            LOOP AT wa_details-links INTO wa_link WHERE category = &apos;N&apos;.
          %&gt;
          &lt;htmlb:link id        = &quot;notifAction&quot;
                      reference = &quot;&lt;%= wa_link-url %&gt;&quot; &gt;
            &lt;%= wa_link-caption %&gt;
          &lt;/htmlb:link&gt;
          &lt;br&gt;
          &lt;%
              l_notif_link = &apos;X&apos;.
            ENDLOOP.
          %&gt;
          &lt;%
            IF l_notif_link = &apos;X&apos;.
          %&gt;
          &lt;hr&gt;&lt;p&gt;
          &lt;%
            ELSE.
          %&gt;
          &lt;p&gt;
          &lt;%
            ENDIF.
          %&gt;
          &lt;%
            LOOP AT wa_details-links INTO wa_link WHERE category = &apos;T&apos;.
          %&gt;
          &lt;htmlb:link id        = &quot;action&quot;
                      reference = &quot;&lt;%= wa_link-url %&gt;&quot; &gt;
            &lt;%= wa_link-caption %&gt;
          &lt;/htmlb:link&gt;
          &lt;br&gt;
          &lt;%
            ENDLOOP.
          %&gt;
        &lt;/htmlb:groupBody&gt;
      &lt;/htmlb:group&gt;
      &lt;p&gt;
      &lt;htmlb:textView wrapping=&quot;TRUE&quot; &gt;
        &lt;%
          IF NOT has_action_attach IS INITIAL.
        %&gt;
        &lt;%= TEXT_GOTO_WI %&gt;
        &lt;%
          ELSEIF NOT has_action_links IS INITIAL.
          &quot;link is already handled by the links LOOP above
        %&gt;
        &lt;%
          ELSE.
        %&gt;
        &lt;%= TEXT_PROCESS_INDIV %&gt;
        &lt;%
          ENDIF.
        %&gt;
        &lt;p&gt;
        &lt;%= TEXT_CONTACT_ADMIN %&gt;
      &lt;/htmlb:textView&gt;
    &lt;/htmlb:documentBody&gt;
  &lt;/htmlb:document&gt;
&lt;/htmlb:content&gt;</layout>
