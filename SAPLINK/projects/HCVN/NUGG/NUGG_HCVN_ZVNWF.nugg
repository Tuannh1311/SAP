  <page APPLNAME="ZSWN_MESSAGE1" PAGEKEY="HTML_MULTI.HTM" PAGENAME="html_multi.htm" PAGETYPE="V" CONTROLLERURL="CL_BSP_CONTROLLER" MIMETYPE="text/html" BROWSER_CACHE="0 " SERVER_CACHE="0 " LAYOUTLANGU="E" VERSION="A" DEVCLASS="$TMP" LANGU="E" DESCRIPT="Several Work Items per Message (Text/HTML)">
   <layout>&lt;%@page language=&quot;abap&quot; %&gt;
&lt;%@extension name=&quot;bsp&quot; prefix=&quot;bsp&quot; %&gt;
&lt;%@extension name=&quot;htmlb&quot; prefix=&quot;htmlb&quot; %&gt;
&lt;%
  DATA wa_details TYPE swnsnotifdetails.
  DATA wa_link    TYPE swnslink.
  DATA l_wi_ref   TYPE string.
  DATA l_index    TYPE string.
  DATA l_notif_link type xfeld.
  DATA l_text TYPE string.
%&gt;
&lt;htmlb:content id                           = &quot;EmailBodyMulti&quot;
               checkForCookiesAndJavaScript = &quot;FALSE&quot; &gt;
  &lt;htmlb:document&gt;
    &lt;htmlb:documentHead&gt;
      &lt;bsp:findAndReplace find    = &quot;/sap/public/b&quot;
                          replace = &quot;&lt;%= BASE_HREF %&gt;/sap/public/b&quot; &gt;
        &lt;htmlb:headInclude/&gt;
      &lt;/bsp:findAndReplace&gt;
    &lt;/htmlb:documentHead&gt;
    &lt;htmlb:documentBody&gt;
      &lt;a name=&quot;overview&quot;/&gt;
      &lt;htmlb:textView wrapping=&quot;TRUE&quot; &gt;
        &lt;%= TEXT_PROLOG_MULTI %&gt;
        &lt;p&gt;
      &lt;/htmlb:textView&gt;
      &lt;htmlb:group&gt;
        &lt;htmlb:groupHeader&gt;
          &lt;%
  l_text = cl_swn_utl_text=&gt;get_text_from_repository( i_id = &apos;NAZVNWF003&apos;
  i_language = language ).
          %&gt;
          &lt;%= l_text %&gt;
        &lt;/htmlb:groupHeader&gt;
        &lt;htmlb:groupBody&gt;
          &lt;%
  LOOP AT notif_details INTO wa_details.
  l_index = sy-tabix.
  condense l_index.
  concatenate &apos;#a&apos; l_index into l_wi_ref.
          %&gt;
          &lt;div style=&quot;text-indent: 1em; font-style: italic&quot; &gt;
                       &lt;%= wa_details-subject %&gt;
                     &lt;br&gt;
           &lt;/div&gt;
          &lt;%
  ENDLOOP.
          %&gt;
          &lt;htmlb:link id        = &quot;Inbox&quot;
                      reference = &quot;&lt;%= inbox_link_url %&gt;&quot; &gt;
            &lt;%= inbox_link_caption %&gt;
          &lt;/htmlb:link&gt;
        &lt;/htmlb:groupBody&gt;
      &lt;/htmlb:group&gt;
      &lt;p&gt;&lt;hr&gt;&lt;p&gt;
      &lt;p&gt;
      &lt;htmlb:textView wrapping=&quot;TRUE&quot; &gt;
        &lt;%
  IF has_action_links IS INITIAL.
        %&gt;
        &lt;%= TEXT_PROCESS_MULTI %&gt;
        &lt;%
  ENDIF.
        %&gt;
        &lt;p&gt;
        &lt;%= TEXT_CONTACT_ADMIN %&gt;
      &lt;/htmlb:textView&gt;
    &lt;/htmlb:documentBody&gt;
  &lt;/htmlb:document&gt;
&lt;/htmlb:content&gt;</layout>
