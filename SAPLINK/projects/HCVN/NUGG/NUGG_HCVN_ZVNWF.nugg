 <WAPA APPLNAME="ZSWN_MESSAGE1" APPLEXT="ZSWN_MESSAGE1" SECURITY="X" DEVCLASS="$TMP" ORIGLANG="E" MODIFLANG="E" TEXT="WF Notification: Create E-Mail Body in Text Format">
  <page APPLNAME="ZSWN_MESSAGE1" PAGEKEY="HTML_INBOX.HTM" PAGENAME="html_inbox.htm" PAGETYPE="V" CONTROLLERURL="CL_BSP_CONTROLLER" MIMETYPE="text/html" BROWSER_CACHE="0 " SERVER_CACHE="0 " LAYOUTLANGU="E" VERSION="A" DEVCLASS="$TMP" LANGU="E" DESCRIPT="One Message per Inbox (Text/HTML)">
   <layout>&lt;%@page language=&quot;abap&quot; %&gt;
&lt;%@extension name=&quot;bsp&quot; prefix=&quot;bsp&quot; %&gt;
&lt;%@extension name=&quot;htmlb&quot; prefix=&quot;htmlb&quot; %&gt;
&lt;%
  DATA wa_details TYPE swnsnotifdetails.
  DATA wa_link    TYPE swnslink.
  READ TABLE notif_details INDEX 1 INTO wa_details.
%&gt;
&lt;htmlb:content id=&quot;EmailBodyInbox&quot; checkForCookiesAndJavaScript=&quot;FALSE&quot; design=&quot;DESIGN2003+DESIGN2008&quot;&gt;
  &lt;htmlb:document&gt;
    &lt;htmlb:documentHead&gt;
      &lt;base href=&quot;&lt;%= BASE_HREF %&gt;&quot;&gt;
      &lt;htmlb:headInclude/&gt;
    &lt;/htmlb:documentHead&gt;
    &lt;htmlb:documentBody&gt;
      &lt;htmlb:textView wrapping=&quot;TRUE&quot; &gt;
        &lt;%= text_prolog_inbox %&gt;
        &lt;%
          IF NOT inbox_link_url IS INITIAL.
        %&gt;
        &lt;br&gt;
        &lt;htmlb:link id        = &quot;Inbox&quot;
                    reference = &quot;&lt;%= inbox_link_url %&gt;&quot; &gt;
          &lt;%= inbox_link_caption %&gt;
        &lt;/htmlb:link&gt;
        &lt;%
          ELSEIF NOT has_action_attach IS INITIAL.
        %&gt;
        &lt;%= text_goto_inbox %&gt;
        &lt;%
          ELSE.
        %&gt;
        &lt;%= text_process_multi %&gt;
        &lt;%
          ENDIF.
        %&gt;
        &lt;p&gt;
