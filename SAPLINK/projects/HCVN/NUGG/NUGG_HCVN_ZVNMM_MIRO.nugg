  <interfaceMethod CLSNAME="ZCL_IM_MRM_HEADER_CHECK" CPDNAME="IF_EX_MRM_HEADER_CHECK~HEADERDATA_CHECK">
   <source>METHOD if_ex_mrm_header_check~headerdata_check.

