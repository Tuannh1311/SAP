  <method CLSNAME="ZCL_IM_MRM_HEADER_CHECK" CMPNAME="DEFAULT_BLART" VERSION="1" LANGU="E" DESCRIPT="HCVN: set default DocType from VendorType" EXPOSURE="0" STATE="1" EDITORDER="1 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_IM_MRM_HEADER_CHECK" CMPNAME="DEFAULT_BLART" SCONAME="I_RBKPV" VERSION="1" LANGU="E" DESCRIPT="Invoice Document Header" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="1" TYPE="MRM_RBKPV"/>
   <parameter CLSNAME="ZCL_IM_MRM_HEADER_CHECK" CMPNAME="DEFAULT_BLART" SCONAME="TI_DRSEG" VERSION="1" LANGU="E" DESCRIPT="Invoice items" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="1" TYPE="MMCR_TDRSEG"/>
   <source>METHOD default_blart.

    &quot;2020(HQ_TTa) DEV_MM_93 Incoming Invoice: default Document Type from Vendor Type

    CHECK: i_rbkpv-lifnr IS NOT INITIAL,  &quot;document already with Vendor
           i_rbkpv-belnr IS     INITIAL.  &quot;document not yet posted/parked (initial entry only)

    &quot;? first run in MIRO (to allow user change the doc-type after defaulting
    &quot;                     ..and not overwrite it repeatedly                 )
    DATA:  lx_run TYPE xflag.
    IMPORT lx_run TO   lx_run FROM MEMORY ID &apos;ZZHCVNMIRO_DEF_BLART&apos;.
    CHECK: lx_run IS INITIAL.

    &quot;get customizing
    SELECT SINGLE c000~ktokk,
                  c000~blart INTO @DATA(ls_c000)
      FROM            zvnmm_xmiro_c000 AS c000
           INNER JOIN lfa1             AS lfa1 ON lfa1~ktokk = c000~ktokk
      WHERE lfa1~lifnr = @i_rbkpv-lifnr.

    CHECK: sy-subrc EQ 0.

    &quot;change doc-type
    ASSIGN: (&apos;(SAPLMR1M)RBKPV-BLART&apos;) TO FIELD-SYMBOL(&lt;rbkpv_blart&gt;).
    CHECK: sy-subrc EQ 0,
           ls_c000-blart NE &lt;rbkpv_blart&gt;.
    MOVE:  ls_c000-blart TO &lt;rbkpv_blart&gt;.

    &quot;notify user
    SELECT SINGLE: txt30 FROM t077y INTO @DATA(lv_ktokk_tx) WHERE ktokk = @ls_c000-ktokk AND spras = @sy-langu,
                   ltext FROM t003t INTO @DATA(lv_blart_tx) WHERE blart = @ls_c000-blart AND spras = @sy-langu.
    MESSAGE s001(zvnmm_miro) WITH ls_c000-ktokk lv_ktokk_tx
                                  ls_c000-blart lv_blart_tx . &quot;msg{Vendor of Account Group &amp; &quot;&amp;&quot;: Document Type defaulted to &amp; &quot;&amp;&quot;.}

    &quot;pass flag run, indicates pre-population already affected
    MOVE: &apos;X&apos; TO lx_run.
    EXPORT lx_run FROM lx_run   TO MEMORY ID &apos;ZZHCVNMIRO_DEF_BLART&apos;.

  ENDMETHOD.</source>
  </method>
 </CLAS>
