  <method CLSNAME="ZVNEINV_IFPE" CMPNAME="CONF_READ" VERSION="1" LANGU="E" DESCRIPT="get config" EXPOSURE="0" STATE="1" EDITORDER="2 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD conf_read.

    &quot;general
    SELECT SINGLE * FROM zvneinv_c000 INTO @me-&gt;c000 WHERE bukrs EQ @me-&gt;isel-bukrs.

    &quot;comp-code
    SELECT SINGLE * FROM t001         INTO @me-&gt;t001 WHERE bukrs EQ @me-&gt;isel-bukrs..

    &quot;map PaymentTypes
    SELECT * FROM zvneinv_c001 INTO TABLE @me-&gt;c001t WHERE bukrs EQ @me-&gt;isel-bukrs.

    &quot;payment methods
    SELECT SINGLE * FROM t052  INTO @me-&gt;t052  WHERE zterm = @me-&gt;bsegd-zterm.
    SELECT SINGLE * FROM t052u INTO @me-&gt;t052u WHERE zterm = @me-&gt;t052-zterm
                                                 AND spras = &apos;쁩&apos;.             &quot;쁩= VI Vietnamese

    &quot;tax codes
    SELECT * FROM zvneinv_c002 INTO TABLE @me-&gt;c002t WHERE bukrs EQ @me-&gt;isel-bukrs.

    &quot;zero tax codes
    SELECT * FROM zvnfi_zero_tax INTO TABLE @me-&gt;ztax WHERE blart = @me-&gt;bkpf-blart.

  ENDMETHOD.</source>
  </method>
