 <PROG NAME="ZVNEINV_IFPE_FORMS" VARCL="X" SUBC="I" RMAND="700" RLOAD="E" UCCHECK="X">
  <textPool>
   <language SPRAS="E">
    <textElement ID="R" ENTRY="ZVNEINV_IFPE forms &amp; routines" LENGTH="29 "/>
   </language>
  </textPool>
  <source>*&amp;---------------------------------------------------------------------*
*&amp; Include          ZVNEINV_IFPE_FORMS
*&amp;---------------------------------------------------------------------*

*&amp;---------------------------------------------------------------------*
*&amp; Form sels_init
*&amp;---------------------------------------------------------------------*
FORM sels_init .

ENDFORM.
*&amp;---------------------------------------------------------------------*
*&amp; Form write_wsep
*&amp;---------------------------------------------------------------------*
*&amp; write selection parametes
*&amp;---------------------------------------------------------------------*
FORM write_wsep .

  WRITE: /,/ &apos;*** Selection parameters:&apos;(ts0).

  LOOP AT so_belnr ASSIGNING FIELD-SYMBOL(&lt;belnr&gt;).
    WRITE: / &apos;  SO_BELNR Document number         =&apos;, &lt;belnr&gt;-sign, &lt;belnr&gt;-option, &lt;belnr&gt;-low, &lt;belnr&gt;-high.
  ENDLOOP.

  LOOP AT so_gjahr ASSIGNING FIELD-SYMBOL(&lt;gjahr&gt;).
    WRITE: / &apos;  SO_GJAHR Fiscal Year             =&apos;, &lt;gjahr&gt;-sign, &lt;gjahr&gt;-option, &lt;gjahr&gt;-low, &lt;gjahr&gt;-high.
  ENDLOOP.

  LOOP AT so_budat ASSIGNING FIELD-SYMBOL(&lt;budat&gt;).
    WRITE: / &apos;  SO_BUDAT Posting Date            =&apos;, &lt;budat&gt;-sign, &lt;budat&gt;-option, &lt;budat&gt;-low, &lt;budat&gt;-high.
  ENDLOOP.

  LOOP AT so_cpudt ASSIGNING FIELD-SYMBOL(&lt;cpudt&gt;).
    WRITE: / &apos;  SO_CPUDT Entry Date              =&apos;, &lt;cpudt&gt;-sign, &lt;cpudt&gt;-option, &lt;cpudt&gt;-low, &lt;cpudt&gt;-high.
  ENDLOOP.

  LOOP AT so_blart ASSIGNING FIELD-SYMBOL(&lt;blart&gt;).
    WRITE: / &apos;  SO_BLART Document Type           =&apos;, &lt;blart&gt;-sign, &lt;blart&gt;-option, &lt;blart&gt;-low, &lt;blart&gt;-high.
  ENDLOOP.

  LOOP AT so_bukrs ASSIGNING FIELD-SYMBOL(&lt;bukrs&gt;).
    WRITE: / &apos;  SO_BUKRS Company Code            =&apos;, &lt;bukrs&gt;-sign, &lt;bukrs&gt;-option, &lt;bukrs&gt;-low, &lt;bukrs&gt;-high.
  ENDLOOP.


  WRITE:   / &apos;  PA_RERR  re-process errors      =&apos;, pa_rerr,
           / &apos;  PA_RSCC  re-process success     =&apos;, pa_rscc,
           / &apos;  PA_WISL  write selected docs    =&apos;, pa_wisl,
           / &apos;  PA_WIFL  write log IFLI         =&apos;, pa_wifl,
           / &apos;  PA_WREQ  write full ws-Request  =&apos;, pa_wreq,
           / &apos;  PA_WRSP  write full ws-Response =&apos;, pa_wrsp,
           / &apos;  PA_WSEP  write sel-parameters   =&apos;, pa_wsep,
           / &apos;  PA_PORT1 logical port (invoice) =&apos;, pa_port1,
           / &apos;  PA_PORT1 logical port (cancel)  =&apos;, pa_port2.

  WRITE: /.


ENDFORM.


*&amp;---------------------------------------------------------------------*
*&amp; Form data_select
*&amp;---------------------------------------------------------------------*
*&amp; text
*&amp;---------------------------------------------------------------------*
FORM data_select .

  SELECT
         &quot;BKPF FIdoc-header
         bkpf~bukrs         AS bkpf_bukrs,
         bkpf~belnr         AS bkpf_belnr,
         bkpf~gjahr         AS bkpf_gjahr,
         bkpf~budat         AS bkpf_budat,
         bkpf~cpudt         AS bkpf_cpudt,
         bkpf~xreversal     AS bkpf_xreve,
         bkpf~stblg         AS bkpf_stblg,
         bkpf~stjah         AS bkpf_stjah,
         &quot;IFLE invoice interface log
         ifle~rdate         AS ifle_rdate,
         ifle~rtime         AS ifle_rtime,
         ifle~bukrs         AS ifle_bukrs,
         ifle~belnr         AS ifle_belnr,
         ifle~gjahr         AS ifle_gjahr,
         ifle~wssta         AS ifle_wssta,
         &quot;IFL2 reversed document orignal send
         ifl1~rdate         AS ifl1_rdate,
         ifl1~rtime         AS ifl1_rtime,
         ifl1~bukrs         AS ifl1_bukrs,
         ifl1~belnr         AS ifl1_belnr,
         ifl1~gjahr         AS ifl1_gjahr,
         ifl1~wssta         AS ifl1_wssta


    INTO TABLE @DATA(lt_dsel)
    FROM            bkpf                  AS bkpf
         INNER JOIN bseg                  AS bseg ON bseg~bukrs       =  bkpf~bukrs     AND
                                                     bseg~belnr       =  bkpf~belnr     AND
                                                     bseg~gjahr       =  bkpf~gjahr     AND
                                                     bseg~koart       = &apos;D&apos;                 &quot;only customer item
         &quot;IFLE log: document itself
         LEFT OUTER JOIN zvneinv_ifle     AS ifle ON ifle~bukrs       =  bkpf~bukrs     AND
                                                     ifle~belnr       =  bkpf~belnr     AND
                                                     ifle~gjahr       =  bkpf~gjahr
         &quot;IFLE log: reversed document
         LEFT OUTER JOIN zvneinv_ifle     AS ifl1 ON ifle~bukrs       =  bkpf~bukrs     AND
                                                     ifle~belnr       =  bkpf~stblg     AND
                                                     ifle~gjahr       =  bkpf~stjah

    WHERE bkpf~bstat    EQ &apos; &apos;      &quot;_= normal document, posted not parked
      AND bkpf~bukrs    IN @so_bukrs
      AND bkpf~blart    IN @so_blart
      AND bkpf~belnr    IN @so_belnr
      AND bkpf~gjahr    IN @so_gjahr
      AND bkpf~budat    IN @so_budat
      AND bkpf~cpudt    IN @so_cpudt.

  &quot;SAP-3624: keep only s-record when there are e-record as well
  IF pa_succ = &apos;X&apos;.
    LOOP AT lt_dsel ASSIGNING FIELD-SYMBOL(&lt;succ&gt;) WHERE ifle_wssta = &apos;S&apos;.
      DELETE lt_dsel WHERE bkpf_bukrs = &lt;succ&gt;-bkpf_bukrs
                       AND bkpf_belnr = &lt;succ&gt;-bkpf_belnr
                       AND bkpf_gjahr = &lt;succ&gt;-bkpf_gjahr
                       AND ifle_wssta = &apos;E&apos;.
    ENDLOOP.
  ENDIF.


  &quot;remove already sent with Success
  IF pa_rscc NE &apos;X&apos;.
    DELETE lt_dsel WHERE ifle_wssta = &apos;S&apos;.
  ENDIF.

  &quot;remove already sent with Errors
  IF pa_rerr NE &apos;X&apos;.
    DELETE lt_dsel WHERE ifle_wssta = &apos;E&apos;.
  ENDIF.

  &quot;remove reversed invoices
  &quot;(= do not sent original invoice if already cancelled)
  IF pa_exr1 EQ &apos;X&apos;.
    DELETE lt_dsel WHERE bkpf_xreve = &apos;1&apos;.  &quot;1= reversed
  ENDIF.

  &quot;remove reversals without original documents sent
  &quot;(= do not sent reversal if orignal was not sent)
  IF pa_exr2 EQ &apos;X&apos;.
    &quot;get original invoices
    SELECT * FROM zvneinv_ifle INTO TABLE @DATA(lt_rev1) FOR ALL ENTRIES IN @lt_dsel
      WHERE bukrs EQ @lt_dsel-bkpf_bukrs
        AND belnr EQ @lt_dsel-bkpf_stblg
        AND gjahr EQ @lt_dsel-bkpf_stjah.

    &quot;check original invoice sent
    LOOP AT lt_dsel ASSIGNING FIELD-SYMBOL(&lt;dsel&gt;) WHERE bkpf_xreve EQ &apos;2&apos;.  &quot;2= reversal
      READ TABLE lt_rev1 ASSIGNING FIELD-SYMBOL(&lt;rev1&gt;) WITH KEY bukrs = &lt;dsel&gt;-bkpf_bukrs
                                                                 belnr = &lt;dsel&gt;-bkpf_stblg
                                                                 gjahr = &lt;dsel&gt;-bkpf_stjah
                                                                 wstyp = &apos;I&apos;                &quot;I= invoice
                                                                 wssta = &apos;S&apos;              . &quot;S= success
      IF sy-subrc NE 0. &quot;original invoice not sent
        CLEAR: &lt;dsel&gt;.
      ENDIF.
    ENDLOOP.
    DELETE lt_dsel WHERE bkpf_belnr IS INITIAL.

  ENDIF.

  &quot;pass to sel-tab
  LOOP AT lt_dsel ASSIGNING &lt;dsel&gt;.
    APPEND INITIAL LINE TO gt_isel ASSIGNING FIELD-SYMBOL(&lt;isel&gt;).
    MOVE: &lt;dsel&gt;-bkpf_bukrs TO &lt;isel&gt;-bukrs,
          &lt;dsel&gt;-bkpf_belnr TO &lt;isel&gt;-belnr,
          &lt;dsel&gt;-bkpf_gjahr TO &lt;isel&gt;-gjahr,
          &lt;dsel&gt;-bkpf_budat TO &lt;isel&gt;-budat.
  ENDLOOP.
  SORT gt_isel.
  DELETE ADJACENT DUPLICATES FROM gt_isel.

  SORT gt_isel BY budat ASCENDING        &quot;!!very-important: force sending to eInvoice by order of PostingDates..
                  belnr ASCENDING.       &quot;                  ..es eInv doesn&apos;t allow posting of invs out of order!!

  &quot;write count
  DESCRIBE TABLE gt_isel LINES DATA(lv_lines).
  WRITE: /,/ &apos;*** Data selection:&apos;.
  WRITE: / &apos; Selected records for processing: &apos;, lv_lines.

  &quot;check anything to process
  IF lv_lines = 0.
    WRITE: / &apos; No data for processing, leaving report.&apos;.
    STOP.
  ENDIF.

ENDFORM.


*&amp;---------------------------------------------------------------------*
*&amp; Form write_wisl
*&amp;---------------------------------------------------------------------*
*&amp; write selected documents
*&amp;---------------------------------------------------------------------*
FORM write_wisl .

  CHECK pa_wisl = &apos;X&apos;.

  WRITE: /,/ &apos;*** Selected invoices:&apos;.

  LOOP AT gt_isel ASSIGNING FIELD-SYMBOL(&lt;isel&gt;).
    WRITE: / &apos;                      &apos;, &lt;isel&gt;-bukrs, &lt;isel&gt;-belnr, &lt;isel&gt;-gjahr, &lt;isel&gt;-budat.
  ENDLOOP.

ENDFORM.


*&amp;---------------------------------------------------------------------*
*&amp; Form data_process
*&amp;---------------------------------------------------------------------*
*&amp; process selected data
*&amp;---------------------------------------------------------------------*
FORM data_process .

  LOOP AT gt_isel ASSIGNING FIELD-SYMBOL(&lt;isel&gt;).

    DATA: ls_ifle TYPE zvneinv_ifle,   &quot;inc/cnl: log
          ls_ws1r TYPE zvneinv_ws1r,   &quot;invoice: request
          ls_ws1s TYPE zvneinv_ws1s,   &quot;invoice: response
          ls_ws2r TYPE zvneinv_ws2r,   &quot;cancel : request
          ls_ws2s TYPE zvneinv_ws2s.   &quot;cancel : response

    &quot;ifc-object
    DATA(lo_ifpe) = NEW zvneinv_ifpe( port1 = pa_port1
                                      port2 = pa_port2
                                      xtst = px_test
                                      isel = &lt;isel&gt;    ).
    &quot;call processing
    lo_ifpe-&gt;process( ).


    &quot;get request/response/log
    MOVE: lo_ifpe-&gt;ws1r TO ls_ws1r,
          lo_ifpe-&gt;ws2r TO ls_ws2r,
          lo_ifpe-&gt;ws1s TO ls_ws1s,
          lo_ifpe-&gt;ws2s TO ls_ws2s,
          lo_ifpe-&gt;ifle TO ls_ifle.

    &quot;write
    PERFORM: write_ws1r USING ls_ws1r,
             write_ws2r USING ls_ws2r,
             write_ws1s USING ls_ws1s,
             write_ws2s USING ls_ws2s,
             write_ifle USING ls_ifle.

    FREE lo_ifpe.

  ENDLOOP.


ENDFORM.



*&amp;---------------------------------------------------------------------*
*&amp; Form write_ifle
*&amp;---------------------------------------------------------------------*
*&amp; write IFLE log
*&amp;---------------------------------------------------------------------*
FORM write_ifle USING ps_ifle TYPE zvneinv_ifle.


  CHECK pa_wifl = &apos;X&apos;.

  WRITE: /,/ &apos;*** IFLE log:&apos;.

  WRITE: / &apos;                      &apos;, ps_ifle-rdate,
                                     ps_ifle-rtime,
                                     ps_ifle-bukrs,
                                     ps_ifle-belnr,
                                     ps_ifle-gjahr,
                                     ps_ifle-wssta,
                                     ps_ifle-wstyp,
                                     ps_ifle-wsmsg.
***                                     / ps_ifle-TRRES,
***                                       ps_ifle-DESCR,
***                                       ps_ifle-TCODR,
***                                       ps_ifle-INVTC,
***                                       ps_ifle-INVCD,
***                                       ps_ifle-INVNO,
***                                       ps_ifle-SIGND,
***                                       ps_ifle-ISLNK.


ENDFORM.


*&amp;---------------------------------------------------------------------*
*&amp; Form write_ws1r
*&amp;---------------------------------------------------------------------*
*&amp; write request1: invoice
*&amp;---------------------------------------------------------------------*
FORM write_ws1r  USING ps_ws1r TYPE zvneinv_ws1r.

  CHECK: pa_wreq = &apos;X&apos;         ,
         ps_ws1r IS NOT INITIAL.

  WRITE: /,/ &apos;*** ws-Request data(Invoice):&apos;.
  IF px_test = &apos;X&apos;.
    WRITE: /, &apos;  (test mode)&apos;.
  ENDIF.

  &quot;line structures
  SELECT * FROM dd03l INTO TABLE @DATA(lt_dd1r) WHERE tabname EQ &apos;ZVNEINV_WS1R&apos;       AND datatype NE &apos;TTYP&apos;.
  SELECT * FROM dd03l INTO TABLE @DATA(lt_dd1d) WHERE tabname EQ &apos;ZVNEINV_WS1R_DLIST&apos;.
  SELECT * FROM dd03l INTO TABLE @DATA(lt_dd1t) WHERE tabname EQ &apos;ZVNEINV_WS1R_TLIST&apos;.

  WRITE: / .
  &quot;invoice
  LOOP AT lt_dd1r ASSIGNING FIELD-SYMBOL(&lt;dd1r&gt;).
    ASSIGN COMPONENT &lt;dd1r&gt;-fieldname OF STRUCTURE ps_ws1r TO FIELD-SYMBOL(&lt;fl1r&gt;).
    WRITE: / &apos;                      &apos;,  &lt;dd1r&gt;-fieldname(20), &apos;:&apos;, &lt;fl1r&gt;.
  ENDLOOP.

  &quot;invoice item
  LOOP AT ps_ws1r-dlistt ASSIGNING FIELD-SYMBOL(&lt;ws1r&gt;).
    WRITE: / .
    LOOP AT lt_dd1d ASSIGNING FIELD-SYMBOL(&lt;dd1d&gt;).
      ASSIGN COMPONENT &lt;dd1d&gt;-fieldname OF STRUCTURE &lt;ws1r&gt; TO FIELD-SYMBOL(&lt;fl1d&gt;).
      WRITE: / &apos;                         &apos;,  &lt;dd1d&gt;-fieldname(20), &apos;:&apos;, &lt;fl1d&gt;.
    ENDLOOP.
  ENDLOOP.

  &quot;invoice tax
  LOOP AT ps_ws1r-tlistt ASSIGNING FIELD-SYMBOL(&lt;ws1t&gt;).
    WRITE: / .
    LOOP AT lt_dd1t ASSIGNING FIELD-SYMBOL(&lt;dd1t&gt;).
      ASSIGN COMPONENT &lt;dd1t&gt;-fieldname OF STRUCTURE &lt;ws1t&gt; TO FIELD-SYMBOL(&lt;fl1t&gt;).
      WRITE: / &apos;                         &apos;,  &lt;dd1t&gt;-fieldname(20), &apos;:&apos;, &lt;fl1t&gt;.
    ENDLOOP.
  ENDLOOP.

ENDFORM.

*&amp;---------------------------------------------------------------------*
*&amp; Form write_ws2r
*&amp;---------------------------------------------------------------------*
*&amp; write request2: cancel
*&amp;---------------------------------------------------------------------*
FORM write_ws2r  USING ps_ws2r TYPE zvneinv_ws2r.

  CHECK: pa_wreq = &apos;X&apos;         ,
         ps_ws2r IS NOT INITIAL.

  WRITE: /,/ &apos;*** ws-Request data(Cancel):&apos;.

  &quot;line structures
  SELECT * FROM dd03l INTO TABLE @DATA(lt_dd2r) WHERE tabname EQ &apos;ZVNEINV_WS2R&apos;       AND datatype NE &apos;TTYP&apos;.
  SELECT * FROM dd03l INTO TABLE @DATA(lt_dd2d) WHERE tabname EQ &apos;ZVNEINV_WS2R_DLIST&apos;.
  SELECT * FROM dd03l INTO TABLE @DATA(lt_dd2t) WHERE tabname EQ &apos;ZVNEINV_WS2R_TLIST&apos;.

  WRITE: / .
  &quot;invoice
  LOOP AT lt_dd2r ASSIGNING FIELD-SYMBOL(&lt;dd2r&gt;).
    ASSIGN COMPONENT &lt;dd2r&gt;-fieldname OF STRUCTURE ps_ws2r TO FIELD-SYMBOL(&lt;fl2r&gt;).
    WRITE: / &apos;                      &apos;,  &lt;dd2r&gt;-fieldname(20), &apos;:&apos;, &lt;fl2r&gt;.
  ENDLOOP.


ENDFORM.

*&amp;---------------------------------------------------------------------*
*&amp; Form write_ws1s
*&amp;---------------------------------------------------------------------*
*&amp; write response1: invoice
*&amp;---------------------------------------------------------------------*
FORM write_ws1s  USING ps_ws1s TYPE zvneinv_ws1s.

  CHECK: pa_wrsp = &apos;X&apos;         ,
         ps_ws1s IS NOT INITIAL.

  WRITE: /,/ &apos;*** ws-Response data(Invoice):&apos;.

  &quot;line structures
  SELECT * FROM dd03l INTO TABLE @DATA(lt_dd1s) WHERE tabname EQ &apos;ZVNEINV_WS1S&apos;.

  WRITE: / .
  &quot;cancel response
  LOOP AT lt_dd1s ASSIGNING FIELD-SYMBOL(&lt;dd1s&gt;).
    ASSIGN COMPONENT &lt;dd1s&gt;-fieldname OF STRUCTURE ps_ws1s TO FIELD-SYMBOL(&lt;fl1s&gt;).
    WRITE: / &apos;                      &apos;,  &lt;dd1s&gt;-fieldname(20), &apos;:&apos;, &lt;fl1s&gt;.
  ENDLOOP.


ENDFORM.



*&amp;---------------------------------------------------------------------*
*&amp; Form write_ws2s
*&amp;---------------------------------------------------------------------*
*&amp; write response2: cancel
*&amp;---------------------------------------------------------------------*
FORM write_ws2s  USING ps_ws2s TYPE zvneinv_ws2s.

  CHECK: pa_wrsp = &apos;X&apos;         ,
         ps_ws2s IS NOT INITIAL.

  WRITE: /,/ &apos;*** ws-Response data(Cancel):&apos;.

  &quot;line structures
  SELECT * FROM dd03l INTO TABLE @DATA(lt_dd2s) WHERE tabname EQ &apos;ZVNEINV_WS2S&apos;.

  WRITE: / .
  &quot;cancel response
  LOOP AT lt_dd2s ASSIGNING FIELD-SYMBOL(&lt;dd2s&gt;).
    ASSIGN COMPONENT &lt;dd2s&gt;-fieldname OF STRUCTURE ps_ws2s TO FIELD-SYMBOL(&lt;fl2s&gt;).
    WRITE: / &apos;                      &apos;,  &lt;dd2s&gt;-fieldname(20), &apos;:&apos;, &lt;fl2s&gt;.
  ENDLOOP.


ENDFORM.</source>
