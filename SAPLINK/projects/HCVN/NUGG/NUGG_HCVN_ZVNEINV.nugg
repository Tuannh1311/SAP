    <textElement ID="S" KEY="PA_RSCC" ENTRY="        re-process success" LENGTH="26 "/>
    <textElement ID="S" KEY="PA_SUCC" ENTRY="        remove Errors when Success" LENGTH="34 "/>
    <textElement ID="S" KEY="PA_WIFL" ENTRY="        write log IFLE" LENGTH="22 "/>
    <textElement ID="S" KEY="PA_WISL" ENTRY="        write selected Invoices" LENGTH="31 "/>
