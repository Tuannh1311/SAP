  <method CLSNAME="ZVNEINV_IFPE" CMPNAME="CONV_AMNT" VERSION="1" LANGU="E" DESCRIPT="conv: amount per currency" EXPOSURE="0" STATE="1" EDITORDER="16 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZVNEINV_IFPE" CMPNAME="CONV_AMNT" SCONAME="IV_AMNT" VERSION="1" LANGU="E" DESCRIPT="Amount in document currency" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="BSEG-WRBTR"/>
   <parameter CLSNAME="ZVNEINV_IFPE" CMPNAME="CONV_AMNT" SCONAME="RT_AMNT" VERSION="1" LANGU="E" DESCRIPT="Numeric fifteen-digit value" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="NUM15"/>
   <source>METHOD conv_amnt.

    &quot;convert currency
    &quot;(do not user fm &apos;CURRENCY_AMOUNT_SAP_TO_DISPLAY&apos;, overflows for VND)

    DATA: lv_amnt1 TYPE bapicurr-bapicurr,
          lv_amnt2 TYPE bapicurr-bapicurr.

    lv_amnt1 = CONV #( iv_amnt ).

    CALL FUNCTION &apos;BAPI_CURRENCY_CONV_TO_EXTERNAL&apos;
      EXPORTING
        currency        = me-&gt;bkpf-waers
        amount_internal = lv_amnt1
      IMPORTING
        amount_external = lv_amnt2.

    rt_amnt = CONV #( lv_amnt2 ).


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZVNEINV_IFPE" CMPNAME="CONV_TEXT" VERSION="1" LANGU="E" DESCRIPT="conv: text to string (also read text)" EXPOSURE="0" STATE="1" EDITORDER="17 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZVNEINV_IFPE" CMPNAME="CONV_TEXT" SCONAME="IV_TDID" VERSION="1" LANGU="E" DESCRIPT="Text ID" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="STXH-TDID"/>
   <parameter CLSNAME="ZVNEINV_IFPE" CMPNAME="CONV_TEXT" SCONAME="RV_TEXT" VERSION="1" LANGU="E" DESCRIPT="text as string" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="STRING"/>
   <source>METHOD conv_text.

    &quot;long text: name ccode+docno+year
    DATA(lv_name) = CONV thead-tdname( me-&gt;bkpf-bukrs &amp;&amp; me-&gt;bkpf-belnr &amp;&amp; me-&gt;bkpf-gjahr ).

    &quot;check long text existence (also get lang)
    SELECT SINGLE * FROM stxh INTO @DATA(ls_stxh) WHERE tdobject = &apos;BELEG&apos;      &quot;BELEG= Document text
                                                    AND tdname   = @lv_name
                                                    AND tdid     = @iv_tdid   .

    IF sy-subrc = 0.
      DATA: lt_line TYPE TABLE OF tline.
      &quot;..read text
      CALL FUNCTION &apos;READ_TEXT&apos;
        EXPORTING
          id       = ls_stxh-tdid
          language = ls_stxh-tdspras
          name     = ls_stxh-tdname
          object   = ls_stxh-tdobject
        TABLES
          lines    = lt_line.
      &quot;..conv to string
      DATA: lt_c360 TYPE TABLE OF ms_rfc3600.
      CALL FUNCTION &apos;CONVERT_ITF_TO_STREAM_TEXT&apos;
        EXPORTING
          language    = ls_stxh-tdspras
        TABLES
          itf_text    = lt_line
          text_stream = lt_c360.
      READ TABLE lt_c360 ASSIGNING FIELD-SYMBOL(&lt;c360&gt;) INDEX 1. CHECK sy-subrc = 0.

      rv_text = &lt;c360&gt;.

    ENDIF.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZVNEINV_IFPE" CMPNAME="CONV_TEXT_ITEM" VERSION="1" LANGU="E" DESCRIPT="conv: text to string (also read text) item" EXPOSURE="0" STATE="1" EDITORDER="18 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZVNEINV_IFPE" CMPNAME="CONV_TEXT_ITEM" SCONAME="IV_BUZEI" VERSION="1" LANGU="E" DESCRIPT="Number of Line Item Within Accounting Document" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="BSEG-BUZEI"/>
