 <PROG NAME="ZVNEINV_IFPE_SELSC" VARCL="X" SUBC="I" RMAND="700" RLOAD="E" UCCHECK="X">
  <textPool>
   <language SPRAS="E">
    <textElement ID="R" ENTRY="ZVNEINV_IFPE selections screen" LENGTH="30 "/>
   </language>
  </textPool>
  <source>*&amp;---------------------------------------------------------------------*
*&amp; Include          ZVNEINV_IFPE_SELSC
*&amp;---------------------------------------------------------------------*

SELECTION-SCREEN: BEGIN OF BLOCK s00 WITH FRAME TITLE TEXT-s00. &quot;block: Invoice selection
  SELECT-OPTIONS:
                  so_belnr FOR bkpf-belnr,
                  so_gjahr FOR bkpf-gjahr,
                  so_budat FOR bkpf-budat,
                  so_cpudt FOR bkpf-cpudt,
                  so_bukrs FOR bkpf-bukrs,
                  so_blart FOR bkpf-blart.
  PARAMETERS: pa_succ TYPE xfeld DEFAULT &apos;X&apos;,
              pa_rerr TYPE xfeld DEFAULT &apos; &apos;,
              pa_rscc TYPE xfeld DEFAULT &apos; &apos;,
              pa_exr1 TYPE xfeld DEFAULT &apos;X&apos;,
              pa_exr2 TYPE xfeld DEFAULT &apos;X&apos;.
SELECTION-SCREEN: END   OF BLOCK s00.

SELECTION-SCREEN: BEGIN OF BLOCK s01 WITH FRAME TITLE TEXT-s01. &quot;block: Processing mode
  PARAMETERS: px_test  TYPE xfeld DEFAULT &apos;X&apos;,
              pa_wisl  TYPE xfeld DEFAULT &apos; &apos;,
              pa_wifl  TYPE xfeld DEFAULT &apos;X&apos;,
              pa_wreq  TYPE xfeld DEFAULT &apos; &apos;,
              pa_wrsp  TYPE xfeld DEFAULT &apos; &apos;,
              pa_wsep  TYPE xfeld DEFAULT &apos; &apos;,
              pa_port1 TYPE prx_logical_port_name OBLIGATORY,
              pa_port2 TYPE prx_logical_port_name OBLIGATORY.
SELECTION-SCREEN: END   OF BLOCK s01.

AT SELECTION-SCREEN ON VALUE-REQUEST FOR pa_port1.
  &quot;get shlp of WS Ports
  pa_port1 = cl_proxy_adapter_test=&gt;outbound_lpt_f4_help( &apos;ZVNEINV_CO_OSB_WS&apos; ).

AT SELECTION-SCREEN ON VALUE-REQUEST FOR pa_port2.
  &quot;get shlp of WS Ports
  pa_port2 = cl_proxy_adapter_test=&gt;outbound_lpt_f4_help( &apos;ZVNEINV_CO_OSB_WS&apos; ).</source>
 </PROG>
 <PROG NAME="ZVNEINV_IFPE" VARCL="X" SUBC="1" RMAND="700" RLOAD="E" FIXPT="X" UCCHECK="X">
  <textPool>
   <language SPRAS="E">
    <textElement ID="I" KEY="S00" ENTRY="selection: Invoice Documents" LENGTH="56 "/>
    <textElement ID="I" KEY="S01" ENTRY="processing" LENGTH="20 "/>
    <textElement ID="I" KEY="T01" ENTRY="*** Selection parameters:" LENGTH="50 "/>
    <textElement ID="I" KEY="TS0" ENTRY="*** Selection parameters:" LENGTH="50 "/>
    <textElement ID="R" ENTRY="eInvoice: Invoice/Cancel interface send" LENGTH="39 "/>
    <textElement ID="S" KEY="PA_EXR1" ENTRY="        excl invoice reversed" LENGTH="29 "/>
    <textElement ID="S" KEY="PA_EXR2" ENTRY="        excl cancel if invo not sent" LENGTH="36 "/>
    <textElement ID="S" KEY="PA_PORT1" ENTRY="        logical port: ws-invoice" LENGTH="32 "/>
    <textElement ID="S" KEY="PA_PORT2" ENTRY="        logical port: ws-cancel" LENGTH="31 "/>
