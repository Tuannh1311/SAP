    &quot;check long text existence (also get lang)
    SELECT SINGLE * FROM stxh INTO @DATA(ls_stxh) WHERE tdobject = &apos;DOC_ITEM&apos;      &quot;DOC_ITEM= Line item texts
                                                    AND tdname   = @lv_name
                                                    AND tdid     = @iv_tdid   .

    IF sy-subrc = 0.
      DATA: lt_line TYPE TABLE OF tline.
      &quot;..read text
      CALL FUNCTION &apos;READ_TEXT&apos;
        EXPORTING
          id       = ls_stxh-tdid
          language = ls_stxh-tdspras
          name     = ls_stxh-tdname
          object   = ls_stxh-tdobject
        TABLES
          lines    = lt_line.
      &quot;..conv to string
      DATA: lt_c360 TYPE TABLE OF ms_rfc3600.
      CALL FUNCTION &apos;CONVERT_ITF_TO_STREAM_TEXT&apos;
        EXPORTING
          language    = ls_stxh-tdspras
        TABLES
          itf_text    = lt_line
          text_stream = lt_c360.
      READ TABLE lt_c360 ASSIGNING FIELD-SYMBOL(&lt;c360&gt;) INDEX 1. CHECK sy-subrc = 0.

      rv_text = &lt;c360&gt;.

    ENDIF.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZVNEINV_IFPE" CMPNAME="DATA_DEQU" VERSION="1" LANGU="E" DESCRIPT="data: dequeue" EXPOSURE="0" STATE="1" EDITORDER="15 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD data_dequ.

    &quot;data: dequeue
    CALL FUNCTION &apos;DEQUEUE_EFBKPF&apos;
      EXPORTING
*       MODE_BKPF       = &apos;E&apos;
*       MANDT = SY-MANDT
        bukrs = me-&gt;isel-bukrs
        belnr = me-&gt;isel-belnr
        gjahr = me-&gt;isel-gjahr
*       X_BUKRS         = &apos; &apos;
*       X_BELNR         = &apos; &apos;
*       X_GJAHR         = &apos; &apos;
*       _SCOPE          = &apos;3&apos;
*       _SYNCHRON       = &apos; &apos;
*       _COLLECT        = &apos; &apos;
      .


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZVNEINV_IFPE" CMPNAME="DATA_ENQU" VERSION="1" LANGU="E" DESCRIPT="data: enqueue" EXPOSURE="0" STATE="1" EDITORDER="4 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD data_enqu.

    &quot;data: enqueue
    &quot;(needed for later update of fdoc)

    CHECK: me-&gt;isel IS NOT INITIAL.

    CALL FUNCTION &apos;ENQUEUE_EFBKPF&apos;
      EXPORTING
*       MODE_BKPF      = &apos;E&apos;
*       MANDT          = SY-MANDT
        bukrs          = me-&gt;isel-bukrs
        belnr          = me-&gt;isel-belnr
        gjahr          = me-&gt;isel-gjahr
*       X_BUKRS        = &apos; &apos;
*       X_BELNR        = &apos; &apos;
*       X_GJAHR        = &apos; &apos;
*       _SCOPE         = &apos;2&apos;
*       _WAIT          = &apos; &apos;
*       _COLLECT       = &apos; &apos;
      EXCEPTIONS
        foreign_lock   = 1
        system_failure = 2
        OTHERS         = 3.
    IF sy-subrc = 0.
      me-&gt;ifle-wssta = &apos; &apos;.
    ELSE.
      MOVE: sy-datum TO me-&gt;ifle-rdate,
            sy-uzeit TO me-&gt;ifle-rtime,
            &apos;E&apos;      TO me-&gt;ifle-wssta.
      MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4 INTO me-&gt;ifle-wsmsg.
    ENDIF.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZVNEINV_IFPE" CMPNAME="DATA_READ" VERSION="1" LANGU="E" DESCRIPT="get fi-document/customer/&amp;tc" EXPOSURE="0" STATE="1" EDITORDER="1 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD data_read.

    &quot;fi-doc head
    SELECT SINGLE * FROM bkpf INTO @me-&gt;bkpf WHERE bukrs = @me-&gt;isel-bukrs
                                               AND belnr = @me-&gt;isel-belnr
                                               AND gjahr = @me-&gt;isel-gjahr.

    &quot;cancel: read reversed document, not reversal
    &quot;        (ie we are resending original document as cancelled..
    &quot;            ..not the cancellation document itselfi           )
    IF me-&gt;bkpf-xreversed = &apos;2&apos;.

      &quot;fi-doc head: orignal, reversed
      SELECT SINGLE * FROM bkpf INTO @me-&gt;bkpf WHERE bukrs = @me-&gt;bkpf-bukrs
                                                 AND belnr = @me-&gt;bkpf-stblg
                                                 AND gjahr = @me-&gt;bkpf-stjah.

    ENDIF.


    &quot;fi-doc items (either invo or cancel, as bkpf above)
    SELECT * FROM bseg INTO TABLE @me-&gt;bsegt WHERE bukrs = @me-&gt;bkpf-bukrs
                                               AND belnr = @me-&gt;bkpf-belnr
                                               AND gjahr = @me-&gt;bkpf-gjahr.

    &quot;fi-doc taxes
    SELECT * FROM bset INTO TABLE @me-&gt;bsett WHERE bukrs = @me-&gt;bkpf-bukrs
                                               AND belnr = @me-&gt;bkpf-belnr
                                               AND gjahr = @me-&gt;bkpf-gjahr
                                               AND ktosl = &apos;MWS&apos;.

    &quot;fi-doc customer item
    SELECT SINGLE * FROM bseg INTO @me-&gt;bsegd WHERE bukrs = @me-&gt;bkpf-bukrs
                                                AND belnr = @me-&gt;bkpf-belnr
                                                AND gjahr = @me-&gt;bkpf-gjahr
                                                AND koart = &apos;D&apos;.            &quot;D= customer

    &quot;customer
    SELECT SINGLE * FROM kna1 INTO @me-&gt;kna1 WHERE kunnr = @me-&gt;bsegd-kunnr.

    &quot;customer address
    SELECT SINGLE * FROM adrc INTO @me-&gt;adrc WHERE addrnumber = @me-&gt;kna1-adrnr.
    SELECT SINGLE * FROM adr2 INTO @me-&gt;adr2 WHERE addrnumber = @me-&gt;kna1-adrnr AND flgdefault = &apos;X&apos;.
    SELECT SINGLE * FROM adr6 INTO @me-&gt;adr6 WHERE addrnumber = @me-&gt;kna1-adrnr AND flgdefault = &apos;X&apos;.
    SELECT * FROM adr6 INTO TABLE  @me-&gt;adr6t WHERE addrnumber = @me-&gt;kna1-adrnr.

    &quot;customer bank account
    SELECT SINGLE * FROM knbk INTO @me-&gt;knbk WHERE kunnr = @me-&gt;bsegd-kunnr
                                               AND bvtyp = @me-&gt;bsegd-bvtyp.

    &quot;bank
    SELECT SINGLE * FROM bnka INTO @me-&gt;bnka WHERE banks = @me-&gt;knbk-banks
                                               AND bankl = @me-&gt;knbk-bankl.

    &quot;user address
    DATA: lt_ret2 TYPE TABLE OF bapiret2.
    CALL FUNCTION &apos;BAPI_USER_GET_DETAIL&apos;
      EXPORTING
        username = me-&gt;bkpf-usnam
      IMPORTING
        address  = me-&gt;uadr
      TABLES
        return   = lt_ret2.

    &quot;business partner
    SELECT SINGLE * FROM but000 INTO @me-&gt;but0 WHERE partner = @me-&gt;kna1-kunnr.




  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZVNEINV_IFPE" CMPNAME="IFLE_INIT" VERSION="1" LANGU="E" DESCRIPT="init IFLE: determine invoice/cancel" EXPOSURE="0" STATE="1" EDITORDER="3 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD IFLE_INIT.

    &quot;ifle: document
    MOVE: &quot;sy-datum        to me-&gt;ifle-rdate, &quot;later, just prior ws-call..
          &quot;sy-uzeit        to me-&gt;ifle-rtime, &quot;..to match with srt_util log
          me-&gt;isel-bukrs  TO me-&gt;ifle-bukrs,
          me-&gt;isel-belnr  TO me-&gt;ifle-belnr,
          me-&gt;isel-gjahr  TO me-&gt;ifle-gjahr.

    &quot;determine: invoice/cancel
    CASE me-&gt;bkpf-xreversal.
      WHEN &apos; &apos;. MOVE: &apos;I&apos; TO me-&gt;ifle-wstyp. &quot;_=normal   -&gt; I=invoice
      WHEN &apos;1&apos;. MOVE: &apos;I&apos; TO me-&gt;ifle-wstyp. &quot;1=reversed -&gt; I=invoice (though testing only)
      WHEN &apos;2&apos;. MOVE: &apos;C&apos; TO me-&gt;ifle-wstyp. &quot;2=reversal -&gt; C=cancel
    ENDCASE.





  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZVNEINV_IFPE" CMPNAME="IFLE_PREP" VERSION="1" LANGU="E" DESCRIPT="prepare IFLE-log from response(s) invoice/cancel" EXPOSURE="0" STATE="1" EDITORDER="13 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD IFLE_PREP.

    &quot;not for test mode
    CHECK me-&gt;xtst EQ &apos; &apos;.

    &quot;prepare IFLE log from ws-response
    MOVE: sy-datum   TO ifle-rdate,
          sy-uzeit   TO ifle-rtime,
          isel-bukrs TO ifle-bukrs,
          isel-belnr TO ifle-belnr,
          isel-gjahr TO ifle-gjahr.

    CASE me-&gt;ifle-wstyp.

      WHEN &apos;I&apos;. &quot;I= invoice

        MOVE: ws1s       TO ifle-_rsp.
        MOVE: ws1r-isprs TO ifle-_sta-isprs.

        IF ws1s-tcodr IS NOT INITIAL.
          ifle-_sta-wssta = &apos;S&apos;.
        ELSE.
          ifle-_sta-wssta = &apos;E&apos;.
        ENDIF.
        MOVE: ws1s-descr TO ifle-_sta-wsmsg.

      WHEN &apos;C&apos;. &quot;C= cancel

        MOVE-CORRESPONDING: ws2s TO ifle-_rsp.
        MOVE: ws2r-isprs TO ifle-_sta-isprs.

        IF ws2s-tcodr IS NOT INITIAL.
          ifle-_sta-wssta = &apos;S&apos;.
        ELSE.
          ifle-_sta-wssta = &apos;E&apos;.
        ENDIF.
        MOVE: ws2s-descr TO ifle-_sta-wsmsg.

    ENDCASE.





  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZVNEINV_IFPE" CMPNAME="IFLE_SAVE" VERSION="1" LANGU="E" DESCRIPT="save IFLI-log to database" EXPOSURE="0" STATE="1" EDITORDER="14 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD IFLE_SAVE.

    CHECK: me-&gt;xtst IS     INITIAL,
           me-&gt;ifle IS NOT INITIAL.

    INSERT zvneinv_ifle FROM me-&gt;ifle.

    CALL FUNCTION &apos;BAPI_TRANSACTION_COMMIT&apos;. &quot;(default commit, see check of test mode above)

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZVNEINV_IFPE" CMPNAME="PREP_WS1R" VERSION="1" LANGU="E" DESCRIPT="prepare request: invoice" EXPOSURE="0" STATE="1" EDITORDER="5 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD prep_ws1r.

    &quot;(HQ_TTa: yeah, I know, this code is seriously ripe for some cleansing &amp; refactoring..
    &quot;         ..lots of changes done under time pressure during UAT; so later, time permitting )

    &quot;for invoice
    CHECK: me-&gt;ifle-wstyp = &apos;I&apos;. &quot;I= invoice

    ASSIGN me-&gt;ws1r TO FIELD-SYMBOL(&lt;ws1r&gt;).

    &quot;tax id: from config
    MOVE: me-&gt;c000-taxid TO &lt;ws1r&gt;-taxid.

    &quot;form/template/code: from fi-doc head text (see config ZVNFIAR_CBKTXT)
    &quot;                                           eg= 01GTKT0/001**AA/20E
    &lt;ws1r&gt;-iform = me-&gt;bkpf-bktxt+0(6)  .       &quot; = 01GTKT
    &lt;ws1r&gt;-templ = me-&gt;bkpf-bktxt+0(11) .       &quot; = 01GTKT0/001
    &lt;ws1r&gt;-icode = me-&gt;bkpf-bktxt+13(6) .       &quot; =              AA/20E


    &quot;sophieu/note number: not used
    &lt;ws1r&gt;-nnote = &apos;&apos;.

    &quot;ma_hoadon/invoice number: full key ccode/docno/year
    &lt;ws1r&gt;-invno = |{ me-&gt;bkpf-bukrs }/{ me-&gt;bkpf-belnr }/{ me-&gt;bkpf-gjahr }|.

    &quot;hoadon_goc/original invoice number: for adjustment, inv reference, orig RedInvo
    &quot;..actually original ccode/docno/year
    IF me-&gt;bsegd-rebzg IS NOT INITIAL.
      &lt;ws1r&gt;-oinvn = |{ me-&gt;bkpf-bukrs }/{ me-&gt;bsegd-rebzg }/{ me-&gt;bsegd-rebzj }|.
      &quot;***      SELECT SINGLE xblnr FROM bkpf INTO @&lt;ws1r&gt;-oinvn WHERE bukrs = @me-&gt;bkpf-bukrs
      &quot;***                                                         AND belnr = @me-&gt;bsegd-rebzg
      &quot;***                                                         AND gjahr = @me-&gt;bsegd-rebzj.
    ENDIF.

    &quot;ngaylap/issuing date: posting date &amp; time YYYY-MM-DD hh:mm:ss
    &quot;                      (the CPUTM is actually updated after posting)
    &lt;ws1r&gt;-isdat = |{ me-&gt;bkpf-budat DATE = ISO  } { me-&gt;bkpf-cputm TIME = ISO  }|.

    &quot;dnmua_mst/buyer tax identity number: customer taxno vn1
    &lt;ws1r&gt;-btxin = me-&gt;kna1-stcd1.

    &quot;dnmua_ten/buyer organization name: customer name
    CASE me-&gt;but0-type.
      WHEN &apos;2&apos;. &quot;2= Organization
        &lt;ws1r&gt;-bonam = |{ me-&gt;but0-name_org1 }{ me-&gt;but0-name_org2 }{ me-&gt;but0-name_org3 }{ me-&gt;but0-name_org4 }|.
      WHEN &apos;3&apos;. &quot;1= Person
        &lt;ws1r&gt;-bonam = |{ me-&gt;but0-name_first } { me-&gt;but0-namemiddle } { me-&gt;but0-name_last }|.
    ENDCASE.
    CONDENSE &lt;ws1r&gt;-bonam.

    &quot;++SAP-3601 SAP Einvoice Space name cropped
    &quot;  introduced in SAP-2993, for HomeSIS -&gt; SAP we need to replace soft-space with hard-space..
    &quot;  ..but then we have to fix it back for eInvoice; so replaced again back
    REPLACE ALL OCCURRENCES OF ` ` IN &lt;ws1r&gt;-bonam WITH  ` `.


    &quot;street1/2/3/4 separated by commas
    DATA: lv_street TYPE text255.
    lv_street = me-&gt;adrc-street.
    IF me-&gt;adrc-str_suppl1 IS NOT INITIAL. lv_street = |{ lv_street }, { me-&gt;adrc-str_suppl1 }|. ENDIF.
    IF me-&gt;adrc-str_suppl2 IS NOT INITIAL. lv_street = |{ lv_street }, { me-&gt;adrc-str_suppl2 }|. ENDIF.
    IF me-&gt;adrc-str_suppl3 IS NOT INITIAL. lv_street = |{ lv_street }, { me-&gt;adrc-str_suppl3 }|. ENDIF.

    &quot;city1/2: separated by commas
    &quot;also city2 ~district, so like &apos;city2, city1&apos;
    DATA: lv_city TYPE text255.
    lv_city = me-&gt;adrc-city1.
    IF me-&gt;adrc-city2      IS NOT INITIAL. lv_city   = |{ me-&gt;adrc-city2 }, { lv_city   }     |. ENDIF.
    &quot;SAP-4127 by HQ_IKH - added space btw house_num1 and house_num2
    &quot;roll back changes - 23/06/21
    &lt;ws1r&gt;-baddr = |{ me-&gt;adrc-house_num1 }{ me-&gt;adrc-house_num2 } { lv_street }, { lv_city } |.
    &quot;&lt;ws1r&gt;-baddr = |{ me-&gt;adrc-house_num1 } { me-&gt;adrc-house_num2 } { lv_street }, { lv_city } |.
    CONDENSE &lt;ws1r&gt;-baddr.

    &quot;SAP-3958 SAP FICO E-invoice misssing Space in House Numer and Supplement House number
    &quot;(like SAP-2993 SAP_Einvoice_Mising space name during Interface between HSIS and SAP)
    &quot;  for HomeSIS -&gt; SAP we need to replace soft-space with hard-space..
    &quot;  ..but then we have to fix it back for eInvoice; so replaced again back
    REPLACE ALL OCCURRENCES OF ` ` IN &lt;ws1r&gt;-baddr WITH  ` `.

    &quot;dnmua_sdt/buyer phone: customer phone
    &lt;ws1r&gt;-bphon = me-&gt;kna1-telf1.

    &quot;dnmua_email/email: customer emails (list all)
    LOOP AT me-&gt;adr6t ASSIGNING FIELD-SYMBOL(&lt;adr6&gt;).
      &lt;ws1r&gt;-email = &lt;ws1r&gt;-email &amp;&amp; &apos;,&apos; &amp;&amp; &lt;adr6&gt;-smtp_addr.
    ENDLOOP.
    CONDENSE &lt;ws1r&gt;-email.
    REPLACE FIRST OCCURRENCE OF &apos;,&apos; IN &lt;ws1r&gt;-email WITH &apos;&apos;.

    &quot;thanhtoan_phuongthuc/payment type: map config
    &quot;thanhtoan_phuongthuc_ten/payment term: pay-term description (=actually pmnt-type description)
    READ TABLE me-&gt;c001t ASSIGNING FIELD-SYMBOL(&lt;c001&gt;) WITH KEY bukrs = me-&gt;t001-bukrs
                                                                 land1 = me-&gt;t001-land1
                                                                 zlsch = me-&gt;bsegd-zlsch.
    IF sy-subrc = 0.
      &lt;ws1r&gt;-ptype = &lt;c001&gt;-ptype.
      &lt;ws1r&gt;-pterm = &lt;c001&gt;-ptype_tx.
    ELSE.
      &lt;ws1r&gt;-ptype = &apos;2&apos;.             &quot;default 2= bank transfer
      &lt;ws1r&gt;-pterm = &apos;Chuyển khoản&apos;.  &quot;default Chuyển khoản = bank transfer
    ENDIF.

    &quot;thanhtoan_phuongthuc_ten/payment term: pay-term description
    &quot;&lt;ws1r&gt;-pterm = me-&gt;t052u-text1.
    &quot;(actually this is used for payment method description, not pay-term)


    &quot;thanhtoan_taikhoan/bank account
    &lt;ws1r&gt;-bkacc = me-&gt;knbk-bankn.

    &quot;thanhtoan_nganhang/bank name
    &lt;ws1r&gt;-bknam = me-&gt;bnka-banka.

    &quot;tiente_ma/currency
    &lt;ws1r&gt;-currc = me-&gt;bkpf-waers.

    &quot;tygiangoaite/exchange rate
    &lt;ws1r&gt;-exchr = me-&gt;bkpf-kursf.

    &quot;thanhtoan_thoihan/payment due date:
    DATA: lv_date TYPE datum.
    lv_date = me-&gt;bsegd-zfbdt + me-&gt;bsegd-zbd1t.
    &lt;ws1r&gt;-pmdue  = |{ lv_date DATE = ISO }|.

    &quot;tongtien_chietkhau/discount amount: not used
    &lt;ws1r&gt;-amdsc = &apos;0&apos;. &quot;(but mandatory, so 0.00)

    &quot;ghichu/remark: not used
    &lt;ws1r&gt;-rmark = &apos;&apos;.

    &quot;tongtien_chuavat/total amount: total w/o vat
    DATA: lv_tamnt LIKE me-&gt;bsegd-wrbtr.
    &quot;summarised from BSET tax items
    LOOP AT me-&gt;bsett ASSIGNING FIELD-SYMBOL(&lt;bset&gt;).
      ADD &lt;bset&gt;-fwbas TO lv_tamnt.
    ENDLOOP.
    &lt;ws1r&gt;-tamnt = me-&gt;conv_amnt( iv_amnt = lv_tamnt ).

    &quot;tienthue/vat amount
    DATA: lv_vamnt LIKE me-&gt;bsegd-wrbtr.
    &quot;summarised from BSET tax items
    LOOP AT me-&gt;bsett ASSIGNING &lt;bset&gt;.
      ADD &lt;bset&gt;-fwste TO lv_vamnt.
    ENDLOOP.
    &lt;ws1r&gt;-vamnt = me-&gt;conv_amnt( lv_vamnt ).

    &quot;tongtien_covat/total payment
    &lt;ws1r&gt;-tpmnt = me-&gt;conv_amnt( me-&gt;bsegd-wrbtr ).


    &quot;nguoilap/invoice issuing person
    &lt;ws1r&gt;-isprs = me-&gt;uadr-fullname.

    &quot;adjustment notes
    &lt;ws1r&gt;-adjrt = me-&gt;conv_text( iv_tdid = &apos;VN01&apos; ). &quot;VN01= Adjustment reason
    &lt;ws1r&gt;-adjcb = me-&gt;conv_text( iv_tdid = &apos;VN02&apos; ). &quot;VN02= Content before adjustment
    &lt;ws1r&gt;-adjca = me-&gt;conv_text( iv_tdid = &apos;VN03&apos; ). &quot;VN03= Content after adjustment



    &quot;***item list
    DATA: lv_ordno TYPE bseg-buzei.
    CLEAR: lv_ordno.
    LOOP AT me-&gt;bsegt ASSIGNING FIELD-SYMBOL(&lt;ibseg&gt;) WHERE koart NE &apos;D&apos;  &quot;not D= customer
                                                        AND buzid NE &apos;T&apos;  &quot;not T= tax item
                                                        AND mwskz NE &apos;&apos; . &quot;not without tax code (special items)

      APPEND INITIAL LINE TO &lt;ws1r&gt;-dlistt ASSIGNING FIELD-SYMBOL(&lt;dlist&gt;).

      &quot;stt/ordinal number
      &quot;&lt;dlist&gt;-ordno = &lt;ibseg&gt;-buzei.
      &quot;force numbering from one, not fi item number
      ADD 1 TO lv_ordno.
      &lt;dlist&gt;-ordno = lv_ordno.


      &quot;hanghoa_loai/goods type
      &lt;dlist&gt;-gdsty = &apos;1&apos;.     &quot;+1= Service

      &quot;khuyenmai/promotion
      &lt;dlist&gt;-promo = &apos;0&apos;.     &quot;+0= Not promoted

      &quot;dieuchinh_tanggiam/adjust
      IF me-&gt;bsegd-rebzg IS NOT INITIAL.  &quot;reference exist
        CASE &lt;ibseg&gt;-shkzg.
          WHEN &apos;S&apos;. &lt;dlist&gt;-adjst = &apos;0&apos;. &quot;S= Debit  =&gt; +0 decrease
          WHEN &apos;H&apos;. &lt;dlist&gt;-adjst = &apos;1&apos;. &quot;H= Credit =&gt; +1 increase
        ENDCASE.
      ENDIF.

      &quot;ma/goods number: not used
      &lt;dlist&gt;-gdsno = &apos;&apos;.

      &quot;ten/goods name: item text
      &quot;&lt;dlist&gt;-gdsnm = &lt;ibseg&gt;-sgtxt.
      &quot;long text, not short text
      &lt;dlist&gt;-gdsnm = me-&gt;conv_text_item( iv_tdid = &apos;0001&apos; iv_buzei = &lt;ibseg&gt;-buzei ). &quot;VN03= Content after adjustment
      &quot;short text if long text is missing
      IF &lt;dlist&gt;-gdsnm IS INITIAL.
        &lt;dlist&gt;-gdsnm = &lt;ibseg&gt;-sgtxt.
      ENDIF.

      &quot;donvitinh/unit: not used
      &lt;dlist&gt;-unit = &apos;&apos;.

      &quot;soluong/quantity: not used (serv only, no goods)
      &lt;dlist&gt;-qtty = &apos;&apos;.

      &quot;dongia/price: not used (serv only, no goods)
      &lt;dlist&gt;-price = &apos;&apos;.

      &quot;phantram_chietkhau/discount percent: not used
      &lt;dlist&gt;-dscpr = &apos;&apos;.

      &quot;tongtien_chietkhau/discount amount: not used
      &lt;dlist&gt;-dscam = &apos;&apos;.

      &quot;tongtien_chuathue/amount before vat: item amount
      &lt;dlist&gt;-abvat = me-&gt;conv_amnt( &lt;ibseg&gt;-wrbtr ).

      &quot;mathue/vat: map vat code
      READ TABLE me-&gt;c002t ASSIGNING FIELD-SYMBOL(&lt;c002&gt;) WITH KEY bukrs = me-&gt;bkpf-bukrs
                                                                   mwskz = &lt;ibseg&gt;-mwskz.
      IF sy-subrc = 0. &lt;dlist&gt;-acvat = &lt;c002&gt;-acvat. ENDIF.

      &quot;tongtien_cothue/amount after vat: item level, so calculate
      DATA: lt_mwdat TYPE TABLE OF rtax1u15.
      DATA: lv_fweste TYPE bset-fwste.

      &quot;gross-vs-net calculation
      CASE &lt;c002&gt;-xgros.
        WHEN &apos; &apos;. &quot;net-tax calculation
          CALL FUNCTION &apos;CALCULATE_TAX_FROM_NET_AMOUNT&apos;
            EXPORTING
              i_bukrs           = me-&gt;bkpf-bukrs
              i_mwskz           = &lt;ibseg&gt;-mwskz
              i_waers           = me-&gt;bkpf-waers
              i_wrbtr           = &lt;ibseg&gt;-wrbtr
            IMPORTING
              e_fwste           = lv_fweste
            TABLES
              t_mwdat           = lt_mwdat
            EXCEPTIONS
              bukrs_not_found   = 1
              country_not_found = 2
              mwskz_not_defined = 3
              mwskz_not_valid   = 4
              ktosl_not_found   = 5
              kalsm_not_found   = 6
              parameter_error   = 7
              knumh_not_found   = 8
              kschl_not_found   = 9
              unknown_error     = 10
              account_not_found = 11
              txjcd_not_valid   = 12
              OTHERS            = 13.
          IF sy-subrc &lt;&gt; 0. MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4. ENDIF.

        WHEN &apos;X&apos;. &quot;gross-tax calculation
          CALL FUNCTION &apos;CALCULATE_TAX_FROM_GROSSAMOUNT&apos;
            EXPORTING
              i_bukrs                   = me-&gt;bkpf-bukrs
              i_mwskz                   = &lt;ibseg&gt;-mwskz
              i_waers                   = me-&gt;bkpf-waers
              i_wrbtr                   = &lt;ibseg&gt;-wrbtr
            IMPORTING
              e_fwste                   = lv_fweste
            TABLES
              t_mwdat                   = lt_mwdat
            EXCEPTIONS
              bukrs_not_found           = 1
              country_not_found         = 2
              mwskz_not_defined         = 3
              mwskz_not_valid           = 4
              account_not_found         = 5
              different_discount_base   = 6
              different_tax_base        = 7
              txjcd_not_valid           = 8
              not_found                 = 9
              ktosl_not_found           = 10
              kalsm_not_found           = 11
              parameter_error           = 12
              knumh_not_found           = 13
              kschl_not_found           = 14
              unknown_error             = 15
              amounts_too_large_for_tax = 16
              OTHERS                    = 17.
          IF sy-subrc &lt;&gt; 0. MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4. ENDIF.
      ENDCASE.
      DATA(lv_tax)  = me-&gt;conv_amnt( lv_fweste ).
      DATA(lv_base) = me-&gt;conv_amnt( &lt;ibseg&gt;-wrbtr ).

      &quot;beforeVAT/afterVAT: by net/gross tax
      CASE &lt;c002&gt;-xgros.
        WHEN &apos; &apos;. &quot;net-tax calculation
          &quot;beforeVAT: base
          &lt;dlist&gt;-abvat = lv_base.
          &quot;afterVAT = tax + base
          &lt;dlist&gt;-aavat = lv_tax + lv_base.

        WHEN &apos;X&apos;. &quot;gross-tax calculation
          &quot;beforeVAT: base - tax
          &lt;dlist&gt;-abvat = lv_base - lv_tax.
          &quot;afterVAT: base itself
          &lt;dlist&gt;-aavat = lv_base.

      ENDCASE.

      &quot;special logic: no amounts for items with zero tax-codes..
      &quot;               ..as defined in fin-table ZVNFI_ZERO_TAX &apos;Tax Report Zero invoice relevant documents&apos;
      READ TABLE me-&gt;ztax WITH KEY blart = me-&gt;bkpf-blart
                                   mwskz = &lt;ibseg&gt;-mwskz  TRANSPORTING NO FIELDS.
      IF sy-subrc = 0.
        CLEAR: &lt;dlist&gt;-abvat,
               &lt;dlist&gt;-aavat.
      ENDIF.

    ENDLOOP.


    &quot;***tax list: based on bset
    LOOP AT me-&gt;bsett ASSIGNING &lt;bset&gt;.

      APPEND INITIAL LINE TO &lt;ws1r&gt;-tlistt ASSIGNING FIELD-SYMBOL(&lt;tlist&gt;).

      &quot;mathue/vat: map vat code
      READ TABLE me-&gt;c002t ASSIGNING &lt;c002&gt; WITH KEY bukrs = me-&gt;bkpf-bukrs
                                                     mwskz = &lt;bset&gt;-mwskz.
      IF sy-subrc = 0. &lt;tlist&gt;-vatco = &lt;c002&gt;-acvat. ENDIF.

      &quot;tongtien_chiuthue/gross amount before tax
      &lt;tlist&gt;-gabtx = me-&gt;conv_amnt( &lt;bset&gt;-fwbas ).

      &quot;tongtien_thue/total amounts of tax paid
      &lt;tlist&gt;-tamtx = me-&gt;conv_amnt( &lt;bset&gt;-fwste ).

      &quot;dieuchinh_tanggiam/adjust
      IF me-&gt;bsegd-rebzg IS NOT INITIAL.  &quot;reference exist
        CASE &lt;bset&gt;-shkzg.
          WHEN &apos;S&apos;.
            &lt;tlist&gt;-adjst = &apos;0&apos;. &quot;S= Debit  =&gt; +0 decrease
          WHEN &apos;H&apos;.
            &lt;tlist&gt;-adjst = &apos;1&apos;. &quot;H= Credit =&gt; +1 increase
        ENDCASE.
      ENDIF.

      &quot;special logic: no amounts for items with zero tax-codes..
      &quot;               ..as defined in fin-table ZVNFI_ZERO_TAX &apos;Tax Report Zero invoice relevant documents&apos;
      READ TABLE me-&gt;ztax WITH KEY blart = me-&gt;bkpf-blart
                                   mwskz = &lt;bset&gt;-mwskz  TRANSPORTING NO FIELDS.
      IF sy-subrc = 0.
        CLEAR: &lt;tlist&gt;-gabtx,
               &lt;tlist&gt;-tamtx.
      ENDIF.

    ENDLOOP.


    &quot;: exclude empty elements?
    &quot;        no here, in request
    &quot;        via soapin-CONTROLLER -&gt; FIELD = element-name,
    &quot;                              -&gt; VALUE =
    &quot;                                         _  Standard Procedure
    &quot;                                         1	Initial Value
    &quot;                                         2	xsi:nil=&quot;true&quot;
    &quot;                                         3	Hide

    &quot;special logic: adjustment &amp; blank invoice
    &quot;from Tuan:   Issue new Zero amount invoice: amount = 0 and mathue = -1, 0, 5,10 as usual --&gt; Inv-ref = &quot;&quot;
    &quot;             Adjust an invoice with blank amount: amount = 0 and mathue = -3 --&gt; Inv-ref &lt;&gt; &apos;&apos;&quot;
    &quot;-&gt;sap-logic: spGLind= Z &amp;&amp; inv-ref==&apos;&apos; --&gt; amount = 0, mathue as-is (from vatco mapping table)
    &quot;             spGLind= Z &amp;&amp; inv-ref&lt;&gt;&apos;&apos; --&gt; amount = 0, mathue forced to -3= when no amount invoice

    IF me-&gt;bsegd-umskz = &apos;Z&apos;      AND
       me-&gt;bsegd-rebzg IS INITIAL    .
      &quot;clear amounts
      CLEAR: &lt;ws1r&gt;-tamnt,
             &lt;ws1r&gt;-vamnt,
             &lt;ws1r&gt;-tpmnt.
      LOOP AT me-&gt;ws1r-dlistt ASSIGNING &lt;dlist&gt;.
        CLEAR: &lt;dlist&gt;-abvat,
               &lt;dlist&gt;-aavat.
        &quot;&lt;dlist&gt;-acvat mathue: derived as-is
      ENDLOOP.
      LOOP AT me-&gt;ws1r-tlistt ASSIGNING &lt;tlist&gt;.
        CLEAR: &lt;tlist&gt;-gabtx,
               &lt;tlist&gt;-tamtx.
        &quot;&lt;tlist&gt;-vatco mathue: derived as-is
      ENDLOOP.
    ENDIF.

    IF me-&gt;bsegd-umskz = &apos;Z&apos;          AND
       me-&gt;bsegd-rebzg IS NOT INITIAL    .
      &quot;clear amounts
      CLEAR: &lt;ws1r&gt;-tamnt,
             &lt;ws1r&gt;-vamnt,
             &lt;ws1r&gt;-tpmnt.
      LOOP AT me-&gt;ws1r-dlistt ASSIGNING &lt;dlist&gt;.
        CLEAR: &lt;dlist&gt;-abvat,
               &lt;dlist&gt;-aavat.
        &quot;force mathue= -3
        &lt;dlist&gt;-acvat = &apos;-3&apos;. &quot;-3= when no amount invoice
      ENDLOOP.
      LOOP AT me-&gt;ws1r-tlistt ASSIGNING &lt;tlist&gt;.
        CLEAR: &lt;tlist&gt;-gabtx,
               &lt;tlist&gt;-tamtx.
        &quot;force mathue= -3
        &lt;tlist&gt;-vatco = &apos;-3&apos;. &quot;-3= when no amount invoice
      ENDLOOP.
    ENDIF.












  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZVNEINV_IFPE" CMPNAME="PREP_WS2R" VERSION="1" LANGU="E" DESCRIPT="prepare request: cancel" EXPOSURE="0" STATE="1" EDITORDER="6 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD prep_ws2r.

    &quot;for cancel
    CHECK: me-&gt;ifle-wstyp = &apos;C&apos;. &quot;I= cancel

    ASSIGN me-&gt;ws2r TO FIELD-SYMBOL(&lt;ws2r&gt;).

    &quot;tax id: from config
    MOVE: me-&gt;c000-taxid TO &lt;ws2r&gt;-taxid.

    &quot;hoadon_loai/invoice form: constant 7 &apos;Removed invoice&apos;
    &lt;ws2r&gt;-icform = &apos;7&apos;.

    &quot;form/template/code: from fi-doc head text (see config ZVNFIAR_CBKTXT)
    &quot;                                           eg= 01GTKT0/001**AA/20E
    &lt;ws2r&gt;-iform = me-&gt;bkpf-bktxt+0(6)  .       &quot; = 01GTKT
    &lt;ws2r&gt;-templ = me-&gt;bkpf-bktxt+0(11) .       &quot; = 01GTKT0/001
    &lt;ws2r&gt;-icode = me-&gt;bkpf-bktxt+13(6) .       &quot; =              AA/20E


    &quot;ma_hoadon/invoice number: full key ccode/docno/year
    &quot;&lt;ws2r&gt;-invno = |{ me-&gt;bkpf-bukrs }/{ me-&gt;bkpf-belnr }/{ me-&gt;bkpf-gjahr }|.
    &quot;..of original invoice (not the reversal)
    &lt;ws2r&gt;-invno = |{ me-&gt;bkpf-bukrs }/{ me-&gt;bkpf-stblg }/{ me-&gt;bkpf-stjah }|.

    &quot;hoadon_goc/original invoice number: for adjustment, inv reference
    &quot;..CyberLotus requires ma_hoadon = hoadon_goc, bit is weird
    &lt;ws2r&gt;-oinvn = &lt;ws2r&gt;-invno.

    &quot;ngaylap/issuing date: posting date &amp; time YYYY-MM-DD hh:mm:ss
    &quot;                      (the CPUTM is actually updated after posting)
    &lt;ws2r&gt;-isdat = |{ me-&gt;bkpf-budat DATE = ISO  } { me-&gt;bkpf-cputm TIME = ISO  }|.


    &quot;nguoilap/invoice issuing person
    &lt;ws2r&gt;-isprs = me-&gt;uadr-fullname.

    &quot;lydo_huy/cancel reason
    &lt;ws2r&gt;-rcanc = me-&gt;conv_text( iv_tdid = &apos;VN01&apos; ). &quot;VN01= Adjustment reason (used as Cancel Reason)




  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZVNEINV_IFPE" CMPNAME="PROCESS" VERSION="1" LANGU="E" DESCRIPT="process" EXPOSURE="2" STATE="1" EDITORDER="2 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD process.

    me-&gt;data_read( ).     &quot;get fi-document/customer/&amp;tc

    me-&gt;conf_read( ).     &quot;read customizing

    me-&gt;ifle_init( ).     &quot;init IFLE (determine invoice/cancel)

    me-&gt;data_enqu( ).     &quot;data: enqueue

    IF me-&gt;ifle-wssta NE &apos;E&apos;. &quot;(no error from enqueue)

      me-&gt;prep_ws1r( ).   &quot;prepare ws-request invoice (from ISEL to WS1R)
      me-&gt;prep_ws2r( ).   &quot;prepare ws-request cancel  (from ISEL to WS2R)

      me-&gt;proc_wscl( ).   &quot;call ws

      me-&gt;upda_fdoc( ).   &quot;update fin-doc

      me-&gt;ifle_prep( ).   &quot;prepare ifli-log from ws-response

    ENDIF.

    me-&gt;ifle_save( ).     &quot;save ifli-log to database

    me-&gt;data_dequ( ).      &quot;data: enqueue

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZVNEINV_IFPE" CMPNAME="PROC_WSCL" VERSION="1" LANGU="E" DESCRIPT="process into ws-call" EXPOSURE="0" STATE="1" EDITORDER="7 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD proc_wscl.

    &quot;prepared request -&gt; call ws
    CHECK me-&gt;xtst IS INITIAL. &quot;(no ws-call in test mode)

    CASE me-&gt;ifle-wstyp.
      WHEN &apos;I&apos;. me-&gt;proc_wscl_ws1r( ).  &quot;ws-invoice
      WHEN &apos;C&apos;. me-&gt;proc_wscl_ws2r( ).  &quot;ws-cancel
    ENDCASE.



  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZVNEINV_IFPE" CMPNAME="PROC_WSCL_WS1R" VERSION="1" LANGU="E" DESCRIPT="process into ws-call: invoice" EXPOSURE="0" STATE="1" EDITORDER="8 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD proc_wscl_ws1r.


    &quot; data from webservice / proxy
    DATA: lo_proxy   TYPE REF TO   zvneinv_co_osb_ws,               &quot;ref to proxy object
          ls_soapin  TYPE         zvneinv_submit_einvoice_reque2,   &quot;input structure
          ls_soapout TYPE         zvneinv_submit_einvoice_respo5,   &quot;output structure
          lo_fault   TYPE REF TO  cx_ai_system_fault.

    &quot; create proxy object
    IF me-&gt;port1 IS INITIAL.
      TRY.
          CREATE OBJECT lo_proxy.
        CATCH cx_ai_system_fault.
      ENDTRY.
    ELSE.
      TRY.
          CREATE OBJECT lo_proxy EXPORTING logical_port_name = me-&gt;port1.
        CATCH cx_ai_system_fault.
      ENDTRY.
    ENDIF.


    &quot; populate input / request (map fm to proxy)
    CLEAR: ls_soapin,
           ls_soapout.

    CHECK me-&gt;ws1r IS NOT INITIAL.

    ASSIGN me-&gt;ws1r  TO FIELD-SYMBOL(&lt;req&gt;).
    ASSIGN ls_soapin TO FIELD-SYMBOL(&lt;soapin&gt;).
    &quot;invoice
    MOVE: &lt;req&gt;-taxid           TO &lt;soapin&gt;-doanhnghiep_mst,
          &lt;req&gt;-iform           TO &lt;soapin&gt;-loaihoadon_ma,
          &lt;req&gt;-templ           TO &lt;soapin&gt;-mauso,
          &lt;req&gt;-icode           TO &lt;soapin&gt;-kyhieu,
          &lt;req&gt;-nnote           TO &lt;soapin&gt;-sophieu,
          &lt;req&gt;-invno           TO &lt;soapin&gt;-ma_hoadon,
          &lt;req&gt;-oinvn           TO &lt;soapin&gt;-hoadon_goc,
          &lt;req&gt;-isdat           TO &lt;soapin&gt;-ngaylap,
          &lt;req&gt;-btxin           TO &lt;soapin&gt;-dnmua_mst,
          &lt;req&gt;-bonam           TO &lt;soapin&gt;-dnmua_ten,
          &lt;req&gt;-bname           TO &lt;soapin&gt;-dnmua_tennguoimua,
          &lt;req&gt;-baddr           TO &lt;soapin&gt;-dnmua_diachi,
          &lt;req&gt;-bphon           TO &lt;soapin&gt;-dnmua_sdt,
          &lt;req&gt;-email           TO &lt;soapin&gt;-dnmua_email,
          &lt;req&gt;-ptype           TO &lt;soapin&gt;-thanhtoan_phuongthuc,
          &lt;req&gt;-pterm           TO &lt;soapin&gt;-thanhtoan_phuongthuc_ten,
          &lt;req&gt;-bkacc           TO &lt;soapin&gt;-thanhtoan_taikhoan,
          &lt;req&gt;-bknam           TO &lt;soapin&gt;-thanhtoan_nganhang,
          &lt;req&gt;-currc           TO &lt;soapin&gt;-tiente_ma,
          &lt;req&gt;-exchr           TO &lt;soapin&gt;-tygiangoaite,
          &lt;req&gt;-pmdue           TO &lt;soapin&gt;-thanhtoan_thoihan,
          &lt;req&gt;-amdsc           TO &lt;soapin&gt;-tongtien_chietkhau,
          &lt;req&gt;-rmark           TO &lt;soapin&gt;-ghichu,
          &lt;req&gt;-tamnt           TO &lt;soapin&gt;-tongtien_chuavat,
          &lt;req&gt;-vamnt           TO &lt;soapin&gt;-tienthue,
          &lt;req&gt;-tpmnt           TO &lt;soapin&gt;-tongtien_covat,
          &lt;req&gt;-isprs           TO &lt;soapin&gt;-nguoilap,
          &lt;req&gt;-adjrt           TO &lt;soapin&gt;-lydo_dieuchinh,
          &lt;req&gt;-adjcb           TO &lt;soapin&gt;-noidung_truocdieuchinh,
          &lt;req&gt;-adjca           TO &lt;soapin&gt;-noidung_saudieuchinh.

    &quot;handle zeros: from &apos;123-&apos; to &apos;-123&apos;
    CALL FUNCTION &apos;CLOI_PUT_SIGN_IN_FRONT&apos; CHANGING: value = &lt;soapin&gt;-tongtien_chuavat,
                                                     value = &lt;soapin&gt;-tienthue,
                                                     value = &lt;soapin&gt;-tongtien_covat.

    &quot;item list
    LOOP AT &lt;req&gt;-dlistt ASSIGNING FIELD-SYMBOL(&lt;dlist&gt;).
      APPEND INITIAL LINE TO &lt;soapin&gt;-dschitiet ASSIGNING FIELD-SYMBOL(&lt;dschitiet&gt;).
      MOVE: &lt;dlist&gt;-ordno   TO &lt;dschitiet&gt;-stt,
            &lt;dlist&gt;-gdsty   TO &lt;dschitiet&gt;-hanghoa_loai,
            &lt;dlist&gt;-promo   TO &lt;dschitiet&gt;-khuyenmai,
            &lt;dlist&gt;-adjst   TO &lt;dschitiet&gt;-dieuchinh_tanggiam,
            &lt;dlist&gt;-gdsno   TO &lt;dschitiet&gt;-ma,
            &lt;dlist&gt;-gdsnm   TO &lt;dschitiet&gt;-ten,
            &lt;dlist&gt;-unit    TO &lt;dschitiet&gt;-donvitinh,
            &lt;dlist&gt;-qtty    TO &lt;dschitiet&gt;-soluong,
            &lt;dlist&gt;-price   TO &lt;dschitiet&gt;-dongia,
            &lt;dlist&gt;-dscpr   TO &lt;dschitiet&gt;-phantram_chietkhau,
            &lt;dlist&gt;-dscam   TO &lt;dschitiet&gt;-tongtien_chietkhau,
            &lt;dlist&gt;-abvat   TO &lt;dschitiet&gt;-tongtien_chuathue,
            &lt;dlist&gt;-acvat   TO &lt;dschitiet&gt;-mathue,
            &lt;dlist&gt;-aavat   TO &lt;dschitiet&gt;-tongtien_cothue.

      &quot;handle zeros: from &apos;123-&apos; to &apos;-123&apos;
      CALL FUNCTION &apos;CLOI_PUT_SIGN_IN_FRONT&apos; CHANGING: value =  &lt;dschitiet&gt;-tongtien_chuathue,
                                                       value =  &lt;dschitiet&gt;-tongtien_cothue.

    ENDLOOP.

    &quot;tax list
    LOOP AT &lt;req&gt;-tlistt ASSIGNING FIELD-SYMBOL(&lt;tlist&gt;).
      APPEND INITIAL LINE TO &lt;soapin&gt;-dsthuesuat ASSIGNING FIELD-SYMBOL(&lt;dsthuesuat&gt;).
      MOVE: &lt;tlist&gt;-vatco TO &lt;dsthuesuat&gt;-mathue,
            &lt;tlist&gt;-gabtx TO &lt;dsthuesuat&gt;-tongtien_chiuthue,
            &lt;tlist&gt;-tamtx TO &lt;dsthuesuat&gt;-tongtien_thue,
            &lt;tlist&gt;-adjst TO &lt;dsthuesuat&gt;-dieuchinh_tanggiam.

      &quot;handle zeros: from &apos;123-&apos; to &apos;-123&apos;
      CALL FUNCTION &apos;CLOI_PUT_SIGN_IN_FRONT&apos; CHANGING: value =  &lt;dsthuesuat&gt;-tongtien_chiuthue,
                                                       value =  &lt;dsthuesuat&gt;-tongtien_thue.

    ENDLOOP.

    &quot;exclude empty elements
    &quot;        via soapin-CONTROLLER -&gt; FIELD = element-name,
    &quot;                              -&gt; VALUE = _  Standard Procedure
    &quot;                                         1	Initial Value
    &quot;                                         2	xsi:nil=&quot;true&quot;
    &quot;                                         3	Hide

    &quot;no hoadon_goc/Original Invoice Number -&gt; no adjustment
    IF &lt;soapin&gt;-hoadon_goc IS INITIAL.
      LOOP AT &lt;soapin&gt;-dschitiet ASSIGNING &lt;dschitiet&gt;.
        APPEND INITIAL LINE TO &lt;dschitiet&gt;-controller ASSIGNING FIELD-SYMBOL(&lt;dcntr&gt;).
        MOVE: &apos;DIEUCHINH_TANGGIAM&apos; TO &lt;dcntr&gt;-field,
              &apos;3&apos;                  TO &lt;dcntr&gt;-value.
      ENDLOOP.
      LOOP AT &lt;soapin&gt;-dsthuesuat ASSIGNING &lt;dsthuesuat&gt;.
        APPEND INITIAL LINE TO &lt;dsthuesuat&gt;-controller ASSIGNING FIELD-SYMBOL(&lt;tcntr&gt;).
        MOVE: &apos;DIEUCHINH_TANGGIAM&apos; TO &lt;tcntr&gt;-field,
              &apos;3&apos;                  TO &lt;tcntr&gt;-value.
      ENDLOOP.
    ENDIF.

    &quot;currency= VND -&gt; no exchange rate
    IF &lt;soapin&gt;-tiente_ma = &apos;VND&apos;.
      APPEND INITIAL LINE TO &lt;soapin&gt;-controller ASSIGNING FIELD-SYMBOL(&lt;scntr&gt;).
      MOVE: &apos;TYGIANGOAITE&apos;   TO &lt;scntr&gt;-field,
            sai_ctrl_none    TO &lt;scntr&gt;-value.
    ENDIF.

    &quot;again items
    LOOP AT &lt;soapin&gt;-dschitiet ASSIGNING &lt;dschitiet&gt;.
      &quot;no dongia/price,
      APPEND INITIAL LINE TO &lt;dschitiet&gt;-controller ASSIGNING &lt;dcntr&gt;.
      MOVE: &apos;DONGIA&apos;             TO &lt;dcntr&gt;-field,
            &apos;3&apos;                  TO &lt;dcntr&gt;-value.
      &quot;no donvitinh/Unit
      APPEND INITIAL LINE TO &lt;dschitiet&gt;-controller ASSIGNING &lt;dcntr&gt;.
      MOVE: &apos;DONVITINH&apos;          TO &lt;dcntr&gt;-field,
            &apos;3&apos;                  TO &lt;dcntr&gt;-value.
      &quot;no soluong/Quantity
      APPEND INITIAL LINE TO &lt;dschitiet&gt;-controller ASSIGNING &lt;dcntr&gt;.
      MOVE: &apos;SOLUONG&apos;            TO &lt;dcntr&gt;-field,
            &apos;3&apos;                  TO &lt;dcntr&gt;-value.
    ENDLOOP.



    &quot; call webservice / consumer
    TRY.

        &quot;set ext-xml-handling, so -CONTROLLER fields are working
        DATA: lo_prot TYPE REF TO if_wsprotocol_payload.
        lo_prot ?= lo_proxy-&gt;get_protocol( protocol_name = if_wsprotocol=&gt;payload ).
        lo_prot-&gt;set_extended_xml_handling(  extended_xml_handling = &apos;X&apos; ).

        lo_proxy-&gt;submit_einvoice(
          EXPORTING
            submit_einvoice_request  = ls_soapin
          IMPORTING
            submit_einvoice_response = ls_soapout
               ).
      CATCH cx_ai_system_fault INTO lo_fault.
        WRITE: / &apos;error in call: &apos;, lo_fault-&gt;errortext.
    ENDTRY.

    &quot;map and summarise response
    ASSIGN ls_soapout TO FIELD-SYMBOL(&lt;soapout&gt;).
    ASSIGN me-&gt;ws1s   TO FIELD-SYMBOL(&lt;rsp&gt;).
    MOVE: &lt;soapout&gt;-result-maketqua       TO &lt;rsp&gt;-trres,
          &lt;soapout&gt;-result-motaketqua     TO &lt;rsp&gt;-descr,
          &lt;soapout&gt;-result-magiaodich     TO &lt;rsp&gt;-tcodr,
          &lt;soapout&gt;-result-mauso          TO &lt;rsp&gt;-invtc,
          &lt;soapout&gt;-result-kyhieu         TO &lt;rsp&gt;-invcd,
          &lt;soapout&gt;-result-sohoadon       TO &lt;rsp&gt;-invno,
          &lt;soapout&gt;-result-ngayky         TO &lt;rsp&gt;-signd,
          &lt;soapout&gt;-result-link_tracuu    TO &lt;rsp&gt;-islnk.
    MOVE: &lt;soapout&gt;-target_url            TO &lt;rsp&gt;-target_url,
          &lt;soapout&gt;-success               TO &lt;rsp&gt;-success,
          &lt;soapout&gt;-error-code            TO &lt;rsp&gt;-error_code,
          &lt;soapout&gt;-error-message         TO &lt;rsp&gt;-error_message,
          &lt;soapout&gt;-error-details         TO &lt;rsp&gt;-error_details.


    &quot; special case: errors from WS calls
    IF lo_fault IS NOT INITIAL.
      IF lo_fault-&gt;errortext IS NOT INITIAL.
        &quot; build response from request
        MOVE: lo_fault-&gt;errortext     TO &lt;rsp&gt;-error_message.
      ENDIF.
    ENDIF.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZVNEINV_IFPE" CMPNAME="PROC_WSCL_WS2R" VERSION="1" LANGU="E" DESCRIPT="process into ws-call: cancel" EXPOSURE="0" STATE="1" EDITORDER="9 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD proc_wscl_ws2r.

    &quot; data from webservice / proxy
    DATA: lo_proxy   TYPE REF TO  zvneinv_co_osb_ws,                &quot;ref to proxy object
          ls_soapin  TYPE         zvneinv_cancel_einvoice_reques,   &quot;input structure
          ls_soapout TYPE         zvneinv_cancel_einvoice_respo5,   &quot;output structure
          lo_fault   TYPE REF TO  cx_ai_system_fault.

    &quot; create proxy object
    IF me-&gt;port2 IS INITIAL.
      TRY.
          CREATE OBJECT lo_proxy.
        CATCH cx_ai_system_fault.
      ENDTRY.
    ELSE.
      TRY.
          CREATE OBJECT lo_proxy EXPORTING logical_port_name = me-&gt;port2.
        CATCH cx_ai_system_fault.
      ENDTRY.
    ENDIF.


    &quot; populate input / request (map fm to proxy)
    CLEAR: ls_soapin,
           ls_soapout.

    CHECK me-&gt;ws2r IS NOT INITIAL.

    ASSIGN me-&gt;ws2r  TO FIELD-SYMBOL(&lt;req&gt;).
    ASSIGN ls_soapin TO FIELD-SYMBOL(&lt;soapin&gt;).
    &quot;invoice
    MOVE: &lt;req&gt;-taxid           TO &lt;soapin&gt;-doanhnghiep_mst,
          &lt;req&gt;-icform          TO &lt;soapin&gt;-hoadon_loai,
          &lt;req&gt;-iform           TO &lt;soapin&gt;-loaihoadon_ma,
          &lt;req&gt;-templ           TO &lt;soapin&gt;-mauso,
          &lt;req&gt;-icode           TO &lt;soapin&gt;-kyhieu,
          &lt;req&gt;-invno           TO &lt;soapin&gt;-ma_hoadon,
          &lt;req&gt;-oinvn           TO &lt;soapin&gt;-hoadon_goc,
          &lt;req&gt;-isdat           TO &lt;soapin&gt;-ngaylap,
          &lt;req&gt;-isprs           TO &lt;soapin&gt;-nguoilap,
          &lt;req&gt;-rcanc           TO &lt;soapin&gt;-lydo_huy.
**          &lt;req&gt;-adjcb           TO &lt;soapin&gt;-noidung_truocdieuchinh,
**          &lt;req&gt;-adjca           TO &lt;soapin&gt;-noidung_saudieuchinh.


    &quot; call webservice / consumer
    TRY.
        lo_proxy-&gt;cancel_einvoice(
          EXPORTING
            cancel_einvoice_request  = ls_soapin
          IMPORTING
            cancel_einvoice_response = ls_soapout
               ).
      CATCH cx_ai_system_fault INTO lo_fault.
        WRITE: / &apos;error in call: &apos;, lo_fault-&gt;errortext.
    ENDTRY.

    &quot;map and summarise response
    ASSIGN ls_soapout TO FIELD-SYMBOL(&lt;soapout&gt;).
    ASSIGN me-&gt;ws2s   TO FIELD-SYMBOL(&lt;rsp&gt;).
    MOVE: &lt;soapout&gt;-result-maketqua       TO &lt;rsp&gt;-trres,
          &lt;soapout&gt;-result-motaketqua     TO &lt;rsp&gt;-descr,
          &lt;soapout&gt;-result-magiaodich     TO &lt;rsp&gt;-tcodr,
          &lt;soapout&gt;-result-mauso          TO &lt;rsp&gt;-invtc,
          &lt;soapout&gt;-result-kyhieu         TO &lt;rsp&gt;-invcd,
          &lt;soapout&gt;-result-sohoadon       TO &lt;rsp&gt;-invno,
          &lt;soapout&gt;-result-ngayky         TO &lt;rsp&gt;-signd,
          &lt;soapout&gt;-result-link_tracuu    TO &lt;rsp&gt;-islnk.
    MOVE: &lt;soapout&gt;-target_url            TO &lt;rsp&gt;-target_url,
          &lt;soapout&gt;-success               TO &lt;rsp&gt;-success.
*          &lt;soapout&gt;-error_message         TO &lt;rsp&gt;-error_message,
*          &lt;soapout&gt;-error_details         TO &lt;rsp&gt;-error_details.


    &quot; special case: errors from WS calls
    IF lo_fault IS NOT INITIAL.
      IF lo_fault-&gt;errortext IS NOT INITIAL.
        &quot; build response from request
        MOVE: lo_fault-&gt;errortext     TO &lt;rsp&gt;-error_message.
      ENDIF.
    ENDIF.


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZVNEINV_IFPE" CMPNAME="UPDA_FDOC" VERSION="1" LANGU="E" DESCRIPT="update fin-doc" EXPOSURE="0" STATE="1" EDITORDER="10 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD upda_fdoc.

    &quot;not for test mode
    CHECK me-&gt;xtst EQ &apos; &apos;.

    CASE me-&gt;ifle-wstyp.

      WHEN &apos;I&apos;. &quot;I= invoice
        &quot;for invoice: both RIno &amp; url
        me-&gt;upda_fdoc_rino( ).      &quot;update fin-doc: RedInvoiceNo -&gt; bkpf-xblnr
        me-&gt;upda_fdoc_urll( ).      &quot;update: url-link -&gt; gos-att-link

      WHEN &apos;C&apos;. &quot;C= cancel
        &quot;for cancel:  url only (no new RIno)
        me-&gt;upda_fdoc_urll( ).      &quot;update: url-link -&gt; gos-att-link
    ENDCASE.



  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZVNEINV_IFPE" CMPNAME="UPDA_FDOC_RINO" VERSION="1" LANGU="E" DESCRIPT="update fin-doc: RedInvoiceNo -&gt; bkpf-xblnr" EXPOSURE="0" STATE="1" EDITORDER="11 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD upda_fdoc_rino.

    &quot;update: red-inv-number -&gt; bkpf-xblnr
    CHECK ws1s-invno IS NOT INITIAL.

    DATA: lt_accchg  TYPE STANDARD TABLE OF accchg.
    APPEND INITIAL LINE TO lt_accchg ASSIGNING FIELD-SYMBOL(&lt;accchg&gt;).
    MOVE: &apos;XBLNR&apos;    TO &lt;accchg&gt;-fdname,
          ws1s-invno TO &lt;accchg&gt;-newval.

    CALL FUNCTION &apos;FI_DOCUMENT_CHANGE&apos;
      EXPORTING
        i_bukrs              = bkpf-bukrs
        i_belnr              = bkpf-belnr
        i_gjahr              = bkpf-gjahr
      TABLES
        t_accchg             = lt_accchg
      EXCEPTIONS
        no_reference         = 1
        no_document          = 2
        many_documents       = 3
        wrong_input          = 4
        overwrite_creditcard = 5
        OTHERS               = 6.
    IF sy-subrc &lt;&gt; 0. MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4. ENDIF.

    &quot;(note: commit in ~SAVE_IFLE)


  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZVNEINV_IFPE" CMPNAME="UPDA_FDOC_URLL" VERSION="1" LANGU="E" DESCRIPT="update fin-doc: url-link -&gt; gos-attachment" EXPOSURE="0" STATE="1" EDITORDER="12 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD upda_fdoc_urll.

    CASE me-&gt;ifle-wstyp.
      WHEN &apos;I&apos;. DATA(lv_islnk) = ws1s-islnk. &quot;I= invoice
      WHEN &apos;C&apos;.      lv_islnk  = ws2s-islnk. &quot;C= cancel
    ENDCASE.

    &quot;update fin-doc: url-link -&gt; gos-attachment
    CHECK lv_islnk IS NOT INITIAL.

    &quot;§0 own logsys
    DATA: lv_logsy TYPE tbdls-logsys.
    CALL FUNCTION &apos;OWN_LOGICAL_SYSTEM_GET&apos;
      IMPORTING
        own_logical_system             = lv_logsy
      EXCEPTIONS
        own_logical_system_not_defined = 1
        OTHERS                         = 2.
    IF sy-subrc &lt;&gt; 0. MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4. ENDIF.

    &quot;§1 get folder-id
    DATA: ls_fldid TYPE soodk.
    CALL FUNCTION &apos;SO_FOLDER_ROOT_ID_GET&apos;
      EXPORTING
        region                = &apos;B&apos;
      IMPORTING
        folder_id             = ls_fldid
      EXCEPTIONS
        communication_failure = 1
        owner_not_exist       = 2
        system_failure        = 3
        x_error               = 4
        OTHERS                = 5.
    IF sy-subrc &lt;&gt; 0. MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4. ENDIF.

    &quot;§2 insert object (folder-with-url)
    DATA: ls_objda TYPE sood1,
          ls_objid TYPE soodk,
          lt_ocont TYPE TABLE OF soli,
          lt_ohead TYPE TABLE OF soli.
    MOVE: sy-langu    TO ls_objda-objla,
          lv_islnk    TO ls_objda-objdes,
          &apos;O&apos;         TO ls_objda-objsns. &quot;O= Standard
    APPEND INITIAL LINE TO lt_ocont ASSIGNING FIELD-SYMBOL(&lt;ocont&gt;).
    CONCATENATE &apos;&amp;KEY&amp;&apos; lv_islnk INTO &lt;ocont&gt;.
    CALL FUNCTION &apos;SO_OBJECT_INSERT&apos;
      EXPORTING
        folder_id                  = ls_fldid
        object_hd_change           = ls_objda
        object_type                = &apos;URL&apos;
      IMPORTING
        object_id                  = ls_objid
      TABLES
        objcont                    = lt_ocont
        objhead                    = lt_ohead
      EXCEPTIONS
        active_user_not_exist      = 1
        communication_failure      = 2
        component_not_available    = 3
        dl_name_exist              = 4
        folder_not_exist           = 5
        folder_no_authorization    = 6
        object_type_not_exist      = 7
        operation_no_authorization = 8
        owner_not_exist            = 9
        parameter_error            = 10
        substitute_not_active      = 11
        substitute_not_defined     = 12
        system_failure             = 13
        x_error                    = 14
        OTHERS                     = 15.
    ASSERT sy-subrc = 0.

    &quot;§3 create binary relation (folder-with-url to gos-object)
    DATA: ls_rolea TYPE borident,
          ls_roleb TYPE borident.
    CONCATENATE me-&gt;bkpf-bukrs me-&gt;bkpf-belnr me-&gt;bkpf-gjahr INTO ls_rolea-objkey RESPECTING BLANKS.
    ls_rolea-objtype = &apos;BKPF&apos;.
    ls_rolea-logsys  = lv_logsy.
    CONCATENATE ls_fldid ls_objid INTO ls_roleb-objkey RESPECTING BLANKS.
    ls_roleb-objtype = &apos;MESSAGE&apos;.
    ls_roleb-logsys  = lv_logsy.

    CALL FUNCTION &apos;BINARY_RELATION_CREATE&apos;
      EXPORTING
        obj_rolea      = ls_rolea
        obj_roleb      = ls_roleb
        relationtype   = &apos;URL&apos;
      EXCEPTIONS
        no_model       = 1
        internal_error = 2
        unknown        = 3
        OTHERS         = 4.
    IF sy-subrc &lt;&gt; 0. MESSAGE ID sy-msgid TYPE sy-msgty NUMBER sy-msgno WITH sy-msgv1 sy-msgv2 sy-msgv3 sy-msgv4. ENDIF.

    &quot;(note: commit in ~SAVE_IFLE)

  ENDMETHOD.</source>
  </method>
